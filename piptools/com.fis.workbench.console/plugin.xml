<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin    
   id="com.fis.workbench.console"
   name="Workbench Console"
   version="3.3.1"
   provider-name="Fidelity National Information Services, Inc."
   class="com.fis.workbench.console.Activator">
   
   <runtime>
      <library name="workbenchconsole.jar">
         <export
               name="*">
         </export>
      </library>
   </runtime>
 
   <requires>
	  <import plugin="org.eclipse.core.resources" />
	  <import plugin="org.eclipse.core.runtime" />
      <import plugin="org.eclipse.ui" />
      <import plugin="org.eclipse.ui.console"/> 
      <import plugin="org.eclipse.jface.text"/>      
   </requires>
   
    <extension
         point="org.eclipse.ui.startup">
      <startup class="com.fis.workbench.console.WorkbenchConsoleStartup"/>
   </extension>

</plugin>
