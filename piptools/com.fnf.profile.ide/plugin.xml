<?eclipse version="3.0"?>
<plugin
   id="com.fnf.profile.ide"
   name="%pluginName"
   version="3.3.2"
   provider-name="%providerName"
   class="com.fnf.profile.ide.IdePlugin">

   <runtime>
      <library name="ide.jar">
         <export name="*"/>
      </library>
      <library name="gnu-regexp-1.1.4.jar"/>
      <library name="xercesImpl.jar"/>
      <library name="ScJDBC.jar">
         <export
               name="*">
         </export>
      </library>
   </runtime>
   <requires>
   <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="com.fnf.studio"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.jdt.ui"/>
      
   </requires>


<!-- =============================================== -->
<!-- Perspectives for all profile extensions         -->
<!-- =============================================== -->
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="%perspectiveName"
            icon="icons/basic/fid_prof_perspective.gif"
            class="com.fnf.FidelityProfilePerspective"
            id="com.fnf.profile.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.fnf.profile.perspective">
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.folder">
         </newWizardShortcut>
         <newWizardShortcut
               id="org.eclipse.ui.wizards.new.file">
         </newWizardShortcut>
         <newWizardShortcut
               id="com.fnf.profile.ide.wizards.NewProfileProjectWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="com.fnf.profile.ide.wizards.GetFromProfileWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="com.fnf.profile.ide.wizards.NewFileWizard">
         </newWizardShortcut>
         <viewShortcut
               id="org.eclipse.ui.externaltools.AntView">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.BookmarkNavigator">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.pde.runtime.LogView">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.ResourceNavigator">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.ContentOutline">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.PropertySheet">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.TaskList">
         </viewShortcut>
         <viewShortcut
               id="org.eclipse.ui.views.ProblemView">
         </viewShortcut>
         <viewShortcut
               id="com.fnf.studio.views.FidelityConsoleView">
         </viewShortcut>
         <view
               id="org.eclipse.ui.views.ProblemView"
               relationship="stack"
               relative="com.fnf.studio.views.FidelityConsoleView" />
         <perspectiveShortcut
               id="com.fnf.profile.dba.perspective">
         </perspectiveShortcut> 
  
      </perspectiveExtension>

   </extension>
<!-- =============================================== -->
<!-- Editors for all profile extensions         -->
<!-- =============================================== -->
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%editorName"
            icon="icons/basic/editor.gif"
            extensions="proc, batch, trig, psl"
            contributorClass="com.fnf.profile.ide.editors.psleditor.PSLActionContributor"
            class="com.fnf.profile.ide.editors.psleditor.PSLEditor"
            id="com.fnf.profile.ide.editors.PSLEditor">
      </editor>
   </extension>
<!-- =============================================== -->
<!-- properties menus for all profile extensions     -->
<!-- =============================================== -->
   <extension
         id="com.fnf.profile.ide.ScProfileConnection"
         name="%connectionName"
         point="org.eclipse.ui.propertyPages">
      <page
            category="com.fnf.profile.ide.page1"
            class="com.fnf.profile.ide.properties.ProfileConnPropertyPage"
            id="com.fnf.profile.ide.properties.ProfileConnPropertyPage"
            name="%connectionPageName"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IProject">
      </page>
      <page
            category="com.fnf.profile.ide.page1"
            class="com.fnf.profile.ide.properties.PSLPropertiesPage"
            id="com.fnf.profile.ide.properties.PSLPropertiesPage"
            name="%referencesPageName"
            nameFilter="*"
            objectClass="org.eclipse.core.resources.IProject">
      </page>
      <page
            class="com.fnf.profile.ide.preferences.WorkbenchPropertyPage1"
            id="com.fnf.profile.ide.page1"
            name="FIS Profile">
      </page>
   </extension>

