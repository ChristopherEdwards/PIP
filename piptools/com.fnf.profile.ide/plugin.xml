<!--   				New wizards  			         -->
<!-- =============================================== -->
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%resourcesCategory"
            id="com.fnf.profile.ide.category">
      </category>
      <wizard
            name="%getResourceWizardName"
            icon="icons/basic/fid_prof_get_small.gif"
            category="com.fnf.profile.ide.category"
            class="com.fnf.profile.ide.wizards.GetFromProfileWizard"
            id="com.fnf.profile.ide.wizards.GetFromProfileWizard">
      </wizard>
      <wizard
            name="%newResourceWizardName"
            icon="icons/basic/fid_prof_new_small.gif"
            category="com.fnf.profile.ide.category"
            class="com.fnf.profile.ide.wizards.NewFileWizard"
            id="com.fnf.profile.ide.wizards.NewFileWizard">
      </wizard>
      <wizard
            name="%newProjectWizardName"
            icon="icons/basic/fid_new_prof_proj.gif"
            category="com.fnf.profile.ide.category"
