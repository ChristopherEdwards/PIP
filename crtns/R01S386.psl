R01S386	// SCA419 - ACH Inclearing Support Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:57 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String POSTDATE
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"
	type String VIN2="ALL"
	type Date VIN3

	set RID="SCA419"
	set RN="ACH Inclearing Support Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[ACH2]COID|[ACH]:NOVAL||D EXT^DBSQRY||T|Company ID|||||"
	set %TAB("VIN2")="|255||[ACH2]PTYPE|[ACH2]PTYPE:DISTINCT:NOVAL||D EXT^DBSQRY||T|Payment Type|||||"
	set %TAB("VIN3")="|10||[ACH2]DATNP|||if X set POSTDATE=X||D|Next Posting Date|||||"

	set %READ="IO/REQ,VIN1#0,VIN2#0,VIN3#1,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String POSTDATE
	type public Date VIN3
	type public String VIN1,VIN2
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,COID,CONAM,PTYPE,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA419",RN="ACH Inclearing Support Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA419","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[ACH2]ACHTC#38#2,[ACH2]TAMT#42#14,[ACH2]INDID#58#15,[ACH2]INDNAM#75#22,[ACH2]PTYPE#99#10,[ACH1]CED#111#10"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[ACH2]COID "_VIN1,SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[ACH2]PTYPE "_VIN2,SEQ=SEQ+1
		set DQQRY(SEQ)="[ACH2]DATNP = "_$$addqts^DBSRWUTL(VIN3),SEQ=SEQ+1
		set DQQRY(SEQ)="[ACH1]COID=[ACH2]COID",SEQ=SEQ+1
		set DQQRY(SEQ)="[ACH1]PTYPE=[ACH2]PTYPE",SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("ACH2,ACH1") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"Company ID"_"|VIN1|"_VIN1.get()
		set VBNRINFO("PROMPTS",2)="WC2|"_"Payment Type"_"|VIN2|"_VIN2.get()
		set VBNRINFO("PROMPTS",3)="WC1|"_"Next Posting Date"_"|VIN3|"_$$DAT^%ZM(VIN3.get(),%MSKD)


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="ACH Inclearing Support Report"
		set VBNRINFO("PGM")="R01S386"
		set VBNRINFO("RID")="SCA419"
		set VBNRINFO("TABLES")="ACH2,ACH1"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,ACH2]COID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,ACH2]PTYPE"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,ACH2]SEQ"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:3 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:1 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"ACH2.COID,ACH2.PTYPE,ACH2.SEQ,ACH2.TAMT,ACH1.CNAME"
	set VSELECT=VSELECT_",ACH2.ACHTC,ACH2.INDID,ACH2.INDNAM,ACH1.CED"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"ACH2,ACH1",VWHERE,"ACH2.COID,ACH2.PTYPE,ACH2.SEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9
	set vc1=V.piece($C(9),1)			// ACH2.COID
	set vc2=V.piece($C(9),2)			// ACH2.PTYPE
	set vc3=V.piece($C(9),3)			// ACH2.SEQ
	set vc4=V.piece($C(9),4)			// ACH2.TAMT
	set vc5=V.piece($C(9),5)			// ACH1.CNAME
	set vc6=V.piece($C(9),6)			// ACH2.ACHTC
	set vc7=V.piece($C(9),7)			// ACH2.INDID
	set vc8=V.piece($C(9),8)			// ACH2.INDNAM
	set vc9=V.piece($C(9),9)			// ACH1.CED
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 //Incoming=POSTDATE
 type Date vsysdate,vrundate

 set vsysdate=%SystemDate
 set vrundate=%CurrentDate
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG2 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(3)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3
	set (vb1,vb2,vb3)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do VSUM2 quit:VFMQ  do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc4
	type Public String VSTATS

	set VT(3)=VT(3)+1
	set VT(3,1,1)=VT(3,1,1)+vc4			// @TOT([ACH2]TAMT)
	quit

VHDG2	// Group Header
	type public String %MSKD,%TIM,COID,CONAM,ER,IOSL,POSTDATE,PTYPE,RID,RN,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	quit:vh(2)  set vh(2)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$E(vc1,1,10)
	set VL=VL_$J("",13-VL.length())_$E(vc5,1,16)
	do VOM
	quit

VSUM2	// Summary
	type public String %TIM,COID,I,IOSL,POSTDATE,PTYPE,V,VFMQ,VL,VLC,VO,VOFFLG,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+2>IOSL do VHDG0 quit:VFMQ  set vh(2)=0 do VHDG2 quit:VFMQ

	set VL="                                         "_"--------------"
	do VOM
	set VL="Company:"
	set VL=VL_$J("",9-VL.length())_$E(vc1,1,10)
	set VL=VL_$J("",24-VL.length())_"Count:"
	set V=VT(2)					// @CNT(,T,8)
	set VL=VL_$J("",31-VL.length())_$E(V,1,8)
	set V=VT(2,1,1)					// @TOT([ACH2]TAMT,)
	set VL=VL_$J("",41-VL.length())_$J($FN(V,",",2),14)
	do VOM
	set VL="" do VOM
	quit

VDTL3	// Detail
	type public String %TIM,COID,IOSL,POSTDATE,PTYPE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                                     "_$E(vc6,1,2)
	set VL=VL_$J("",41-VL.length())_$E(vc4,1,14)
	set VL=VL_$J("",57-VL.length())_$E(vc7,1,15)
	set VL=VL_$J("",74-VL.length())_$E(vc8,1,22)
	set VL=VL_$J("",98-VL.length())_$E(vc2,1,10)
	set VL=VL_$J("",110-VL.length())_$E(vc9,1,10)
	do VOM
	quit

VSUM3	// Summary
	type public String %TIM,COID,I,IOSL,POSTDATE,PTYPE,V,VFMQ,VL,VLC,VO,VOFFLG,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                         "_"--------------"
	do VOM
	set VL="   "_"Type: "
	set VL=VL_$J("",9-VL.length())_$E(vc2,1,10)
	set VL=VL_$J("",24-VL.length())_"Count:"
	set V=VT(3)					// @CNT(,T,8)
	set VL=VL_$J("",31-VL.length())_$E(V,1,8)
	set V=VT(3,1,1)					// @TOT([ACH2]TAMT,)
	set VL=VL_$J("",41-VL.length())_$J($FN(V,",",2),14)
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,COID,CONAM,POSTDATE,PTYPE,RID,RN,VL,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="Posting Date: "
	set VL=VL_$J("",14-VL.length())_$J($$DAT^%ZM(POSTDATE),10)
	do VOM
	set VL="" do VOM
	set VL="Company      Name                   Code         Amount  Individual       Name                    Pmt Type    Desc"
	do VOM
	set VL="=================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,COID,POSTDATE,PTYPE,vc1,vc2,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="                                         "_"--------------"
	do VOM
	set VL="  "_"* Grand Totals *      Count: "
	set V=(VT(0)+VT(1)+VT(2)+VT(3))			// @CNT(0,T,8)
	set VL=VL_$J("",31-VL.length())_$E(V,1,8)
	set VL=VL_$J("",39-VL.length())_" "
	set V=0						// @TOT([ACH2]TAMT,0)
	for I=0:1:3 set V=V+VT(I,1,1)
	set VL=VL_$J("",41-VL.length())_$J($FN(V,",",2),14)
	do VOM
	set VL="                                         "_"=============="
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors
