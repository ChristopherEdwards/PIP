	set VL="Extract File"
	set VL=VL_$J("",14-VL.length())_$E(vc2,1,40)
	do VOM
	set VL="" do VOM
	set VL="              "_"Company    Statement   Form     Total        Total        Total"
	do VOM
	set VL="System Date    Code       Form ID     Num   Statements    Accounts  Transactions"
	do VOM
	set VL="================================================================================"
	do VOM

	set VNEWHDR=0
