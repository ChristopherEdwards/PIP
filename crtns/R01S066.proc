	type public String %TIM,FDESC,GRTOTAL,IOSL,SUBCO,SUBEXT,SUBFORM,SUBTJD,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),ZZ,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

