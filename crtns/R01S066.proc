	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String EXTR="ALL"
	type String VIN1="ALL"
	type String VIN3="ALL"
	type String VIN4="ALL"
	type String VIN5="ALL"

	set RID="LETSTMTTOT"
	set RN="Totals for Statements by Extract"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("EXTR")="|60|||LKTBL(||D EXTPP^LETSTCMB(.X,IO)|D SJDPP^LETSTCMB|T|Extract File Name|||||"
	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[LETSTMTTOT]SJD|[LETSTMTTOT]SJD:DISTINCT:NOVAL||S SJD=X D EXT^DBSQRY||T|System Date|||||"
	set %TAB("VIN3")="|255||[LETSTMTTOT]CO|[UTBLCO]:NOVAL||D EXT^DBSQRY||T|Company Code|||||"
	set %TAB("VIN4")="|255||[LETSTMTTOT]FORM|[LETSTMTTOT]FORM:DISTINCT:NOVAL||D EXT^DBSQRY||T|Form ID|||||"
	set %TAB("VIN5")="|255||[LETSTMTTOT]VAR|[LETSTMTTOT]VAR:DISTINCT:NOVAL||D EXT^DBSQRY||T|Variation Number|||||"

	set %READ="IO/REQ,VIN1#0,EXTR#0,VIN3#0,VIN4#0,VIN5#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String VIN1,EXTR,VIN3,VIN4,VIN5
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,FDESC,GRTOTAL,RID,RN,SUBCO,SUBEXT,SUBFORM,SUBTJD,VL,VLOF,VRF(),VSTATS(),ZZ,vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="LETSTMTTOT",RN="Totals for Statements by Extract"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","LETSTMTTOT","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=81")
		write $$CLEARXY^%TRMVT
		}

	do INIT^%ZM()

	set vCOL="[LETSTMTTOT]SJD#1#10,[LETSTMTTOT]CO#15#6,[LETSTMTTOT]FORM#25#12,[LETSTMTTOT]VAR#39#3,[LETSTMTTOT]TOTSTM#44#11,[LETSTMTTOT]TOTCID#57#11,[LETSTMTTOT]TOTTRN#70#11"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[LETSTMTTOT]SJD "_VIN1,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[LETSTMTTOT]CO "_VIN3,SEQ=SEQ+1
		if VIN4.get()="" set VIN4="ALL"
		if VIN4'="ALL" set DQQRY(SEQ)="[LETSTMTTOT]FORM "_VIN4,SEQ=SEQ+1
		if VIN5.get()="" set VIN5="ALL"
		if VIN5'="ALL" set DQQRY(SEQ)="[LETSTMTTOT]VAR "_VIN5,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("LETSTMTTOT") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"System Date"_"|VIN1|"_VIN1.get()
		set VBNRINFO("PROMPTS",3)="WC2|"_"Company Code"_"|VIN3|"_VIN3.get()
		set VBNRINFO("PROMPTS",4)="WC2|"_"Form ID"_"|VIN4|"_VIN4.get()
		set VBNRINFO("PROMPTS",5)="WC2|"_"Variation Number"_"|VIN5|"_VIN5.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Totals for Statements by Extract"
		set VBNRINFO("PGM")="R01S066"
		set VBNRINFO("RID")="LETSTMTTOT"
		set VBNRINFO("TABLES")="LETSTMTTOT"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,LETSTMTTOT]SJD"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,LETSTMTTOT]EXTRACT"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,LETSTMTTOT]CO"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,LETSTMTTOT]FORM"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,LETSTMTTOT]VAR"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:5 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:3 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"LETSTMTTOT.SJD,LETSTMTTOT.EXTRACT,LETSTMTTOT.CO,LE"
	set VSELECT=VSELECT_"TSTMTTOT.FORM,LETSTMTTOT.VAR,LETSTMTTOT.TOTSTM,LET"
	set VSELECT=VSELECT_"STMTTOT.TOTCID,LETSTMTTOT.TOTTRN"

	#ACCEPT DATE=01/31/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"LETSTMTTOT",VWHERE,"LETSTMTTOT.SJD,LETSTMTTOT.EXTRACT,LETSTMTTOT.CO,LETSTMTTOT.FORM,LETSTMTTOT.VAR","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4,vovc5
	set vovc1="",vovc2="",vovc3="",vovc4="",vovc5=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4,vovc5,vc5
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4,vovc5=vc5
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8
	set vc1=V.piece($C(9),1)			// LETSTMTTOT.SJD
	set vc2=V.piece($C(9),2)			// LETSTMTTOT.EXTRACT
	set vc3=V.piece($C(9),3)			// LETSTMTTOT.CO
	set vc4=V.piece($C(9),4)			// LETSTMTTOT.FORM
	set vc5=V.piece($C(9),5)			// LETSTMTTOT.VAR
	set vc6=V.piece($C(9),6)			// LETSTMTTOT.TOTSTM
	set vc7=V.piece($C(9),7)			// LETSTMTTOT.TOTCID
	set vc8=V.piece($C(9),8)			// LETSTMTTOT.TOTTRN
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

 type public Number SUBFORM,SUBCO,SUBEXT,SUBTJD,GRTOTAL,ZZ
 
 set (SUBFORM,SUBCO,SUBEXT,SUBTJD,GRTOTAL)=0
 set ZZ=0
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0,vs(5)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1,vskp(5)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(5).get() do VDTL5 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(5)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4,vc5,vovc5
	type Number vb1,vb2,vb3,vb4,vb5
	set (vb1,vb2,vb3,vb4,vb5)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,vb5=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,vb5=1,VH0=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,vb5=1,VH0=1
	if vb4!(vovc4'=vc4) set vs(5)=0,vh(5)=0,VD(4)=0,vb5=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(5) set vs(5)=1 do VSUM5 quit:VFMQ  do stat^DBSRWUTL(5)
	if 'vs(4) set vs(4)=1 do VSUM4 quit:VFMQ  do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do VSUM2 quit:VFMQ  do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc7,vc6,vc8
	type Public String VSTATS

	set VT(5)=VT(5)+1
	set VT(5,1,1)=VT(5,1,1)+vc7			// @TOT([LETSTMTTOT]TOTCID)
	set VT(5,2,1)=VT(5,2,1)+vc6			// @TOT([LETSTMTTOT]TOTSTM)
	set VT(5,3,1)=VT(5,3,1)+vc8			// @TOT([LETSTMTTOT]TOTTRN)
	quit

VSUM2	// Summary
	type public String %TIM,FDESC,GRTOTAL,I,IOSL,SUBCO,SUBEXT,SUBFORM,SUBTJD,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),ZZ,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                           "_"-----------  -----------  -----------"
	do VOM
