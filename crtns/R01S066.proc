	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,FDESC,GRTOTAL,SUBCO,SUBEXT,SUBFORM,SUBTJD,VPN,ZZ,vc1,vc2,vc3,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="********************************************************************************"
	do VOM
	set VL="Grand Totals for all Statements Processed:"
	set V=0						// @TOT([LETSTMTTOT]TOTSTM,0)
	for I=0:1:5 set V=V+VT(I,2,1)
	set VL=VL_$J("",43-VL.length())_$J(V,11)
	set V=0						// @TOT([LETSTMTTOT]TOTCID,0)
	for I=0:1:5 set V=V+VT(I,1,1)
	set VL=VL_$J("",56-VL.length())_$J(V,11)
	set V=0						// @TOT([LETSTMTTOT]TOTTRN,0)
	for I=0:1:5 set V=V+VT(I,3,1)
	set VL=VL_$J("",69-VL.length())_$J(V,11)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: SJD

	type public String vc1
 type public Date SJD
 
 set SJD=vc1
	quit

VP2	// Column pre-processor - Variable: EXTRACT

	type public String vc2
 type public String EXTRACT 
 
 set EXTRACT=vc2
	quit

VP3	// Column pre-processor - Variable: FDESC

	type public String vc4
 type public String FDESC
 
 type String FORM
 
 set FORM=vc4
 type RecordSTBLFORMS stblforms = Db.getRecord("STBLFORMS","FORM=:FORM")
 set FDESC=stblforms.DES
	quit
