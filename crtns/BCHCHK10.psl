vEXEC(vCONTEXT,CID,EXPDT,RCID) //
	type RecordACN acn
	
	set acn=Db.getRecord("ACN","CID")

        do EXEC(.acn)

	quit	


EXEC(RecordACN acn)	// Processing account
	
	type Number HLD

 	type ResultSet rs
	set rs=Db.select("RSKAMT","HLD10","CID=:CID AND EXPDT>:%SystemDate")

 	set HLD=0   				// sum of unexpired risk holds
                       
 	while rs.next() set HLD=HLD+rs.getCol(1)

	set acn.donrsk=HLD
	do acn.bypassSave()	

	quit


