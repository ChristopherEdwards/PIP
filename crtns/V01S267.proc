 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="EXTINTTRN",VPGM=$T(+0),VSNAME="External Interface Tran Code Setup"
 set VFSN("UTBLEXTTRN1")="zfEXTINT8"
 set vPSL=1
 set KEYS(1)=fEXTINT8.INTRFACE
 set KEYS(2)=fEXTINT8.CODE
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fEXTINT8),VDA1(.fEXTINT8),^DBSPNT()

 if '%ProcessMode do VNEW(.fEXTINT8),VPR(.fEXTINT8),VDA1(.fEXTINT8)
 if %ProcessMode do VLOD(.fEXTINT8) quit:$G(ER)  do VPR(.fEXTINT8),VDA1(.fEXTINT8)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fEXTINT8)
	quit


VNEW(RecordUTBLEXTTRN1 fEXTINT8) // Initialize arrays if %O=0
 
 do VDEF(.fEXTINT8)
 do VLOD(.fEXTINT8)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordUTBLEXTTRN1 fEXTINT8)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
