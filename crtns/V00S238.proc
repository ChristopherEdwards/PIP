  //user-defined post procs
 //
VP1(RecordEFTPAY fEFTPAY) //
 type public Boolean ER
 type public Number CID,ogl,X
 
 if X="" quit
 do CID^UACN1
 if ER quit
 
 type RecordACN acn=Db.getRecord("ACN","CID=:X",1)
 if acn.stat=4 do Runtime.setErrMSG("ACN",6050,X) quit
 
 set CID=X
 set ogl=0
 
 set fEFTPAY.TTYPE=0  //customer account
 do DEFAULT^DBSMACRO("DEP.CRCD",acn.crcd)
 do DEFAULT^DBSMACRO("EFTPAY.BRCD",acn.boo)
 do DEFAULT^DBSMACRO("EFTPAY.BENACN",X)
 do DEFAULT^DBSMACRO("@BENGL","")
 do DISPLAY^DBSMACRO("@BENGL",BENGL)
 do DEFAULT^DBSMACRO("EFTPAY.CRCC","")
 do DISPLAY^DBSMACRO("EFTPAY.CRCC",fEFTPAY.CRCC)
 
 quit
VP2(RecordEFTPAY fEFTPAY) //
 type public Number BENACN,CID,ogl,X
 
 if BENACN'="",X'="" do Runtime.setErrMSG("EFTPAY",1831) quit
 if X="",BENACN="" do Runtime.setErrMSG("EFTPAY",3456) quit
 if X="" quit
 
 type RecordGLAD glad=Db.getRecord("GLAD","ACN=:X",1)
 if 'glad.getMode() do Runtime.setErrMSG("GLAD",1364,X) quit
 set fEFTPAY.TTYPE=3
 set ogl=1
 
 do DEFAULT^DBSMACRO("@BENACN","")
 do DISPLAY^DBSMACRO("@BENACN",BENACN)
 set fEFTPAY.BENACN=X
 set CID=X
 
 quit
VP3(RecordEFTPAY fEFTPAY) //
 type public Number X
 
 do DEFAULT^DBSMACRO("EFTPAY.BRCD",X)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


