				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Loan Sale Remittance Detail Report"
		set VBNRINFO("PGM")="R01S139"
		set VBNRINFO("RID")="SCA071"
		set VBNRINFO("TABLES")="LNLS8,LNLS6,LNLS3,LNLS2,LNLS1,LN"

