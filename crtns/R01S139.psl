		set VBNRINFO("ORDERBY",2)="[SYSDEV,LNLS8]PL"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,LNLS8]GRP"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,LNLS8]CID"

