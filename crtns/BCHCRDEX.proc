vEXEC(vCONTEXT,CRDTYP,CRDNUM) //
 new crd
 type RecordCRD crd
 set crd=Db.getRecord("CRD","CRDTYP,CRDNUM")

 do crd.setAuditFlag(1)				// keep track of changes

 if crd.stat=5 quit				// Cancelled
 if crd.stat=6 quit				// Expiration pending
 set crd.stat=12				// Change status to Expired

 if crd.rplcrd'="" do {
	new rencard
 	type RecordCRD rencard
	set rencard=Db.getRecord("CRD","CRDTYP,crd.rplcrd")
 	do rencard.setAuditFlag(1)		// keep track of changes
 	set rencard.stat=0			// Change status to Active
	do rencard.save()				// Update new card record
 }
 do crd.save()					// Update old card record

 quit

