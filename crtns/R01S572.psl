	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %JD,%MSKD,%NOBANNER,%NOCLOSE,%NOOPEN,%TIM,ACRCF,CONAM,DPE,EOTY,LNAM,MPE,PGM,RID,RN,VL,YPE,ZJD,ZRID,ZZRID,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vp1,vrundate,vsysdate
