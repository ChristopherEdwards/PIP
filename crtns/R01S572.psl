VFPOST	// FETCH post-processor

 // Replaced calendar year-end references with the Tax year-end look-up.

 type public Date EOTY,%JD,ZJD
 type public String MPE
 type public Number DPE,YPE

 // End of Tax year
 set EOTY=$$EOTY^SCADAT(%SystemDate,1)

 // Year portion of EOTY
 set YPE=$$YEAR^SCADAT(EOTY,1)-1

 // Day portion of EOTY
 set DPE=$$DAY^SCADAT(EOTY,1)

 // Month portion of EOTY
 set MPE=$$MON^SCADAT(EOTY,1)

 set %JD=$$DSJD^SCADAT(MPE_"/"_DPE_"/"_YPE) 
 set ZJD=%JD
	quit

VPREBQ	// Pre-processor (before query)

 // Clean up the TMP files
 do Db.fastDelete("CD1099","PID=:%ProcessID")
	quit

VRPOST	// Report post-processor

 type public Boolean VFMQ
 type public String LNAM,%NOBANNER,%NOCLOSE,%NOOPEN,PGM,RID,ZZRID
 type public Number VLC,VRG,vcrt
 type Number PN

 set ZZRID=RID
