	type ResultSet rwrs=Db.select("CIF.TAXID,CIF.LNM,DEP.CC,DEP.BOO,DEP.TYPE,DEP.CID,DEP.BAL,DEP.ACRPY,DEP.IPTY,DEP.IRN,DEP.PENPTY,DEP.BWPTY","DEP,CIF","DEP.GRP='CD' AND ((DEP.IPTY-(DEP.NRPOSIPTY-DEP.NRNEGIPTY))>0 OR (DEP.BWPTY-DEP.NRWHPTY)>0 OR (DEP.PENPTY-DEP.NRPPTY)>0)","CIF.TAXID,CIF.LNM,DEP.CC,DEP.BOO,DEP.TYPE,DEP.CID","","DQMODE=1/PROTECTION=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
	type Boolean VSKIPREC = 0
		type String V,VI
