exec //

 type Number newstat
 type public Number ESC

 
 type RecordDAYENDSTAT destat=Db.getRecord("DAYENDSTAT","TJD,CID")

 
 type RecordDEP dep=Db.getRecord("DEP","CID")

 do dep.setAuditFlag(1)

 set newstat=destat.stat
 if newstat=dep.stat quit

 // If account escheat "suspect" run full escheat analysis

 set ESC=1
 /*
  bypass is rquired since the escheat program does not
  have a procedure definition for the compiler to check
  the paramaters
 */
 // Escheat processing
 #bypass
 if newstat=3 set ESC=$$^ESCHEAT(CID,.dep)
 #endbypass

 // Remove from dayend if not escheatable

 if 'ESC do Db.fastDelete("DAYENDSTAT","TJD,CID") quit

 set dep.stat=newstat		// Change account status
 do dep.save()			// File data
 quit


