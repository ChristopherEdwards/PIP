vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLZTSO utblztso

	if utblztso.key.length()>20 set vRM = $$^MSG(1076,20) do vdderr("KEY", vRM) quit
	set X = utblztso.ddec if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("DDEC", vRM) quit
	if utblztso.des.length()>40 set vRM = $$^MSG(1076,40) do vdderr("DES", vRM) quit
	set X = utblztso.dlen if 'X.isNull(),X'?1.5N,X'?1"-"1.4N set vRM=$$^MSG(742,"N") do vdderr("DLEN", vRM) quit
	if utblztso.dtbl.length()>255 set vRM = $$^MSG(1076,255) do vdderr("DTBL", vRM) quit
	set X = utblztso.qtyp if 'X.isNull(),'Db.isDefined("STBLTSSPQLF","X") set vRM = $$^MSG(1485,X) do vdderr("QTYP", vRM) quit
	set X = utblztso.typ if 'X.isNull(),'Db.isDefined("DBCTLDVFM","X") set vRM = $$^MSG(1485,X) do vdderr("TYP", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("UTBLZTSO","MSG",979,"UTBLZTSO."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordUTBLZTSO utblztso

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("KEY")
	set voldkey = vux.piece("|",1),vobj(utblztso,-3) = voldkey // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/TRIGBEF/" do VBU
	if vpar["/VALDD/" do vddver
	do vexec

	set vnewkey = vux.piece("|",2),vobj(utblztso,-3) = vnewkey // Copy new keys into object
	type RecordUTBLZTSO vnewrec = utblztso.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("UTBLZTSO",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	set vobj(utblztso,-3) = vux.piece("|",1)               // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit
