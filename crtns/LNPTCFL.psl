	// Allow global reference - Delete record
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	ZWI ^PCBD("PCHND",vobj(lnptchg,-3),vobj(lnptchg,-4))
	#ENDBYPASS
	quit


