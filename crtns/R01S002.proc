VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CID,CONAM,GRP,INIT,OPT,POS,RID,RN,RUN,TDEP,TLD,TLN,TOT,TYPE,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL="Audit Notice Run Report - "_$$DAT^%ZM(AUDDATE,$G(%MSKD))_" - Run "_vc2
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="                                                                          "_"Audit Date                    Current"
	do VOM
	set VL="                                                                      "_"------------------           ------------------       Last"
	do VOM
	set VL="Group       Account  Customer                                              Balance   Seq                Balance   Seq      Active"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: INIT

	type public Number TOTDEP,TOTLN
	type public String INIT

	set INIT=""
	set (TOTDEP,TOTLN)=0
	quit

VP2	// Column post-processor - [SYSDEV,DEP]GRP

	type public String vc19
 type public String CLS,TGRP()
 type public Number TOTDEP,TOTLN
 
 if TGRP(V).exists() set TGRP(V)=TGRP(V)+1
 else  set TGRP(V)=1

 set CLS=vc19

 if CLS="L" set TOTLN=TOTLN+1 quit

 set TOTDEP=TOTDEP+1

	quit

VP3	// Column post-processor - [SYSDEV,DEP]TLSEQ

 type public Number X

 set X=X-1
	quit

VP4	// Column pre-processor - ( <<TLN>> LOANS, <<TDEP>> DEPOSITS )

 type public Number TOTDEP,TOTLN
 type public String TDEP,TLN

 set (TDEP,TLN)=""

 if 'TOTLN set TLN="NO"
 else  set TLN=TOTLN

 if 'TOTDEP set TDEP="NO"
 else  set TDEP=TOTDEP
	quit
