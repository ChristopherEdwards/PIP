	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,BALDIFF,CONAM,ETC,ICID,IPLD,IPTJD,ITYPE,PGM,PURCH,Q(),REDEMP,RID,RN,VL,X,exe,fDEPINVC,fDEPINVS,fDEPINVT,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc3,vovc4,vovc5,vovc6,vovc7,vovc8,vrundate,vsql,vsysdate
