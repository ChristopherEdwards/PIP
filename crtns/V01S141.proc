 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S652
 if %ProcessMode>1 quit
 do VTAB^V01S652(.fUTBLCTF)
 quit
 
VPG2(RecordUTBLCTFPLN fUTBLCTF)  // Commission/Tax/Fee Calculations
