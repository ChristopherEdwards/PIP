VPG(RecordUTBLCTFPLN fUTBLCTF)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fUTBLCTF),VPG0(.fUTBLCTF) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fUTBLCTF),VPG0(.fUTBLCTF) quit:vDONE
  }
 quit

VPG0(RecordUTBLCTFPLN fUTBLCTF)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fUTBLCTF) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordUTBLCTFPLN fUTBLCTF)  // Commissions, Taxes, Fees - Data Entry
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="UTBLCTFPLN",DFID="UTBLCTFPLN"
