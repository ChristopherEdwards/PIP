 set VSID="CTFPLNCTL"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+1 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+2
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Commissions, Taxes, Fees - Data Entry|UTBLCTFPLN"
 set VPG(%PG+1)="Commission/Tax/Fee Calculations|UTBLCTFPLN2"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fUTBLCTF)
 if %ProcessMode do VLOD(.fUTBLCTF) if $G(ER) set VFMQ="Q" quit
 do VPG(.fUTBLCTF)
 quit

VNEW(RecordUTBLCTFPLN fUTBLCTF)
 
 quit
