	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("EFFDT").exists()!vx("LOADDT").exists()!vx("RECINST").exists()!vx("SEQ").exists()!vx("RECDTYP").exists() do vkchged quit // Primary key changed
