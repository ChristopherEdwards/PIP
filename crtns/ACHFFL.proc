vrequ // Valid required columns on update


	type public String vx()
	type public RecordACHF achf

	if achf.effdt.isNull() do vreqerr("EFFDT") quit
	if achf.loaddt.isNull() do vreqerr("LOADDT") quit
	if achf.recinst.isNull() do vreqerr("RECINST") quit
	if achf.seq.isNull() do vreqerr("SEQ") quit
	if achf.recdtyp.isNull() do vreqerr("RECDTYP") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("ACHF","MSG",1767,"ACHF."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordACHF achf

	set X = achf.effdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EFFDT", vRM) quit
	set X = achf.loaddt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("LOADDT", vRM) quit
	if achf.recinst.length()>15 set vRM = $$^MSG(1076,15) do vdderr("RECINST", vRM) quit
	set X = achf.seq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("SEQ", vRM) quit
	set X = achf.recdtyp if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("RECDTYP", vRM) quit
	if achf.record.length()>200 set vRM = $$^MSG(1076,200) do vdderr("RECORD", vRM) quit
	if achf.sent.length()>2 set vRM = $$^MSG(1076,2) do vdderr("SENT", vRM) quit
	set X = achf.sentdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("SENTDT", vRM) quit
	quit

