vkchged // Access key changed

	type public RecordACHF achf

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("EFFDT").exists() set vux("EFFDT") = vx("EFFDT")
	if vx("LOADDT").exists() set vux("LOADDT") = vx("LOADDT")
	if vx("RECINST").exists() set vux("RECINST") = vx("RECINST")
	if vx("SEQ").exists() set vux("SEQ") = vx("SEQ")
	if vx("RECDTYP").exists() set vux("RECDTYP") = vx("RECDTYP")
	do vkey(1) set voldkey = achf.effdt_","_achf.loaddt_","_achf.recinst_","_achf.seq_","_achf.recdtyp // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = achf.effdt_","_achf.loaddt_","_achf.recinst_","_achf.seq_","_achf.recdtyp // Copy new keys into object
	type RecordACHF vnewrec = achf.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("ACHF",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordACHF achf

	if vux("EFFDT").exists() set achf.effdt = vux("EFFDT").piece("|",i)
	if vux("LOADDT").exists() set achf.loaddt = vux("LOADDT").piece("|",i)
	if vux("RECINST").exists() set achf.recinst = vux("RECINST").piece("|",i)
	if vux("SEQ").exists() set achf.seq = vux("SEQ").piece("|",i)
	if vux("RECDTYP").exists() set achf.recdtyp = vux("RECDTYP").piece("|",i)
	quit
