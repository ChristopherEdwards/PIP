vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSBNETMSG sbnetmsg

	set X = sbnetmsg.procdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("PROCDT", vRM) quit
	if sbnetmsg.tm.length()>8 set vRM = $$^MSG(1076,8) do vdderr("TM", vRM) quit
	set X = sbnetmsg.bitnum if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("BITNUM", vRM) quit
	if sbnetmsg.bitdata.length()>255 set vRM = $$^MSG(1076,255) do vdderr("BITDATA", vRM) quit
	quit

