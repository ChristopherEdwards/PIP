	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("PROCDT").exists()!vx("TM").exists()!vx("BITNUM").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("SBNETMSG",.vx)
