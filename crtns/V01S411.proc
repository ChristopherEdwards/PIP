VP1(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF) //
 /*
 
    ---- Revision History------------------------------------------------
 
         05/26/05 - SWARNALATHA P - CR 15982
         Converted to PSL
 
    ---------------------------------------------------------------------
 */

 type Date DIST1ND,ICHND,SCHLD
 type Number INTOFF,PCHM
 type String DIST1FRE,INTFRE,PCM,PCMVAL

 if X.isNull() quit

 type RecordLN ln=Db.getRecord("LN","CID=:CID")

 set DIST1ND=ln.dist1nd
 if 'fLNRNW.dist1nd.isNull() set DIST1ND=fLNRNW.DIST1ND

 set DIST1FRE=ln.dist1fre
 if 'fLNRNW.dist1fre.isNull() set DIST1FRE=fLNRNW.DIST1FRE

 set PCHM=ln.pchm 
 if 'fLNRNW.PCHM.isNull() set PCHM=fLNRNW.PCHM

 set INTFRE=ln.intfre 
 if 'fLNRNW.INTFRE.isNull() set INTFRE=fLNRNW.INTFRE

 set ICHND=ln.ichnd 
 if 'fLNRNW.ICHND.isNull() set ICHND=fLNRNW.ICHND

 set INTOFF=ln.intoff 
 if 'fLNRNW.INTOFF.isNull() set INTOFF=fLNRNW.INTOFF

 set PCM=ln.pcm
 if 'fLNRNW.PCM.isNull() set PCM=fLNRNW.PCM

 set SCHLD=ln.schld
 set PCMVAL=$$PCMVAL^LNCDI(PCM)

 // Payment change frequency requires int/div change next date 
 if ICHND.isNull() do Runtime.setErrMSG("LNICNV", 3216) quit

 do PCFREF^LNPPC3(X,DIST1ND,DIST1FRE,PCHM,INTFRE,ICHND,SCHLD)
 // Payment change frequency does not match master frequency
 if ER.get() S RM=$$^MSG(3242) quit
VP2(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF) //
 /*
 
    ---- Revision History------------------------------------------------
 
         05/26/05 - SWARNALATHA P - CR 15982
         Converted to PSL
 
    ---------------------------------------------------------------------
 */
 
 type public Boolean ER
 type public String X

 type Date DIST1ND,ICHND
 type Number PCHM,PCOFF
 type String DIST1FRE,INTFRE,PCFRE,PCM

 if X.isNull() quit

 type RecordLN ln=Db.getRecord("LN","CID=:CID")

 set DIST1ND=ln.dist1nd 
 if 'fLNRNW.dist1nd.isNull() set DIST1ND=fLNRNW.DIST1ND

 set INTFRE=ln.intfre 
 if 'fLNRNW.intfre.isNull() set INTFRE=fLNRNW.INTFRE

 set PCFRE=ln.pcfre 
 if 'fLNRNW.PCFRE.isNull() set PCFRE=fLNRNW.PCFRE

 set ICHND=ln.ichnd 
 if 'fLNRNW.ICHND.isNull() set ICHND=fLNRNW.ICHND

 set DIST1FRE=ln.dist1fre 
 if 'fLNRNW.DIST1FRE.isNull() set DIST1FRE=fLNRNW.DIST1FRE

 set PCHM=ln.pchm 
 if 'fLNRNW.PCHM.isNull() set PCHM=fLNRNW.PCHM

 set PCOFF=ln.pcoff 
 if 'fLNRNW.PCOFF.isNull() set PCOFF=fLNRNW.PCOFF

 S PCM=ln.pcm 
 if 'fLNRNW.PCM.isNull() set PCM=fLNRNW.PCM

 // Payment change next date requires payment change frequency
 if PCFRE.isNull() do Runtime.setErrMSG("LNICNV", 3217) quit

 // Payment change offset required with payment change date
 if PCOFF.isNull() do Runtime.setErrMSG("LNICNV", 3237) quit

 do PCHNDF^LNPPC3(X,DIST1ND,INTFRE,PCFRE,ICHND,DIST1FRE,PCHM)
 // Payment change next date does not match master frequency
 if ER.get() set RM=$$^MSG(3241) quit

 quit
