VPO1(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)  // user library LN_BLOFF
 
 /*
 
    ---- Revision History------------------------------------------------
 
         05/26/05 - SWARNALATHA P - CR 15982
         Converted to PSL
 
    ---------------------------------------------------------------------
 */
 type public Boolean ER
 type public Date OBD,OBO,NBD
 type public String X,V
 
 if X'=V,LN.bndt,X="1P"!(X?.N) do { quit:ER
        // May not transfer from/to frequency "1P" on other than payment date
 	if X="1P"!(V="1P"),TJD'=LN.schld set ER=1,ET="LNBLOFF3" do ^UTLERR quit
 	
        // Find previous value of next billing date and billing offset
 	set OBD=$SELECT(UX("LN","BNDT").data():UX("LN","BNDT").piece("|",1),1:+LN.bndt)
 	set OBO=$SELECT(UX("LN","BLOFF").data():UX("LN","BLOFF").piece("|",1),1:V)
 
   	if '(X="1P"!(V="1P")) do { quit:ER
		// Determine whether or not change is allowed
 		// New billing offset must be greater than 0
 		if X'>0 set ER=1,ET="LNBLOFF4" do ^UTLERR quit
 
 		//Find (A) last billing date
 		set A=$SELECT(UX("LN","BLDT").data():UX("LN","BLDT").piece("|",2),1:LN.bldt)
 
 		// May not change billing offset if currently in offset period
 		if LN.bseq,A+OBO>TJD set ER=1,ET="LNBLOFF1" do ^UTLERR quit
 
 		// Invalid if change would put account in billing offset period
 		if OBD+OBO-X<TJD set ER=1,ET="LNBLOFF2" do ^UTLERR quit
        	}
 	set NBD=$SELECT(X="1P":LN.schnd,1:(OBD+OBO-X))
        do DEFAULT^DBSMACRO("LN.bndt",NBD,"1","0","0")
	}
 quit
 #ACCEPT DATE=11/05/03;pgm=screen compiler
 quit
