vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordSYSMAPVAR sysmapvar

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("SYSMAPVAR","TARGET = :sysmapvar.target and LABEL = :sysmapvar.label and VAR = :sysmapvar.var") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(sysmapvar)) S ^SYSMAP("DATA",vobj(sysmapvar,-3),vobj(sysmapvar,-4),"V",vobj(sysmapvar,-5))=vobj(sysmapvar)
		#ENDBYPASS
	}

	quit

vdelete(Boolean vkeychg) // Record Delete

