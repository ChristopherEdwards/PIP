R01S330	// SCA341 - ARS Outstanding Settlement Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:56 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Number QCID
	type Date TDT

	set RID="SCA341"
	set RN="ARS Outstanding Settlement Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("QCID")="|12||[ARSB]CID|[ARSB]CID:DISTINCT||||N|Account Number|||||"
	set %TAB("TDT")="|10|||[ARSB]||||D|Period Ending Date|||||"

	set %READ="IO/REQ,QCID#1,TDT#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public Date TDT
	type public Number QCID
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CID,CONAM,POA,RID,RN,TITL,VAL,VAL1,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA341",RN="ARS Outstanding Settlement Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA341","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=81")
		write $$CLEARXY^%TRMVT
		}

	do INIT^%ZM()


	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE="ARSB.CID=:QCID AND ARSB.ARSNDT=:TDT"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="ARS Outstanding Settlement Report"
		set VBNRINFO("PGM")="R01S330"
		set VBNRINFO("RID")="SCA341"
		set VBNRINFO("TABLES")="ARSB"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,ARSB]CID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,ARSB]ARSNDT"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:2 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("ARSB.CID,ARSB.ARSNDT,ARSB.ISSIP,ARSB.REVIP,ARSB.PDIP,ARSB.PNIIP,ARSB.VDIP,ARSB.DELIP,ARSB.CKSOUTP","ARSB","ARSB.CID=:QCID AND ARSB.ARSNDT=:TDT","ARSB.CID,ARSB.ARSNDT","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2
	set vovc1="",vovc2=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2
	set vovc1=vc1,vovc2=vc2
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9
	set vc1=V.piece($C(9),1)			// ARSB.CID
	set vc2=V.piece($C(9),2)			// ARSB.ARSNDT
	set vc3=V.piece($C(9),3)			// ARSB.ISSIP
	set vc4=V.piece($C(9),4)			// ARSB.REVIP
	set vc5=V.piece($C(9),5)			// ARSB.PDIP
	set vc6=V.piece($C(9),6)			// ARSB.PNIIP
	set vc7=V.piece($C(9),7)			// ARSB.VDIP
	set vc8=V.piece($C(9),8)			// ARSB.DELIP
	set vc9=V.piece($C(9),9)			// ARSB.CKSOUTP
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

 type public Number CID,QCID
 set CID=QCID
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(2)
	type Public String vc1,vovc1,vc2,vovc2
	type Number vb1,vb2
	set (vb1,vb2)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,VH0=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(2)=VT(2)+1
	quit

VDTL2	// Detail
	type public String CID,IOSL,POA,QCID,TITL,V,VAL,VAL1,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2

	if VLC+11>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="Previous Outstanding Amount"
	set VL=VL_$J("",39-VL.length())_":"
	do VP1 quit:VFMQ!verror.get()  set V=$J($FN(POA,",",2),12)
	set VL=VL_$J("",42-VL.length())_V
	do VOM
	set VL="" do VOM
	set VL="    "_"+ New Issues"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc3,",",2),12)
	do VOM
	set VL="    "_"+ Adjustments (Reversals)"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc4,",",2),12)
	do VOM
	set VL="                   "_"- Paid (Issued)"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc5,",",2),12)
	do VOM
	set VL="                   "_"- Paid (Not Issued)"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc6,",",2),12)
	do VOM
	set VL="                   "_"- Void"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc7,",",2),12)
	do VOM
	set VL="                   "_"- Deleted"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc8,",",2),12)
	do VOM
	set VL="                                          "_"------------"
	do VOM
	set VL="New Outstanding Amount"
	set VL=VL_$J("",39-VL.length())_":"
	set VL=VL_$J("",42-VL.length())_$J($FN(vc9,",",2),12)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CID,CONAM,POA,QCID,RID,RN,TITL,VAL,VAL1,VL,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+9,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL="                        "_"Outstanding Settlement Report"
	do VOM
	set VL="" do VOM
	set VL="" do VOM
	do VP2 quit:VFMQ!verror.get()  set V="Account Number: "_vc1_" " set VL=V
	do VP3 quit:VFMQ!verror.get()  set V="Account Title: "_TITL
	set VL=VL_$J("",29-VL.length())_V
	do VOM
	set VL="" do VOM
	do VP4 quit:VFMQ!verror.get()  set V="For the Period "_VAL_" through "_VAL1 set VL=V
	do VOM
	set VL="" do VOM
	set VL="================================================================================"
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: POA

 type public Date FDT
 type public Number CID,POA

 type RecordARSB arsb=Db.getRecord("ARSB","CID=:CID,ARSNDT=:FDT",1)
 set POA=arsb.cksoutp

 
	quit

VP2	// Column pre-processor - Account Number: <<CID>>

 type public Number CID

 if CID.isNull() set CID="NONE"
	quit

VP3	// Column pre-processor - Account Title: <<TITL>>

 type public Number CID,TITL
 set TITL="NONE" quit:'CID

 type RecordACN acn=Db.getRecord("ACN","CID=:CID")
 set TITL=(acn.title1).extract(1,30)

	quit

VP4	// Column pre-processor - For the Period <<VAL>> through <<VAL1>>

 type public Date FDT,TDT,VAL,VAL1
 type public Number CID
 type Date JD
 type String FRE

 set VAL1=TDT
 set FRE=""

 type ResultSet rs=Db.select("ARSNDT","ARSB","CID=:CID AND ARSNDT<:TDT","ARSNDT DESC")
 if rs.next() set VAL=rs.getCol("ARSNDT")

 if (CID="NONE") quit

 if '((VAL.get()).isNull()) set FDT=VAL
 else  do {
	type RecordARSCID arscid=Db.getRecord("ARSCID","CID=:CID",1)
	if arscid.getMode() set FRE="-"_arscid.arsfre	
	set JD=VAL1
	set (VAL,FDT)=$$NJD^UFRE(JD,FRE)
	}
 set VAL=VAL+1
 set VAL=$$DAT^%ZM(VAL)
 set VAL1=$$DAT^%ZM(VAL1)
	quit
