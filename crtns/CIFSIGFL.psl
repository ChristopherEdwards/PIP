	if 'Db.isDefined("CIF",":cifsig.acn") set vERRMSG = $$^MSG(8563,"CIFSIG(ACN) -> CIF") throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))
	quit


