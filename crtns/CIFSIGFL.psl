	if 'cifsig.acn.isNull() set vfkey("^CIF("_""""_cifsig.acn_""""_","_1_")")="CIFSIG(ACN) -> CIF"
	quit

CHKFKS   // Check foreign keys when not under buffer


	type public RecordCIFSIG cifsig
