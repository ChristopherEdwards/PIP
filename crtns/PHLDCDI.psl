PHLDCDI		/*
	ORIG: ofaltr - 06/29/2000
	DESC: Computed Feilds for PHLD
         
         LIBRARY:
	 $$ PHLDDESC Permanent Hold Code Description
	
        ---- Revision History------------------------------------------------

	11/24/05 - NATRAJAH - 13734
		   General DBI3 cleanup.        

	-------------------------------------------------------------------*/
	
	quit

PHLDDESC(XCID,XSEQ)	// Permanent Hold Code Description
	/*
         The function is used to compute [PHLD]PHLDDESC field
        
         ARGUMENTS:
	 . CID         Account Number		/TYP=N/REQ/MECH=VAL
	 . SEQ		Sequence Number		/TYP=N/REQ/MECH=VAL	
        
         RETURNS:
               $$  Permanent Hold Code Description
        
         EXAMPLE:$$PHLDDESC^PHLDCDI(CID,SEQ)
	*/
        
	type Number XPHC
	type String PHLDDESC
	
	type ResultSet rs1=Db.select("PHC","PHLD","CID=:XCID AND SEQ=:XSEQ")
	if rs1.next() set XPHC=rs1.getCol("PHC")
	
	type ResultSet rs2=Db.select("DESC","UTBLPHC","PHC=:XPHC")
	if rs2.next() set PHLDDESC=rs2.getCol("DESC")
	quit PHLDDESC
	
 #OPTION ResultClass ON
Public String vSIG()	quit "60232^21536^Hari Natrajan^994"	// Signature - LTD^TIME^USER^SIZE
