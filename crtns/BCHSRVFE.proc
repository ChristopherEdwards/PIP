vEXEC(vCONTEXT,CID) //
	type RecordDEP dep=Db.getRecord("DEP","CID=:CID")
	
	// Only handle accounts where ANLTYP NOT = 1 - place here to remove
	// from batch query to allow compiler to use index on SCND
	quit:dep.anltyp = 1
	
	do START(.dep)
	
	quit

Public START(RecordDEP dep)

	/* 
	Though this section is a PUBLIC section, prior to calling it,
	the variable PLTP must be set to 0. It is used to indicate that
	the processing being performed is service fee processing and not
	useage credit processing.
	*/

	kill %TRN
	set SCND=%SystemDate

	// PAR 2/11/2000
	set FEECAT=0
	do Db.delete("SRVD","CID=:CID AND PLTP=:PLTP AND SCND=:SCND AND FEECAT=:FEECAT")	// for repeats
	set FEECAT=4
	do Db.delete("SRVD","CID=:CID AND PLTP=:PLTP AND SCND=:SCND AND FEECAT=:FEECAT")	// for all types
	
	
	new ttxset

 	set ttxset=0
		
	/*
	If DEP.SCND is not today, then this must be restart processing.
	This account should not be processed since it has already been
	processed on this date. This replaces the RESTART processing flag
	on the batch control page.
	*/

	if 'PLTP,dep.scnd'=%SystemDate quit	// Service Charge - Next Date
	if PLTP,dep.upldt'=%SystemDate quit	// Usage Credit Plan Next Date
	
	set CID=dep.cid				//  Original Account Number

	type TranSet ts=Class.new("TranSet")
	do PROC^BCHFEEUT(CID,1,%SystemDate,.ts,.dep) if ER quit
	
	// ttxset will be set to 1 if a transaction was created
	if ttxset do { 
		new par
		set par("IPMODE")=3
		set par("OPTION")=2
		do ts.postTSet(%SystemDate,BRCD,.par)
	}
	
	do BUMP^BCHFEEUT(.dep)
	quit

	

 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
