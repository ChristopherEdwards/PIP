																if PFID'="LN" do Runtime.setErrMSG("UTBLECOSTOP",1849) quit:ER 
																}

								if $G(ER)=1 quit

								// Rebuild Deliquency Query M routine DELQRY1.
								do ^DELQRY
							 
	quit


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLECOSTOP utblecostop

	set X = utblecostop.costyp if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("COSTYP", vRM) quit
	set X = utblecostop.ddel if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("DDEL", vRM) quit
	if utblecostop.delqry.length()>80 set vRM = $$^MSG(1076,80) do vdderr("DELQRY", vRM) quit
	if utblecostop.des.length()>40 set vRM = $$^MSG(1076,40) do vdderr("DES", vRM) quit
	set X = utblecostop.pdel if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("PDEL", vRM) quit
	quit

