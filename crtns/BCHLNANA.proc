vPROC(EJD,CID) //
 type public Number ER
 type public String ET,%EVENT,%FN,%INTRPT(),RM,vCONTEXT
 catch vERROR {
 type public Number ER
 type public String RM
 
 do Runtime.rollback()
 
 do LOG^UTLEXC("BCHLNANAP","*","",EJD.get()_","_CID.get(), vERROR.thrownAt, vERROR.type)
 
 // DBFILER errors do not log on a call to ZE^UTLERR
 if vERROR.type="%PSL-E-DBFILER" do {
  type String ET = vERROR.type
  do ^UTLERR
 }
 else  do ZE^UTLERR
 
 set ER = 1
 set RM = vERROR.description
 }
 if ('%INTRPT.get().isNull())!(%INTRPT.data() > 1) do INTRPT^BCHUTL(%EVENT.get())
 if %BatchRestart,$$CHKLOG^BCHUTL(%SystemDate,%FN,"BCHLNANAP",EJD.get()_","_CID.get()) do { quit
 do LOG^BCHUTL(%SystemDate,%FN,"BCHLNANAP",EJD.get()_","_CID.get(),"Record already processed")
 }
 do Runtime.start("BA")
 set vCONTEXT=""
 set (ET,RM)=""
 set ER=0
 do vEXEC(.vCONTEXT,EJD,CID)
 if ER.get() do { quit
 type String et
 set et=$S(ET.get().isNull():RM.get(),1:ET)
 
 do Runtime.rollback()
 do LOG^UTLEXC("BCHLNANAP","*","",EJD.get()_","_CID.get(),"",et)
 }
 do UPDLOG^BCHUTL(%SystemDate,%FN,"BCHLNANAP",EJD.get()_","_CID.get(),vCONTEXT)
 do Runtime.commit()
 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
vEXEC(vCONTEXT,EJD,CID) //
	new CRCD,NAGL,TYPE

	new delnnao
	type RecordDAYENDLNNAO delnnao
	set delnnao=Db.getRecord("DAYENDLNNAO","EJD,CID")

	// Exclude non-automatic loans
	if '+delnnao.auto quit
	
	// Exclude loans removed from automatic process
	if delnnao.del quit

	new ln
	type RecordLN ln
	set ln=Db.getRecord("LN","CID")
	do ln.setAuditFlag(1)			// JMH - 06/14/00

	// Exclude accounts which already are in nonaccrual
	if ln.nai quit
	
	set TYPE=ln.type
	set CRCD=ln.crcd

	// Exclude accounts which are not at least ANPP days delinquent
	if %SystemDate-ln.dist1nd<ln.anpp quit

	new ltype
	type RecordPRODCTL ltype
	set ltype=Db.getRecord("PRODCTL","TYPE")

	// Nonaccrual G/L
	set NAGL=ltype.nagl
	if NAGL="" set ER=1,ET="LNANAP" quit
	
	set ln.glsc=NAGL		// G/L set code
	set ln.nai=1			// Nonaccrual Indicator
	do ln.save()			// Update loan record

	quit

 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
