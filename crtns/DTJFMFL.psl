	if dtjfm.cls.isNull() do vreqerr("CLS") quit
	if dtjfm.grp.isNull() do vreqerr("GRP") quit
	if dtjfm.typ.isNull() do vreqerr("TYP") quit
	if dtjfm.cid.isNull() do vreqerr("CID") quit
	if dtjfm.tseq.isNull() do vreqerr("TSEQ") quit

		if vx("CDT").exists(),dtjfm.cdt.isNull() do vreqerr("CDT") quit
	quit

