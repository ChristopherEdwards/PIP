	if vux("TRNJD").exists() set depia.trnjd = vux("TRNJD").piece("|",i)
	if vux("CID").exists() set depia.cid = vux("CID").piece("|",i)
	if vux("TSEQ").exists() set depia.tseq = vux("TSEQ").piece("|",i)
	quit
