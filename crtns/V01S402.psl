	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fLNPOPT.exists() set fLNPOPT = Class.new("RecordLNPOPT")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNPOPT2",VPGM=$T(+0),VSNAME="Loan Late Charge Methods - Page 2"
 set VFSN("LNPOPT")="zfLNPOPT"
 set vPSL=1
 set KEYS(1)=fLNPOPT.GRP
 set KEYS(2)=fLNPOPT.POPT
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fLNPOPT),VDA1(.fLNPOPT),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fLNPOPT),VPR(.fLNPOPT),VDA1(.fLNPOPT)
 if %ProcessMode do VLOD(.fLNPOPT) quit:$G(ER)  do VPR(.fLNPOPT),VDA1(.fLNPOPT)

	// ====================  Display Form
	do ^DBSPNT()
