 do VLOD(.fLNPOPT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordLNPOPT fLNPOPT)
 quit:fLNPOPT.GRP.isNull() ! fLNPOPT.POPT.isNull() 
 quit:%ProcessMode  set ER=0 if (fLNPOPT.GRP="")!(fLNPOPT.POPT="") set ER=1,RM=$$^MSG(1767,"GRP,POPT") Q
 if Db.isDefined("LNPOPT","LNPOPT.GRP=:fLNPOPT.GRP AND LNPOPT.POPT=:fLNPOPT.POPT") S ER=1,RM=$$^MSG(2327) Q
 do fLNPOPT.setAuditFlag(1)
 if fLNPOPT.CWA="" set fLNPOPT.CWA=0
 if fLNPOPT.SUBTOVAT="" set fLNPOPT.SUBTOVAT=0
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
