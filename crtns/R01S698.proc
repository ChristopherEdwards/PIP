	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"VERACN2.PID,VERACN2.TJD,VERACN2.CID,VERACN2.SEQ,VE"
	set VSELECT=VSELECT_"RACN1.GRP,VERACN1.TYP,VERACN1.CID,VERACN1.BAL,VERA"
