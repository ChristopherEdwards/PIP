	do VOM
	set VL="              "_"Debits           Credits          Debits           Credits           Debits          Credits                   Balance"
	do VOM
	set VL="------------------------------------------------------------------------------------------------------------------------------------"
	do VOM
	quit

VDTL4	// Detail
	type public String %MCP,%TIM,CID,CRCD,CRCDDESC,EBAL,IOSL,TJD,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(4)=0 do VHDG4 quit:VFMQ

	set VL=$J($$DAT^%ZM(vc4),10)
	set VL=VL_$J("",11-VL.length())_$J($FN(vc5,",",2),14)
	set VL=VL_$J("",26-VL.length())_$J($FN(vc6,",",2),16)
	set VL=VL_$J("",43-VL.length())_$J($FN(vc7,",",2),16)
	set VL=VL_$J("",60-VL.length())_$J($FN(vc8,",",2),16)
	set VL=VL_$J("",77-VL.length())_$J($FN(vc9,",",2),16)
	set VL=VL_$J("",94-VL.length())_$J($FN(vc10,",",2),16)
	set V=vc13,VO=V set V=$J($FN(V,",",2),21) do VP2 quit:VFMQ!verror.get()
	set VL=VL_$J("",111-VL.length())_V		// [SYSDEV,NOSTROREP]ENDBAL
	do VOM
	quit

VSUM4	// Summary
	type public String %MCP,%TIM,CID,CRCD,CRCDDESC,EBAL,I,IOSL,TJD,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc10,vc11,vc12,vc13,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(4) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(4)=0 do VHDG4 quit:VFMQ

	set VL="===================================================================================================================================="
	do VOM
	set VL="Totals:"
	set V=VT(4,5,1)					// @TOT([NOSTROREP]DEALDR,)
	set VL=VL_$J("",10-VL.length())_$J($FN(V,",",2),15)
	set V=VT(4,4,1)					// @TOT([NOSTROREP]DEALCR,)
	set VL=VL_$J("",27-VL.length())_$J($FN(V,",",2),15)
	set V=VT(4,3,1)					// @TOT([NOSTROREP]CLNDR,)
	set VL=VL_$J("",44-VL.length())_$J($FN(V,",",2),15)
	set V=VT(4,2,1)					// @TOT([NOSTROREP]CLNCR,)
	set VL=VL_$J("",61-VL.length())_$J($FN(V,",",2),15)
	set V=VT(4,7,1)					// @TOT([NOSTROREP]FCVDR,)
	set VL=VL_$J("",78-VL.length())_$J($FN(V,",",2),15)
	set V=VT(4,6,1)					// @TOT([NOSTROREP]FCVCR,)
	set VL=VL_$J("",95-VL.length())_$J($FN(V,",",2),15)
	set VL=VL_$J("",112-VL.length())_$J($FN(EBAL,",",2),20)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MCP,%MSKD,%TIM,CID,CONAM,CRCD,CRCDDESC,EBAL,RID,RN,TJD,VL,vc1,vc10,vc11,vc12,vc13,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+3,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",97-VL.length())_"System Date:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="==================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: CRCDDESC

	type Public String CRCDDESC,%SystemCurrency

	type String CO

	set CO=CUVAR.CO

	type RecordCRCD crcd=Db.getRecord("CRCD","CO=:CO,CRCD=%SystemCurrency",1)
	set CRCDDESC=crcd.desc
	quit

VP2	// Column post-processor - [SYSDEV,NOSTROREP]ENDBAL

	type public String vc13
	type Public String EBAL,ENDBAL,NOSTROREP

	set EBAL=vc13
	quit 
