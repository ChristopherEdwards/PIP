vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL22 dbtbl22

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL22","%LIBS = :dbtbl22.%libs and AGID = :dbtbl22.agid") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(dbtbl22)) S ^DBTBL(vobj(dbtbl22,-3),22,vobj(dbtbl22,-4))=vobj(dbtbl22)
		#ENDBYPASS
	}

	quit

