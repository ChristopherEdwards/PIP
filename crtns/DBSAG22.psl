	type RecordDBTBL22 dbtbl22 = Db.getRecord("DBTBL22","%LIBS=:%LIBS,AGID=:AGID")
	if (%ProcessMode = 2) do {
		do dbtbl22.setMode(2)

		do DBSAG22(dbtbl22,vpar)
	}

