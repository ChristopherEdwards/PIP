vinit // Initialize default values

	type public RecordDBTBL22 dbtbl22

	// Type local variables for access keys for defaults
	type public String %LIBS
	type public String AGID

	if dbtbl22.dtl.isNull() set dbtbl22.dtl = 0            // dtl
	quit


vreqn // Validate required data items

	type public RecordDBTBL22 dbtbl22

	if dbtbl22.dtl.isNull() do vreqerr("DTL") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordDBTBL22 dbtbl22

	if dbtbl22.%libs.isNull() do vreqerr("%LIBS") quit
	if dbtbl22.agid.isNull() do vreqerr("AGID") quit

		if vx("DTL").exists(),dbtbl22.dtl.isNull() do vreqerr("DTL") quit
	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("DBTBL22","MSG",1767,"DBTBL22."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordDBTBL22 dbtbl22

	if dbtbl22.%libs.length()>12 set vRM = $$^MSG(1076,12) do vdderr("%LIBS", vRM) quit
	if dbtbl22.agid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("AGID", vRM) quit
	if dbtbl22.des.length()>40 set vRM = $$^MSG(1076,40) do vdderr("DES", vRM) quit
	if '("01"[dbtbl22.DTL) set vRM=$$^MSG(742,"L") do vdderr("DTL", vRM) quit
	set X = dbtbl22.dtp if 'X.isNull(),'{List}"0,1,2".contains(X) set vRM = $$^MSG(1485,X) do vdderr("DTP", vRM) quit
	if dbtbl22.frm.length()>40 set vRM = $$^MSG(1076,40) do vdderr("FRM", vRM) quit
	if dbtbl22.grp.length()>40 set vRM = $$^MSG(1076,40) do vdderr("GRP", vRM) quit
	if dbtbl22.rtn.length()>8 set vRM = $$^MSG(1076,8) do vdderr("RTN", vRM) quit
	if dbtbl22.whr1.length()>80 set vRM = $$^MSG(1076,80) do vdderr("WHR1", vRM) quit
	if dbtbl22.whr2.length()>80 set vRM = $$^MSG(1076,80) do vdderr("WHR2", vRM) quit
	if dbtbl22.whr3.length()>80 set vRM = $$^MSG(1076,80) do vdderr("WHR3", vRM) quit
	if dbtbl22.whr4.length()>80 set vRM = $$^MSG(1076,80) do vdderr("WHR4", vRM) quit
	if dbtbl22.whr5.length()>80 set vRM = $$^MSG(1076,80) do vdderr("WHR5", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("DBTBL22","MSG",979,"DBTBL22."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordDBTBL22 dbtbl22

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("%LIBS").exists() set vux("%LIBS") = vx("%LIBS")
	if vx("AGID").exists() set vux("AGID") = vx("AGID")
	do vkey(1) set voldkey = dbtbl22.%libs_","_dbtbl22.agid // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = dbtbl22.%libs_","_dbtbl22.agid // Copy new keys into object
	type RecordDBTBL22 vnewrec = dbtbl22.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("DBTBL22",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordDBTBL22 dbtbl22

	if vux("%LIBS").exists() set dbtbl22.%libs = vux("%LIBS").piece("|",i)
	if vux("AGID").exists() set dbtbl22.agid = vux("AGID").piece("|",i)
	quit
