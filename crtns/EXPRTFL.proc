	if vpar["/VALST/" if '(''Db.isDefined("UTBLEXPORT","PROJID = :utblexport.projid and EXPDATE = :utblexport.expdate") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		if %O = 0, vpar'["/NOLOG/" do ^DBSLOGIT(utblexport,%O,.vxins())
