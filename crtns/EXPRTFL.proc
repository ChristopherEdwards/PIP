vinit // Initialize default values

	type public RecordUTBLEXPORT utblexport

	// Type local variables for access keys for defaults
	type public String PROJID
	type public String EXPDATE

	if utblexport.complete.isNull() set utblexport.complete = 0 // complete
	quit


vreqn // Validate required data items

	type public RecordUTBLEXPORT utblexport

	if utblexport.complete.isNull() do vreqerr("COMPLETE") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordUTBLEXPORT utblexport

	if utblexport.projid.isNull() do vreqerr("PROJID") quit
	if utblexport.expdate.isNull() do vreqerr("EXPDATE") quit

		if vx("COMPLETE").exists(),utblexport.complete.isNull() do vreqerr("COMPLETE") quit
	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("UTBLEXPORT","MSG",1767,"UTBLEXPORT."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLEXPORT utblexport

	if utblexport.projid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("PROJID", vRM) quit
	set X = utblexport.expdate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EXPDATE", vRM) quit
	if '("01"[utblexport.COMPLETE) set vRM=$$^MSG(742,"L") do vdderr("COMPLETE", vRM) quit
	if utblexport.expname.length()>12 set vRM = $$^MSG(1076,12) do vdderr("EXPNAME", vRM) quit
	set X = utblexport.loaddate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("LOADDATE", vRM) quit
	if utblexport.projdesc.length()>40 set vRM = $$^MSG(1076,40) do vdderr("PROJDESC", vRM) quit
	if utblexport.version.length()>10 set vRM = $$^MSG(1076,10) do vdderr("VERSION", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("UTBLEXPORT","MSG",979,"UTBLEXPORT."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordUTBLEXPORT utblexport

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("PROJID").exists() set vux("PROJID") = vx("PROJID")
	if vx("EXPDATE").exists() set vux("EXPDATE") = vx("EXPDATE")
	do vkey(1) set voldkey = utblexport.projid_","_utblexport.expdate // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = utblexport.projid_","_utblexport.expdate // Copy new keys into object
	type RecordUTBLEXPORT vnewrec = utblexport.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("UTBLEXPORT",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordUTBLEXPORT utblexport

	if vux("PROJID").exists() set utblexport.projid = vux("PROJID").piece("|",i)
	if vux("EXPDATE").exists() set utblexport.expdate = vux("EXPDATE").piece("|",i)
	quit
