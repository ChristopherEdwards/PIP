 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLCOL.exists() set fUTBLCOL = Class.new("RecordUTBLCOLCD")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCOLCD",VPGM=$T(+0),VSNAME="Collateral Codes"
 set VFSN("UTBLCOLCD")="zfUTBLCOL"
 set vPSL=1
 set KEYS(1)=fUTBLCOL.GRP
 set KEYS(2)=fUTBLCOL.KEY
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLCOL),VDA1(.fUTBLCOL),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fUTBLCOL),VPR(.fUTBLCOL),VDA1(.fUTBLCOL)
 if %ProcessMode do VLOD(.fUTBLCOL) quit:$G(ER)  do VPR(.fUTBLCOL),VDA1(.fUTBLCOL)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLCOL)
	quit


VNEW(RecordUTBLCOLCD fUTBLCOL) // Initialize arrays if %O=0
 
 do VDEF(.fUTBLCOL)
 do VLOD(.fUTBLCOL)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
