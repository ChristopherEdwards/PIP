 set %MAX=13,VPT=1,VPB=16,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLCOLCD"
 set OLNTB=16042
 
 set VFSN("UTBLCOLCD")="zfUTBLCOL"
 //
 // 
	
	
 set %TAB(1)=$C(2,41,4)_"21T12401|1|[UTBLCOLCD]GRP|[STBLGRP]:QU ""[STBLGRP]CLS=""""L"""""""
 set %TAB(2)=$C(3,41,6)_"21T12402|1|[UTBLCOLCD]KEY|[UTBLCOLCD]"
 set %TAB(3)=$C(5,41,20)_"01T12401|1|[UTBLCOLCD]DES"
 set %TAB(4)=$C(6,41,4)_"01N12407|1|[UTBLCOLCD]TYPE|[STBLCOLTYP]"
 set %TAB(5)=$C(7,41,1)_"00L12402|1|[UTBLCOLCD]SEC"
 set %TAB(6)=$C(8,41,39)_"00T12405|1|[UTBLCOLCD]PCS|[DBTBL1D]:NOVAL:QU ""[DBTBL1D]FID=""""COL""""""||do VP1^V01S641(.fUTBLCOL)||||||60"
