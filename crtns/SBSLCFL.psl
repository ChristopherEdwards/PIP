	set vnewkey = vux.piece("|",2),vobj(stblsbslc,-3) = vnewkey // Copy new keys into object
	type RecordSTBLSBSLC vnewrec = stblsbslc.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

