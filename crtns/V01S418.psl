VP4(RecordLN LN,RecordCIF CIF) //
 /*
 -----Revision History---------------------------------------------------
 
 05/24/05 - MBUIM - CR15982
            Converted to PSL.
 */

 if '$L(X) do VPX2(.LN) quit

 set ARUF=LN.ARUF 
 // Transaction to Reduce Balance
 set TRB=LN.TRB      
 set BAL=LN.BAL

 // Unadvanced Balance
 if ARUF set BAL=BAL-LN.UDBAL   
 
 if X'="" do RNDPP^UINDX quit:ER 

 type String INDDATA

 set INDDATA=LN.INDEX_" "_LN.INTSPR_" "_X

 do CTL^UINDX(.LN,%EffectiveDate,BAL,"000101",INDDATA) if ER quit   

 do DEFAULT^DBSMACRO("LN.IRN",RATE,"1","0","0")

 // Default necessary data items for variable interest
 do DEFAULT^DBSMACRO("LN.ICHLD",$G(%EffectiveDate),"1","0","0")

 quit 

VPX2(RecordLN LN)
 
 if LN.INTFRE'="" do DEFAULT^DBSMACRO("LN.INTFRE","","1","0","0")
 if LN.ICHM'="" do DEFAULT^DBSMACRO("LN.ICHM","","1","0","0")
 if LN.INTOFF'="" do DEFAULT^DBSMACRO("LN.INTOFF","","1","0","0")
 if LN.ICHND'="" do DEFAULT^DBSMACRO("LN.ICHND","","1","0","0")
 if LN.ICHLD'="" do DEFAULT^DBSMACRO("LN.ICHLD","","1","0","0")
 
