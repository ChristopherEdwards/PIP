VP1(RecordLN LN,RecordCIF CIF) //
 /*
 -----Revision History---------------------------------------------------
	
 05/24/05 - MBUIM - CR15982
            Converted to PSL.
 
 11/06/01 - Carrie Baildon - 47410
 	    Default the newly computed billing date that 
	    corresponds to the new scheduled payment date.

 */

 type String DATA,LBDD
 
 set LBDD=LN.LBDD 
 if LN.LBDD="" set LBDD=LN.SCHLD
 
 set DATA=$$DIST1FRE^LNU(LN.DIST1FRE,X,LBDD,LN.DIST1ND)

 do DEFAULT^DBSMACRO("LN.SCHLD",$P(DATA,"|",3),"1","0","0")
 do DEFAULT^DBSMACRO("LN.BNDT",$P(DATA,"|",2),"1","0","0")
 
 quit

