V01S498( Number %ProcessMode,RecordNOSTTRF fNOSTTRF)   //PBS -  - SID= <NOSTTRF> Nostro Transfer Screen Linkage
 ;;Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 11:51 - shetyes
 #WARN ACCESS,MISMATCH OFF
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fNOSTTRF)
 if %ProcessMode do VLOD(.fNOSTTRF) if $G(ER) set VFMQ="Q" quit
 do VPG(.fNOSTTRF)
 quit

VNEW(RecordNOSTTRF fNOSTTRF)
 
 quit
 //
 ;
VLOD(RecordNOSTTRF fNOSTTRF)  // User defined access section
	quit

VPG(RecordNOSTTRF fNOSTTRF)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fNOSTTRF),VPG0(.fNOSTTRF) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fNOSTTRF),VPG0(.fNOSTTRF) quit:vDONE
  }
 quit

VPG0(RecordNOSTTRF fNOSTTRF)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fNOSTTRF) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordNOSTTRF fNOSTTRF)  // Nostro Transfer Screen 1
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="NOSTTRF1",DFID="NOSTTRF"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S501
 if %ProcessMode>1 quit
 do VTAB^V01S501(.fNOSTTRF)
 quit
 
VPG2(RecordNOSTTRF fNOSTTRF)  // Nostro Transfer Screen 2
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="NOSTTRF2",DFID="NOSTTRF"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S502
 if %ProcessMode>1 quit
 do VTAB^V01S502(.fNOSTTRF)
 quit
 

VBTM(RecordNOSTTRF fNOSTTRF)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
VSCRPRE(RecordNOSTTRF fNOSTTRF) // Screen Pre-Processor
 new %TAB,vtab  // Disable .MACRO. references to %TAB()
 //
 set CO=CUVAR.co
 if %ProcessMode'=0 quit
 
 set fNOSTTRF.status=1
 set fNOSTTRF.edt=%SystemDate
 set fNOSTTRF.uid=%UserID
 type RecordNOSTTRF nosttrf=Db.getRecord("NOSTTRF","TRREFNO=:TRREFNO",1)
 set STATDSC=$$DSTATDSC^PROCOMP(nosttrf.status)
 quit
 set ER=0 do VSCRPRE(.fNOSTTRF) if ER quit  // Screen Pre-Processor
 //
 type Public Number ER
 type Public String %PAG,%PAGE,%PG,%PGSV,MULSCR,RM,VFMQ
 type String KVAR,VPG(),vPSL,VSID
 set VSID="NOSTTRF"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+1 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+2
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Nostro Transfer Screen 1|NOSTTRF1"
 set VPG(%PG+1)="Nostro Transfer Screen 2|NOSTTRF2"
