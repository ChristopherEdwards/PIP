 set %TAB(1)=$C(4,55,4)_"01N12407|1|[CUVAR]DIN"
 set %TAB(2)=$C(5,55,4)_"01N12401|1|[CUVAR]DRMT"
 set %TAB(3)=$C(6,55,5)_"01N12402|1|[CUVAR]ESCH"
 set %TAB(4)=$C(7,55,12)_"00U12401|1|[CUVAR]ESCHEAT|[DBTBL4]"
 set %TAB(5)=$C(12,24,1)_"00L12403|1|[CUVAR]STMLCC"
 set %TAB(6)=$C(12,64,1)_"00L12404|1|[CUVAR]LCCADR"
 set %TAB(7)=$C(13,24,1)_"00L12405|1|[CUVAR]LCCTIT"
