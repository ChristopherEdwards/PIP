 set VO="11||13|"
 set VO(0)="|0"
 set VO(1)=$C(1,26,29,1,0,0,0,0,0,0)_"01T Deposit Account Information "
 set VO(2)=$C(3,24,33,2,0,0,0,0,0,0)_"01TDormancy and Escheat Information "
 set VO(3)=$C(5,19,36,1,0,0,0,0,0,0)_"01T Days Inactive To Consider Inactive:"
