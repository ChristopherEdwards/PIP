 set VO(19)=$C(14,65,1,2,0,0,0,0,0,0)_"00L"_$S(fCUVAR.LCCPU:"Y",1:"N")
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
	
