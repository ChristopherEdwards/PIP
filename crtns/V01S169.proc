 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=8,VPT=1,VPB=14,PGM=$T(+0),DLIB="SYSDEV",DFID="CUVAR"
 set OLNTB=14065
 
 set VFSN("CUVAR")="zfCUVAR"
 //
