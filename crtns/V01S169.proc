 set VO="19|12|13|"
 set VO(12)=$C(5,56,4,2,0,0,0,0,0,0)_"00N"_fCUVAR.DIN
 set VO(13)=$C(6,56,4,2,0,0,0,0,0,0)_"00N"_fCUVAR.DRMT
 set VO(14)=$C(7,56,5,2,0,0,0,0,0,0)_"00N"_fCUVAR.ESCH
 set VO(15)=$C(8,56,12,2,0,0,0,0,0,0)_"00U"_$E(fCUVAR.ESCHEAT,1,12)
 set VO(16)=$C(13,25,1,2,0,0,0,0,0,0)_"00L"_$S(fCUVAR.STMLCC:"Y",1:"N")
 set VO(17)=$C(13,65,1,2,0,0,0,0,0,0)_"00L"_$S(fCUVAR.LCCADR:"Y",1:"N")
