vai1 // Trigger AFTER_INSERT - Modify fee charge account 

	type public RecordSTOP7 stop7
	/*
	 --------- Revision History -----------------------------------------
	
	05/24/00 - JARVISG - 39638
		   Modified to add FEECAT,FEETYP parameters to SRVSTP call.
	
	12/20/99 - JARVISG - 34535
		   Add for stop fee processing.
	*/

	if %ProcessMode'=0 quit			
	
	set FEECID7=stop7.feecid                       // Fee Charge Account
	if $G(FEECID7) do {  quit:ER
		new values
		set values=Db.getOneRow("STAT,CLS","DEP","FEECID7")
		// Error if fee account is closed or not a deposit account.
		if $P(values,$C(9),1)=4 do Runtime.setErrMSG("STOP7",457,"FEECID7") quit:ER
		if $P(values,$C(9),2)'="D" do Runtime.setErrMSG("STOP7",803) quit:ER
		do ^SRVSTP(FEECID7,7,2,"STP")           // Assess stop type 7 fee
		}

	quit

vau1 // Trigger AFTER_UPDATE - Modify fee charge account

	type public RecordSTOP7 stop7
	if %ProcessMode'=1 quit     // 1 = Update
	set FEECID7=stop7.feecid
	if (stop7.feecid.oldVal'=stop7.feecid) do Runtime.setErrMSG("STOP7",412,"FEECID7") quit:ER
	quit


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSTOP7 stop7

	set X = stop7.inst if 'X.isNull(),'Db.isDefined("UTBLEFTINST","X") set vRM = $$^MSG(1485,X) do vdderr("INST", vRM) quit
	if stop7.outacc.length()>20 set vRM = $$^MSG(1076,20) do vdderr("OUTACC", vRM) quit
	if stop7.outchk.length()>20 set vRM = $$^MSG(1076,20) do vdderr("OUTCHK", vRM) quit
	set X = stop7.act if 'X.isNull(),'Db.isDefined("STBLSTOP","X") set vRM = $$^MSG(1485,X) do vdderr("ACT", vRM) quit
	if stop7.cmt.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CMT", vRM) quit
	set X = stop7.dp if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("DP", vRM) quit
	set X = stop7.exp if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EXP", vRM) quit
	set X = stop7.feecid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("FEECID", vRM) quit
	if stop7.outchke.length()>20 set vRM = $$^MSG(1076,20) do vdderr("OUTCHKE", vRM) quit
	if stop7.own.length()>60 set vRM = $$^MSG(1076,60) do vdderr("OWN", vRM) quit
	if '("01"[stop7.STCON) set vRM=$$^MSG(742,"L") do vdderr("STCON", vRM) quit
	set X = stop7.stpwhy if 'X.isNull(),'Db.isDefined("UTBLSTPWHY","X") set vRM = $$^MSG(1485,X) do vdderr("STPWHY", vRM) quit
	set X = stop7.time if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"C") do vdderr("TIME", vRM) quit
	if stop7.who.length()>40 set vRM = $$^MSG(1076,40) do vdderr("WHO", vRM) quit
	quit

