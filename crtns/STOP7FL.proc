vreqn // Validate required data items

	type public RecordSTOP7 stop7

	if stop7.act.isNull() do vreqerr("ACT") quit
	if stop7.dp.isNull() do vreqerr("DP") quit
	if stop7.stcon.isNull() do vreqerr("STCON") quit
	if stop7.time.isNull() do vreqerr("TIME") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordSTOP7 stop7

	if stop7.inst.isNull() do vreqerr("INST") quit
	if stop7.outacc.isNull() do vreqerr("OUTACC") quit
	if stop7.outchk.isNull() do vreqerr("OUTCHK") quit

		if vx("ACT").exists(),stop7.act.isNull() do vreqerr("ACT") quit
		if vx("DP").exists(),stop7.dp.isNull() do vreqerr("DP") quit
		if vx("STCON").exists(),stop7.stcon.isNull() do vreqerr("STCON") quit
		if vx("TIME").exists(),stop7.time.isNull() do vreqerr("TIME") quit
	quit

