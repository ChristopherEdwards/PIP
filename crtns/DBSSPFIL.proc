public VINDEX(RecordDBTBLSP dbtblsp) // Update index entries

		type Public String vx()

	if %ProcessMode=1 do { quit
		if vx("HASHKEY").exists() do vi1(.dbtblsp)
	}
	do vi1(.dbtblsp)

	quit
	

vi1(RecordDBTBLSP dbtblsp) // Maintain HASHKEY index entries (Hash Key)

	type Public String vx()
	type Boolean vdelete = 0
	type String v3 = dbtblsp.hashkey
	if v3.isNull() set v3=(PSL.maxCharValue-1).char()
	type String v4 = dbtblsp.pid

	if %ProcessMode=2 do { quit

		// Allow global reference
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
		if $D(^DBTBLSP(vobj(dbtblsp,-3)))#2,'$D(^DBINDX("SYSDEV","SPHK",v3,v4)) do vidxerr("HASHKEY")
		}


	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	if %ProcessMode<2 set ^DBINDX("SYSDEV","SPHK",v3,v4)="" 
	quit:%ProcessMode=0

	if %ProcessMode=3 set vdelete=1
	if vx("HASHKEY").exists() set v3=vx("HASHKEY").piece("|",1) set:v3.isNull() v3=(PSL.maxCharValue-1).char()

	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	kill ^DBINDX("SYSDEV","SPHK",v3,v4)
	quit

public VIDXBLD(List vlist) // Rebuild index files (External call)


	type Number %ProcessMode=0                             // Create mode
	type Number i

	if vlist.get().isNull() set vlist="VINDEX"             // Build all

	type DbSet ds=Db.selectDbSet("DBTBLSP")

	while ds.next() do {
		type RecordDBTBLSP dbtblsp=ds.getRecord("DBTBLSP")
		if vlist.contains("VINDEX") do VINDEX(.dbtblsp) quit
		if vlist.contains("HASHKEY") do vi1(.dbtblsp)
	}

	quit


public VIDXBLD1(RecordDBTBLSP dbtblsp, List vlist) // Rebuild index files for one record (External call)


	type Number i

	if vlist.contains("VINDEX") do VINDEX(.dbtblsp) quit
	if vlist.contains("HASHKEY") do vi1(.dbtblsp)

	quit


vidxerr(di) // Error message

	D SETERR^DBSEXECU("DBTBLSP","MSG",1225,"DBTBLSP."_di)

	quit


vkchged // Access key changed

	type public RecordDBTBLSP dbtblsp

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("PID")
	set voldkey = vux.piece("|",1),vobj(dbtblsp,-3) = voldkey // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	set vnewkey = vux.piece("|",2),vobj(dbtblsp,-3) = vnewkey // Copy new keys into object
	type RecordDBTBLSP vnewrec = dbtblsp.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("DBTBLSP",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	set vobj(dbtblsp,-3) = vux.piece("|",1)                // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

