		   precedence" warning. Removed old revision history.
		   Cleaned up to comply with DBI standards.
	*/ 
	
	quit


BILSEQ(RecordLNBIL1 lnbil1(),	// LNBIL1 object array
       Number CID,		// Account Number	
       Number firstseq,		// Billing Sequence
       String LNBIL1)		// LNBIL1 array

	// Returns next billing sequences
	// Called from LNBIL0 trigger which loads bills
	
	type Number NXTSEQ

	set firstseq=0

	type ResultSet rs=Db.select("SCHSEQ","LNBIL1","CID=:CID")
	
	// Quit with null in case there are no biils 
	if rs.isEmpty() quit ""
	
	while rs.next() do {
		set NXTSEQ=rs.getCol("SCHSEQ")
		if firstseq=0 set firstseq=NXTSEQ
		set lnbil1(NXTSEQ)=Db.getRecord("LNBIL1","CID,NXTSEQ")
		set LNBIL1(NXTSEQ)=""
		}
		
	quit NXTSEQ


