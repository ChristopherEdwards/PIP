public ELECHK(Number PPDCAT1,	// Payment Path
              String ELEMENT)	// Payment Element		

	// Checks if Principal element exists in bill record.

	type String check

	set check="-"_PPDCAT1_"-"

	if check[("-"_ELEMENT_"-") quit 1

	quit 0


