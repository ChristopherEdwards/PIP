	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - Variable: CATVAL

 type public Number CATCNT,CATVAL

 set CATCNT=0
 set CATVAL=0
	quit

VP2	// Column post-processor - Variable: TYVAL

 type public Number CATCNT,CATVAL,TYPCNT,TYVAL

 set CATCNT=CATCNT+TYPCNT
 set CATVAL=CATVAL+TYVAL
 set TYPCNT=0
 set TYVAL=0
	quit

VP3	// Column post-processor - [SYSDEV,UTBLSRVTYP]TYPVAL

	type public String vc8
 type public Number GRPCNT,GRPVAL

 set GRPCNT=GRPCNT+1
 set GRPVAL=GRPVAL+vc8
	quit

VP4	// Column post-processor - Variable: GRPVAL

 type public Number GRPCNT,GRPVAL,TYPCNT,TYVAL
 
 set TYPCNT=TYPCNT+GRPCNT
 set TYVAL=TYVAL+GRPVAL
 set GRPCNT=0
 set GRPVAL=0
	quit
