			quit:'$D(vobj(memopymts,n))
			// Allow global reference and M source code
			#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
			#BYPASS
				S ^MEMO("PAYMENTS",vobj(memopymts,-3),vobj(memopymts,-4),n)=vobj(memopymts,n)
			#ENDBYPASS
		}


		if vpar["/JOURNAL/" do VJOURNAL(.memopymts)           // Create journal files
	}

	quit

vload // Record Load - force loading of unloaded data

