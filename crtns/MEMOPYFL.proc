	type public RecordMEMOPYMTS memopymts

	if vux("SERVICE").exists() set memopymts.service = vux("SERVICE").piece("|",i)
	if vux("ACCKEY").exists() set memopymts.acckey = vux("ACCKEY").piece("|",i)
	quit
