vdelete(Boolean vkeychg) // Record Delete

	type public String vobj(),vpar
	type public RecordMEMOPYMTS memopymts

	if 'vkeychg.get(),memopymts.isChanged() throw Class.new("Error","%PSL-E-DBFILER,Deleted object cannot be modified")

