vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordMEMOPYMTS memopymts

	if (%O = 2) do vload

	if vobj(memopymts,1).exists() do {

		if memopymts.acctname.length()>25 set vRM = $$^MSG(1076,25) do vdderr("ACCTNAME", vRM) quit
		set X = memopymts.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
		if memopymts.identifier.length()>25 set vRM = $$^MSG(1076,25) do vdderr("IDENTIFIER", vRM) quit
		set X = memopymts.memorelc if 'X.isNull(),'Db.isDefined("UTBLRELSRV","X") set vRM = $$^MSG(1485,X) do vdderr("MEMORELC", vRM) quit
		set X = memopymts.odt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ODT", vRM) quit
		set X = memopymts.status if 'X.isNull(),'Db.isDefined("STBLESTATUS","X") set vRM = $$^MSG(1485,X) do vdderr("STATUS", vRM) quit
	}

	if vobj(memopymts,20).exists() do {

		if memopymts.rtn.length()>9 set vRM = $$^MSG(1076,9) do vdderr("RTN", vRM) quit
	}
	if memopymts.service.length()>20 set vRM = $$^MSG(1076,20) do vdderr("SERVICE", vRM) quit
	if memopymts.acckey.length()>40 set vRM = $$^MSG(1076,40) do vdderr("ACCKEY", vRM) quit
	quit

