VJOURNAL(RecordMEMOPYMTS memopymts)	//MEMOPYMTS Journal file entries

	type Public Date %EffectiveDate
	type Public String %TSRC,vpar,vx()
	type String TSRC,vdi,vdx()

	if %TSRC.get().isNull() set TSRC="O"
	else  set TSRC=%TSRC

	if %ProcessMode=3 do {
		if TSRC="B" do {
			do vj1(.memopymts)	// Mode=D Tran=B EFD=N,E Seq=1 JRNID=CIFH_D
			}
		else  if TSRC="O" do {
			do vj1(.memopymts)	// Mode=D Tran=O EFD=N,E Seq=1 JRNID=CIFH_D
			}
		}
	else  if %ProcessMode=0 do {
		if TSRC="B" do {
			do vj2(.memopymts)	// Mode=I Tran=B EFD=N,E Seq=1 JRNID=CIFH_I
			}
		else  if TSRC="O" do {
			do vj2(.memopymts)	// Mode=I Tran=O EFD=N,E Seq=1 JRNID=CIFH_I
			}
		}
	else  if %ProcessMode=1 do {
		if TSRC="B" do {
			do vj3(.memopymts)	// Mode=U Tran=B EFD=N,E Seq=1 JRNID=CIFH_U
			}
		else  if TSRC="O" do {
			do vj3(.memopymts)	// Mode=U Tran=O EFD=N,E Seq=1 JRNID=CIFH_U
			}
		}

	quit


