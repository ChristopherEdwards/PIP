vEXEC(vCONTEXT,ACN,TKS) //
	type public Boolean ER

	type Date NJD
 	
 	type RecordTIKLCIF tiklcif=Db.getRecord("TIKLCIF","ACN=:ACN,TKS=:TKS",1)
 	
 	quit:tiklcif.arf.isNull()
 	
	set NJD=%SystemDate.nextFreqDate(tiklcif.arf)

	// Invalid frequency
	if (ER)!(NJD<0) do Runtime.setErrSTBLER("TIKLCIF","INVLDFRE1") quit:ER
 
 	set tiklcif.ARLD=%SystemDate
 
	if ('tiklcif.expd)!(NJD'>tiklcif.expd) set tiklcif.arnd=NJD

 	do tiklcif.bypassSave()

	quit

