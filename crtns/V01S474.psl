	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
vREAD(fid,di)
 type Public RecordSWIFT fSWIFT
 if fid="SWIFT" quit $$vREAD1(.fSWIFT,di)
 quit ""
vREAD1(RecordSWIFT fSWIFT,di)
 quit fSWIFT.@di
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
 //
VSCRPRE(RecordSWIFT fSWIFT)  // Screen Pre-Processor
 new %TAB,vtab // Disable .MACRO. references to %TAB()
 //
 ; The purpose of this code is to be able to display free form text on this
 ; screen.  This code is also in the screen pre-processor for driver screen
 ; MT292DRV.  Modifications to this code should be made in both places.
 ;
 new CS2,I,REC
 set REC=$$MULT^SWIFTCDI(SWDIRECT,TRREFNO,MSG,79),CS2=$C(13,10)
 for I=1:1:6 S SWARRAY(I)=$P(REC,CS2,I)
 ;
 #ACCEPT date=11/05/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
