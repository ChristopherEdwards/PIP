vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSBADVDTL sbadvdtl

	set X = sbadvdtl.procdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("PROCDT", vRM) quit
	if sbadvdtl.msgid.length()>40 set vRM = $$^MSG(1076,40) do vdderr("MSGID", vRM) quit
	set X = sbadvdtl.brcd if 'X.isNull(),'Db.isDefined("UTBLBRCD","X") set vRM = $$^MSG(1485,X) do vdderr("BRCD", vRM) quit
	if '("01"[sbadvdtl.FLDFLG) set vRM=$$^MSG(742,"L") do vdderr("FLDFLG", vRM) quit
	set X = sbadvdtl.isobin if 'X.isNull(),'Db.isDefined("UTBLSBCU","X") set vRM = $$^MSG(1485,X) do vdderr("ISOBIN", vRM) quit
	if sbadvdtl.msgtyp.length()>4 set vRM = $$^MSG(1076,4) do vdderr("MSGTYP", vRM) quit
	if sbadvdtl.srccid.length()>28 set vRM = $$^MSG(1076,28) do vdderr("SRCCID", vRM) quit
	set X = sbadvdtl.tamt if 'X.isNull() set vRM = $$VAL^DBSVER("$",18,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"SBADVDTL.TAMT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	set X = sbadvdtl.tmssnt if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("TMSSNT", vRM) quit
	if sbadvdtl.tocid.length()>28 set vRM = $$^MSG(1076,28) do vdderr("TOCID", vRM) quit
	if sbadvdtl.trntyp.length()>40 set vRM = $$^MSG(1076,40) do vdderr("TRNTYP", vRM) quit
	if sbadvdtl.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

