 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fLNRNW.exists() set fLNRNW = Class.new("RecordLNRENEW")
  if 'LN.exists() set LN = Class.new("RecordLN")
  if 'CIF.exists() set CIF = Class.new("RecordCIF")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNRNWOPT1",VPGM=$T(+0),VSNAME="Loan Renewal Options Screen 1"
 set VFSN("CIF")="zCIF",VFSN("LN")="zLN",VFSN("LNRENEW")="zfLNRNW"
 set vPSL=1
 set KEYS(1)=LN.CID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fLNRNW,.LN,.CIF),VDA1(.fLNRNW,.LN,.CIF),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fLNRNW,.LN,.CIF),VPR(.fLNRNW,.LN,.CIF),VDA1(.fLNRNW,.LN,.CIF)
 if %ProcessMode do VLOD(.fLNRNW,.LN,.CIF) quit:$G(ER)  do VPR(.fLNRNW,.LN,.CIF),VDA1(.fLNRNW,.LN,.CIF)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fLNRNW,.LN,.CIF)
	quit


VNEW(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF) // Initialize arrays if %O=0
 
 do VDEF(.fLNRNW,.LN,.CIF)
 do VLOD(.fLNRNW,.LN,.CIF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

