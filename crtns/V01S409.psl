 new V
 //
 set VO="37|20|13|0"
 set VO(20)=$C(2,20,12,2,0,0,0,0,0,0)_"01N"_fLNRNW.CID
 set VO(21)=$C(2,34,40,2,0,0,0,0,0,0)_"01T"_$E(CIF.NAM,1,40)
