VTAB(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=18,VPT=2,VPB=20,PGM=$T(+0),DLIB="SYSDEV",DFID="LNRENEW,LN,CIF"
 set OLNTB=20065
 
 set VFSN("CIF")="zCIF",VFSN("LN")="zLN",VFSN("LNRENEW")="zfLNRNW"
 //
 // 
	
	
 set %TAB(1)=$C(1,19,12)_"21N12401|1|[LNRENEW]CID|[ACN]"
 set %TAB(2)=$C(1,33,40)_"21T12401|1|[CIF]NAM"
 set %TAB(3)=$C(2,19,3)_"21N12402|1|[LNRENEW]ROSEQ|||||||||12"
 set %TAB(4)=$C(7,22,1)_"00L12401|1|[LNRENEW]CNVAPP|||do VP1^V01S409(.fLNRNW,.LN,.CIF)"
 set %TAB(5)=$C(7,64,1)_"00L12403|1|[LNRENEW]MROPT|||do VP2^V01S409(.fLNRNW,.LN,.CIF)"
 set %TAB(6)=$C(8,64,12)_"00T12402|1|[LNRENEW]STRQRY|[UTBLSTRQ]"
 set %TAB(7)=$C(10,22,6)_"00N12407|1|[LNRENEW]CC|[UTBLCCNTR]"
 set %TAB(8)=$C(10,64,8)_"00T12408|1|[LNRENEW]TRM"
 set %TAB(9)=$C(11,22,4)_"00N12401|1|[LNRENEW]TYPE|[PRODCTL]"
 set %TAB(10)=$C(11,64,8)_"00T12410|1|[LNRENEW]PTRM"
 set %TAB(11)=$C(12,22,4)_"00T12421|1|[LNRENEW]GLSC|[UTBLGLSC]"
 set %TAB(12)=$C(12,64,10)_"00D12402|1|[LNRENEW]MDT"
 set %TAB(13)=$C(13,64,12)_"00F12403|1|[LNRENEW]DIST1FRE"
 set %TAB(14)=$C(17,22,10)_"00D12437|1|[LNRENEW]RDT"
 set %TAB(15)=$C(17,64,3)_"00N12441|1|[LNRENEW]RNOFF"
 set %TAB(16)=$C(18,22,12)_"00F12438|1|[LNRENEW]RFREQ"
