	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Date VIN1=$$RPDT()
	type Number VIN2

	set RID="SCA735"
	set RN="FIDM All Accounts Audit Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|10||[FIDMALL]REPDATE|[FIDMALL]REPDATE:DISTINCT:NOVAL||||D|Report Date|||||"
	set %TAB("VIN2")="|1||[FIDMALL]REPTYPE|[STBLFIDM]:NOVAL||||N|Report Type|||||"

	set %READ="IO/REQ,VIN1#1,VIN2#1,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public Date VIN1
	type public Number VIN2
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,RID,RN,TOTACCT,TOTACCTCL,TOTACCTIRA,TOTACCTM,TOTACCTN,TOTACCTTR,TOTAMT,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA735",RN="FIDM All Accounts Audit Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA735","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[FIDMALL]CID#1#12,[FIDMALL]PROWNER#19#40,[FIDMALL]TAXIDPR#61#20,[FIDMALL]ACCTIND#94#1,[FIDMALL]BAL#98#14,[FIDMALL]CIDTYPE#126#2\[FIDMALL]SECOWNER#19#40,[FIDMALL]TAXIDSEC#61#20,[FIDMALL]TRUSTIND#111#1,[FIDMALL]UPDIND#127#1"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE

		set VBNRINFO("PROMPTS",1)="WC1|"_"Report Date"_"|VIN1|"_$$DAT^%ZM(VIN1.get(),%MSKD)
		set VBNRINFO("PROMPTS",2)="WC1|"_"Report Type"_"|VIN2|"_VIN2.get()

		set VWHERE="FIDMALL.REPDATE=:VIN1 AND FIDMALL.REPTYPE=:VIN2"

		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="FIDM All Accounts Audit Report"
		set VBNRINFO("PGM")="R01S620"
		set VBNRINFO("RID")="SCA735"
		set VBNRINFO("TABLES")="FIDMALL,FIDMALLTOT"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,FIDMALL]REPDATE"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,FIDMALL]REPTYPE"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,FIDMALL]TAXIDPR"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,FIDMALL]CID"

		set VBNRINFO("DOC",1)="08/17/05 - SWARNALATHAP - CR16734"
		set VBNRINFO("DOC",2)="	Made the following changes with respect to DBI3 Project - ""US "
		set VBNRINFO("DOC",3)="	Regulatory"" system area"
		set VBNRINFO("DOC",4)="	.	Removed quotes around report prompt in Prompts/Query section"
		set VBNRINFO("DOC",5)="	.       Report fully converted to PSL"

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("FIDMALL.REPDATE,FIDMALL.REPTYPE,FIDMALL.TAXIDPR,FIDMALL.CID,FIDMALL.PROWNER,FIDMALL.ACCTIND,FIDMALL.BAL,FIDMALL.CIDTYPE,FIDMALL.SECOWNER,FIDMALL.TAXIDSEC,FIDMALL.TRUSTIND,FIDMALL.UPDIND,FIDMALLTOT.TOTACCT,FIDMALLTOT.TOTACCTTR,FIDMALLTOT.TOTAMT,FIDMALLTOT.TOTACCTIRA,FIDMALLTOT.TOTACCTCL,FIDMALLTOT.TOTACCTN,FIDMALLTOT.TOTACCTM","FIDMALL,FIDMALLTOT","FIDMALL.REPDATE=:VIN1 AND FIDMALL.REPTYPE=:VIN2","FIDMALL.REPDATE,FIDMALL.REPTYPE,FIDMALL.TAXIDPR,FIDMALL.CID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19
	set vc1=V.piece($C(9),1)			// FIDMALL.REPDATE
	set vc2=V.piece($C(9),2)			// FIDMALL.REPTYPE
	set vc3=V.piece($C(9),3)			// FIDMALL.TAXIDPR
	set vc4=V.piece($C(9),4)			// FIDMALL.CID
	set vc5=V.piece($C(9),5)			// FIDMALL.PROWNER
	set vc6=V.piece($C(9),6)			// FIDMALL.ACCTIND
	set vc7=V.piece($C(9),7)			// FIDMALL.BAL
	set vc8=V.piece($C(9),8)			// FIDMALL.CIDTYPE
	set vc9=V.piece($C(9),9)			// FIDMALL.SECOWNER
	set vc10=V.piece($C(9),10)			// FIDMALL.TAXIDSEC
	set vc11=V.piece($C(9),11)			// FIDMALL.TRUSTIND
	set vc12=V.piece($C(9),12)			// FIDMALL.UPDIND
	set vc13=V.piece($C(9),13)			// FIDMALLTOT.TOTACCT
	set vc14=V.piece($C(9),14)			// FIDMALLTOT.TOTACCTTR
	set vc15=V.piece($C(9),15)			// FIDMALLTOT.TOTAMT
	set vc16=V.piece($C(9),16)			// FIDMALLTOT.TOTACCTIRA
	set vc17=V.piece($C(9),17)			// FIDMALLTOT.TOTACCTCL
	set vc18=V.piece($C(9),18)			// FIDMALLTOT.TOTACCTN
	set vc19=V.piece($C(9),19)			// FIDMALLTOT.TOTACCTM
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

	quit


RPDT()
	type Date RPDATE

	set RPDATE = ""

	type ResultSet rs = Db.select("REPDATE","FIDMALL",,"REPDATE DESC")
	if rs.next() set RPDATE = rs.getCol("REPDATE")
	
	quit RPDATE

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do VSUM2 quit:VFMQ  do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VSUM2	// Summary
	type public String %TIM,I,IOSL,TOTACCT,TOTACCTCL,TOTACCTIRA,TOTACCTM,TOTACCTN,TOTACCTTR,TOTAMT,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+8>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="Total Accts Reported:"
	set VL=VL_$J("",37-VL.length())_$J(vc13,9)
	do VOM
	set VL="Total Trust Accts:"
	set VL=VL_$J("",37-VL.length())_$J(vc14,9)
	do VOM
	set VL="Total Balances:"
	set VL=VL_$J("",32-VL.length())_$J($FN(vc15,",",2),14)
	do VOM
	set VL="Total IRAs: "
	set VL=VL_$J("",37-VL.length())_$J(vc16,9)
	do VOM
	set VL="Total Closed  (Update Only):"
	set VL=VL_$J("",37-VL.length())_$J(vc17,9)
	do VOM
	set VL="Total New Accounts (Update Only):"
	set VL=VL_$J("",37-VL.length())_$J(vc18,9)
	do VOM
	set VL="Total"
	set VL=VL_$J("",6-VL.length())_"Modified"
	set VL=VL_$J("",16-VL.length())_"(Update Only):"
	set VL=VL_$J("",37-VL.length())_$J(vc19,9)
	do VOM
	set VL="" do VOM
	quit

VDTL4	// Detail
	type public String %TIM,IOSL,TOTACCT,TOTACCTCL,TOTACCTIRA,TOTACCTM,TOTACCTN,TOTACCTTR,TOTAMT,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc4,12)
	set VL=VL_$J("",18-VL.length())_$E(vc5,1,40)
	set VL=VL_$J("",60-VL.length())_$E(vc3,1,20)
	set VL=VL_$J("",93-VL.length())_$J(vc6,1)
	set VL=VL_$J("",97-VL.length())_$J($FN(vc7,",",2),14)
	set VL=VL_$J("",125-VL.length())_$J(vc8,2)
	do VOM
	set VL="                  "_$E(vc9,1,40)
	set VL=VL_$J("",60-VL.length())_$E(vc10,1,20)
	set VL=VL_$J("",110-VL.length())_$J(vc11,1)
	set VL=VL_$J("",126-VL.length())_$J(vc12,1)
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,TOTACCT,TOTACCTCL,TOTACCTIRA,TOTACCTM,TOTACCTN,TOTACCTTR,TOTAMT,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+8,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL="" do VOM
	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="                                        "
	do VOM
	set VL="" do VOM
	set VL="Account Number    Primary Owner                             Primary SSN            Acct Status   Ledger Balance       Acct Type"
	do VOM
	set VL="                  "_"Secondary Owner                           Secondary SSN                        Trust Fund Ind      Update Ind"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors
