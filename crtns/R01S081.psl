		set VBNRINFO("ORDERBY",2)="[SYSDEV,LNACR3]3"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,LNACR3]GRP"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,LNACR3]TYPE"
