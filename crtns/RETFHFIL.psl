vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordRETFHIST retfhist

	if retfhist.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	if '("01"[retfhist.FEESTAT) set vRM=$$^MSG(742,"L") do vdderr("FEESTAT", vRM) quit
	if retfhist.pretres.length()>12 set vRM = $$^MSG(1076,12) do vdderr("PRETRES", vRM) quit
	set X = retfhist.fdate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("FDATE", vRM) quit
	set X = retfhist.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	set X = retfhist.fseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("FSEQ", vRM) quit
	set X = retfhist.feeamt if 'X.isNull(),X'?1.18N,X'?1"-"1.17N set vRM=$$^MSG(742,"N") do vdderr("FEEAMT", vRM) quit
	quit

