	type public String verrors()

	type String vx(), vxins()                              // audit column array
	type Number %O = retfhist.getMode()                    // Processing mode
	set vpar = vpar.get()                                  // Initialize vpar

	if %O = 0 do AUDIT^UCUTILN(retfhist,.vxins(),1,"|")
	if %O = 1 do AUDIT^UCUTILN(retfhist,.vx(),1,"|")

	if 'vparNorm.get() set vpar = $$initPar^UCUTILN(vpar)  // Run-time qualifiers

	if %O = 0 do { quit                                    // Create record control block
		do vinit                                              // Initialize column values
		if vpar["/VALREQ/" do vreqn                           // Check required
		if vpar["/VALDD/" do vddver                           // Check values
		do vexec
	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("UID").exists()!vx("FEESTAT").exists()!vx("PRETRES").exists()!vx("FDATE").exists()!vx("CID").exists()!vx("FSEQ").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("RETFHIST",.vx)
		set %O = 1 do vexec
	}

	if %O = 2 do { quit                                    // Verify record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		set vpar = $$setPar^UCUTILN(vpar,"NOJOURNAL/NOUPDATE")
		do vexec
	}

	if %O = 3 do { quit                                    // Delete record control block
		quit:'Db.isDefined("RETFHIST","UID = :retfhist.uid and FEESTAT = :retfhist.feestat and PRETRES = :retfhist.pretres and FDATE = :retfhist.fdate and CID = :retfhist.cid and FSEQ = :retfhist.fseq") // No record exists
		do vdelete(0)
	}

	quit

