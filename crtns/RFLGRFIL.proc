	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	ZWI ^HLD(13,vobj(rflgrpa,-3),vobj(rflgrpa,-4),vobj(rflgrpa,-5)) // Delete Record
	quit


