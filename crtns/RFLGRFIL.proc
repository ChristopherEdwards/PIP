vad1 // Trigger AFTER_DELETE - RFLGRPA After Delete Trigger

	type public RecordRFLGRPA rflgrpa
	/* 
	   Revision History

	   09/17/99 - CHOK - 31126
		      Converted to PSL
	*/
	
	new B,XRFLG
	set XRFLG=Db.getOneRow("RFLG","IRATYPE","ACN,RPASEQ")
	
	new rs 
	type ResultSet rs=Db.select("RFLG","RFLGRPA","ACN=:ACN AND RPASEQ=:RPASEQ") 
	if rs.isEmpty() set B=""
	else  set B=rs.next(),B=rs.getCol(1)
	
	if XRFLG,(B="") do Db.update("IRATYPE","RFLG=0","ACN=:ACN AND RPASEQ=:RPASEQ")
	set TCMT="[IRATYPE]RFLG::"_$$^MSG(2884)_":"_RFLG do CIFHBLD^CIFFUNCS(TCMT)     //User-defined restriction ~p1 deleted
	quit
	

vai1 // Trigger AFTER_INSERT - RFLGRPA After Insert Trigger

	type public RecordRFLGRPA rflgrpa
	// Set Restriction Flags after Insert
	
	set RFLG=Db.getOneRow("RFLG","IRATYPE","ACN,RPASEQ")
	if RFLG'=1 do Db.update("IRATYPE","RFLG=1","ACN=:ACN AND RPASEQ=:RPASEQ")
	set TCMT="[IRATYPE]RFLG::"_$$^MSG(2883)_":"_RFLG do CIFHBLD^CIFFUNCS(TCMT)     //User-defined restriction ~p1 created
	quit

vbd1 // Trigger BEFORE_DELETE - RFLGRPA Before Delete Trigger

	type public RecordRFLGRPA rflgrpa
	// Invalid customer number
	if 'Db.isDefined("CIF","ACN") do Runtime.setErrMSG("RFLGRPA",1295) quit:ER 

	// Restriction must exist in OVR system table or RFLG user table
	new GRP
	set GRP="RPA"
	if 'Db.isDefined("UTBLRFLG","GRP,RFLG") do Runtime.setErrMSG("RFLGRPA",2408) quit:ER 

	// No restrictions on retirement plan
	if 'Db.isDefined("RFLGRPA","ACN,RPASEQ,RFLG") do Runtime.setErrMSG("RFLGRPA",3962) quit:ER 
	quit
	

vbi1 // Trigger BEFORE_INSERT - RFLGRPA Before Insert Trigger

	type public RecordRFLGRPA rflgrpa
	// Invalid customer number
	if 'Db.isDefined("CIF","ACN") do Runtime.setErrMSG("RFLGRPA",1295) quit:ER 

	// Restriction must exist in OVR system table or RFLG user table
	new GRP
	set GRP="RPA"
	if 'Db.isDefined("UTBLRFLG","GRP,RFLG") do Runtime.setErrMSG("RFLGRPA",2408) quit:ER 

	// Default values
	set rflgrpa.tlo=%UserStation
	set rflgrpa.tim=%CurrentTime
	quit
	


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordRFLGRPA rflgrpa

	set X = rflgrpa.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
	set X = rflgrpa.rpaseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("RPASEQ", vRM) quit
	if rflgrpa.rflg.length()>4 set vRM = $$^MSG(1076,4) do vdderr("RFLG", vRM) quit
	set X = rflgrpa.exdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EXDT", vRM) quit
	set X = rflgrpa.jdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("JDT", vRM) quit
	set X = rflgrpa.stdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("STDT", vRM) quit
	if rflgrpa.tcmt.length()>30 set vRM = $$^MSG(1076,30) do vdderr("TCMT", vRM) quit
	set X = rflgrpa.tim if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"C") do vdderr("TIM", vRM) quit
	if rflgrpa.tlo.length()>40 set vRM = $$^MSG(1076,40) do vdderr("TLO", vRM) quit
	if rflgrpa.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

