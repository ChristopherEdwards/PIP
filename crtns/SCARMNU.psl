SCARMNU		/*
	ORIG: alagarss - 08/11/2005
	DESC: Profile Menu Report

	---- Comments --------------------------------------------------------

	---- Revision History ------------------------------------------------
	03/22/06 - chhabris - CR20216
		   Replaced global reference ^SCAMENU(0, with file [SCAMENU0]
		   for lookup table.

	08/11/05 - ALAGARSS - CR16677
	           Converted to PSL.
	           Also removed the revision history prior to 2004.
	
	----------------------------------------------------------------------
	*/
	
	type Public String %READ,%TAB
	type Public Number OLNTB,IO,POP,F,T,%NOPRMT
	
	type ResultSet rs=Db.select("MENU","SCAMENU0",,"MENU")
	if rs.next() set F(2)=rs.getCol("MENU")

	type ResultSet rs1=Db.select("MENU","SCAMENU0",,"MENU DESC")
	if rs1.next() set T(2)=rs1.getCol("MENU")

	kill OLNTB,IO,POP
	
	set %TAB("F(2)")=".F2/TBL=[SCAMENU0]MENU,DES"
	set %TAB("T(2)")=".T2/TBL=[SCAMENU0]MENU,DES"
	set %TAB("IO")=$$IO^SCATAB($I)
	set %READ="F(2),T(2),IO",%NOPRMT=1
	do ^UTLREAD if '(F(2).exists()) quit
	do OPEN^SCAIO
	do START^SCARMNU1
	quit
 #OPTION ResultClass ON
Public String vSIG()	quit "60346^6641^Sanjay Chhrabria^1030"	// Signature - LTD^TIME^USER^SIZE
