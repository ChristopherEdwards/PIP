ZPAT		/*
	 ORIG: 8700 - 08/07/2005
	 DESC: 
	*/

	type DbSet ds=Db.selectDbSet("DBTBL1D","%LIBS='SYSDEV' AND FID='PRODDFTL'","DI ASC")
	while ds.next() do {
		type RecordDBTBL1D dbtbl1d=ds.getRecord("DBTBL1D")
		type RecordDBTBL1D lndi=Db.getRecord("DBTBL1D","%LIBS='SYSDEV',FID='LN',DI=:dbtbl1d.di",1)
		if 'lndi.getMode() write "PRODDFTL: Data item "_dbtbl1d.di_" doesn't exist in LN.",! quit
		}

	type DbSet ds2=Db.selectDbSet("DBTBL1D","%LIBS='SYSDEV' AND FID='PRODDFTD'","DI ASC")
	while ds2.next() do {
		type RecordDBTBL1D dbtbl1d2=ds2.getRecord("DBTBL1D")
		type RecordDBTBL1D depdi=Db.getRecord("DBTBL1D","%LIBS='SYSDEV',FID='DEP',DI=:dbtbl1d2.di",1)
		if 'depdi.getMode() write "PRODDFTD: Data item "_dbtbl1d2.di_" doesn't exists in DEP.",!
		}

	quit
 #OPTION ResultClass ON
Public String vSIG()	quit "60717^38373^kellyp^735"	// Signature - LTD^TIME^USER^SIZE
