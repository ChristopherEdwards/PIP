VPREAQ	// Pre-processor (after query)

 /* DEFINE ARRAY OF TYPES WHICH WILL BE INCLUDED IN REPORT
	VARIABLES INCLUDED IN ARRAYS:
    START(CLS or GRP or TYPE) -  This variable set is utilized
            as a "print indicator".  Since two passes through ^XCLS
            (one to accumulate, one to print detail) required, this flags
            if accumulate (START(GPR)=1) or print pass (START(TYPE)=0).
            This variable then controls collate (returns to VKEYS on accumulate
            pass, drops through to VDTL on print pass).
    QTYPE(TYPE) - Defines TYPES to include (all that pass query).
    TOT(TYPE)   - Totals count and amount for all from ^XCLS, regardless
                  of query values.
    QTOT(TYPE)  - Totals count and amount for TYPES that pass queries.
    OTYPE(TYPE) - Status indicator if other type has been reviewed for detail
                  TYPE.
    COMMON(TYPE,OTYPE) - Totals counts of occurances for each "other" type
                  on report for each detail TYPE.
 */
 
 type public String CLS,GRP,TYPE

 set CLS="*",(GRP,TYPE)="",START=1

 type public Number OTYP(),QTYPE(),QTOT(),START(),TOT()
 type public String CLS,GRP,Q,START(),TYPE ,X
        
 type ResultSet rs=Db.select("CLS,GRP,TYPE","CIF")
 while rs.next() do {
        set CLS=rs.getCol("CLS")
        if CLS="" do SET1 quit
	set GRP=rs.getCol("GRP")
	if GRP.isNull() quit
	set TYPE=rs.getCol("TYPE")
	}
 set START(CLS)=1
 set START(GRP)=1
      
 set (QTYPE(TYPE),TOT(TYPE),QTOT(TYPE))="0|0",(START(TYPE),OTYP(TYPE))=1

 if 'OTYPE.exists() set OTYPE=""

SET1 // DEFINE COMMON ARRAY
 type public String COMMON()
 type String VAL,VAL1
 type Number I,J

 set (VAL,VAL1)=""
 for I=1:1 set VAL=QTYPE(VAL).order() quit:VAL.isNull()  for J=1:1 set VAL1=QTYPE(VAL1).order() quit:VAL1.isNull()  set COMMON(VAL,VAL1)=""
 quit

START //

 type public Number ACN,BAL,CID,COUNT,OPT,STAT,TBAL
 type public String OTYPE,QTYPE(),TOT(),TYPE,VAL
 type Number I

 type RecordDEP dep=Db.getRecord("DEP","CID=:CID",1)
 if dep.getMode() do {
	set STAT=dep.stat
	set BAL=dep.bal
	}

 // Disregard closed accounts
 quit:STAT=4

 //  Accumulate TYPE totals
 set COUNT=TOT(TYPE).piece("|",1),TBAL=TOT(TYPE).piece("|",2)
 set TBAL=TBAL+BAL,TOT(TYPE)=COUNT_"|"_TBAL

 // Verify if valid CIF account
 set ACN=dep.acn quit:ACN.isNull()

 type RecordTMPRPT1 tmprpt1=Db.getRecord("TMPRPT1","PID=%ProcessID,KEY1=:ACN",1)
 if tmprpt1.getMode() set OPT=tmprpt1.data

 // OPT="" then ACN not checked yet
 // OPT=0 ACN checked and not valid

 // Verify if valid CIF per queries, set OPT in ^TMP
 if OPT.isNull() do CHECK1

 // Account has been processed for this type
 type RecordTMPRPT2 tmprpt22=Db.getRecord("TMPRPT2","PID=%ProcessID,KEY1=:ACN,KEY2=:TYPE",1)
 if tmprpt22.getMode() quit

 type RecordTMPRPT2 tmprpt2=Class.new("RecordTMPRPT2")
 set tmprpt2.pid=%ProcessID
 set tmprpt2.key1=ACN
 set tmprpt2.key2=TYPE
 set tmprpt2.data=""
 do tmprpt2.save()

 // New CIF for type, so add count
 set COUNT=COUNT+1,TOT(TYPE).piece("|",1)=COUNT

 // CIF invalid, back to collate to pick up next CID
 if OPT=0 quit

 set VAL=""
 for I=1:1 set VAL=OTYP(VAL).order() quit:VAL.isNull()  set OTYP(VAL)=1
 
 quit

COL9 // COLLATE THROUGH NODE 99

 type public Number BAL,TBAL
 type public String TYPE,OTYP()

 // Then collate through all CID's held

 type ResultSet rs=Db.select("CID","RELCIF","ACN=:ACN")
 while rs.next() do {
	set VAL=rs.getCol("CID")
	
	// This CIF not primary for this CID
	type RecordACN acn=Db.getRecord("ACN","CID=:VAL",1)
	if acn.acn'=ACN quit
	set OTYPE=acn.type

	// Not included TYPE
	if '(QTYPE(OTYPE).exists()) quit

	// Account closed, disregard
	if acn.stat=4 quit

	set BAL=acn.bal

	set COUNT=COMMON(TYPE,OTYPE).piece("|",1)
 	set TBAL=COMMON(TYPE,OTYPE).piece("|",2)

 	// Variable OTYP indicates if for this TYPE this CIF has been 
	// counted.  Where a given CIF has multiple accounts of a given TYPE, 
	// only count the CIF once,
	// but total the dollars for all accounts of that TYPE.

 	if OTYP(OTYPE) set COUNT=COUNT+1,OTYP(OTYPE)=0
	set TBAL=TBAL+BAL
	set COMMON(TYPE,OTYPE)=COUNT_"|"_TBAL
	}
 
CHECK1 // Verify if valid CIF account
 
 type public Number CIF,OPT
 type public String CIFQ

 set VAL="",OPT=1
 kill CIF
 for  set VAL=CIFL(VAL).order() quit:VAL.isNull()  xecute CIFL(VAL)
 for  set VAL=CIFQ(VAL).order() quit:VAL.isNull()  if 1 xecute CIFQ(VAL) else  quit
 else  set OPT=0
 
 type RecordTMPRPT1 tmprpt11=Db.getRecord("TMPRPT1","PID=:%ProcessID,KEY1=:ACN",1)
 if tmprpt11.getMode() do {
	set tmprpt11.data=OPT
	do tmprpt11.save()
	}
 quit
	quit

