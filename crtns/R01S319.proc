LASCIF // PROCESS AFTER LAST CIF QUERY INPUT ENTRY

 type public String A,CIFQ,LC(),Q,QCIF(),X
 type Number CTR,I
 type String FILES,LOOP,VAL,VAL1

 do POSCIF
