vinit // Initialize default values

	type public RecordSTOP6 stop6

	// Type local variables for access keys for defaults
	type public String INST
	type public String OUTACC

	type public String TJD
	if stop6.dp.isNull() set stop6.dp = TJD                // dp
	if stop6.stcon.isNull() set stop6.stcon = 0            // stcon
	if stop6.time.isNull() set stop6.time = $P($H,",",2)   // time
	quit


vreqn // Validate required data items

	type public RecordSTOP6 stop6

	if stop6.act.isNull() do vreqerr("ACT") quit
	if stop6.dp.isNull() do vreqerr("DP") quit
	if stop6.stcon.isNull() do vreqerr("STCON") quit
	if stop6.time.isNull() do vreqerr("TIME") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordSTOP6 stop6

	if stop6.inst.isNull() do vreqerr("INST") quit
	if stop6.outacc.isNull() do vreqerr("OUTACC") quit

		if vx("ACT").exists(),stop6.act.isNull() do vreqerr("ACT") quit
		if vx("DP").exists(),stop6.dp.isNull() do vreqerr("DP") quit
		if vx("STCON").exists(),stop6.stcon.isNull() do vreqerr("STCON") quit
		if vx("TIME").exists(),stop6.time.isNull() do vreqerr("TIME") quit
	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("STOP6","MSG",1767,"STOP6."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


VAI //
	type public Number ER = 0
	type public String vx(),RM
	do vai1 if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit

VAU //
	type public Number ER = 0
	type public String vx(),RM
	if vx("").order().isNull() quit
	if vx("FEECID").exists() do vau1 if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vai1 // Trigger AFTER_INSERT - Modify fee charge account field PFW only

	type public RecordSTOP6 stop6
	/*
	---- Revision History ------------------------------------------------
	
	05/22/00 - JARVISG - 39638
		   Modified to add FEECAT,FEETYP parameters to SRVSTP call. 

	10/21/99 - JARVISG - 34535
		   Add for stop fee processing.


	*/
	
	if %ProcessMode'=0 quit
	set FEECID6=stop6.feecid		// Fee Charge Account

	if $G(FEECID6) do {  quit:ER
		new values
		set values=Db.getOneRow("STAT,CLS","DEP","FEECID6")
		// Error if fee account is closed or not a deposit account.
		if $P(values,$C(9),1)=4 do Runtime.setErrMSG("STOP6",457,"FEECID6") quit:ER
		if $P(values,$C(9),2)'="D" do Runtime.setErrMSG("STOP6",803) quit:ER
		do ^SRVSTP(FEECID6,6,2,"STP")	// Assess stop type 6 fee
		}

	quit

vau1 // Trigger AFTER_UPDATE_FEECID - Fee Charge Account modification

	type public RecordSTOP6 stop6
	if %ProcessMode'=1 quit     // 1 = Update
	set FEECID6=stop6.feecid
	if (stop6.feecid.oldVal'=stop6.feecid) do Runtime.setErrMSG("STOP6",412,"FEECID6") quit:ER
	quit


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSTOP6 stop6

	set X = stop6.inst if 'X.isNull(),'Db.isDefined("UTBLEFTINST","X") set vRM = $$^MSG(1485,X) do vdderr("INST", vRM) quit
	if stop6.outacc.length()>20 set vRM = $$^MSG(1076,20) do vdderr("OUTACC", vRM) quit
	set X = stop6.act if 'X.isNull(),'Db.isDefined("STBLSTOP","X") set vRM = $$^MSG(1485,X) do vdderr("ACT", vRM) quit
	if stop6.cmt.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CMT", vRM) quit
	set X = stop6.dp if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("DP", vRM) quit
	set X = stop6.exp if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EXP", vRM) quit
	set X = stop6.feecid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("FEECID", vRM) quit
	if stop6.own.length()>60 set vRM = $$^MSG(1076,60) do vdderr("OWN", vRM) quit
	if '("01"[stop6.STCON) set vRM=$$^MSG(742,"L") do vdderr("STCON", vRM) quit
	set X = stop6.stpwhy if 'X.isNull(),'Db.isDefined("UTBLSTPWHY","X") set vRM = $$^MSG(1485,X) do vdderr("STPWHY", vRM) quit
	set X = stop6.time if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"C") do vdderr("TIME", vRM) quit
	if stop6.who.length()>40 set vRM = $$^MSG(1076,40) do vdderr("WHO", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
