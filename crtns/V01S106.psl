 set %TAB(15)=$C(20,31,12)_"00T12418|1|[AGENT]COMGRP|[UTBLCOMGRPT]"
 do VTBL(.fAGENT)
	do ^DBSCRT8 	// data entry
	quit


VREQ   // Create REQ() array
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


