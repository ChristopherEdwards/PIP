 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fAGENT.exists() set fAGENT = Class.new("RecordAGENT")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="AGENTDTL",VPGM=$T(+0),VSNAME="Agent Maintenance Screen"
 set VFSN("AGENT")="zfAGENT"
 set vPSL=1
 set KEYS(1)=fAGENT.AGENT
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fAGENT),VDA1(.fAGENT),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fAGENT),VPR(.fAGENT),VDA1(.fAGENT)
 if %ProcessMode do VLOD(.fAGENT) quit:$G(ER)  do VPR(.fAGENT),VDA1(.fAGENT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fAGENT)
	quit


VNEW(RecordAGENT fAGENT) // Initialize arrays if %O=0
 
 do VDEF(.fAGENT)
 do VLOD(.fAGENT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordAGENT fAGENT)
 quit:fAGENT.AGENT.isNull() 
 quit:%ProcessMode  set ER=0 if (fAGENT.AGENT="") set ER=1,RM=$$^MSG(1767,"AGENT") Q
 if Db.isDefined("AGENT","AGENT.AGENT=:fAGENT.AGENT") S ER=1,RM=$$^MSG(2327) Q
 do fAGENT.setAuditFlag(1)
 if fAGENT.AGENCY="" set fAGENT.AGENCY=0
 if fAGENT.CRCD="" set fAGENT.CRCD=%CRCD
 if fAGENT.START="" set fAGENT.START=TJD
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordAGENT fAGENT) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

