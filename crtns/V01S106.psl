 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=15,VPT=2,VPB=21,PGM=$T(+0),DLIB="SYSDEV",DFID="AGENT"
 set OLNTB=21032
 
 set VFSN("AGENT")="zfAGENT"
 //
 // 
	
	
 set %TAB(1)=$C(3,31,6)_"21N12401|1|[AGENT]AGENT|[UTBLBRCD]"
 set %TAB(2)=$C(4,31,12)_"01N12401|1|[AGENT]CIF||||do VP1^V01S106(.fAGENT)"
 set %TAB(3)=$C(6,31,1)_"00L12417|1|[AGENT]AGENCY"
 set %TAB(4)=$C(7,31,12)_"00N12402|1|[AGENT]AGNUM"
 set %TAB(5)=$C(8,31,12)_"00N12404|1|[AGENT]EXTAGN1"
 set %TAB(6)=$C(9,31,12)_"00N12405|1|[AGENT]EXTAGN2"
 set %TAB(7)=$C(10,31,12)_"00N12403|1|[AGENT]FLDSTAFF|[UTBLFLDSTAFF]"
 set %TAB(8)=$C(11,31,10)_"01D12412|1|[AGENT]START"
 set %TAB(9)=$C(12,31,10)_"00D12413|1|[AGENT]END"
 set %TAB(10)=$C(15,31,1)_"00N12406|1|[AGENT]COMOPT|[STBLCOMPMTH]"
 set %TAB(11)=$C(16,31,3)_"00T12410|1|[AGENT]CRCD|[UTBLCRCDX]"
