 //
 set VO="32|18|13|0"
 set VO(18)=$C(4,32,6,2,0,0,0,0,0,0)_"01N"_fAGENT.AGENT
 set VO(19)=$C(5,32,12,2,0,0,0,0,0,0)_"00N"_fAGENT.CIF
