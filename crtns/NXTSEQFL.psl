vreqn // Validate required data items

	type public RecordNXTSEQ nxtseq

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordNXTSEQ nxtseq

	if nxtseq.id.isNull() do vreqerr("ID") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("NXTSEQ","MSG",1767,"NXTSEQ."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


VBU //
	type public Number ER = 0
	type public String vx(),RM
	type public RecordNXTSEQ nxtseq
	if vx("").order().isNull() do AUDIT^UCUTILN(nxtseq,.vx(),1,"|") quit
	if vx("UNQ").exists() do vbu1 if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	do AUDIT^UCUTILN(nxtseq,.vx(),1,"|")
	quit


vbu1 // Trigger ZBU_UNQ - Custom Before Update UNQ

	type public RecordNXTSEQ nxtseq
	do nxtseq.setAuditFlag(1)
	// Trigger applies to Manual Authorizations only (ID='ISOMAN')
	if nxtseq.id'="ISOMAN" quit
	// Manual Authorizations must be 6 digits
	if nxtseq.unq.length()<6 set nxtseq.unq=100000
	quit


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordNXTSEQ nxtseq

	if nxtseq.id.length()>20 set vRM = $$^MSG(1076,20) do vdderr("ID", vRM) quit
	set X = nxtseq.unq if 'X.isNull(),X'?1.18N,X'?1"-"1.17N set vRM=$$^MSG(742,"N") do vdderr("UNQ", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("NXTSEQ","MSG",979,"NXTSEQ."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordNXTSEQ nxtseq

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("ID")
	set voldkey = vux.piece("|",1),vobj(nxtseq,-3) = voldkey // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/TRIGBEF/" do VBU
	if vpar["/VALDD/" do vddver
	do vexec

	set vnewkey = vux.piece("|",2),vobj(nxtseq,-3) = vnewkey // Copy new keys into object
	type RecordNXTSEQ vnewrec = nxtseq.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("NXTSEQ",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	set vobj(nxtseq,-3) = vux.piece("|",1)                 // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit
