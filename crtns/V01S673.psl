 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLGLR.exists() set fUTBLGLR = Class.new("RecordUTBLGLSCR")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLGLSCR",VPGM=$T(+0),VSNAME="G/L Set Code User Table Reclassification"
 set VFSN("UTBLGLSCR")="zfUTBLGLR"
 set vPSL=1
 set KEYS(1)=fUTBLGLR.GLSC
 set KEYS(2)=fUTBLGLR.AC
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLGLR),VDA1(.fUTBLGLR),^DBSPNT() quit

 // Display Pre-Processor
 //
 if '%ProcessMode do VNEW(.fUTBLGLR),VDSPPRE(.fUTBLGLR) quit:$G(ER)  do VPR(.fUTBLGLR),VDA1(.fUTBLGLR)
 if %ProcessMode do VLOD(.fUTBLGLR) quit:$G(ER)  do VDSPPRE(.fUTBLGLR) quit:$G(ER)  do VPR(.fUTBLGLR),VDA1(.fUTBLGLR)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLGLR)
	quit


VNEW(RecordUTBLGLSCR fUTBLGLR) // Initialize arrays if %O=0
 
 do VDEF(.fUTBLGLR)
 do VLOD(.fUTBLGLR)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordUTBLGLSCR fUTBLGLR)
 quit:fUTBLGLR.GLSC.isNull() ! fUTBLGLR.AC.isNull() 
 quit:%ProcessMode  set ER=0 if (fUTBLGLR.GLSC="")!(fUTBLGLR.AC="") set ER=1,RM=$$^MSG(1767,"GLSC,AC") Q
 if Db.isDefined("UTBLGLSCR","UTBLGLSCR.GLSC=:fUTBLGLR.GLSC AND UTBLGLSCR.AC=:fUTBLGLR.AC") S ER=1,RM=$$^MSG(2327) Q
 do fUTBLGLR.setAuditFlag(1)
 if fUTBLGLR.DESC="" set fUTBLGLR.DESC="[UTBLACC]DESC"
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordUTBLGLSCR fUTBLGLR) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

