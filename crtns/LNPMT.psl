public	LNPMT(RecordLN ln,Number PNUM,Number STPMT,Number ENDPMT,Number PMTAMT)

	/*
	ORIG: CARROLLJ - 11/11/2003
	DESC: Payment amounts associated with the loan

	---- Comments --------------------------------------------------------

	---- Revision History ------------------------------------------------

	02/10/05 - TITOVE - CR 13726
		   Corrected misspelled PNUm and ENPMT variables. 
		   Made this procedure public. Cleaned up warnings.

	11/11/03 - CARROLLJ - 51630
		   Created for LNAPR procedure to avoid indirection.


	*/

	type public String changes()

	if PNUM = 1 do PMT1(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 2 do PMT2(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 3 do PMT3(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 4 do PMT4(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 5 do PMT5(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 6 do PMT6(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 7 do PMT7(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 8 do PMT8(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 9 do PMT9(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 10 do PMT10(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 11 do PMT11(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 12 do PMT12(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 13 do PMT13(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 14 do PMT14(.ln,STPMT,ENDPMT,PMTAMT) quit
	if PNUM = 15 do PMT15(.ln,STPMT,ENDPMT,PMTAMT) quit

	quit


PMT1(RecordLN ln,STPMT,ENDPMT,PMTAMT)
	
	type public String changes()
	
	do ln.setAuditFlag(1)
	set ln.pa1f=STPMT
	set changes("PA1F")=STPMT

	set ln.pa1t=ENDPMT
	set changes("PA1T")=ENDPMT

	set ln.pa1a=PMTAMT
	set changes("PA1A")=PMTAMT
	quit


