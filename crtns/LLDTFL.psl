vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordUTBLLLDT utbllldt

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("UTBLLLDT","DFTDEF = :utbllldt.dftdef") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		if %O = 0, vpar'["/NOLOG/" do ^DBSLOGIT(utbllldt,%O,.vxins())
