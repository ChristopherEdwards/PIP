  //user-defined post procs
 //
VP1(RecordM1099 ffA) //
 type String V,XX
 
 set XX=X.upperCase()
 set V=ffA.nam.upperCase()
 
 // Last name is not part of full name
 if " "[XX!(V'[XX) do Runtime.setErrMSG("M1099",1593)
 
 quit
VP2(RecordM1099 ffA) //
 if ffA.lnam.length() quit
 
 type public Number ACN
 
 type String LN,V
 
 if 'ACN.get() set ACN=""
 set V=ffA.nam
 do FBL^XALPHA quit:ER
 set LN=LN.piece(",",1).extract(1,20)
 do DEFAULT^DBSMACRO("[M1099]LNAM",LN,"1","0","0")
 quit
VP3(RecordM1099 ffA) //
 type public String CNTRY
 
 set CNTRY=X
 quit
VP4(RecordM1099 ffA) //
 type public String CNTRY
 
 type ResultSet rs

 set rs=Db.select("STATE","STBLCNTRY1","CNTRY=:CNTRY")
 if rs.isEmpty() quit

 // Required
 if X="" do Runtime.setErrMSG("M1099",2385)
 quit
VP5(RecordM1099 ffA) //
 type public String CNTRY
 
 if CNTRY'="US" quit

 do ZIP^PROCZIP(CNTRY,ffA.state,X)
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit								// User defined post processor's


