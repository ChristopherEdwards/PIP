 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'ffA.exists() set ffA = Class.new("RecordM1099")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="M1099A",VPGM=$T(+0),VSNAME="Abandoned Property Report"
 set VFSN("M1099")="zffA"
 set vPSL=1
 set KEYS(1)=ffA.TAXID
 set KEYS(2)=ffA.SEQ
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.ffA),VDA1(.ffA),^DBSPNT() quit

 if '%ProcessMode do VNEW(.ffA),VPR(.ffA),VDA1(.ffA)
 if %ProcessMode do VLOD(.ffA) quit:$G(ER)  do VPR(.ffA),VDA1(.ffA)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.ffA)
	quit


VNEW(RecordM1099 ffA) // Initialize arrays if %O=0
 
 do VDEF(.ffA)
 do VLOD(.ffA)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordM1099 ffA)
 quit:ffA.TAXID.isNull() ! ffA.SEQ.isNull() 
 quit:%ProcessMode  set ER=0 if (ffA.TAXID="")!(ffA.SEQ="") set ER=1,RM=$$^MSG(1767,"TAXID,SEQ") Q
 if Db.isDefined("M1099","M1099.TAXID=:ffA.TAXID AND M1099.SEQ=:ffA.SEQ") S ER=1,RM=$$^MSG(2327) Q
 do ffA.setAuditFlag(1)
 if ffA.CNTRY="" set ffA.CNTRY=CUVAR.CNTRY
 if ffA.MF="" set ffA.MF=1
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordM1099 ffA) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

