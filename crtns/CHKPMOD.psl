CHKPMOD	// CHECK FORMATS FOR MODEL
	/*
	   DESC:  Standard check print formats

	   Data Elements:

	   NM(1)-NM(7) 		Name and Address
	   CKAMT       		Check Amount
	   AMT(3)      		Interest Amount
	   AMT(2)      		Principle Amount
	   CID         		Account Number
	   CKNUM       		Check Number
	   EFD         		Date of Payout
	   IYTD        		Interest Year to Date
	   AMT(4)      		Fee Amt
	   AMT(5)      		Federal Witholding Tax
	   EN          		Amount (string - 1st half)
	   ENC         		Amount (string - 2nd half)
	   BKAD(1)-BKAD(5) 	Bank name/address
	   X=LEN         	Length of check amt field (ex. *****52.41 = 10)
	   CMT         		Comment

	  ---- Revision History -----------------------------------------------

	   08/14/06 - KinI - 22447
	   	      Modified TRES section to remove SS variable from 
	   	      processing. Removed EDIT section as it used to be called 
	   	      from obsoleted AP, Accounts payable section.

	   06/03/02 - CHHABRIAS - 49794
		      Converted to PSL

	  ---------------------------------------------------------------------

	   I18N=QUIT: Excluded from I18N Standards
		
	  ---------------------------------------------------------------------

	*/	

MO	// Money order
STND	// Standard check
TC	// Teller Check
TRES	// Treasurer's check

	type public Number AMT(), CID, CHKSIZ, CKAMT1, EN, ENC, IYTD, N
	type public String BKAD(), CKNUM, CMT, %MSKD, NM()
	
	type String CKAMT
	type Number I
	
	set CKAMT1=CKAMT1.roundDec()
	set CKAMT="**********".extract(1,10-CKAMT1.length())_CKAMT1
	set N=CKAMT1 set CHKSIZ=80 do ^CHKNUM

	write !,BKAD(1)
	write !,BKAD(2),?82,CID.justify(9)
	write ?93,?106,%EffectiveDate.toString(%MSKD.get())
	write ?115,"0000000".extract(1,7-CKNUM.length())_CKNUM.justify(11)
	write !,BKAD(3),?69,%EffectiveDate.toString(%MSKD.get()),!,BKAD(4),!,BKAD(5)

	write ?84,AMT(2).roundDec("","",10)
	write ?98,AMT(3).roundDec("","",14)
	write ?113,IYTD.roundDec("","",13)

	write !!,?7,CMT,?62,CKAMT
	write !!,?7,EN_" "_ENC,?110,CKAMT1.roundDec("","",16)
	write ! write:AMT(4)'="" ?84,"FEE AMT",?110,AMT(4).roundDec("","",16)
	write !!,?6,NM(1) write:AMT(5)'="" ?84,"FED WHLDAMT",?110,AMT(5).roundDec("","",16)
	for I=2:1:7 write !,?6,NM(I)
	write !!!
	
	quit

XMAS	// Christmas check
XTFR	// Transfer check

	type public Number CID, CKAMT1, CHKSIZ, EN, ENC, N
	type public String CKNUM, %MSKD, NM()
	
	type Number I
	type String CKAMT
	
	set CKAMT="**********".extract(1,10-CKAMT1.length())_CKAMT1
	set N=CKAMT1 set CHKSIZ=80 do ^CHKNUM

	write !!,?10,"0000000".extract(1,7-CKNUM.length())_CKNUM
	write !,?10,CID,?65,%EffectiveDate.toString(%MSKD.get())
	write !!!!!!,EN_" "_ENC.justify(75)
	write !,?55,CKAMT,!!
	for I=1:1:7 write !,?12,NM(I)
	write !!
	
	quit

	
