public VIDXBLD(List vlist) // Rebuild index files (External call)


	type Number %ProcessMode=0                             // Create mode
	type Number i

	if vlist.get().isNull() set vlist="VINDEX"             // Build all

	type DbSet ds=Db.selectDbSet("APPMGMTCI")

	while ds.next() do {
		type RecordAPPMGMTCI appmgmtci=ds.getRecord("APPMGMTCI")
		if vlist.contains("VINDEX") do VINDEX(.appmgmtci) quit
		if vlist.contains("TAXID") do vi1(.appmgmtci)
	}

	quit


public VIDXBLD1(RecordAPPMGMTCI appmgmtci, List vlist) // Rebuild index files for one record (External call)


	type Number i

