 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="INTRECHQ00"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+3 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+4
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Incoming Foreign Payment (Page 1)|INTRECHQ10"
 set VPG(%PG+1)="Incoming Foreign Payment (Page 2)|INTRECHQ15"
 set VPG(%PG+2)="Incoming Foreign Payment (Page 2)|INTRECHQ20"
 set VPG(%PG+3)="Incoming Foreign Payment (Page 4)|INTRECHQ40"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fEFTPAY,.DEP)
 if %ProcessMode do VLOD(.fEFTPAY,.DEP) if $G(ER) set VFMQ="Q" quit
 do VPG(.fEFTPAY,.DEP)
 quit

VNEW(RecordEFTPAY fEFTPAY,RecordDEP DEP)
 
 quit
 //
 ;
VLOD(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // User defined access section
	quit

VPG(RecordEFTPAY fEFTPAY,RecordDEP DEP)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fEFTPAY,.DEP),VPG0(.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fEFTPAY,.DEP),VPG0(.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.fEFTPAY,.DEP),VPG0(.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.fEFTPAY,.DEP),VPG0(.fEFTPAY,.DEP) quit:vDONE
  }
 quit

VPG0(RecordEFTPAY fEFTPAY,RecordDEP DEP)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fEFTPAY,.DEP) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Incoming Foreign Payment (Page 1)
 
 new x
 set SID="INTRECHQ10",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S224
 if %ProcessMode>1 quit
 do VTAB^V00S224(.fEFTPAY)
 quit
 
VPG2(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Incoming Foreign Payment (Page 2)
 
 new x
 set SID="INTRECHQ15",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S225
 if %ProcessMode>1 quit
 do VTAB^V00S225(.fEFTPAY)
 quit
 
VPG3(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Incoming Foreign Payment (Page 2)
 
 new x
 set SID="INTRECHQ20",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S226
 if %ProcessMode>1 quit
 do VTAB^V00S226(.fEFTPAY,.DEP)
 quit
 
VPG4(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Incoming Foreign Payment (Page 4)
 
 new x
 set SID="INTRECHQ40",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 S %MODS=1 I '$D(%REPEAT) S %REPEAT=8
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S227
 if %ProcessMode>1 quit
 do VTAB^V00S227(.fEFTPAY)
 quit
 

VBTM(RecordEFTPAY fEFTPAY,RecordDEP DEP)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
