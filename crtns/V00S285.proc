VPG3(RecordLNFEEP fLNFEEP)  // Loan Fee Plan Maintenance (Page 2)
 
 new x
 set SID="LNFEEPB",DFID="LNFEEP"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
