VPG4(RecordLNFEEP fLNFEEP)  // Loan Fee Plan Maintenance (Page 3)
 
 new x
 set SID="LNFEEPC",DFID="LNFEEP"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 S %MODS=1 I '$D(%REPEAT) S %REPEAT=11
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S293
 if %ProcessMode>1 quit
 do VTAB^V00S293(.fLNFEEP)
 quit
 

VBTM(RecordLNFEEP fLNFEEP)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
