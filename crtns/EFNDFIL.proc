vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordEFUNDS efunds

	if efunds.id.length()>12 set vRM = $$^MSG(1076,12) do vdderr("ID", vRM) quit
	set X = efunds.amt if 'X.isNull() set vRM = $$VAL^DBSVER("$",18,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"EFUNDS.AMT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	set X = efunds.amtack if 'X.isNull() set vRM = $$VAL^DBSVER("$",18,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"EFUNDS.AMTACK"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	set X = efunds.cid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CID", vRM) quit
	if '("01"[efunds.DISC) set vRM=$$^MSG(742,"L") do vdderr("DISC", vRM) quit
	set X = efunds.dt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("DT", vRM) quit
	set X = efunds.hseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("HSEQ", vRM) quit
	set X = efunds.status if 'X.isNull(),X'?1N set vRM=$$^MSG(742,"N") do vdderr("STATUS", vRM) quit
	quit

