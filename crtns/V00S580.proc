  //user-defined post procs
 //
VP1(RecordUTBLCTFPLN fUTBLCTF) //
 type public String X

 // Must define a primary rate before defining a secondary rate.

 if 'X.isNull(),fUTBLCTF.rate.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4848) 
VP2(RecordUTBLCTFPLN fUTBLCTF) //
 type public String X

 // if Index defined, Currency code must be defined
 if X.isNull(),'fUTBLCTF.index.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4861) quit

 // Only one (Fixed Amt,Rate,Index) can be defined
 if 'fUTBLCTF.rate.isNull(),'fUTBLCTF.index.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4846) quit
VP3(RecordUTBLCTFPLN fUTBLCTF) //
 type public String X

 // Specify a Calculation Formula or a Procedure Call, not both
 if 'X.isNull(),'fUTBLCTF.comcalc.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4847) quit 
 // Enter Rate, Index, Calc Formula, or Procedure
 if fUTBLCTF.rate.isNull(),fUTBLCTF.index.isNull(),fUTBLCTF.comcalc.isNull(),fUTBLCTF.pcall.isNull(),X.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4849)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


