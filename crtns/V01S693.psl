 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLACC.exists() set fUTBLACC = Class.new("RecordUTBLACC")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLPROV",VPGM=$T(+0),VSNAME="Provision Classification"
 set VFSN("UTBLACC")="zfUTBLACC"
 set vPSL=1
 set KEYS(1)=fUTBLACC.CLS
 set KEYS(2)=fUTBLACC.AC
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLACC),VDA1(.fUTBLACC),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fUTBLACC),VPR(.fUTBLACC),VDA1(.fUTBLACC)
 if %ProcessMode do VLOD(.fUTBLACC) quit:$G(ER)  do VPR(.fUTBLACC),VDA1(.fUTBLACC)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLACC)
	quit


VNEW(RecordUTBLACC fUTBLACC) // Initialize arrays if %O=0
 
 do VDEF(.fUTBLACC)
 do VLOD(.fUTBLACC)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordUTBLACC fUTBLACC)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordUTBLACC fUTBLACC) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

