 new V
 //
 set VO="22|13|13|0"
 set VO(13)=$C(2,33,1,2,0,0,0,0,0,0)_"01T"_$E(fUTBLACC.CLS,1,1)
 set VO(14)=$C(2,53,2,2,0,0,0,0,0,0)_"01N"_fUTBLACC.AC
 set VO(15)=$C(8,33,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLACC.DESC,1,40)
 set VO(16)=$C(10,33,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLACC.PROVQRY,1,40)
 set V=$S(fUTBLACC.PROVPCT="":"",1:$J(fUTBLACC.PROVPCT,0,5)) set VO(17)=$C(11,33,9,2,0,0,0,0,0,0)_"00N"_$S(fUTBLACC.PROVPCT="":"",1:$J(fUTBLACC.PROVPCT,0,5))
 set VO(18)=$C(12,33,12,2,0,0,0,0,0,0)_"00N"_fUTBLACC.CRGL
 set VO(19)=$C(13,33,12,2,0,0,0,0,0,0)_"00N"_fUTBLACC.DRGL
 set VO(20)=$C(14,33,12,2,0,0,0,0,0,0)_"00N"_fUTBLACC.PROVIGL
 set VO(21)=$C(19,33,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLACC.PROVEXT,1,40)
 set VO(22)=$C(20,33,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLACC.PROVCALC,1,40)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
