	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLDAR2",VPGM=$T(+0),VSNAME="Reclassification G/L Set Code User Table"
 set VFSN("UTBLACC")="zfUTBLACC"
 set vPSL=1
 set KEYS(1)=fUTBLACC.CLS
 set KEYS(2)=fUTBLACC.AC
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLACC),VDA1(.fUTBLACC),^DBSPNT()

 if '%ProcessMode do VNEW(.fUTBLACC),VPR(.fUTBLACC),VDA1(.fUTBLACC)
 if %ProcessMode do VLOD(.fUTBLACC) quit:$G(ER)  do VPR(.fUTBLACC),VDA1(.fUTBLACC)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLACC)
	quit


