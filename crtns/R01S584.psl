	type Public String vc1,vc2,vc3,vc4,vp4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14
	set vc1=V.piece($C(9),1)			// DEP.TYPE
	set vc2=V.piece($C(9),2)			// DEP101.CID
	set vc3=V.piece($C(9),3)			// CIF.LNM
	set vc4=V.piece($C(9),4)			// CIF.TAXID
	set vp4=VI.extract(4)				// Protection indicator
	set vc5=V.piece($C(9),5)			// DEP101.ICID
	set vc6=V.piece($C(9),6)			// CIF.NRCNTRY
	set vc7=V.piece($C(9),7)			// DEP101.ITYPE
	set vc8=V.piece($C(9),8)			// DEP.USRESTAT
	set vc9=V.piece($C(9),9)			// DEP.BWF
	set vc10=V.piece($C(9),10)			// DEP.BNOT1DT
	set vc11=V.piece($C(9),11)			// DEP.BNOT2DT
	set vc12=V.piece($C(9),12)			// DEP.CNOTDT
	set vc13=V.piece($C(9),13)			// DEP.IPTY
	set vc14=V.piece($C(9),14)			// DEP.BWPTY
	quit

