 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fLNAEU2,.fLNAEU1,.LN),VDA1(.fLNAEU2,.fLNAEU1,.LN),^DBSPNT()

 if '%ProcessMode do VNEW(.fLNAEU2,.fLNAEU1,.LN),VPR(.fLNAEU2,.fLNAEU1,.LN),VDA1(.fLNAEU2,.fLNAEU1,.LN)
