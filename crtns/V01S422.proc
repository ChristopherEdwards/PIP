 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNTRS1",VPGM=$T(+0),VSNAME="Escrow Payee Maintenance"
 set VFSN("LNTRS1")="zfLNTRS1"
 set vPSL=1
 set KEYS(1)=fLNTRS1.TRTYPE
 set KEYS(2)=fLNTRS1.PAYID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fLNTRS1),VDA1(.fLNTRS1),^DBSPNT()

 if '%ProcessMode do VNEW(.fLNTRS1),VPR(.fLNTRS1),VDA1(.fLNTRS1)
 if %ProcessMode do VLOD(.fLNTRS1) quit:$G(ER)  do VPR(.fLNTRS1),VDA1(.fLNTRS1)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fLNTRS1)
	quit


