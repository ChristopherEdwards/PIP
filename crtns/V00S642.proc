 set VO(30)=$C(18,23,1,2,0,0,0,0,0,0)_"00L"_$S(fUTBZTSD.REQD:"Y",1:"N")
 set VO(31)=$C(20,23,3,2,0,0,0,0,0,0)_"00N"_fUTBZTSD.MRPC
