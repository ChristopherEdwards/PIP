 set VO(5)=$C(6,10,12,0,0,0,0,0,0,0)_"01TDescription:"
 set VO(6)=$C(7,7,15,0,0,0,0,0,0,0)_"01TTeller Prompt :"
 set VO(7)=$C(9,7,15,0,0,0,0,0,0,0)_"01TFunction/Query:"
