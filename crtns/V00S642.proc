 set VO(22)=$C(7,23,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBZTSD.PROMPT,1,40)
 set VO(23)=$C(9,23,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBZTSD.FUNQRY,1,40)
