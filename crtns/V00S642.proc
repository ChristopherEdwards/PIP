 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLZTSD",VPGM=$T(+0),VSNAME="Transaction Qualifier Screen"
 set VFSN("UTBLZTSD")="zfUTBZTSD"
 set vPSL=1
 set KEYS(1)=fUTBZTSD.SEQ
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBZTSD),VDA1(.fUTBZTSD),^DBSPNT()

 if '%ProcessMode do VNEW(.fUTBZTSD),VPR(.fUTBZTSD),VDA1(.fUTBZTSD)
 if %ProcessMode do VLOD(.fUTBZTSD) quit:$G(ER)  do VPR(.fUTBZTSD),VDA1(.fUTBZTSD)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBZTSD)
	quit


VNEW(RecordUTBLZTSD fUTBZTSD) // Initialize arrays if %O=0
 
 do VDEF(.fUTBZTSD)
 do VLOD(.fUTBZTSD)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordUTBLZTSD fUTBZTSD)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordUTBLZTSD fUTBZTSD) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

VPR(RecordUTBLZTSD fUTBZTSD) // Display screen prompts
 set VO="17||13|0"
 set VO(0)="|0"
 set VO(1)=$C(1,20,43,0,0,0,0,0,0,0)_"01TCustom Transaction Support Data Maintenance"
 set VO(2)=$C(3,13,9,1,0,0,0,0,0,0)_"01TSequence:"
