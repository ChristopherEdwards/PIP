 quit

	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
 
vSET(sn,di,X)
 type Public RecordUTBLZTSD fUTBZTSD
 if sn="UTBLZTSD" do vSET1(.fUTBZTSD,di,X)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
vSET1(RecordUTBLZTSD fUTBZTSD,di,X)
 do fUTBZTSD.setAuditFlag(1)
 set fUTBZTSD.@di=X
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
vREAD(fid,di)
 type Public RecordUTBLZTSD fUTBZTSD
 if fid="UTBLZTSD" quit $$vREAD1(.fUTBZTSD,di)
