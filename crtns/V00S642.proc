 new V
 //
 set VO="32|18|13|0"
 set VO(18)=$C(3,23,4,2,0,0,0,0,0,0)_"01N"_fUTBZTSD.SEQ
 set VO(19)=$C(4,23,15,2,0,0,0,0,0,0)_"00T"_$E(fUTBZTSD.ACRONYM,1,15)
