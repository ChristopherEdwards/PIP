R01S701	// YER434 - T5 Year-End Detail Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 15:04 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String IO,SIO
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Number VIN1
	type String VIN2="ALL"
	type String VIN3="ALL"

	set RID="YER434"
	set RN="T5 Year-End Detail Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|1||[T5INF]RC|[T5INF]RC:DISTINCT:NOVAL||||N|Report Code is equal to|||||"
	set %TAB("VIN2")="|255||[T5INF]BOO|[UTBLBRCD]||D EXT^DBSQRY||T|Branch of Ownership|||||"
	set %TAB("VIN3")="|255||[T5INF]ACN|[T5INF]ACN:DISTINCT:NOVAL||D EXT^DBSQRY||T|Customer Number|||||"

	set %READ="IO/REQ,VIN1#1,VIN2#0,VIN3#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String IO,SIO
	type public Number VIN1
	type public String VIN2,VIN3
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,ACN,BOO,CONAM,NONREP,NRMESG,PRIACN,RID,RN,T5RECTYP,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	set CONAM=CUVAR.conam
	set ER=0,RID="YER434",RN="T5 Year-End Detail Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","YER434","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=81")
		write $$CLEARXY^%TRMVT
		}

	do INIT^%ZM()

	set vCOL="[T5INF]ACN#2#12,[T5INF]CID#44#12,[T5INF]CRI#68#12"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		set DQQRY(SEQ)="[T5INF]RC = "_$$addqts^DBSRWUTL(VIN1),SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[T5INF]BOO "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[T5INF]ACN "_VIN3,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("T5INF,DEP,CIF") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:1 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"T5INF.RC,T5INF.BOO,T5INF.ACN,T5INF.CID,T5INF.CRI,T"
	set VSELECT=VSELECT_"5INF.NRI,CIF.XNAME,DEP.ACNRELC"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"T5INF,DEP,CIF",VWHERE,"T5INF.RC,T5INF.BOO,T5INF.ACN,T5INF.CID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8
	set vc1=V.piece($C(9),1)			// T5INF.RC
	set vc2=V.piece($C(9),2)			// T5INF.BOO
	set vc3=V.piece($C(9),3)			// T5INF.ACN
	set vc4=V.piece($C(9),4)			// T5INF.CID
	set vc5=V.piece($C(9),5)			// T5INF.CRI
	set vc6=V.piece($C(9),6)			// T5INF.NRI
	set vc7=V.piece($C(9),7)			// CIF.XNAME
	set vc8=V.piece($C(9),8)			// DEP.ACNRELC
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 //Incoming=IO,SIO
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do VSUM4 quit:VFMQ  do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc5
	type Public String VSTATS

	set VT(4)=VT(4)+1
	set VT(4,1,1)=VT(4,1,1)+vc5			// @TOT([T5INF]CRI)
	quit

VSUM3	// Summary
	type public String %TIM,ACN,BOO,I,IO,IOSL,NONREP,NRMESG,PRIACN,SIO,T5RECTYP,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                                         "_"Total for Branch"
	set VL=VL_$J("",58-VL.length())_$J(vc2,6)
	set VL=VL_$J("",64-VL.length())_":"
	set V=VT(3,1,1)					// @TOT([T5INF]CRI,)
	set VL=VL_$J("",67-VL.length())_$J($FN(V,",",2),12)
	do VOM
	set VL="" do VOM
	quit

VDTL4	// Detail
	type public String %TIM,ACN,BOO,IO,IOSL,NONREP,NRMESG,PRIACN,SIO,T5RECTYP,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+2>IOSL do VHDG0 quit:VFMQ

	do VP1 quit:VFMQ!verror.get()  set V=$E(NONREP,1,1) set VL=V
	set VL=VL_$J("",1-VL.length())_$J(vc3,12)
	do VP2 quit:VFMQ!verror.get()  set V=$E(PRIACN,1,26) do VP3 quit:VFMQ!verror.get()
	set VL=VL_$J("",15-VL.length())_V
	set VL=VL_$J("",43-VL.length())_$J(vc4,12)
	do VP4 quit:VFMQ!verror.get()  set V=$J(T5RECTYP,1)
	set VL=VL_$J("",61-VL.length())_V
	set VL=VL_$J("",67-VL.length())_$J($FN(vc5,",",2),12)
	do VOM
	set VL="               "_$E(NRMESG,1,26)
	do VOM
	quit

VSUM4	// Summary
	type public String %TIM,ACN,BOO,I,IO,IOSL,NONREP,NRMESG,PRIACN,SIO,T5RECTYP,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(4) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                                 "_"Total for Customer "
	set VL=VL_$J("",52-VL.length())_$J(vc3,12)
	set VL=VL_$J("",64-VL.length())_":"
	set V=VT(4,1,1)					// @TOT([T5INF]CRI,)
	set VL=VL_$J("",67-VL.length())_$J($FN(V,",",2),12)
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,ACN,BOO,CONAM,IO,NONREP,NRMESG,PRIACN,RID,RN,SIO,T5RECTYP,VL,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+8,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL="" do VOM
	set VL=$E($G(CONAM),1,40)
	set VL=VL_$J("",49-VL.length())_"Run Date:"
	set VL=VL_$J("",59-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",70-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=$E(RN,1,50)
	set VL=VL_$J("",51-VL.length())_"System:"
	set VL=VL_$J("",59-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",70-VL.length())_"Page:"
	set VL=VL_$J("",76-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="Report Code:"
	set VL=VL_$J("",13-VL.length())_$J(vc1,1)
	do VOM
	set VL="" do VOM
	set VL=" "_"Customer Nbr  Name                             Account  Rec Type      Interest"
	do VOM
	set VL="================================================================================"
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,ACN,BOO,NONREP,NRMESG,PRIACN,T5RECTYP,VPN,vc2,vc3,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                    "_"                               Report Totals:"
	set V=0						// @TOT([T5INF]CRI,0)
	for I=0:1:4 set V=V+VT(I,1,1)
	set VL=VL_$J("",66-VL.length())_$J($FN(V,",",2),13)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: NONREP

	type public String vc6
 // If this account has non-reportable interest, set this variable.
 type public String NONREP

 if vc6 set NONREP="*"
 else  set NONREP=" "
	quit

VP2	// Column pre-processor - Variable: PRIACN

	type public String vc7,vc6
 type public String NRMESG,PRIACN

 // Retrieve the name of the primary ACN.
 
 set PRIACN=vc7
 set NRMESG=""

 /* If this account contains negative non-reportable interest, then set the
 PRIACN variable and the NRMESG variable to an error message.*/
 
 if vc6<0 do { quit
	set PRIACN="Tax data not reported - "
	set NRMESG="negative reportable int."
	}

 /* If this account contains non-reportable interest, then set the PRIACN
 variable and the NRMESG variable to an error message.*/

 if vc6 do {
	set PRIACN="Tax data not reported -not"
	set NRMESG="able to calc currncy exchg"
	}
	quit

VP3	// Column post-processor - Variable: PRIACN

 type public String PRIACN

 set PRIACN=""
	quit

VP4	// Column pre-processor - Variable: T5RECTYP

	type public String vc8
 /* This section will retrieve the correct recipient type for each 
 individual account selected for the T5 form.*/

 type public Number RELC,T5RECTYP
 set RELC=vc8
 type RecordRELCODE1 relcode1=Db.getRecord("RELCODE1","REL=:RELC")
 set T5RECTYP=relcode1.t5rectype
	quit
