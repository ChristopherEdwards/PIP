vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordDEPSCH depsch

	set X = depsch.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	set X = depsch.depdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("DEPDT", vRM) quit
