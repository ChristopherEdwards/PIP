 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="MT192PG4",VPGM=$T(+0),VSNAME="Request to Cancel 100 (Page 4)"
 set VFSN("SWIFT")="zfSWIFT"
 set vPSL=1
 set KEYS(1)=fSWIFT.SWDIRECT
 set KEYS(2)=fSWIFT.TRREFNO
 set KEYS(3)=fSWIFT.MSG
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fSWIFT),VDA1(.fSWIFT),^DBSPNT()

 set ER=0 do VSCRPRE(.fSWIFT) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fSWIFT),VPR(.fSWIFT),VDA1(.fSWIFT)
 if %ProcessMode do VLOD(.fSWIFT) quit:$G(ER)  do VPR(.fSWIFT),VDA1(.fSWIFT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fSWIFT)
	quit


