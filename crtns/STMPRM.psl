STMPRM	/*	Statement Parameter Set-up

	---- Revision History ------------------------------------------------

	03/14/07 - KumarSS - CR 25177
		   Removed Invalid Unicode Characters.

	01/28/05 - HILLANBRAND 14046
		   Modified PSL object name ST be lower case st per
		   programming standards. Scoped all variables and changed
		   new to Type.  Corrected year on revision history. Removed
		   unnecessary code.
			
	01/27/05 - HILLANBRAND 14046
		   Modification to load the object in STMPRM to use 
		   Db.getRecord("STMPRM", "KEY=1").
		
	09/23/02 - DATTAR - 49451
		   Converted to PSL.

	----------------------------------------------------------------------
	*/
	

	if Db.isDefined("STMPRM","1") do {
		set %ProcessMode=1
		type RecordSTMPRM st=Db.getRecord("STMPRM","KEY=1")
		do INIT(.st)
		}

	else  	do {
		set %ProcessMode=0
		type RecordSTMPRM st=Class.new("RecordSTMPRM","KEY=1")
		do INIT(.st)
		}		
	
	quit


INIT(RecordSTMPRM st) //

	type Public Number ER
	type Public String RM,VFMQ
		
	do DRV^USID(%ProcessMode,"STMPRM",.st)
	

	// File data
	if "F"[VFMQ do st.save()

	if $G(ER) set ER="W" quit

	if VFMQ="Q" do {
		// Statement parameters not created
		if %ProcessMode=0 set RM=$$^MSG(6860) quit
		
		// Statement parameters not modified
		if %ProcessMode=1 set RM=$$^MSG(6862) quit
		
		}
	else  do {
		// Statement parameters created
		if %ProcessMode=0 set RM=$$^MSG(6857) quit

		// Statement parameters modified
		if %ProcessMode=1 set RM=$$^MSG(6859) quit

	S ER="W"

	quit
 #OPTION ResultClass ON
Public String vSIG()	quit "60703^23777^Sudanthiran S. Kumar^1399"	// Signature - LTD^TIME^USER^SIZE
