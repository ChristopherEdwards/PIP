	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("PKGID").exists()!vx("FID").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("PKGFID",.vx)
