VSPP   // screen post proc
 type Public RecordUTBLLNSUB fUTBL
 do VSPP1(.fUTBL)
 #ACCEPT Date=11/05/03; pgm=Screen Compilerxxx
 quit
VSPP1(RecordUTBLLNSUB fUTBL)
	// Invalid frequency	
	if fUTBL.rfreopt'=2,fUTBL.calcopt.isNull() do Runtime.setErrMSG("UTBLLNSUB",1353) quit
	// Invalid frequency
	if fUTBL.rfreopt.isNull(),fUTBL.rfre.isNull() do Runtime.setErrMSG("UTBLLNSUB",1353) quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


  //user-defined post procs
 //
VP1(RecordUTBLLNSUB fUTBL) //
	type public String X

	if X.isNull() quit

	type RecordTRN trn=Db.getRecord("TRN","ETC=:X",1)
		
	// Transaction code for ~p1 must be a debit
	if 'X.isNull() , trn.dc do Runtime.setErrMSG("TRN",2696) quit

VP2(RecordUTBLLNSUB fUTBL) //
	type public String X

	if X.isNull() do DEFAULT^DBSMACRO("UTBLLNSUB.GLINCTRN","")
VP3(RecordUTBLLNSUB fUTBL) //
	type public String X
	
	if X.isNull() quit

	type RecordTRN trn=Db.getRecord("TRN","ETC=:X",1)
	
	// Transaction code for ~p1 must be a credit
	if 'X.isNull() , 'trn.dc do Runtime.setErrMSG("TRN",2695) quit
	
VP4(RecordUTBLLNSUB fUTBL) //
	type public String X

	if X.isNull() do {

		// Null calculation option requires subsidy percentage to be defined
		if fUTBL.prcntg="" do Runtime.setErrMSG("UTBLLNSUB",780) quit
		do DEFAULT^DBSMACRO("UTBLLNSUB.RFRE","")
		do DEFAULT^DBSMACRO("UTBLLNSUB.RFREOPT",2)
		do PROTECT^DBSMACRO("UTBLLNSUB.RFRE")
		do PROTECT^DBSMACRO("UTBLLNSUB.RFREOPT")
		}

	if 'X.isNull() do {
		do UNPROT^DBSMACRO("UTBLLNSUB.RFRE")
		do UNPROT^DBSMACRO("UTBLLNSUB.RFREOPT")
		}	
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


