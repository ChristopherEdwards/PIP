 set %MAX=10,VPT=2,VPB=15,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLLNSUB",VSCRPP=1,VSCRPP=1
 set OLNTB=15025
 
 set VFSN("UTBLLNSUB")="zfUTBL"
 //
 // 
	
	
 set %TAB(1)=$C(3,24,12)_"21T12401|1|[UTBLLNSUB]SUBPLN"
 set %TAB(2)=$C(4,24,40)_"01T12401|1|[UTBLLNSUB]DESC"
 set %TAB(3)=$C(7,24,12)_"01N12402|1|[UTBLLNSUB]GLCID|[GLAD]"
 set %TAB(4)=$C(7,64,12)_"00T12403|1|[UTBLLNSUB]GLTRN|[TRN]:QU ""[TRN]CLS=""""M""""""||do VP1^V00S607(.fUTBL)"
 set %TAB(5)=$C(8,24,12)_"00N12408|1|[UTBLLNSUB]GLINC|[GLAD]||do VP2^V00S607(.fUTBL)"
 set %TAB(6)=$C(8,64,12)_"00T12409|1|[UTBLLNSUB]GLINCTRN|[TRN]:QU ""[TRN]CLS=""""M""""""||do VP3^V00S607(.fUTBL)"
 set %TAB(7)=$C(9,24,9)_"00N12406|1|[UTBLLNSUB]PRCNTG|||||0|100|5"
 set %TAB(8)=$C(9,64,2)_"00N12407|1|[UTBLLNSUB]CALCOPT|[UTBLSLCO]||do VP4^V00S607(.fUTBL)"
 set %TAB(9)=$C(13,24,12)_"00F12404|1|[UTBLLNSUB]RFRE"
 set %TAB(10)=$C(14,24,1)_"00N12405|1|[UTBLLNSUB]RFREOPT|[STBLRFREOPT]"
 do VTBL(.fUTBL)
