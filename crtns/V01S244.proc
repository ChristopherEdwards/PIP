V01S244( Number %ProcessMode,RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)   // -  - SID= <DEAL1SIP> Deal Settlement Instructions (Purchase)
 ;;Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 11:53 - shetyes
 #WARN ACCESS,MISMATCH OFF
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
 type Public Number ER
 type Public String %PAG,%PAGE,%PG,%PGSV,MULSCR,RM,VFMQ
 type String KVAR,VPG(),vPSL,VSID
 set VSID="DEAL1SIP"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+7 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+8
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Deal Processing (Buy)|DEAL1"
 set VPG(%PG+1)="Deal Settlement Instruction Screen 1|DEAL1SI1"
 set VPG(%PG+2)="Deal Settlement Instruction Screen 2|DEAL1SI2"
 set VPG(%PG+3)="Deal Settlement Instruction Screen 3|DEAL1SI3"
 set VPG(%PG+4)="Deal Settlement Instruction Screen 4|DEAL1SI4"
 set VPG(%PG+5)="Deal Settlement Instruction Screen 5|DEAL1SI5"
 set VPG(%PG+6)="Deal Settlement Instruction Screen 6|DEAL1SI6"
 set VPG(%PG+7)="Deal Settlement Instruction Screen 7|DEAL1SI7"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.DEALMSG2,.DEALMSG3,.fDEAL1)
 if %ProcessMode do VLOD(.DEALMSG2,.DEALMSG3,.fDEAL1) if $G(ER) set VFMQ="Q" quit
 do VPG(.DEALMSG2,.DEALMSG3,.fDEAL1)
 quit

VNEW(RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)
 
 quit
 //
 ;
VLOD(RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)  // User defined access section
	quit

VPG(RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+4) do VPG5(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+5) do VPG6(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+6) do VPG7(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  if %PG=(%PGSV+7) do VPG8(.DEALMSG2,.DEALMSG3,.fDEAL1),VPG0(.DEALMSG2,.DEALMSG3,.fDEAL1) quit:vDONE
  }
 quit

VPG0(RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.DEALMSG2,.DEALMSG3,.fDEAL1) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordDEALMSG2 DEALMSG2,RecordDEALMSG3 DEALMSG3,RecordDEAL1 fDEAL1)  // Deal Processing (Buy)
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="DEAL1",DFID="DEAL1"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S039
 if %ProcessMode>1 quit
 do VTAB^V01S039(.fDEAL1)
 quit
 
