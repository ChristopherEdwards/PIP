vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL13D dbtbl13d

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL13D","LIBS = :dbtbl13d.libs and PID = :dbtbl13d.pid and SEQ = :dbtbl13d.seq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
