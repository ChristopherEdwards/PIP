		#BYPASS
		if $D(vobj(dbtbl13d))  S ^DBTBL(vobj(dbtbl13d,-3),13,vobj(dbtbl13d,-4),vobj(dbtbl13d,-5))=vobj(dbtbl13d)
		#ENDBYPASS
	}

	quit

