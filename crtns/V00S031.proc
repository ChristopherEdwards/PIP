 set V=$S(fCRCD.MIDRATE="":"",1:$J(fCRCD.MIDRATE,0,7)) set VO(17)=$C(8,30,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.MIDRATE="":"",1:$J(fCRCD.MIDRATE,0,7))
 set VO(18)=$C(8,46,30,2,0,0,0,0,0,0)_"00T"_$G(DSP1)
 set V=$S(fCRCD.FINSPOT="":"",1:$J(fCRCD.FINSPOT,0,7)) set VO(19)=$C(9,30,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.FINSPOT="":"",1:$J(fCRCD.FINSPOT,0,7))
