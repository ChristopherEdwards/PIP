 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab,DSP1,DSP2",VSID="CRCDMR",VPGM=$T(+0),VSNAME="Currency Exchange Rates"
 set VFSN("CRCD")="zfCRCD"
 set vPSL=1
 set KEYS(1)=fCRCD.CO
 set KEYS(2)=fCRCD.CRCD
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fCRCD),VDA1(.fCRCD),^DBSPNT()

 // Display Pre-Processor
 //
 if '%ProcessMode do VNEW(.fCRCD),VDSPPRE(.fCRCD) quit:$G(ER)  do VPR(.fCRCD),VDA1(.fCRCD)
 if %ProcessMode do VLOD(.fCRCD) quit:$G(ER)  do VDSPPRE(.fCRCD) quit:$G(ER)  do VPR(.fCRCD),VDA1(.fCRCD)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


