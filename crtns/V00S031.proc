 set V=$S(fCRCD.SPOTOFFER="":"",1:$J(fCRCD.SPOTOFFER,0,7)) set VO(22)=$C(11,59,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.SPOTOFFER="":"",1:$J(fCRCD.SPOTOFFER,0,7))
 set V=$S(fCRCD.OFFBID="":"",1:$J(fCRCD.OFFBID,0,7)) set VO(23)=$C(13,30,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.OFFBID="":"",1:$J(fCRCD.OFFBID,0,7))
 set V=$S(fCRCD.OFFOFFER="":"",1:$J(fCRCD.OFFOFFER,0,7)) set VO(24)=$C(13,59,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.OFFOFFER="":"",1:$J(fCRCD.OFFOFFER,0,7))
 set V=$S(fCRCD.OTCBID="":"",1:$J(fCRCD.OTCBID,0,7)) set VO(25)=$C(14,30,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.OTCBID="":"",1:$J(fCRCD.OTCBID,0,7))
 set V=$S(fCRCD.OTCOFFER="":"",1:$J(fCRCD.OTCOFFER,0,7)) set VO(26)=$C(14,59,13,2,0,0,0,0,0,0)_"00N"_$S(fCRCD.OTCOFFER="":"",1:$J(fCRCD.OTCOFFER,0,7))
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
	
