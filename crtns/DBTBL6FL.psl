DBTBL6FL(RecordDBTBL6F dbtbl6f, String vpar, Boolean vparNorm) // DBTBL6F - QWIK Report Format Definition Filer
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 11/09/2007 15:13 - chenardp
	// Generated from DATA-QWIK schema in: /profile/v72framework_gtm  by: /v72framework_gtmlx/crtns/DBSFILB.obj

	// Data Dictionary Data Items (10)             02/06/2006
	/*
		vpar      Runtime qualifiers:      /NOREQ/MECH=REFARR:R

		/[NO]CASDEL   - Cascade delete
		/[NO]FRMFILER - Called from another filer
		/[NO]INDEX    - Update Indexes
		/[NO]JOURNAL  - Journal update
		/[NO]TRIGAFT  - After update triggers
		/[NO]TRIGBEF  - Before update triggers
		/[NO]UPDATE   - Update primary table
		/[NO]VALDD    - Validate column values
		/[NO]VALFK    - Validate foreign keys
		/[NO]VALREQ   - Validate not null values
		/[NO]VALRI    - Validate transaction integrity
		/[NO]VALST    - Validate database state
	*/

	type Public Number ER = 0
	type Public String RM

	type public String verrors()

	catch fERROR {
		if fERROR.type="%PSL-E-DBFILER" do {
			set ER = 1
			set RM = fERROR.description
		}
		else  throw fERROR
	}
	type String vx(), vxins()                              // audit column array
	type Number %O = dbtbl6f.getMode()                     // Processing mode
	set vpar = vpar.get()                                  // Initialize vpar

	if %O = 0 do AUDIT^UCUTILN(dbtbl6f,.vxins(),1,"|")
	if %O = 1 do AUDIT^UCUTILN(dbtbl6f,.vx(),1,"|")

	if 'vparNorm.get() set vpar = $$initPar^UCUTILN(vpar)  // Run-time qualifiers

	if %O = 0 do { quit                                    // Create record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		if vpar["/VALDD/" do vddver                           // Check values
		do vexec
	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("LIBS").exists()!vx("QRID").exists()!vx("SEQ").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("DBTBL6F",.vx)
		set %O = 1 do vexec
	}

	if %O = 2 do { quit                                    // Verify record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		set vpar = $$setPar^UCUTILN(vpar,"NOJOURNAL/NOUPDATE")
		do vexec
	}

	if %O = 3 do { quit                                    // Delete record control block
		quit:'Db.isDefined("DBTBL6F","LIBS = :dbtbl6f.libs and QRID = :dbtbl6f.qrid and SEQ = :dbtbl6f.seq") // No record exists
		do vdelete(0)
	}

	quit

vlegacy(Number %ProcessMode,String vpar) // Legacy interface (^DBSDF9 for %O=0, EXT^DBSFILER for %O = 2)

	type public String LIBS
	type public String QRID
	type public String SEQ

	type RecordDBTBL6F dbtbl6f = Db.getRecord("DBTBL6F","LIBS=:LIBS,QRID=:QRID,SEQ=:SEQ")
	if (%ProcessMode = 2) do {
		do dbtbl6f.setMode(2)

		do DBTBL6FL(dbtbl6f,vpar)
	}

	quit

vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL6F dbtbl6f

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL6F","LIBS = :dbtbl6f.libs and QRID = :dbtbl6f.qrid and SEQ = :dbtbl6f.seq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(dbtbl6f)) S ^DBTBL(vobj(dbtbl6f,-3),6,vobj(dbtbl6f,-4),vobj(dbtbl6f,-5))=vobj(dbtbl6f)
		#ENDBYPASS
	}

	quit

vdelete(Boolean vkeychg) // Record Delete

	type public String vobj(),vpar
	type public RecordDBTBL6F dbtbl6f


	// Allow global reference - Delete record
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	ZWI ^DBTBL(vobj(dbtbl6f,-3),6,vobj(dbtbl6f,-4),vobj(dbtbl6f,-5))
	#ENDBYPASS
	quit


vreqn // Validate required data items

	type public RecordDBTBL6F dbtbl6f

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordDBTBL6F dbtbl6f

	if dbtbl6f.libs.isNull() do vreqerr("LIBS") quit
	if dbtbl6f.qrid.isNull() do vreqerr("QRID") quit
	if dbtbl6f.seq.isNull() do vreqerr("SEQ") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("DBTBL6F","MSG",1767,"DBTBL6F."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordDBTBL6F dbtbl6f

	if dbtbl6f.libs.length()>10 set vRM = $$^MSG(1076,10) do vdderr("LIBS", vRM) quit
	if dbtbl6f.qrid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("QRID", vRM) quit
	set X = dbtbl6f.seq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("SEQ", vRM) quit
	if dbtbl6f.formdesc.length()>35 set vRM = $$^MSG(1076,35) do vdderr("FORMDESC", vRM) quit
	if dbtbl6f.formdi.length()>12 set vRM = $$^MSG(1076,12) do vdderr("FORMDI", vRM) quit
	set X = dbtbl6f.formfmt if 'X.isNull(),'Db.isDefined("DBCTLRFMT","X") set vRM = $$^MSG(1485,X) do vdderr("FORMFMT", vRM) quit
	set X = dbtbl6f.formfun if 'X.isNull(),'Db.isDefined("DBCTLQFUN","X") set vRM = $$^MSG(1485,X) do vdderr("FORMFUN", vRM) quit
	set X = dbtbl6f.formidn if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("FORMIDN", vRM) quit
	set X = dbtbl6f.formlf if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("FORMLF", vRM) quit
	set X = dbtbl6f.formsize if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("FORMSIZE", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("DBTBL6F","MSG",979,"DBTBL6F."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordDBTBL6F dbtbl6f

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("LIBS").exists() set vux("LIBS") = vx("LIBS")
	if vx("QRID").exists() set vux("QRID") = vx("QRID")
	if vx("SEQ").exists() set vux("SEQ") = vx("SEQ")
	do vkey(1) set voldkey = dbtbl6f.libs_","_dbtbl6f.qrid_","_dbtbl6f.seq // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = dbtbl6f.libs_","_dbtbl6f.qrid_","_dbtbl6f.seq // Copy new keys into object
	type RecordDBTBL6F vnewrec = dbtbl6f.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("DBTBL6F",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordDBTBL6F dbtbl6f

	if vux("LIBS").exists() set dbtbl6f.libs = vux("LIBS").piece("|",i)
	if vux("QRID").exists() set dbtbl6f.qrid = vux("QRID").piece("|",i)
	if vux("SEQ").exists() set dbtbl6f.seq = vux("SEQ").piece("|",i)
	quit
