		set VBNRINFO("ORDERBY",2)="[SYSDEV,DAYENDM1099]""M1099"""
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DAYENDM1099]TAXID"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,DAYENDM1099]SEQ"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,DAYENDM1099]TSEQ"

		set VBNRINFO("DOC",1)="/*"
