	type public String %TIM,DAT,DI,DLIB,FR,IOSL,NUM,NV,OPTION,TRAIL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set V=vc2,VO=V do VP1 quit:VFMQ!verror.get()  set V=$J($$DAT^%ZM(V),10) do VP2 quit:VFMQ!verror.get()  set VL=V
	set V=vc3,VO=V set V=$E(V,1,11) do VP3 quit:VFMQ!verror.get()
	set VL=VL_$J("",11-VL.length())_V		// [SYSDEV,DAYENDM1099]TAXID
	set V=vc4,VO=V set V=$J(V,3) do VP4 quit:VFMQ!verror.get()
	set VL=VL_$J("",23-VL.length())_V		// [SYSDEV,DAYENDM1099]SEQ
	set V=vc6,VO=V set V=$E(V,1,20) do VP5 quit:VFMQ!verror.get()
	set VL=VL_$J("",27-VL.length())_V		// [SYSDEV,DAYENDM1099]UID
	do VP6 quit:VFMQ!verror.get()  set V=$E(OPTION,1,6)
	set VL=VL_$J("",49-VL.length())_V
	set VL=VL_$J("",58-VL.length())_$E(DI,1,24)
	do VP7 quit:VFMQ!verror.get()  set V=$E(FR,1,6)
	set VL=VL_$J("",84-VL.length())_V
	do VP8 quit:VFMQ!verror.get()  set V=$E(DAT,1,40)
	set VL=VL_$J("",90-VL.length())_V
	do VOM
	set V=vc1,VO=V do VP9 quit:VFMQ!verror.get()  set V=$E(V,1,47) set VL="                                                                                   "_V
