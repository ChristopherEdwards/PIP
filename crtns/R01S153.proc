		set DQQRY(SEQ)="[DAYENDM1099]TSEQ'=""""",SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("DAYENDM1099") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
