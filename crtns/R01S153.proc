		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[DAYENDM1099]TJD#1#10,[DAYENDM1099]TAXID#12#11,[DAYENDM1099]SEQ#24#3,[DAYENDM1099]UID#28#20"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
