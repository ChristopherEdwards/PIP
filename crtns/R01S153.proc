		set VBNRINFO("DOC",3)=""
		set VBNRINFO("DOC",4)="    04/19/06 - SWARNALP - 20418"
		set VBNRINFO("DOC",5)="	       Modifid pre processor of data item 'TJD' to get the IRS "
		set VBNRINFO("DOC",6)="	       details from DAYENDM1099 table instead of M1099 table as "
		set VBNRINFO("DOC",7)="	       the details of the same will get deleted from M1099 table "
		set VBNRINFO("DOC",8)="	       when IRS Transaction Deletion function DEP024 is executed "
		set VBNRINFO("DOC",9)="	       and changed the delimiter from ""|"" to ""#"" to extract each "
		set VBNRINFO("DOC",10)="	       data value of the record since the details are stored using "
		set VBNRINFO("DOC",11)="	       '#' as delimiter in DAYENDM1099.DATA."
		set VBNRINFO("DOC",12)=""
		set VBNRINFO("DOC",13)="    12/30/05 - SWARNALP - 18751"
		set VBNRINFO("DOC",14)="               Added quit in the if condition present in the pre processor "
		set VBNRINFO("DOC",15)="	       of data item 'NV' to avoid going into the else section "
		set VBNRINFO("DOC",16)="	       after setting NUM=0 within the if condition."
		set VBNRINFO("DOC",17)=""
		set VBNRINFO("DOC",18)="    12/19/05 - SWARNALP - 17293"
		set VBNRINFO("DOC",19)="               Converted to PSL"
		set VBNRINFO("DOC",20)=""
		set VBNRINFO("DOC",21)="   ------------------------------------------------------------------------ "
		set VBNRINFO("DOC",22)="*/"

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VHIT = 0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
