R01S712	// YER445 - NR4 RPA Distributions Detail Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:55 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"

	set RID="YER445"
	set RN="NR4 RPA Distributions Detail Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[NR4DINF]ACN|[NR4DINF]ACN:DISTINCT:NOVAL||D EXT^DBSQRY||T|Customer Number|||||"

	set %READ="IO/REQ,VIN1#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String VIN1
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,ACN,BOO,CCODE,CONAM,DIST(),INCD(),IRATYP,NONREP,NRMESG,PRIACN,RID,RN,RPASEQ,SEQ,TOTDIST,TOTDIST(),TOTWHLD,TOTWHLD(),VL,VLOF,VRF(),VSTATS(),WHLD(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate

	set CONAM=CUVAR.conam
	set ER=0,RID="YER445",RN="NR4 RPA Distributions Detail Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","YER445","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=99999999			// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()


	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[NR4DINF]ACN "_VIN1,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("NR4DINF,CIF") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:3 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"NR4DINF.ACN,NR4DINF.BOO,NR4DINF.RPASEQ,NR4DINF.PLA"
	set VSELECT=VSELECT_"NID,NR4DINF.NRD,NR4DINF.NRDW,NR4DINF.NEGAMT,CIF.XN"
	set VSELECT=VSELECT_"AME,CIF.NRCNTRY"

	#ACCEPT DATE=01/31/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"NR4DINF,CIF",VWHERE,"NR4DINF.ACN,NR4DINF.BOO,NR4DINF.RPASEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9
	set vc1=V.piece($C(9),1)			// NR4DINF.ACN
	set vc2=V.piece($C(9),2)			// NR4DINF.BOO
	set vc3=V.piece($C(9),3)			// NR4DINF.RPASEQ
	set vc4=V.piece($C(9),4)			// NR4DINF.PLANID
	set vc5=V.piece($C(9),5)			// NR4DINF.NRD
	set vc6=V.piece($C(9),6)			// NR4DINF.NRDW
	set vc7=V.piece($C(9),7)			// NR4DINF.NEGAMT
	set vc8=V.piece($C(9),8)			// CIF.XNAME
	set vc9=V.piece($C(9),9)			// CIF.NRCNTRY
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

 type public Number TOTDIST(),TOTWHLD()
 
 set (TOTDIST,TOTDIST(1),TOTWHLD,TOTWHLD(1))=0
 quit
 
ZDATABLD // Build DATA array
 
 // Build DATA array to allow correct number of forms to be produced.
 type public Number ACN,BOO,DIST(),INCD(),WHLD(),RPASEQ
 type Number SEQ,I
 set SEQ=0
 kill INCD,DIST,WHLD
 for I=1:1:5 set INCD(I)="",DIST(I)="",WHLD(I)=""
 type RecordNR4DINF nr4dinf=Db.getRecord("NR4DINF","ACN=:ACN,BOO=:BOO,RPASEQ=:RPASEQ")
 if 'nr4dinf.getMode() quit

 if (nr4dinf.rifpp)!(nr4dinf.rifppw) do ZFRM(26,nr4dinf.rifpp,nr4dinf.rifppw)
 if (nr4dinf.rifls)!(nr4dinf.riflsw) do ZFRM(27,nr4dinf.rifls,nr4dinf.riflsw)
 if (nr4dinf.rspap)!(nr4dinf.rspapw) do ZFRM(28,nr4dinf.rspap,nr4dinf.rspapw)
 if (nr4dinf.rsprp)!(nr4dinf.rsprpw) do ZFRM(29,nr4dinf.rsprp,nr4dinf.rsprpw)
 if (nr4dinf.rspea)!(nr4dinf.rspeaw) do ZFRM(30,nr4dinf.rspea,nr4dinf.rspeaw)
 // PAR 27935 Income Code changed from 31 to 43
 if (nr4dinf.rspwp)!(nr4dinf.rspwpw) do ZFRM(43,nr4dinf.rspwp,nr4dinf.rspwpw)
 if (nr4dinf.rspdp)!(nr4dinf.rspdpw) do ZFRM(33,nr4dinf.rspdp,nr4dinf.rspdpw)
 
 quit
 
ZFRM(Number I,Number D,Number W) //

 type public Number DIST(),INCD(),SEQ,TOTDIST(),TOTWHLD(),WHLD()
 
 set SEQ=SEQ+1
 set INCD(SEQ)=I
 set DIST(SEQ)=D
 set WHLD(SEQ)=W
 set TOTDIST=TOTDIST.get()+DIST(SEQ)
 set TOTDIST(1)=TOTDIST(1).get()+DIST(SEQ)
 set TOTWHLD=TOTWHLD.get()+WHLD(SEQ)
 set TOTWHLD(1)=TOTWHLD(1).get()+WHLD(SEQ)
 
 quit
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(2)=1,vskp(3)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(3)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3
	set (vb1,vb2,vb3)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,VH0=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(3)=VT(3)+1
	quit

VDTL3	// Detail
	type public String %TIM,ACN,BOO,CCODE,DIST(),INCD(),IOSL,IRATYP,NONREP,NRMESG,PRIACN,RPASEQ,SEQ,TOTDIST,TOTDIST(),TOTWHLD,TOTWHLD(),V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),WHLD(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if VLC+5>IOSL do VHDG0 quit:VFMQ

	do VP1 quit:VFMQ!verror.get()  set V=$E(NONREP,1,1) set VL=V
	set V=vc1,VO=V set V=$J(V,11) do VP2 quit:VFMQ!verror.get()
	set VL=VL_$J("",2-VL.length())_V		// [SYSDEV,NR4DINF]ACN
	do VP3 quit:VFMQ!verror.get()  set V=$E(PRIACN,1,20) do VP4 quit:VFMQ!verror.get()
	set VL=VL_$J("",14-VL.length())_V
	set V=vc3,VO=V set V=$J(V,13) do VP5 quit:VFMQ!verror.get()
	set VL=VL_$J("",35-VL.length())_V		// [SYSDEV,NR4DINF]RPASEQ
	set VL=VL_$J("",50-VL.length())_$E(vc4,1,12)
	do VP6 quit:VFMQ!verror.get()  set V=$J(IRATYP,1)
	set VL=VL_$J("",64-VL.length())_V
	do VP7 quit:VFMQ!verror.get()  set V=$E(CCODE,1,3)
	set VL=VL_$J("",71-VL.length())_V
	set VL=VL_$J("",78-VL.length())_$J(INCD(1),2)
	set VL=VL_$J("",86-VL.length())_$J($FN(DIST(1),",",2),12)
	set VL=VL_$J("",101-VL.length())_$J($FN(WHLD(1),",",2),12)
	do VOM
	do VP8 quit:VFMQ!verror.get()  set V=$E(NRMESG,1,20) do VP9 quit:VFMQ!verror.get()  set VL="              "_V
	do VP10 quit:VFMQ!verror.get()  set V=$J(INCD(2),2)
	set VL=VL_$J("",78-VL.length())_V
	set VL=VL_$J("",86-VL.length())_$J(DIST(2),12)
	set VL=VL_$J("",101-VL.length())_$J(WHLD(2),12)
	do VOM
	set VL="                                                                              "_$J(INCD(3),2)
	set VL=VL_$J("",86-VL.length())_$J(DIST(3),12)
	set VL=VL_$J("",101-VL.length())_$J(WHLD(3),12)
	do VOM
	set VL="                                                                              "_$J(INCD(4),2)
	set VL=VL_$J("",86-VL.length())_$J(DIST(4),12)
	set VL=VL_$J("",101-VL.length())_$J(WHLD(4),12)
	do VOM
	set VL="                                                                              "_$J(INCD(5),2)
	set VL=VL_$J("",86-VL.length())_$J(DIST(5),12)
	set VL=VL_$J("",101-VL.length())_$J(WHLD(5),12)
	do VOM
	quit

VSUM3	// Summary
	type public String %TIM,ACN,BOO,CCODE,DIST(),I,INCD(),IOSL,IRATYP,NONREP,NRMESG,PRIACN,RPASEQ,SEQ,TOTDIST,TOTDIST(),TOTWHLD,TOTWHLD(),V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),WHLD(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set V=$J($FN(TOTDIST(1),",",2),12) do VP11 quit:VFMQ!verror.get()  set VL="                                                                                      "_V
	set V=$J($FN(TOTWHLD(1),",",2),12) do VP12 quit:VFMQ!verror.get()
	set VL=VL_$J("",101-VL.length())_V
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,ACN,BOO,CCODE,CONAM,DIST(),INCD(),IRATYP,NONREP,NRMESG,PRIACN,RID,RN,RPASEQ,SEQ,TOTDIST,TOTDIST(),TOTWHLD,TOTWHLD(),VL,WHLD(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+8,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="Branch "
	set VL=VL_$J("",8-VL.length())_$J(vc2,5)
	do VOM
	set VL="" do VOM
	set VL="                                                               "_"Plan  Cntry  Income    Distribution       Tax"
	do VOM
	set VL=" "_"Customer Nbr Name                      Plan Seq  Plan ID      Type   Code   Code        Income        Withheld"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,ACN,BOO,CCODE,DIST(),INCD(),IRATYP,NONREP,NRMESG,PRIACN,RPASEQ,SEQ,TOTDIST,TOTDIST(),TOTWHLD,TOTWHLD(),VPN,WHLD(),vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+4>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="" do VOM
	set VL="" do VOM
	set VL="                                                                 "_"REPORT TOTALS:"
	set VL=VL_$J("",85-VL.length())_$J($FN(TOTDIST,",",2),13)
	set VL=VL_$J("",100-VL.length())_$J($FN(TOTWHLD,",",2),13)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	use IO if 'VLC,'vcrt set $Y=1
	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: NONREP

	type public String vc5,vc6,vc7
 // If this account has non-reportable information (i.e. a foreign currency
 // code but no exchange rate.
 
 type public String NONREP
 
 set NONREP=""
 if (vc5)!(vc6) set NONREP="*"
 if vc7 set NONREP="*"
	quit

VP2	// Column post-processor - [SYSDEV,NR4DINF]ACN

	type public String vc1,vc2,vc3
 type public Number ACN,BOO,RPASEQ

 set ACN=vc1
 set BOO=vc2
 set RPASEQ=vc3
	quit

VP3	// Column pre-processor - Variable: PRIACN

	type public String vc8
 type public String PRIACN
 
 // retrieve the name of the primary ACN.
