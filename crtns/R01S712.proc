 	type RecordSTBLCNTRY cntry=Db.getRecord("STBLCNTRY","CNTRY=:CNTRY",1)
 	set CCODE=cntry.isoc
 	}
	quit

VP8	// Column pre-processor - Variable: NRMESG

	type public String vc7,vc5,vc6
 // if this PLAN contains non reportable distributions then display the
 // error message.
 
