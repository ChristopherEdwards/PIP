V00S597(%ProcessMode,RecordUTBLGLSC fUTBLGLS)   // -  - SID= <UTBLGLSCL00> G/L Set Codes User Table (Loan)
 ;;Copyright(c)2006 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/24/2006 12:32 - kini
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="UTBLGLSCL00"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+1 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+2
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="G/L Set Codes User Table (Loan)|UTBLGLSCL"
 set VPG(%PG+1)="G/L Set Codes User Table (Loan) Page 2|UTBLGLSCL1"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fUTBLGLS)
 if %ProcessMode do VLOD(.fUTBLGLS) if $G(ER) set VFMQ="Q" quit
 do VPG(.fUTBLGLS)
 quit

VNEW(RecordUTBLGLSC fUTBLGLS)
 
 quit
 //
 ;
VLOD(RecordUTBLGLSC fUTBLGLS)  // User defined access section
	quit

VPG(RecordUTBLGLSC fUTBLGLS)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fUTBLGLS),VPG0(.fUTBLGLS) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fUTBLGLS),VPG0(.fUTBLGLS) quit:vDONE
  }
 quit

VPG0(RecordUTBLGLSC fUTBLGLS)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fUTBLGLS) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordUTBLGLSC fUTBLGLS)  // G/L Set Codes User Table (Loan)
 
 new x
 set SID="UTBLGLSCL",DFID="UTBLGLSC"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S596
 if %ProcessMode>1 quit
 do VTAB^V00S596(.fUTBLGLS)
 quit
 
VPG2(RecordUTBLGLSC fUTBLGLS)  // G/L Set Codes User Table (Loan) Page 2
 
 new x
 set SID="UTBLGLSCL1",DFID="UTBLGLSC"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S599
 if %ProcessMode>1 quit
 do VTAB^V00S599(.fUTBLGLS)
 quit
 

VBTM(RecordUTBLGLSC fUTBLGLS)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
