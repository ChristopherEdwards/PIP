VFPOST	// FETCH post-processor

	type public String vc1,vc2
 /*
  ---- Revision History ------------------------------------------------
 
	05/08/06 - TITOVE - CR 21132
		   Added logic to format EIN that is now
		   a 12-character field.

        12/06/05 - TITOVE - CR 17287
                   Converted into PSL.
 */
 // I18N=OFF  Excluded from I18N Standards

 type public Boolean BDTINOPT
 type public String A(), BDTIN, OLDINST

 type Number I, J
 type String NM, R

 kill OLDINST

 for I = 1:1:9 set A(I) = ""

 // Institution number
 set A(9) = CUVAR.sbinstno

 set R = vc1.piece("_",2)

 type RecordUTBLREGION reg 
 
 if 'R.isNull() set reg = Db.getRecord("UTBLREGION", "KEY = :R", 1)

 set NM = $select('R.isNull():reg.taxid,1:CUVAR.ein)

 // Employer ID number
 if 'NM.isLike("%-%") set A(1) = NM.extract(1,2)_"-"_NM.extract(3,12)
 else  set A(1) = NM
 
 set A(1) = A(1).justify(12)

 if 'R.isNull() set NM = reg.city_"|"_reg.state_"|"_reg.mzip_"|"_reg.ad1_"|"_reg.ad2_"|"_reg.ad3
 else  set NM = CUVAR.ccity_"|"_CUVAR.cstate_"|"_CUVAR.czip_"|"_CUVAR.cad1_"|"_CUVAR.cad2_"|"_CUVAR.cad3_"|"_CUVAR.cname

 // Institution name
 set A(2) = $select('R.isNull():reg.desc,1:NM.piece("|",7))

 set J = 3
 
 // Address 1-3
 for I = 4:1:6 if 'NM.piece("|",I).isNull() set A(J) = NM.piece("|",I),J = J + 1
 
 set A(J) = NM.piece("|",1).extract(1,15)_" "_NM.piece("|",2)_" "_NM.piece("|",3)
 
 if BDTINOPT set BDTIN = "Number of Bad TINs: "_$FN(vc2,",").justify(9)
 else  set BDTIN = ""

 if 'OLDINST.exists() set OLDINST = "" quit
 
 for I = 1:1:9 set A(I) = ""

 // I18N=ON  Included in I18N Standards
	quit

