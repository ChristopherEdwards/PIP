 do VLOD(.fLNCPTS,.LN,.fAOAMO)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordLNCYCPTS fLNCPTS,RecordLN LN,RecordAOAMO fAOAMO)
