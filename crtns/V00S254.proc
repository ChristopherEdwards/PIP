 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LIAOP1",VPGM=$T(+0),VSNAME="Loan Inquiry - Net Fee Income "
 set VFSN("AOAMO")="zfAOAMO",VFSN("LN")="zLN",VFSN("LNCYCPTS")="zfLNCPTS"
 set vPSL=1
 set KEYS(1)=LN.CID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fLNCPTS,.LN,.fAOAMO),VDA1(.fLNCPTS,.LN,.fAOAMO),^DBSPNT()

 if '%ProcessMode do VNEW(.fLNCPTS,.LN,.fAOAMO),VPR(.fLNCPTS,.LN,.fAOAMO),VDA1(.fLNCPTS,.LN,.fAOAMO)
 if %ProcessMode do VLOD(.fLNCPTS,.LN,.fAOAMO) quit:$G(ER)  do VPR(.fLNCPTS,.LN,.fAOAMO),VDA1(.fLNCPTS,.LN,.fAOAMO)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fLNCPTS,.LN,.fAOAMO)
	quit


