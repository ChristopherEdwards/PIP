vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL5PR dbtbl5pr

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL5PR","LIBS = :dbtbl5pr.libs and RID = :dbtbl5pr.rid and SEQ = :dbtbl5pr.seq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(dbtbl5pr))  S ^DBTBL(vobj(dbtbl5pr,-3),5,vobj(dbtbl5pr,-4),vobj(dbtbl5pr,-5))=vobj(dbtbl5pr)
		#ENDBYPASS
	}

	quit

