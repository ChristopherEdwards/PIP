VPREAQ	// Pre-processor (after query)

 /*
  ---- Revision History ------------------------------------------------
 
        08/19/05 - TITOVE - CR 16844
                   Added to replace old record access logic.
 */
 
 /* 
    Create a single table that provides the pointers to the LN and
    DAYENDDARCB tables needed for the report.
 
    Structure of TMPRPT6 is as follows:
 
                - KEY1 = CRCD
                - KEY2 = CC
                - KEY3 = NPDT
                - KEY4 = ACN
                - KEY5 = CID
                - KEY6 = GLSC
 */
 
 do Db.delete("TMPRPT6", "PID=:%ProcessID")

 type public Date NPDT

 type Boolean DETAIL = 0
 type Number ACN, CC, CID, DARCLS
 type String CRCD, DATA, GLSC, TGLTOT(,,)

 type DbSet dards 
 
 if NPDT="ALL" set dards = Db.selectDbSet("DAYENDDARCB")
 else  set dards = Db.selectDbSet("DAYENDDARCB", "NPDT=:NPDT")
 
 while dards.next() do {
 	
	type RecordDAYENDDARCB darcb = dards.getRecord("DAYENDDARCB")

	if darcb.suspect=1 set DARCLS=darcb.darcovr
	else  set DARCLS=darcb.darcls

	if DARCLS.isNull() quit

	set NPDT=darcb.npdt

	set ACN=darcb.acn
	set CID=darcb.cid
	
	type RecordLN ln = Db.getRecord("LN", "CID=:CID", 1)
	
	if 'ln.getMode() quit	// In case account was deleted
	
	set CC = +ln.cc
	set CRCD = ln.crcd
	set GLSC = ln.glsc
 
	// Create GL array for later use by DARCB^RGLDARC
	do ACCGL^RGLDARC

	// Create TGLTOT array
	do DARCB^RGLDARC(NPDT)
 
	set CRCD="" for  set CRCD=TGLTOT(CRCD).order() quit:CRCD.isNull()  do {
	 set GLSC="" for  set GLSC=TGLTOT(CRCD,GLSC).order() quit:GLSC.isNull()  do {
	  set CC="" for  set CC=TGLTOT(CRCD,GLSC,CC).order() quit:CC.isNull()  do {

	    type RecordTMPRPT6 rpt6=Db.getRecord("TMPRPT6","PID=:%ProcessID,KEY1=:CRCD,KEY2=:CC,KEY3=:NPDT,KEY4=:ACN,KEY5=:CID,KEY6=:GLSC",1)

	    set DATA=rpt6.data
	    set DATA.piece("|",1)=DATA.piece("|",1)+TGLTOT(CRCD,GLSC,CC).piece("|",1)
	    set DATA.piece("|",2)=DATA.piece("|",2)+TGLTOT(CRCD,GLSC,CC).piece("|",2)
	    set DATA.piece("|",3)=DATA.piece("|",3)+TGLTOT(CRCD,GLSC,CC).piece("|",3)
	    set DATA.piece("|",4)=DATA.piece("|",4)+TGLTOT(CRCD,GLSC,CC).piece("|",4)
	    set rpt6.data=DATA
	
	    do rpt6.bypassSave()
	    }
	   }
	  }
 	}

	quit

