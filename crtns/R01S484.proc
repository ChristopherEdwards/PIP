	type public String %TIM,ACLS,CRAMT,DESC,DRAMT,I,IOSL,NPDT,SUSPECT,TOTAL,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate

