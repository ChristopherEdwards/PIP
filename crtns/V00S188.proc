 set VO(5)=$C(6,1,1,0,52,49,56,52,0,0)_"11Tx"
 set VO(6)=$C(6,78,1,0,52,49,56,52,0,0)_"11Tx"
 set VO(7)=$C(7,1,78,0,0,0,0,0,0,0)_"11Ttqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqu"
 set VO(8)=$C(8,1,1,0,0,0,0,0,0,0)_"11Tx"
