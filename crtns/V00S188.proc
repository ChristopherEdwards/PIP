 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab,srcphr,targphr",VSID="I18NMLD",VPGM=$T(+0),VSNAME="I18NMLD Maintenance Screen "
 set VFSN("I18NMLD")="zfMLD"
 set vPSL=1
 set KEYS(1)=fMLD.SRCPHR
 set KEYS(2)=fMLD.TARGPHR
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fMLD),VDA1(.fMLD),^DBSPNT()

 set ER=0 do VSCRPRE(.fMLD) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fMLD),VPR(.fMLD),VDA1(.fMLD)
 if %ProcessMode do VLOD(.fMLD) quit:$G(ER)  do VPR(.fMLD),VDA1(.fMLD)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fMLD)
	quit


VNEW(RecordI18NMLD fMLD) // Initialize arrays if %O=0
 
 do VDEF(.fMLD)
 do VLOD(.fMLD)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordI18NMLD fMLD)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordI18NMLD fMLD) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

