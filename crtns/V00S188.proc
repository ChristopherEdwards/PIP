VSCRPRE(RecordI18NMLD fMLD)  // Screen Pre-Processor
 new %TAB,vtab // Disable .MACRO. references to %TAB()
 //
 type public Number %ODELETE
 type public String VSNAME
 
 // Defaulting in correct screen headings
 if %ProcessMode=0 set VSNAME=$$^MSG(6073)
 if %ProcessMode=1 set VSNAME=$$^MSG(6074)
 if %ODELETE.get() if %ProcessMode=1 set VSNAME=$$^MSG(6075)
 if %ProcessMode=0 do {
	set fMLD.transuid=%UserID
	set fMLD.transdt=%CurrentDate
	}
 #ACCEPT date=11/05/03;PGM=Screen Compiler
 quit
