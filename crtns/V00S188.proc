	quit
vREAD(fid,di)
 type Public RecordI18NMLD fMLD
 if fid="I18NMLD" quit $$vREAD1(.fMLD,di)
 quit ""
vREAD1(RecordI18NMLD fMLD,di)
 quit fMLD.@di
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
