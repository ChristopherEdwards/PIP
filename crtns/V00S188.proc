 set VO(10)=$C(8,78,1,0,0,0,0,0,0,0)_"11Tx"
 set VO(11)=$C(9,1,1,0,52,49,56,52,0,0)_"11Tx"
 set VO(12)=$C(9,78,1,0,52,49,56,52,0,0)_"11Tx"
 set VO(13)=$C(10,1,1,0,0,0,0,0,0,0)_"11Tx"
 set VO(14)=$C(10,3,17,0,0,0,0,0,0,0)_"01TPreferred Phrase:"
 set VO(15)=$C(10,78,1,0,0,0,0,0,0,0)_"11Tx"
 set VO(16)=$C(11,1,78,0,0,0,0,0,0,0)_"11Ttqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqqu"
 set VO(17)=$C(12,1,1,0,0,0,0,0,0,0)_"11Tx"
