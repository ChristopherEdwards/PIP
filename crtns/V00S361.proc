VPG3(RecordSWIFT fSWIFT)  // Request to Cancel 100 (Page 2)
 
 new x
 set SID="MT192PG2",DFID="SWIFT"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S369
 if %ProcessMode>1 quit
 do VTAB^V00S369(.fSWIFT)
