	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA646"
	set RN="CD Yearend Regular 1099 by Customer"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,LNAM,RID,RN,VL,VLOF,VRF(),VSTATS(),ZRID,ZZRID,vCOL,vHDG,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vp1,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA646",RN="CD Yearend Regular 1099 by Customer"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA646","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[DEP]CC#43#6,[DEP]BOO#50#6,[DEP]TYPE#57#4,[DEP]CID#63#12,[DEP]IPTY#77#18,[DEP]PENPTY#100#18,[DEP]STAT#125#1\[DEP]BWPTY#100#18"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE=""
		set VWHERE=VWHERE_"DEP.GRP='CD' AND (DEP.STAT=0 OR DEP.STAT=4) AND (("
		set VWHERE=VWHERE_"(DEP.IPTY-(DEP.NRPOSIPTY-DEP.NRNEGIPTY)>0) OR (DEP"
		set VWHERE=VWHERE_".BWPTY-DEP.NRWHPTY)>0 OR (DEP.PENPTY-DEP.NRPPTY)>0"
		set VWHERE=VWHERE_"))"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="CD Yearend Regular 1099 by Customer"
		set VBNRINFO("PGM")="R01S567"
		set VBNRINFO("RID")="SCA646"
		set VBNRINFO("TABLES")="DEP,CIF"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,CIF]TAXID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,CIF]LNM"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DEP]CC"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,DEP]BOO"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,DEP]TYPE"
		set VBNRINFO("ORDERBY",6)="[SYSDEV,DEP]CID"

		set VBNRINFO("DOC",1)=" CD Yearend Regular 1099 Report by Customer within Bank (SCA646)."
		set VBNRINFO("DOC",2)=" The selection criteria for this report will consider all certificate of deposit"
		set VBNRINFO("DOC",3)=" accounts with an Account Status equal to ""0"" or ""4"" (Active/Closed) and where"
		set VBNRINFO("DOC",4)=" one or more of the following is greater than .00: Interest Paid Prior Year less"
		set VBNRINFO("DOC",5)=" Non Resident Interest Paid Prior Year, Backup Withholding Prior Year less Non"
		set VBNRINFO("DOC",6)=" Resident Interest Withheld Prior Year, Penalty Prior Year less Non Resident"
		set VBNRINFO("DOC",7)=" Penalty Prior Year. "
		set VBNRINFO("DOC",8)=" This report is broken down by Tax ID Number, Customer Name,"
		set VBNRINFO("DOC",9)=" Bank, Branch, Product Type, Account Number."
		set VBNRINFO("DOC",10)=" This report calls subreport SCA646A."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vp1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:6 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("CIF.TAXID,CIF.LNM,DEP.CC,DEP.BOO,DEP.TYPE,DEP.CID,DEP.IPTY,DEP.PENPTY,DEP.STAT,DEP.BWPTY","DEP,CIF","DEP.GRP='CD' AND (DEP.STAT=0 OR DEP.STAT=4) AND (((DEP.IPTY-(DEP.NRPOSIPTY-DEP.NRNEGIPTY)>0) OR (DEP.BWPTY-DEP.NRWHPTY)>0 OR (DEP.PENPTY-DEP.NRPPTY)>0))","CIF.TAXID,CIF.LNM,DEP.CC,DEP.BOO,DEP.TYPE,DEP.CID","","DQMODE=1/PROTECTION=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=rwrs.getRowProt()		// Data item protection
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4,vovc5,vovc6
	set vovc1="",vovc2="",vovc3="",vovc4="",vovc5="",vovc6=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4,vovc5,vc5,vovc6,vc6
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4,vovc5=vc5,vovc6=vc6
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vp1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10
	set vc1=V.piece($C(9),1)			// CIF.TAXID
	set vp1=VI.extract(1)				// Protection indicator
	set vc2=V.piece($C(9),2)			// CIF.LNM
	set vc3=V.piece($C(9),3)			// DEP.CC
	set vc4=V.piece($C(9),4)			// DEP.BOO
	set vc5=V.piece($C(9),5)			// DEP.TYPE
	set vc6=V.piece($C(9),6)			// DEP.CID
	set vc7=V.piece($C(9),7)			// DEP.IPTY
	set vc8=V.piece($C(9),8)			// DEP.PENPTY
	set vc9=V.piece($C(9),9)			// DEP.STAT
	set vc10=V.piece($C(9),10)			// DEP.BWPTY
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 // Clean up the TMP files
 do Db.delete("CD1099","PID=:%ProcessID")
	quit

VRPOST	// Report post-processor

 type String %NOOPEN,%NOCLOSE,%NOBANNER,PGM
 type public String LNAM,ZZRID,RID
 type public Number vcrt
 type public Boolean VFMQ
 type Number PN
 
 set ZZRID=RID
 set LNAM="Certificate Of Deposit Yearend Regular 1099 Report by Customer Within Bank"
 if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
 set (%NOOPEN,%NOCLOSE,%NOBANNER)=""
 set RID="SCA646A"  do ^URID
 if 'PGM.length() do Runtime.setErrMSG("",8074,"RID") Quit 
 do V0^@PGM

 // Clean up the TMP files
 do Db.fastDelete("CD1099","PID=:%ProcessID")
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0,vs(5)=0,vs(6)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1,vskp(5)=1,vskp(6)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG2 quit:VFMQ
	if 'vskp(6).get() do VDTL6 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(6)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4,vc5,vovc5,vc6,vovc6
	type Number vb1,vb2,vb3,vb4,vb5,vb6
	set (vb1,vb2,vb3,vb4,vb5,vb6)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,vb5=1,vb6=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,vb5=1,vb6=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,vb5=1,vb6=1
	if vb4!(+vovc4'=+vc4) set vs(5)=0,vh(5)=0,VD(4)=0,vb5=1,vb6=1
	if vb5!(+vovc5'=+vc5) set vs(6)=0,vh(6)=0,VD(5)=0,vb6=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(6) set vs(6)=1 do stat^DBSRWUTL(6)
	if 'vs(5) set vs(5)=1 do stat^DBSRWUTL(5)
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(6)=VT(6)+1
	quit

VHDG2	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,LNAM,RID,RN,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,ZRID,ZZRID,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vp1,vrundate,vsysdate

	quit:vh(2)  set vh(2)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$S(vp1=3:"********************",1:$E(vc1,1,20))
	set VL=VL_$J("",21-VL.length())_$E(vc2,1,20)
	do VOM
	quit

VDTL6	// Detail
	type public String %TIM,IOSL,LNAM,RID,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),ZRID,ZZRID,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vp1,vrundate,vsysdate

	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                          "_$J(vc3,6)
	set VL=VL_$J("",49-VL.length())_$J(vc4,6)
	set V=vc5,VO=V do VP1 quit:VFMQ!verror.get()  set V=$J(V,4)
	set VL=VL_$J("",56-VL.length())_V		// [SYSDEV,DEP]TYPE
	set VL=VL_$J("",62-VL.length())_$J(vc6,12)
	set VL=VL_$J("",76-VL.length())_$J($FN(vc7,",",2),18)
	set VL=VL_$J("",99-VL.length())_$J($FN(vc8,",",2),18)
	set V=vc9,VO=V do VP2 quit:VFMQ!verror.get()  set V=$J(V,1)
	set VL=VL_$J("",124-VL.length())_V		// [SYSDEV,DEP]STAT
	do VOM
	set VL="                                                                                                   "_$J($FN(vc10,",",2),18)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,LNAM,RID,RN,VL,ZRID,ZZRID,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vp1,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+6,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL="Cert of Deposit Year-End Regular 1099 Report by Customer Within Cost Ctr"
	do VP3 quit:VFMQ!verror.get()  set V=$E(ZRID,1,14)
	set VL=VL_$J("",76-VL.length())_V
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set V=$J(VPN,3) do VP4 quit:VFMQ!verror.get()
	set VL=VL_$J("",128-VL.length())_V
	do VOM
	set VL="" do VOM
	set VL="Tax ID Number        Customer Name        Cost   Branch Type    Account Number   Interest Paid     Interest Penalty         Account"
	do VOM
	set VL="                                          "_"Ctr                                                      Backup Withholding       Status"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - [SYSDEV,DEP]TYPE

	type public String vc5
 type public Number TYPE
 set TYPE=vc5
	quit

VP2	// Column pre-processor - [SYSDEV,DEP]STAT

	type public String vc7,vc8,vc10
 type public String TYPE
 type RecordCD1099 cd1099=Db.getRecord("CD1099","PID=:%ProcessID,TYPE=:TYPE",1)
 
 set cd1099.ipy=cd1099.ipy+vc7
 set cd1099.ppy=cd1099.ppy+vc8
 set cd1099.bwp=cd1099.bwp+vc10
 set cd1099.cnt=cd1099.cnt+1

 do cd1099.save()

 
	quit

VP3	// Column pre-processor - Variable: ZRID

 type public String ZRID,RID
 
 set ZRID="("_RID_")"
	quit

VP4	// Column post-processor - Variable: VPN

 type public Number OLDPAGE,VPN

 set OLDPAGE=VPN
	quit
