  //user-defined post procs
 //
VP1(RecordLNCYCPTS fLNCPTS,RecordLN LN,RecordAOAMO fAOAMO) //
 /*
  ---- Revision History ------------------------------------------------
 
        03/25/06 - TITOVE - CR 20320
                   Converted into PSL.
 
 */
 type public String X

 // Int/Div method may not be '3'
 if 'fAOAMO.FMET,(X = 3) do Runtime.setErrMSG("AOAMO",1253) quit

 // Straight line amortization requires method '3'
 if fAOAMO.FMET,(X '= 3) do Runtime.setErrMSG("AOAMO",2559) quit

 quit
VP2(RecordLNCYCPTS fLNCPTS,RecordLN LN,RecordAOAMO fAOAMO) //
 /*
  ---- Revision History ------------------------------------------------
 
        03/25/06 - TITOVE - CR 20320
                   Converted into PSL.
 
 */

 if fAOAMO.FMET do GOTO^DBSMACRO("END")

 quit
VP3(RecordLNCYCPTS fLNCPTS,RecordLN LN,RecordAOAMO fAOAMO) //
 /*
  ---- Revision History ------------------------------------------------
 
        03/25/06 - TITOVE - CR 20320
                   Converted into PSL.
 
 */
 type public Number ER
 type public String X

 type Date JD, NJD
 type String FRE

 if X.isNull() quit

 do EDT^UFRE quit:ER

 // Recalculation requires ledger balance to be used
 if 'fAOAMO.SPP do Runtime.setErrSTBLER("AOAMO","NFRSPP") quit
 
 // Plan may not have a recalculation frequency and recalculation yield flag set
 if fAOAMO.RYPC do Runtime.setErrSTBLER("AOAMO","NFRPCF") quit
 
 // May not have a recalculation frequency with method 1
 if fAOAMO.FMET do Runtime.setErrSTBLER("AOAMO","NFRFMET") quit
 
 if 'fLNCPTS.RYND.isNull() quit
 
 set JD = fLNCPTS.NXADT

 for  set NJD = JD.nextFreqDate(X) quit:ER!(NJD > %SystemDate)  set JD = NJD
 
 if ER quit

 do DEFAULT^DBSMACRO("LNCYCPTS.RYND",NJD)

 quit
VP4(RecordLNCYCPTS fLNCPTS,RecordLN LN,RecordAOAMO fAOAMO) //
 /*
  ---- Revision History ------------------------------------------------
 
        03/25/06 - TITOVE - CR 20320
                   Converted into PSL.
 
 */
 type public Number E5

 if 'fLNCPTS.RYFRE.isNull() set E5 = 1

 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


