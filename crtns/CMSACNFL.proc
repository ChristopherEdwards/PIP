	set v2=$$GFCTYP^MPSRECU(cmsrecacn.acn)
	set v3=$$GETCO1^MPSRECU(cmsrecacn.acn,%CompanyName)
	set v4=$$GFCNUM^MPSRECU(cmsrecacn.acn)
