 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fZUTBLPA.exists() set fZUTBLPA = Class.new("RecordZUTBLPAUTH")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="ZUTBLPAUTH",VPGM=$T(+0),VSNAME="PROSA Approval/Decline "
 set VFSN("ZUTBLPAUTH")="zfZUTBLPA"
 set vPSL=1
 set KEYS(1)=fZUTBLPA.FILTER
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fZUTBLPA),VDA1(.fZUTBLPA),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fZUTBLPA),VPR(.fZUTBLPA),VDA1(.fZUTBLPA)
 if %ProcessMode do VLOD(.fZUTBLPA) quit:$G(ER)  do VPR(.fZUTBLPA),VDA1(.fZUTBLPA)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fZUTBLPA)
	quit


VNEW(RecordZUTBLPAUTH fZUTBLPA) // Initialize arrays if %O=0
 
 do VDEF(.fZUTBLPA)
 do VLOD(.fZUTBLPA)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordZUTBLPAUTH fZUTBLPA)
 quit:fZUTBLPA.FILTER.isNull() 
 quit:%ProcessMode  set ER=0 if (fZUTBLPA.FILTER="") set ER=1,RM=$$^MSG(1767,"FILTER") Q
 if Db.isDefined("ZUTBLPAUTH","ZUTBLPAUTH.FILTER=:fZUTBLPA.FILTER") S ER=1,RM=$$^MSG(2327) Q
 do fZUTBLPA.setAuditFlag(1)
 if fZUTBLPA.APPROVE="" set fZUTBLPA.APPROVE=0
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordZUTBLPAUTH fZUTBLPA) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

