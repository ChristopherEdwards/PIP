  //user-defined post procs
 //
VP1(RecordZUTBLPAUTH fZUTBLPA) //
 set FILES="ZFILTER"
 set ZX=X N X
 set X=ZX
 set (cnt,CNT)=""
 do ^DBSQRY
 if ER quit
 for  set cnt=$O(Q(cnt)) quit:(cnt="")  do {  quit:ER
 	set seq=""
 	for  set seq=$O(Q(cnt,seq)) quit:(seq="")  do {  quit:ER
 		set P1=$P($P(Q(cnt,seq),"|",1),$C(1),2)
 		set elem="SYSDEV."_FILES_"."
 		set DI=$P(P1,elem,2)
 		if DI="" set ER=1
	}
 }
 if ER kill RM do Runtime.setErrMSG("ZFILTER",1298,$P(Q(cnt,seq),"|",1))
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit								// User defined post processor's


