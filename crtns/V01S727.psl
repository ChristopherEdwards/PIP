 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=4,VPT=1,VPB=21,PGM=$T(+0),DLIB="SYSDEV",DFID="ZUTBLPAUTH"
 set OLNTB=21019
 
 set VFSN("ZUTBLPAUTH")="zfZUTBLPA"
 //
 // 
	
	
 set %TAB(1)=$C(0,27,12)_"21T12401|1|[ZUTBLPAUTH]FILTER"
 set %TAB(2)=$C(1,27,40)_"00T12401|1|[ZUTBLPAUTH]FMDESC"
 set %TAB(3)=$C(2,27,1)_"00L12402|1|[ZUTBLPAUTH]APPROVE"
 set %TAB(4)=$C(3,12,60)_"00T12403|1|[ZUTBLPAUTH]TEST|||do VP1^V01S727(.fZUTBLPA)||||||300"
 do VTBL(.fZUTBLPA)
