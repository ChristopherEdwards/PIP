 quit
 
VPG3(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Incoming Foreign Payment (Page 2)
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="INTRECHQ20",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S315
