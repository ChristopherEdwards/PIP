LETMDT	  /*
ORIG: burnsm - 06/19/2000
DESC: Account Maturity Letters

---- Comments --------------------------------------------------------

---- Revision History ------------------------------------------------
	
	01/30/06 Radhika - CR 17423
		 .Eliminated PSL warnings.
		 
 */
	//Variable Definition
	type public String %FN
	type Date FORMTJD
	type Number DAY
	type String  SAVFN,XFORM,XTJD(),zz
	
	set XFORM="MATURITY"
	set SAVFN=%FN
	type ResultSet rs1=Db.select("TJDVAL","UTBLFORMVAR","FORM=:XFORM")
        if rs1.isEmpty() quit
        while rs1.next() do {
        	set XTJD=rs1.getCol("TJDVAL")
		if XTJD.isNull() quit
	        set XTJD(XTJD)=""
        	} //end of while

	set zz=""
	for  set zz=XTJD(zz).order() quit:zz.isNull()  do {
                set DAY=zz.piece("T",2)
                set FORMTJD=%SystemDate+DAY
		set %FN=SAVFN_"_"_zz
		do DRV^UBCHID("BCHLETMDT")
                } //end of for
        
        quit
 #OPTION ResultClass ON
Public String vSIG()	quit "60295^30992^Radhika Panchanathan^889"	// Signature - LTD^TIME^USER^SIZE
