 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCNTRY",VPGM=$T(+0),VSNAME="Country Limits"
 set VFSN("UTBLCNTRYLIM")="zfUTBLCNT"
 set vPSL=1
 set KEYS(1)=fUTBLCNT.CNTRY
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLCNT),VDA1(.fUTBLCNT),^DBSPNT()

 set ER=0 do VSCRPRE(.fUTBLCNT) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fUTBLCNT),VPR(.fUTBLCNT),VDA1(.fUTBLCNT)
 if %ProcessMode do VLOD(.fUTBLCNT) quit:$G(ER)  do VPR(.fUTBLCNT),VDA1(.fUTBLCNT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLCNT)
	quit


VNEW(RecordUTBLCNTRYLIM fUTBLCNT) // Initialize arrays if %O=0
 
 do VDEF(.fUTBLCNT)
 do VLOD(.fUTBLCNT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordUTBLCNTRYLIM fUTBLCNT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordUTBLCNTRYLIM fUTBLCNT) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

VPR(RecordUTBLCNTRYLIM fUTBLCNT) // Display screen prompts
 set VO="7||13|0"
 set VO(0)="|0"
 set VO(1)=$C(3,27,14,1,0,0,0,0,0,0)_"01TCountry Limits"
 set VO(2)=$C(8,17,14,1,0,0,0,0,0,0)_"01T Country Code:"
 set VO(3)=$C(10,18,13,1,0,0,0,0,0,0)_"01T Description:"
 set VO(4)=$C(12,17,14,1,0,0,0,0,0,0)_"01T Limit Amount:"
 set VO(5)=$C(14,11,20,0,0,0,0,0,0,0)_"01TCountry Review Date:"
 set VO(6)=$C(16,6,25,1,0,0,0,0,0,0)_"01T Government Limit Amount:"
 set VO(7)=$C(18,7,24,0,0,0,0,0,0,0)_"01TGovernment Review Date :"
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


VDA1(RecordUTBLCNTRYLIM fUTBLCNT)  // Display screen data
 new V
 s STBLDESC=$G(STBLDESC)
 //
 set VO="13|8|13|0"
 set VO(8)=$C(8,32,2,2,0,0,0,0,0,0)_"01T"_$E(fUTBLCNT.CNTRY,1,2)
 set VO(9)=$C(10,32,49,2,0,0,0,0,0,0)_"01T"_$G(STBLDESC)
 set V=$S(fUTBLCNT.AMT="":"",1:$J(fUTBLCNT.AMT,0,2)) set VO(10)=$C(12,32,15,2,0,0,0,0,0,0)_"00$"_$S(fUTBLCNT.AMT="":"",1:$J(fUTBLCNT.AMT,0,2))
 set VO(11)=$C(14,32,10,2,0,0,0,0,0,0)_"00D"_$$DAT^%ZM(fUTBLCNT.CNTRYDATE)
 set V=$S(fUTBLCNT.GOVTAMT="":"",1:$J(fUTBLCNT.GOVTAMT,0,2)) set VO(12)=$C(16,32,15,2,0,0,0,0,0,0)_"00$"_$S(fUTBLCNT.GOVTAMT="":"",1:$J(fUTBLCNT.GOVTAMT,0,2))
 set VO(13)=$C(18,32,10,2,0,0,0,0,0,0)_"00D"_$$DAT^%ZM(fUTBLCNT.GOVTDATE)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
	
VTAB(RecordUTBLCNTRYLIM fUTBLCNT)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=6,VPT=3,VPB=18,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLCNTRYLIM"
 set OLNTB=18032
 
 set VFSN("UTBLCNTRYLIM")="zfUTBLCNT"
 //
 // 
	
	
 set %TAB(1)=$C(7,31,2)_"21T12401|1|[UTBLCNTRYLIM]CNTRY|[STBLCNTRY]"
 set %TAB(2)=$C(9,31,49)_"21T|*STBLDESC|[*]@STBLDESC|||||||||60"
 set %TAB(3)=$C(11,31,15)_"01$12401|1|[UTBLCNTRYLIM]AMT|||||||2"
 set %TAB(4)=$C(13,31,10)_"00D12402|1|[UTBLCNTRYLIM]CNTRYDATE"
 set %TAB(5)=$C(15,31,15)_"00$12403|1|[UTBLCNTRYLIM]GOVTAMT|||||||2"
 set %TAB(6)=$C(17,31,10)_"00D12404|1|[UTBLCNTRYLIM]GOVTDATE"
 do VTBL(.fUTBLCNT)
	do ^DBSCRT8 	// data entry
	quit


VREQ   // Create REQ() array
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


