 set VO(0)="|0"
 set VO(1)=$C(2,23,33,0,0,0,0,0,0,0)_"01TCard Transaction Parameter Screen"
 set VO(2)=$C(5,3,34,1,0,0,0,0,0,0)_"01T Merchant Discount Income Account:"
 set VO(3)=$C(6,11,17,1,0,0,0,0,0,0)_"01T General Suspense"
 set VO(4)=$C(6,28,9,1,0,0,0,0,0,0)_"01T Account:"
 set VO(5)=$C(8,3,34,1,0,0,0,0,0,0)_"01T Merchant Credit Transaction Code:"
 set VO(6)=$C(9,4,33,1,0,0,0,0,0,0)_"01T Merchant Debit Transaction Code:"
 set VO(7)=$C(10,1,36,1,0,0,0,0,0,0)_"01T Merchant Discount Transaction Code:"
 set VO(8)=$C(11,1,9,1,0,0,0,0,0,0)_"01T Discount"
