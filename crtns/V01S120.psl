 set VO(10)=$C(12,3,34,1,0,0,0,0,0,0)_"01T Rounding Credit Transaction Code:"
 set VO(11)=$C(13,4,33,1,0,0,0,0,0,0)_"01T Rounding Debit Transaction Code:"
