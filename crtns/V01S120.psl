VDA
 type Public RecordCMSOPT fCMSOPT
 do VDA1(.fCMSOPT)
 quit

	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
 
vSET(sn,di,X)
 type Public RecordCMSOPT fCMSOPT
 if sn="CMSOPT" do vSET1(.fCMSOPT,di,X)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
vSET1(RecordCMSOPT fCMSOPT,di,X)
