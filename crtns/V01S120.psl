	09/24/03 - Pete Chenard - 45497
		       Created screen template for compiler.
----------------------------------------------------------------------

 */

 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fCMSOPT.exists() set fCMSOPT = Class.new("RecordCMSOPT")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="CMSOPT",VPGM=$T(+0),VSNAME="Card Transaction Parameter Screen"
 set VFSN("CMSOPT")="zfCMSOPT"
 set vPSL=1
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fCMSOPT),VDA1(.fCMSOPT),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fCMSOPT),VPR(.fCMSOPT),VDA1(.fCMSOPT)
 if %ProcessMode do VLOD(.fCMSOPT) quit:$G(ER)  do VPR(.fCMSOPT),VDA1(.fCMSOPT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fCMSOPT)
	quit


VNEW(RecordCMSOPT fCMSOPT) // Initialize arrays if %O=0
 
 do VDEF(.fCMSOPT)
 do VLOD(.fCMSOPT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordCMSOPT fCMSOPT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordCMSOPT fCMSOPT) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

