VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,CID,VPN,vc3,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="        "_"Report Totals:"
	set V=0						// @TOT([LNFEE]FEEREM,0)
	for I=0:1:5 set V=V+VT(I,4,1)
	set VL=VL_$J("",24-VL.length())_$J($FN(V,",",2),12)
	set V=0						// @TOT([LNFEE]DEFTOT,0)
	for I=0:1:5 set V=V+VT(I,3,1)
	set VL=VL_$J("",38-VL.length())_$J($FN(V,",",2),12)
	set V=0						// @TOT([LNFEE]CNAMT,0)
	for I=0:1:5 set V=V+VT(I,2,1)
	set VL=VL_$J("",52-VL.length())_$J($FN(V,",",2),12)
	set V=0						// @TOT([LNFEE]ASYTD,0)
	for I=0:1:5 set V=V+VT(I,1,1)
	set VL=VL_$J("",66-VL.length())_$J($FN(V,",",2),12)
	set V=0						// @TOT([LNFEE]FEEYTD,0)
	for I=0:1:5 set V=V+VT(I,5,1)
	set VL=VL_$J("",80-VL.length())_$J($FN(V,",",2),12)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,LNFEEP]CFWA

	// Converted to PSL by CNV70RW
	type Public String DTL,V,V0,VL

	if 'DTL set (V,VL,V0)=""
	quit

VP2	// Column post-processor - [SYSDEV,LNFEE]CID

	type Public String DTL,V

	if DTL set V=""
	quit
