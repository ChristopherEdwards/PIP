	do VOM
	set VL=" "_"Loan Type:"
	set VL=VL_$J("",12-VL.length())_$J(vc2,4)
	set VL=VL_$J("",18-VL.length())_$E(vc5,1,40)
	do VOM
	set VL="" do VOM
	set VL="   "_"Account #  Fee         Receivable      Deferred    Computed                                ---------Assessment----------   Coll @"
