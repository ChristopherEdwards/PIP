	do vkey(1) set voldkey = dayenddarcb.npdt_","_dayenddarcb.acn_","_dayenddarcb.cid // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
