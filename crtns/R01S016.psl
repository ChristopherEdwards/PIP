R01S016	// ZAMLTRN - Transaction Summary Report for Experian
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 15:06 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type String ZCRDR,ZDC,ZDET,ZTRN,ZVR2
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"

	set RID="ZAMLTRN"
	set RN="Transaction Summary Report for Experian"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||ZAMLTRN.ZDTJ|ZAMLTRN.ZDTJ:DISTINCT:NOVAL||D EXT^DBSQRY||T|System Date|||||"

	set %READ="IO/REQ,VIN1#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String ZCRDR,ZDC,ZDET,ZTRN,ZVR2
	type public String VIN1
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,DES,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="ZAMLTRN",RN="Transaction Summary Report for Experian"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","ZAMLTRN","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()


	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="ZAMLTRN.ZDTJ "_VIN1,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("ZAMLTRN,TRN") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"System Date"_"|VIN1|"_VIN1.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Transaction Summary Report for Experian"
		set VBNRINFO("PGM")="R01S016"
		set VBNRINFO("RID")="ZAMLTRN"
		set VBNRINFO("TABLES")="ZAMLTRN,TRN"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,ZAMLTRN]ZDTJ"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,ZAMLTRN]CRDR"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,ZAMLTRN]ETC"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,ZAMLTRN]JOBNUM"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:2 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"ZAMLTRN.ZDTJ,ZAMLTRN.CRDR,ZAMLTRN.ETC,ZAMLTRN.JOBN"
	set VSELECT=VSELECT_"UM,ZAMLTRN.COUNT,ZAMLTRN.AMOUNT"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"ZAMLTRN,TRN",VWHERE,"ZAMLTRN.ZDTJ,ZAMLTRN.CRDR,ZAMLTRN.ETC,ZAMLTRN.JOBNUM","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6
	set vc1=V.piece($C(9),1)			// ZAMLTRN.ZDTJ
	set vc2=V.piece($C(9),2)			// ZAMLTRN.CRDR
	set vc3=V.piece($C(9),3)			// ZAMLTRN.ETC
	set vc4=V.piece($C(9),4)			// ZAMLTRN.JOBNUM
	set vc5=V.piece($C(9),5)			// ZAMLTRN.COUNT
	set vc6=V.piece($C(9),6)			// ZAMLTRN.AMOUNT
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 type public String ZDET,ZTRN,ZVR2,ZCRDR,ZDC
 S (ZCRDR,ZDC,ZDET,ZTRN,ZVR2)=""
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do VSUM4 quit:VFMQ  do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc6,vc5
	type Public String VSTATS

	set VT(4)=VT(4)+1
	set VT(4,1,1)=VT(4,1,1)+vc6			// @TOT([ZAMLTRN]AMOUNT)
	set VT(4,2,1)=VT(4,2,1)+vc5			// @TOT([ZAMLTRN]COUNT)
	quit

VSUM3	// Summary
	type public String %TIM,DES,I,IOSL,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),ZCRDR,ZDC,ZDET,ZTRN,ZVR2,vc1,vc2,vc3,vc4,vc5,vc6,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="================================================================================================"
	do VOM
	do VP1 quit:VFMQ!verror.get()  set V=$E(ZVR2,1,20) set VL="    "_V
	set V=VT(3,2,1)					// @TOT([ZAMLTRN]COUNT,)
	set VL=VL_$J("",65-VL.length())_$J(V,12)
	set V=VT(3,1,1)					// @TOT([ZAMLTRN]AMOUNT,)
	set VL=VL_$J("",81-VL.length())_$J($FN(V,",",2),12)
	do VOM
	set VL="================================================================================================"
	do VOM
	quit

VDTL4	// Detail
	type public String %TIM,DES,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),ZCRDR,ZDC,ZDET,ZTRN,ZVR2,vc1,vc2,vc3,vc4,vc5,vc6,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	do VP2 quit:VFMQ!verror.get()  set V=$E(ZDET,1,20) do VP3 quit:VFMQ!verror.get()  set VL="  "_V
	if 'VL.translate(" ").isNull() do VOM
	quit

VSUM4	// Summary
	type public String %TIM,DES,I,IOSL,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),ZCRDR,ZDC,ZDET,ZTRN,ZVR2,vc1,vc2,vc3,vc4,vc5,vc6,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(4) quit
	if VLC+0>IOSL do VHDG0 quit:VFMQ

	set VL="    "_$E(ZTRN,1,12)
	set VL=VL_$J("",28-VL.length())_$E(DES,1,35)
	set V=VT(4,2,1)					// @TOT([ZAMLTRN]COUNT,)
	set VL=VL_$J("",65-VL.length())_$J(V,12)
	set V=VT(4,1,1)					// @TOT([ZAMLTRN]AMOUNT,)
	set VL=VL_$J("",81-VL.length())_$J($FN(V,",",2),12)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,DES,RID,RN,VL,ZCRDR,ZDC,ZDET,ZTRN,ZVR2,vc1,vc2,vc3,vc4,vc5,vc6,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+11,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=" "_$E(RN,1,40)
	set VL=VL_$J("",63-VL.length())_"System Date:"
	set VL=VL_$J("",76-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",88-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=" "_$E(RID,1,12)
	set VL=VL_$J("",66-VL.length())_"Run Date: "
	set VL=VL_$J("",76-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",88-VL.length())_"Page: "
	set VL=VL_$J("",94-VL.length())_$J(VPN,2)
	do VOM
	set VL="" do VOM
	set VL="" do VOM
	set VL="" do VOM
	do VP4 quit:VFMQ!verror.get()  set V=$E(ZDC,1,20) set VL=" "_V
	set VL=VL_$J("",69-VL.length())_"Processing Date:"
	set VL=VL_$J("",86-VL.length())_$J($$DAT^%ZM(vc1),10)
	do VOM
	set VL="" do VOM
	set VL="================================================================================================"
	do VOM
	set VL="Transaction Code            Transaction Description                      Count         Amount"
	do VOM
	set VL="================================================================================================"
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: ZVR2

 I ZCRDR=1 S ZVR2="Total Credit"
 E  S ZVR2="Total Debit"
	quit

VP2	// Column pre-processor - Variable: ZDET

 S ZDET=""
	quit

VP3	// Column post-processor - Variable: ZDET

	type public String vc3,vc2
 type public RecordTRN trn
 S ZTRN=vc3
 S ZCRDR=vc2
 S trn=Db.getRecord("TRN","ETC=:ZTRN")
 S DES=trn.des
	quit

VP4	// Column pre-processor - Variable: ZDC

	type public String vc2
 S ZCRDR=vc2
 I ZCRDR=1 S ZDC="Credit Transactions"
 E  S ZDC="Debit Transactions"
	quit
