VDEPRE(RecordAOAMO fAOAMO)  // Data Entry Pre-processor
 

 if '%ProcessMode,fAOAMO.paidby="" do DEFAULT^DBSMACRO("AOAMO.PAIDBY",0,"1","0","0")
 if '%ProcessMode quit

 // Protect APR and PAIDBY items in modify mode
 do PROTECT^DBSMACRO("AOAMO.APR")

 do PROTECT^DBSMACRO("AOAMO.PAIDBY")

 quit

	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


  //user-defined post procs
 //
VP1(RecordAOAMO fAOAMO) //
 type public String X
 type public Number %EXT

 if 'X.length() quit

 set %EXT=1

 do CUS^UACN1

 quit
VP2(RecordAOAMO fAOAMO) //
 type public String X
	
 if +X=1 do {

 	do PROTECT^DBSMACRO("AOAMO.MET")

 	do UNPROT^DBSMACRO("AOAMO.DCP")

 	}

 if +X=0 do {

 	do PROTECT^DBSMACRO("AOAMO.DCP")

 	do UNPROT^DBSMACRO("AOAMO.MET")
 	}
	
 quit
VP3(RecordAOAMO fAOAMO) //

 if 'fAOAMO.rcm do CHANGE^DBSMACRO("REQ")

 quit

VP4(RecordAOAMO fAOAMO) //
 if fAOAMO.rcm do CHANGE^DBSMACRO("REQ")

 quit
 
VP5(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public Number %EXT
 type public String ET,X,ZCLS
 
 if X.isNull() quit

 if 'fAOAMO.gld set %EXT=1,ZCLS="D" do CID^UACN1 quit

 if 'Db.isDefined("GLAD","ACN=:X") set ER=1,ET="INVLDACN" do ^UTLERR

 quit
VP6(RecordAOAMO fAOAMO) //

 if 'fAOAMO.gld do CHANGE^DBSMACRO("TBL","")

 if fAOAMO.gld do CHANGE^DBSMACRO("TBL","GLAD")

 quit

VP7(RecordAOAMO fAOAMO) //
 
 type public String QRY

 type String CLS,CID,GRP,quot

 set quot=$CHAR(34)

 if fAOAMO.gld set QRY="[TRN]CLS="_"""M"""

 // I18N=OFF
 if  set QRY=QRY_" & [TRN]DC=0"

 // I18N=ON
 if  do CHANGE^DBSMACRO("TBL","[TRN]:QU QRY") quit
 
 set CID=fAOAMO.dgl

 type RecordLN ln=Db.getRecord("LN","CID=:CID")
 set CLS=ln.cls
 
 // Product Group
 set GRP=ln.grp
 set QRY="I [TRN]CLS="_quot_CLS_quot_"&([TRN]GRP="_quot_GRP_quot_")"
 set QRY=QRY_"&([TRN]DC=0)"

 do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY")

 quit

VP8(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public String ET,X

 if 'fAOAMO.gld,'X set ER=1,ET="NO2DEP" do ^UTLERR 

 quit

VP9(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public Number %EXT
 type public String ET,X,ZCLS

 if X.isNull() quit

 if 'fAOAMO.glc set %EXT=1,ZCLS="D" do CID^UACN1 quit

 if 'Db.isDefined("GLAD","ACN=:X") set ER=1,ET="INVLDACN" do ^UTLERR

 quit

VP10(RecordAOAMO fAOAMO) //
 if 'fAOAMO.glc do CHANGE^DBSMACRO("TBL","")

 if fAOAMO.glc do CHANGE^DBSMACRO("TBL","GLAD")
 
 quit
 
VP11(RecordAOAMO fAOAMO) //
 type public String QRY
 type String CID,CLS,GRP,quot

 set quot=$CHAR(34)

 if fAOAMO.glc set QRY="I [TRN]CLS="_"""M"""

 if  set QRY=QRY_"&([TRN]DC=1)"

 if  do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY") quit
 
 set CID=fAOAMO.cgl
 
 type RecordLN ln=Db.getRecord("LN","CID=:CID")
 set CLS=ln.cls
 
 // Product Group
 set GRP=ln.grp
 set QRY="I [TRN]CLS="_quot_CLS_quot_"&([TRN]GRP="_quot_GRP_quot_")"
 set QRY=QRY_"&([TRN]DC=1)"

 do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY")

 quit

