	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="SRVPLN0",VPGM=$T(+0),VSNAME="Service Fee Plan Definition "
 set VFSN("FEEPLN")="zfFEEPLN"
 set vPSL=1
 set KEYS(1)=fFEEPLN.PLAN
 set KEYS(2)=fFEEPLN.FEEDT
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fFEEPLN),VDA1(.fFEEPLN),^DBSPNT()

 if '%ProcessMode do VNEW(.fFEEPLN),VPR(.fFEEPLN),VDA1(.fFEEPLN)
 if %ProcessMode do VLOD(.fFEEPLN) quit:$G(ER)  do VPR(.fFEEPLN),VDA1(.fFEEPLN)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fFEEPLN)
	quit


