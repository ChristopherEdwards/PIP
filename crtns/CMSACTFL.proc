vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordCMSACT cmsact

	set X = cmsact.seq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("SEQ", vRM) quit
	set X = cmsact.ac if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("AC", vRM) quit
	set X = cmsact.acn if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("ACN", vRM) quit
	set X = cmsact.cid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CID", vRM) quit
	if cmsact.crdnum.length()>20 set vRM = $$^MSG(1076,20) do vdderr("CRDNUM", vRM) quit
	if cmsact.crdtyp.length()>12 set vRM = $$^MSG(1076,12) do vdderr("CRDTYP", vRM) quit
	if cmsact.dttime.length()>20 set vRM = $$^MSG(1076,20) do vdderr("DTTIME", vRM) quit
	if cmsact.encpin.length()>200 set vRM = $$^MSG(1076,200) do vdderr("ENCPIN", vRM) quit
	if cmsact.ocrdnum.length()>20 set vRM = $$^MSG(1076,20) do vdderr("OCRDNUM", vRM) quit
	set X = cmsact.oprimcid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("OPRIMCID", vRM) quit
	set X = cmsact.primcid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("PRIMCID", vRM) quit
	if '("01"[cmsact.RECRETR) set vRM=$$^MSG(742,"L") do vdderr("RECRETR", vRM) quit
	if cmsact.tlo.length()>40 set vRM = $$^MSG(1076,40) do vdderr("TLO", vRM) quit
	quit

