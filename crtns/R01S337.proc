	type ResultSet rwrs=Db.select(VSELECT,"ACHDTL,ACHBCH,ACHFIL",VWHERE,"ACHFIL.FORIGF,ACHBCH.BORIGF,ACHBCH.ND,ACHDTL.TJD,ACHDTL.FILE,ACHDTL.BATCH,ACHDTL.SEQ","","DQMODE=1")
