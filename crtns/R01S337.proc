		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+5,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: NDF1

	type public String vc11
 type public Date NDF1
 type Date EFFDATE

 set EFFDATE=vc11
 set NDF1=EFFDATE.toString("MM/DD/YEAR")
 set NDF1=NDF1.toString("MM/DD/YEAR")
	quit

VP2	// Column pre-processor - [SYSDEV,ACHDTL]TAMT

 type public Number BOCOF,BOCOU,BODOF,BODOU,EDCOF,EDDOF,EDCOU,EDDOU
 type public Number FOCOF,FOCOU,FODOF,FODOU,TBOC,TBOD,TEDC,TEDD,TFOC,TFOD

 set (TFOD,TFOC,FODOU,FOCOU,FODOF,FOCOF,TBOD,TBOC,BODOU)=0 
 set (EDDOU,EDCOU,EDDOF,EDCOF,TEDD,TEDC,BOCOU,BODOF,BOCOF)=0
	quit

VP3	// Column post-processor - [SYSDEV,ACHDTL]TAMT

	type public String vc12,vc13,vc10,vc14,vc15
 type public Boolean ON
 type public Number SET,TBOC,TBOD,TEDC,TEDD,TEDO,TFOC,TFOD
 type Boolean OU
 type Number AMT,DC,I
 type String ART,TRN,V,VL

 set ART=vc12_vc13
 set AMT=vc10,TRN=vc14

 if 'TRN.length() set TRN=vc15 

 type RecordSTBLACHTC stblachtc=Db.getRecord("STBLACHTC","ACHTC=:TRN",1)
 if 'stblachtc.getMode() do {
	set TRN=vc15
	}

 // Invalid transaction code ~p1
 if 'TRN.length() set (V,VL)=$$^MSG(1498) quit

 set AMT=AMT/100

 if 'ART.isNull() do {
	type RecordUTBLACHRT utblachrt=Db.getRecord("UTBLACHRT","KEY=:ART",1)
 	set OU=utblachrt.onus
 	set ON=$S(OU:"OU",1:"OF")
	}
 else  set ON="OU"

 type RecordSTBLACHTC stblachtctrn=Db.getRecord("STBLACHTC","ACHTC=:TRN",1)
 if stblachtctrn.getMode() do {
	set DC=stblachtctrn.dc
	}

 else  do {
 	type RecordTRN trn=Db.getRecord("TRN","ETC=:TRN",1)
	set DC=trn.itc
	}

 if DC set TFOC=TFOC+AMT,TBOC=TBOC+AMT,TEDC=TEDC+AMT for I="FOC","BOC","EDC" set SET=I_ON,SET=@SET+AMT

 else  set TFOD=TFOD+AMT,TBOD=TBOD+AMT,TEDD=TEDD+AMT for I="FOD","BOD","EDD" set SET=I_ON,SET=@SET+AMT

 set (V,VL)=""

 quit
	quit
