 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="REGCCX",VPGM=$T(+0),VSNAME="Regulation CC Exceptions"
 set VFSN("DAYENDRCCX2")="zfDAYENDR"
 set vPSL=1
 set KEYS(1)=fDAYENDR(1).TJD.get()
 set KEYS(2)=fDAYENDR(1).BRCD.get()
 set KEYS(3)=fDAYENDR(1).UID.get()
 set KEYS(4)=fDAYENDR(1).TSEQ.get()
 set KEYS(5)=fDAYENDR(1).SEQ.get()
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 set %MODS=1,%REPEAT=13 do VPR(.fDAYENDR()),VDA1(.fDAYENDR()),V5^DBSPNT quit

 set ER=0 do VSCRPRE(.fDAYENDR()) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fDAYENDR()),VPR(.fDAYENDR()),VDA1(.fDAYENDR())
 if %ProcessMode do VLOD(.fDAYENDR()) quit:$G(ER)  do VPR(.fDAYENDR()),VDA1(.fDAYENDR())

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fDAYENDR())
 type Number ptr
 set ptr=""
 for  set ptr = fDAYENDR(ptr).order() quit:ptr.isNull()  do {
     if fDAYENDR(ptr).SEQ="" kill fDAYENDR(ptr)
     }
	quit


VNEW(RecordDAYENDRCCX2 fDAYENDR()) // Initialize arrays if %O=0
 
 do VLOD(.fDAYENDR())
 do VDEF(.fDAYENDR())
 do VLOD(.fDAYENDR())
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
