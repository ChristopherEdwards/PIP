VJOURNAL(RecordCMSRECCRD cmsreccrd)	//CMSRECCRD Journal file entries

	type Public Date %EffectiveDate
	type Public String %TSRC,vpar,vx()
	type String TSRC,vdi,vdx()

	if %TSRC.get().isNull() set TSRC="O"
	else  set TSRC=%TSRC

	if %ProcessMode=0 do {
		if TSRC="O" do {
			if 'EFD.get() do {
				do vj1(.cmsreccrd)	// Mode=I Tran=O EFD=N Seq=1 JRNID=MPSRECCRD
				}
			}
		}
	else  if %ProcessMode=1 do {
		if TSRC="O" do {
			if 'EFD.get() do {
				do vj1(.cmsreccrd)	// Mode=U Tran=O EFD=N Seq=1 JRNID=MPSRECCRD
				}
			}
		}

	quit


vj1(RecordCMSRECCRD cmsreccrd)	// MPSRECCRD  Table MPSREC  MPS card record

	if cmsreccrd.mpsjrnl'=""
	else  quit

	type Public String TJD
	type String v1,v2,v3,v4,v5,vlastkey
	set v1=$$EXTDT^MPSRECU(TJD,cmsreccrd.crdtyp,cmsreccrd.crdnum)
	set v2=cmsreccrd.crdtyp
	set v3=$$GETCO1^MPSRECU(cmsreccrd.acn,%CompanyName)
	set v4=cmsreccrd.crdnum
	set v5="CRD"
	set vlastkey=cmsreccrd.mode
	type RecordMPSREC mpsrec=Db.getRecord("MPSREC","PROCDATE=:v1,CRDTYP=:v2,CO=:v3,CRDNUM=:v4,FID=:v5,MODE=:vlastkey",1)
	set mpsrec.refnum=$$GREFNUM^MPSRECU(cmsreccrd.acn,cmsreccrd.crdtyp,cmsreccrd.crdnum)

	do mpsrec.save("/NOVALFK/NOVALDD/NOVALRI")

	quit



vkchged // Access key changed

	type public RecordCMSRECCRD cmsreccrd

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("SEQ")
	set voldkey = vux.piece("|",1),vobj(cmsreccrd,-3) = voldkey // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	set vnewkey = vux.piece("|",2),vobj(cmsreccrd,-3) = vnewkey // Copy new keys into object
	type RecordCMSRECCRD vnewrec = cmsreccrd.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("CMSRECCRD",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	set vobj(cmsreccrd,-3) = vux.piece("|",1)              // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit
