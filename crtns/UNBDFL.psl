	type String vRM,X
	type public RecordUTBLNBD utblnbd

	if utblnbd.nbdc.length()>6 set vRM = $$^MSG(1076,6) do vdderr("NBDC", vRM) quit
	if utblnbd.caldes.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CALDES", vRM) quit
	set X = utblnbd.cntry if 'X.isNull(),'Db.isDefined("STBLCNTRY","X") set vRM = $$^MSG(1485,X) do vdderr("CNTRY", vRM) quit
