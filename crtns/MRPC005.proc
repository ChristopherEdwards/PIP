MRPC005(String return,Number versn,String FRE,Date JD,String CAL,Boolean FULL)	//Public;Frequency Validation
	/*
	   ORIG: MATTSON - 09/17/93

	   This extrinsic function validates the PROFILE frequency
	   specified by parameter 'p1'.

	   [SCATBL5]RPCID=5

	   KEYWORDS: Client/Server, Frequencies

	   ARGUMENTS:
	       . return 	Next date   		/TYP=D/REQ
	        					/MECH=REFNAM:W

	       . versn 		^MRPC005 version number /TYP=N/REQ
	    			Current version = 1  	/MECH=VAL

	       . FRE 		PROFILE frequency  	/TYP=T/REQ
	        					/MECH=VAL

	       . JD 		Start date   		/TYP=D/NOREQ
	        					/DFT=[CUVAR]TJD
	        					/MECH=VAL

	       . CAL 		Calendar   		/TYP=T/NOREQ
	        					/DFT="IBS"
	        					/MECH=VAL

	       . FULL 		Full cycle flag   	/TYP=N/NOREQ
	        					/MECH=VAL
	    			0 = No
	   	 		1 = Yes

	   RETURNS:
	       . $$ 		Error message   	/TYP=T
	    			Null = No error

	   RELATED:
	       . $$^PBSMRPC - MRPC Service Class Driver



	------------------ Revision History ----------------------------------

        07/27/05 - SkariahV- CR16679
	           Removed #WARN directive.
	
	01/10/03 - GRAY - 51351
		   Converted to PSL


	*/

	
	type Date NJD
	
	type Public Number ER
	type Public String ET,RM

	// Version number of client message is not compatible with server
	if versn.get()'=1 quit $$ERRMSG^PBSUTL($$^MSG(2951))

	if JD.get()="" set JD=%SystemDate
	if CAL.get()="" set CAL="IBS"
	set FRE=FRE_"/"_CAL
	set FULL=FULL.get()
	set ER=0

	if 'FULL set NJD=$$NJD^UFRE(JD,FRE)
	else  do FULL^UFRE

	if ER quit $$ERRMSG^PBSUTL(RM.get(),ET.get()))

	set return=NJD

	quit ""

vSIG()	quit "60108^20901^Viji Skariah^1453"	// Signature - LTD^TIME^USER^SIZE
