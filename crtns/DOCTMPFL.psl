public VIDXBLD(List vlist) // Rebuild index files (External call)


	type Number %ProcessMode=0                             // Create mode
	type Number i

	if vlist.get().isNull() set vlist="VINDEX"             // Build all

	type DbSet ds=Db.selectDbSet("DOCTMPLT")

	while ds.next() do {
		type RecordDOCTMPLT doctmplt=ds.getRecord("DOCTMPLT")
		if vlist.contains("VINDEX") do VINDEX(.doctmplt) quit
		if vlist.contains("ACTDATE") do vi1(.doctmplt)
		if vlist.contains("EXPDATE") do vi2(.doctmplt)
	}

	quit


public VIDXBLD1(RecordDOCTMPLT doctmplt, List vlist) // Rebuild index files for one record (External call)


	type Number i

	if vlist.contains("VINDEX") do VINDEX(.doctmplt) quit
	if vlist.contains("ACTDATE") do vi1(.doctmplt)
	if vlist.contains("EXPDATE") do vi2(.doctmplt)

	quit


