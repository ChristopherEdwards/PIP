vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSW942 sw942

	set X = sw942.swdate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("SWDATE", vRM) quit
	set X = sw942.swtime if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"C") do vdderr("SWTIME", vRM) quit
	set X = sw942.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
