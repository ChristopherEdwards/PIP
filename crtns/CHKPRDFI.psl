vau2 // Trigger AU_STAT - After update of STAT

	type public RecordCHKPRD chkprd
	/*
	-----Revision History--------------------------------------------------
	06/04/07 - VARGAJ - 27408
		   When checkbook goes to status "Ordered" update the check
		   numbers on DEP. In addition, dont create checks until
		   the status changes to "activated".

	04/17/07 - VARGAJ - 24940
		   When checkbook goes to status "Ordered", create new checks
		   in CHECK and update account with number.

	09/13/00 - LEVINTOLR - 42013
																			Modified to delete the entry from ^XCHKPRD only if Status
																			is equal to 2 (Cancel).

	09/07/99 - CARROLLJ - 33890
		   Convert to PSL.


	*/

	type public Boolean ER
	type String XCID,XCHKS
	
	set XCID=chkprd.cid
	set XCHKS=chkprd.chks
	if chkprd.stat=2 do { quit:ER
		// Checkbook Order can only be cancelled when in a status of Pending.
		if chkprd.stat.oldVal '= 0 do { quit:ER
			// Chkbk order can only be canc when status is pend		
			do Runtime.setErrXBAD("CHKPRD","INVCHKBOOKOR","STAT")
		}
		
		set ER=0 
		do Db.delete("XCHKPRD","CID=:XCID AND CHKS=:XCHKS")
			
	}
	
	if (chkprd.stat = 3),(chkprd.stat.oldVal '= 3) do {
		// update the account with the check numbers
		type RecordDEP dep = Db.getRecord("DEP","CID=chkprd.cid",1)
		quit:'dep.getMode()
	
		set dep.lscn = dep.nscn
		set dep.lecn = dep.nscn + chkprd.chkcnt - 1
		set dep.nscn = ""
		do dep.save()
	}
	
	if (chkprd.stat = 5),(chkprd.stat.oldVal '= 5) do {
		type String GCHKNUM()
		type Number ACTVREF,CID,CURRCHK,CHKSTART,CHKEND
		type String REFMSG
		
		type RecordDEP dep = Db.getRecord("DEP","CID=chkprd.cid",1)
		quit:'dep.getMode()

		set CHKSTART = chkprd.scn
		
		// get the block of global check numbers
		do ALLOCRNG^CHKNUTL(chkprd.brcd, chkprd.chks, chkprd.chkcnt, CHKSTART, .GCHKNUM)
	
																set CHKEND=GCHKNUM("").order(-1)
 		
		for CURRCHK=CHKSTART:1:CHKEND do {
			type RecordCHECK check = Class.new("RecordCHECK")
			do check.setAuditFlag(1)
					
			set check.chks = chkprd.chks
			set check.chknum = CURRCHK
			set check.cid = chkprd.cid
			set check.issd = %SystemDate
			set check.brcd = chkprd.brcd
			set check.acn = chkprd.acn
			set check.gchecknum = GCHKNUM(CURRCHK)
			// force the status of the new check to be "Issued - Active"
			set check.chksts = 11	
			set check.%uid = %UserID
			set check.scd = %SystemDate			
			do check.save("FRMFILER")
			
		}
		
		type RecordXCHECK xcheck = Class.new("RecordXCHECK")
		
		do xcheck.setAuditFlag(1)
		
		set xcheck.chks = chkprd.chks
		set xcheck.cid = chkprd.cid
		set xcheck.high = GCHKNUM(CHKEND)
		set xcheck.low = GCHKNUM(CHKSTART)
		set xcheck.acn = chkprd.acn
		set xcheck.nam = dep.title1
		
		do xcheck.save()	
		
		// Confirmation Number ~p1
		set REFMSG = $$^MSG(6877,chkprd.refnum)
		set CID=chkprd.cid
		
		do HISTBLD^ACNFUNCS(REFMSG,dep.boo)
	}
	quit
	

vbi1 // Trigger BEFORE_INSERT - Before Insert of CHKPRD record

	type public RecordCHKPRD chkprd
	/*
								-----Revision History-------------------------------------------------
	
								11/14/05 - PUTTASWH - 17011
        	   Modified the error message 8694 that checks for the 
        	   record in CHKPRD table and replaced it by 8693.
        	   Deleted the code that checks for the record in XCHKPRD.         
	
								*/

	
	type public Boolean ER	
	type String MSG

	set MSG=$$^MSG(7094)		//Check
	if '$$EUROCHK^CHKPROC(chkprd.cid) do Runtime.setErrMSG("CHKPRD",69,"MSG") quit:ER
	 
	set XCID=chkprd.cid
	
	type RecordACN acn=Db.getRecord("ACN","CID=:chkprd.cid",1)
	
	// Account has invalid check type 	
	if acn.chks="" do Runtime.setErrMSG("CHKPRD",8708) quit:ER	
	// Account ~p1 closed	
	if acn.stat=4 do Runtime.setErrMSG("CHKPRD",55,chkprd.cid) quit:ER 
	
	type ResultSet rs=Db.select("STAT","CHKPRD","CID=:chkprd.cid AND CHKS=:acn.chks AND STAT=0")
	// Pending check book request already exists 
	if 'rs.isEmpty() do Runtime.setErrMSG("CHKPRD",8693) quit:ER	
		
	type RecordXCHKPRD xchkprd=Db.getRecord("XCHKPRD","CID=:chkprd.cid,CHKS=:chkprd.chks",1)
	do xchkprd.save()	
	
	quit

vbu1 // Trigger BU_STAT - Before Update of STAT

	type public RecordCHKPRD chkprd
	do chkprd.setAuditFlag(1)
	/*
	-----Revision History--------------------------------------------------
	04/17/07 - VARGAJ - 24940
		   When checkbook goes to status "Activated" get a Reference
		   Number.

	*/
	if (chkprd.stat = 5),(chkprd.stat.oldVal '= 5) do {
	
		// Check Activation Allocation
		type Number ACTVREF
	
		do ^TRREFNO(6,.ACTVREF)
		set chkprd.refnum = ACTVREF
	}
			
	quit


vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordCHKPRD chkprd

	set X = chkprd.brcd if 'X.isNull(),'Db.isDefined("UTBLBRCD","X") set vRM = $$^MSG(1485,X) do vdderr("BRCD", vRM) quit
	set X = chkprd.tjd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("TJD", vRM) quit
	set X = chkprd.cid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CID", vRM) quit
	set X = chkprd.chks if 'X.isNull(),'Db.isDefined("UTBLCHKS","X") set vRM = $$^MSG(1485,X) do vdderr("CHKS", vRM) quit
	set X = chkprd.seq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("SEQ", vRM) quit
	set X = chkprd.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
	set X = chkprd.chkcnt if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("CHKCNT", vRM) quit
	set X = chkprd.refnum if 'X.isNull(),X'?1.10N,X'?1"-"1.9N set vRM=$$^MSG(742,"N") do vdderr("REFNUM", vRM) quit
	set X = chkprd.scn if 'X.isNull(),X'?1.10N,X'?1"-"1.9N set vRM=$$^MSG(742,"N") do vdderr("SCN", vRM) quit
	set X = chkprd.stat if 'X.isNull(),'Db.isDefined("STBLCHKPRD","X") set vRM = $$^MSG(1485,X) do vdderr("STAT", vRM) quit
	if chkprd.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

