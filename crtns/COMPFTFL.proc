vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLCOMPFT utblcompft

	if utblcompft.pft.length()>12 set vRM = $$^MSG(1076,12) do vdderr("PFT", vRM) quit
	if utblcompft.descr.length()>40 set vRM = $$^MSG(1076,40) do vdderr("DESCR", vRM) quit
	if utblcompft.query.length()>12 set vRM = $$^MSG(1076,12) do vdderr("QUERY", vRM) quit
	if utblcompft.wpft.length()>12 set vRM = $$^MSG(1076,12) do vdderr("WPFT", vRM) quit
