  //user-defined post procs
 //
VP1(RecordCUVAR fCUVAR) //
 if X=fCUVAR.provfreq quit
 if fCUVAR.provnpdt quit
 if X'=fCUVAR.provfreq,X'="" do	{
 	new JD,NJD
  	set JD=$S(fCUVAR.provlpdt:fCUVAR.provlpdt,1:fCUVAR.TJD)
   	for  set NJD=$$NJD^UFRE(JD,X) quit:ER  quit:NJD'<fCUVAR.tjd  
 	do DEFAULT^DBSMACRO("CUVAR.PROVNPDT",NJD,"1","0","0")
 	}
 quit

VP2(RecordCUVAR fCUVAR) //
 if X="" quit
 if X<fCUVAR.tjd set ER=1,RM=$$^MSG(755)
 quit

VP3(RecordCUVAR fCUVAR) //
 if fCUVAR.provlpdt="",X,fCUVAR.provnpdt-X'>fCUVAR.tjd set ER=1,RM=$$^MSG(5791)
 else  if X,fCUVAR.provnpdt-X'>fCUVAR.provlpdt set ER=1,RM=$$^MSG(5791)
 quit

VP4(RecordCUVAR fCUVAR) //
 if X=fCUVAR.darcfreq quit
 if fCUVAR.darcnpdt quit
 if X'=fCUVAR.darcfreq do {
 	new NJD
 	set NJD=$S(fCUVAR.darclpdt:fCUVAR.darclpdt,1:fCUVAR.tjd)
 	for  set NJD=$$NJD^UFRE(NJD,X) quit:ER  quit:NJD'<fCUVAR.tjd
 	do DEFAULT^DBSMACRO("CUVAR.DARCNPDT",NJD,"1","0","0")
 	}
 quit

VP5(RecordCUVAR fCUVAR) //
 if X'="",(X-fCUVAR.darcoff)<fCUVAR.tjd set ER=1,RM=$$^MSG(746,$$DAT^%ZM(X,$G(%MSKD)))
 quit

VP6(RecordCUVAR fCUVAR) //
 if fCUVAR.darcoff quit
 if fCUVAR.darclpdt="",X,fCUVAR.darcnpdt-X'>fCUVAR.tjd set ER=1,RM=$$^MSG(5791)
 else  if X,fCUVAR.darcnpdt-X'>fCUVAR.darclpdt set ER=1,RM=$$^MSG(5791)
 quit

VP7(RecordCUVAR fCUVAR) //
 do PROG^SCADRV4
 quit

	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


