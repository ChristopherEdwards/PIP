 new V
 //
 set VO="33|19|13|"
 set VO(19)=$C(5,27,12,2,0,0,0,0,0,0)_"00F"_$E(fCUVAR.PROVFREQ,1,12)
 set VO(20)=$C(5,67,10,2,0,0,0,0,0,0)_"00D"_$$DAT^%ZM(fCUVAR.PROVNPDT)
