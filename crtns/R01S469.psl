	type public String %MSKD,%TIM,BASE,CONAM,DESC,DUMMY,ER,IOSL,PROCAT,RID,RN,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,XCC,XCRCD,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate

