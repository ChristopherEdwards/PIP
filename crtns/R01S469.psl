	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="Currency Code: "
	set V=vc1,VO=V set V=$E(V,1,3) do VP6 quit:VFMQ!verror.get()
	set VL=VL_$J("",15-VL.length())_V		// [SYSDEV,ACN]CRCD
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,BASE,DESC,DUMMY,PROCAT,VPN,XCC,XCRCD,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="                                          "_"Total Provisions (Base Equivalent "
	do VP7 quit:VFMQ!verror.get()  set V=$E(BASE,1,3)
	set VL=VL_$J("",76-VL.length())_V
	set VL=VL_$J("",80-VL.length())_"):"
	set V=0						// @TOT([ACN]PROVBAL,0)
	for I=0:1:7 set V=V+VT(I,2,1)
	set VL=VL_$J("",84-VL.length())_$J($FN(V,",",2),18)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,ACN]CC

	type public String vc2
 type public Number XCC
 set XCC=vc2
	quit

VP2	// Column pre-processor - Variable: DUMMY

	type public String vc4
 /*
  Variable DUMMY is used so that we have a pre-processor to store the value of 
  [ACN]PROVCAT to be displayed and used later in the "PROVISION CATEGORY 
  SUBTOTALS" section.  This line is set with blank in the post-processor and 
  then blank line suppressed so that this dummy line will not get displayed in 
  the report.
 */

 type public Number DUMMY

 set DUMMY=vc4
	quit

VP3	// Column post-processor - Variable: DUMMY

 type public String V,VL

 set (V,VL)=""
	quit

VP4	// Column pre-processor - Variable: PROCAT

 type public Number DUMMY,PROCAT

 set PROCAT=DUMMY
	quit

VP5	// Column pre-processor - Variable: DESC

	type public String vc3
 type public Number PROCAT
 type public String DESC
 type String CLS

 set CLS = vc3

 if CLS.isNull() ! PROCAT.isNull() set DESC = "" quit
                   
 type RecordUTBLACC utblacc = Db.getRecord("UTBLACC", "CLS = :CLS,AC = :PROCAT", 1)

 if 'utblacc.getMode() set DESC = "" quit

 set DESC = utblacc.desc
	quit

VP6	// Column post-processor - [SYSDEV,ACN]CRCD

	type public String vc1
 type public String XCRCD
 set XCRCD=vc1
	quit

VP7	// Column pre-processor - Variable: BASE

 /*
  ---- Revision History ------------------------------------------------
 
        04/18/05 - TITOVE - CR 15089
                   Added as part of DBI2 project.
 
 */

 type public String BASE

 set BASE = CUVAR.%CRCD
	quit

VP8(KEY1)	// @TBL([ACN]CLS)

	if KEY1="" quit ""
	type Public Cache %CACHE()
	type RecordSTBLCLS tbl=%CACHE("STBLCLS").getRecord("STBLCLS","CLS=:KEY1")
	quit tbl.DES
