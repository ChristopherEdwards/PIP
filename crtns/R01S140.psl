	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc20,vc21,vc22,vc23,vc24,vc25,vc26
	set vc1=V.piece($C(9),1)			// LNLS8.INCD
	set vc2=V.piece($C(9),2)			// LNLS8.PL
	set vc3=V.piece($C(9),3)			// LNLS8.GRP
	set vc4=V.piece($C(9),4)			// LNLS8.CID
	set vc5=V.piece($C(9),5)			// LNLS2.PICM
	set vc6=V.piece($C(9),6)			// LNLS2.YIELD
	set vc7=V.piece($C(9),7)			// LNLS2.IGP
	set vc8=V.piece($C(9),8)			// LNLS2.REMFRE
	set vc9=V.piece($C(9),9)			// LNLS2.FEEP
	set vc10=V.piece($C(9),10)			// LNLS2.SFF
	set vc11=V.piece($C(9),11)			// LNLS2.WASH
	set vc12=V.piece($C(9),12)			// LNLS2.ESCPP
	set vc13=V.piece($C(9),13)			// LNLS2.PP
	set vc14=V.piece($C(9),14)			// LNLS2.ACTGRP
	set vc15=V.piece($C(9),15)			// LNLS2.REMLD
	set vc16=V.piece($C(9),16)			// LNLS2.SFGL
	set vc17=V.piece($C(9),17)			// LNLS2.PRGL
	set vc18=V.piece($C(9),18)			// LNLS2.ESCWASH
	set vc19=V.piece($C(9),19)			// LNLS2.EFD
	set vc20=V.piece($C(9),20)			// LNLS2.SVN
	set vc21=V.piece($C(9),21)			// LNLS2.REMND
	set vc22=V.piece($C(9),22)			// LNLS2.SFCM
	set vc23=V.piece($C(9),23)			// LNLS2.INTGL
	set vc24=V.piece($C(9),24)			// LNLS2.ESCGL
	set vc25=V.piece($C(9),25)			// LNLS2.SFPF
	set vc26=V.piece($C(9),26)			// LNLS2.PIPF
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VDTL2	// Detail
	type public String %TIM,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(2)  set VD(2)=1				// Print flag
	if VLC+4>IOSL do VHDG0 quit:VFMQ

	set VL=$E(vc2,1,10)
	set VL=VL_$J("",14-VL.length())_$E(vc5,1,3)
	set VL=VL_$J("",22-VL.length())_$J(vc6,8)
	set VL=VL_$J("",46-VL.length())_$J(vc7,2)
	set VL=VL_$J("",52-VL.length())_$E(vc8,1,12)
	set VL=VL_$J("",70-VL.length())_$J(vc9,6,3)
	set VL=VL_$J("",81-VL.length())_$S(vc10:"Y",1:"N")
	set VL=VL_$J("",92-VL.length())_$J(vc11,12)
	set VL=VL_$J("",122-VL.length())_$J(vc12,3)
	do VOM
	set VL="                      "_$J(vc13,9,5)
	set VL=VL_$J("",41-VL.length())_$J(vc14,7)
	set VL=VL_$J("",53-VL.length())_$J($$DAT^%ZM(vc15),10)
	set VL=VL_$J("",70-VL.length())_$J(vc16,12)
	set VL=VL_$J("",92-VL.length())_$J(vc17,12)
	set VL=VL_$J("",113-VL.length())_$J(vc18,12)
	do VOM
	set VL="                     "_$J($$DAT^%ZM(vc19),10)
	set V=vc20,VO=V set V=$E(V,1,12) do VP1 quit:VFMQ!verror.get()
	set VL=VL_$J("",36-VL.length())_V		// [SYSDEV,LNLS2]SVN
	set VL=VL_$J("",53-VL.length())_$J($$DAT^%ZM(vc21),10)
	set VL=VL_$J("",70-VL.length())_$E(vc22,1,3)
	set VL=VL_$J("",92-VL.length())_$J(vc23,12)
	set VL=VL_$J("",113-VL.length())_$J(vc24,12)
	do VOM
	set VL="                                                                      "_$S(vc25:"Y",1:"N")
	set VL=VL_$J("",103-VL.length())_$S(vc26:"Y",1:"N")
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+10,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  (SCA072)"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="Pool          P&I     Yield %       Int Grace        Remittance     Fee %   Reduce Int      P&I Custodial     Participation %"
	do VOM
	set VL="                     "_"  Sale %       Pool ID          Last Date      Fee G/L Account         Principl G/L      Escrow Custodial"
	do VOM
	set VL="                    "_"  Agr Date      Servicer #       Next Date      Fee Calc Method         Interest G/L      Escrow G/L"
	do VOM
	set VL="                                                                    "_"Auto Post Fee           Auto Post P&I"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="Investor:"
	set VL=VL_$J("",15-VL.length())_$E(vc1,1,5)
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,LNLS2]SVN

	type Public String V

	set V=$J(V,12,0)
	quit
