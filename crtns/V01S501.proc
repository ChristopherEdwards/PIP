 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="NOSTTRF1",VPGM=$T(+0),VSNAME="Nostro Transfer Screen 1"
 set VFSN("NOSTTRF")="zfNOSTTRF"
 set vPSL=1
 set KEYS(1)=fNOSTTRF.TRREFNO
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fNOSTTRF),VDA1(.fNOSTTRF),^DBSPNT()

 if '%ProcessMode do VNEW(.fNOSTTRF),VPR(.fNOSTTRF),VDA1(.fNOSTTRF)
 if %ProcessMode do VLOD(.fNOSTTRF) quit:$G(ER)  do VPR(.fNOSTTRF),VDA1(.fNOSTTRF)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fNOSTTRF)
	quit


VNEW(RecordNOSTTRF fNOSTTRF) // Initialize arrays if %O=0
 
 do VDEF(.fNOSTTRF)
 do VLOD(.fNOSTTRF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordNOSTTRF fNOSTTRF)
 if fNOSTTRF.EDT="" set fNOSTTRF.EDT=+$H
 if fNOSTTRF.STATUS="" set fNOSTTRF.STATUS=1
 if fNOSTTRF.UID="" set fNOSTTRF.UID=%UID
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordNOSTTRF fNOSTTRF) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

VPR(RecordNOSTTRF fNOSTTRF) // Display screen prompts
 set VO="12||13|1"
 set VO(0)="|0"
 set VO(1)=$C(3,2,22,0,0,0,0,0,0,0)_"01T Trans. Reference No.:"
 set VO(2)=$C(3,52,8,0,0,0,0,0,0,0)_"01T Status:"
 set VO(3)=$C(4,6,18,0,0,0,0,0,0,0)_"01T Date of Creation:"
 set VO(4)=$C(4,48,12,0,0,0,0,0,0,0)_"01T Entered by:"
 set VO(5)=$C(6,31,18,1,0,0,0,0,0,0)_"01T Nostro Transfers "
 set VO(6)=$C(8,14,10,1,0,0,0,0,0,0)_"01T Currency:"
 set VO(7)=$C(9,16,8,1,0,0,0,0,0,0)_"01T Amount:"
 set VO(8)=$C(10,12,12,1,0,0,0,0,0,0)_"01T Value Date:"
 set VO(9)=$C(12,7,17,1,0,0,0,0,0,0)_"01T Pay from Nostro:"
 set VO(10)=$C(12,40,8,0,0,0,0,0,0,0)_"01TPaid by:"
 set VO(11)=$C(18,9,15,1,0,0,0,0,0,0)_"01T Pay to Nostro:"
 set VO(12)=$C(18,40,8,0,0,0,0,0,0,0)_"01TPaid to:"
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


