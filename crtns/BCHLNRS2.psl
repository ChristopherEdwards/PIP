vEXEC(vCONTEXT,CID) //
	type RecordLN ln
	set ln=Db.getRecord("LN","CID")
 
	do EXEC(.ln)
 
	do ln.save()

	quit
 
 
EXEC(RecordLN ln)     // Processing account

	// quit if account is closed
	if ln.stat=4 quit

	// always reset return notification flag and full/partial indicator
	set ln.rnflg=0
	set ln.fpind=0

	// reset fields to null if not set up for continuous return processing
	if (ln.rnoff="")!(ln.rfreq="")!(ln.bal=0) do { quit
		do RESET(.ln)
		do FILE(.ln)
		}

	new NXTDT
	// calculate next return date
	set NXTDT=$$NJD^UFRE(TJD,ln.rfreq) quit:ER
	
	new roseq
	type ResultSet roseq
	// get approved option record
	set roseq=Db.select("ROSEQ","LNRENEW","CID=:CID AND CNVAPP=1")

	new ROSEQ
	set ROSEQ=""
	if roseq.next() set ROSEQ=roseq.getCol(1)

	// if there is an approved conversion option, set retrun date to conversion date
	if ln.cnvdt,ROSEQ,NXTDT>ln.cnvdt do { quit
		set ln.rdt=ln.cnvdt
		do FILE(.ln)
		}

	// update new return date
	set ln.rdt=NXTDT

	do FILE(.ln)

	quit


RESET(RecordLN ln)	// reset return mechanism parameters

	set ln.rdt=""
	set ln.rfreq=""
	set ln.rnoff=""

	quit

FILE(RecordLN ln)	// file reset columns

	do ln.setAuditFlag(1)
	do ln.save()

	quit

 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
