R01S166	// SCA099CB - IRS 1099-C Report by Account Number
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:44 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA099CB"
	set RN="IRS 1099-C Report by Account Number"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,CTOF,EDT,RID,RN,SDT,TCNT,VL,VLOF,VRF(),VSTATS(),YR,vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vovc1,vovc2,vovc3,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA099CB",RN="IRS 1099-C Report by Account Number"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA099CB","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[M1099]SEQ#1#3,[M1099]NAM#8#40,[M1099]TAXID#49#11,[M1099]CID#61#12,[M1099]COTOT#78#12,[M1099]COINT#98#12,[M1099]FMKTV#117#12"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE=""
		set VWHERE=VWHERE_"(M1099.TD NOT <:SDT AND M1099.TD NOT >:EDT) AND M1"
		set VWHERE=VWHERE_"099.FORMTYP=5 AND M1099.COTOT NOT <:CTOF"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="IRS 1099-C Report by Account Number"
		set VBNRINFO("PGM")="R01S166"
		set VBNRINFO("RID")="SCA099CB"
		set VBNRINFO("TABLES")="M1099,CIF"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,M1099]CID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,M1099]TAXID"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,M1099]SEQ"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:3 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:3 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("M1099.CID,M1099.TAXID,M1099.SEQ,M1099.COTOT,M1099.COINT,M1099.FMKTV,M1099.NAM","M1099,CIF","(M1099.TD NOT <:SDT AND M1099.TD NOT >:EDT) AND M1099.FORMTYP=5 AND M1099.COTOT NOT <:CTOF","M1099.CID,M1099.TAXID,M1099.SEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7
	set vc1=V.piece($C(9),1)			// M1099.CID
	set vc2=V.piece($C(9),2)			// M1099.TAXID
	set vc3=V.piece($C(9),3)			// M1099.SEQ
	set vc4=V.piece($C(9),4)			// M1099.COTOT
	set vc5=V.piece($C(9),5)			// M1099.COINT
	set vc6=V.piece($C(9),6)			// M1099.FMKTV
	set vc7=V.piece($C(9),7)			// M1099.NAM
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

	type public Date EDT,SDT
	type public Number CTOF,TCNT,YR
	
	type RecordUTBLIRS utblirs
	
	// beginning of tax year
	set SDT=$$BOTY^SCADAT($$BOTY^SCADAT(%SystemDate,1)-1,1)
	// end of tax year
	set EDT=$$EOTY^SCADAT(SDT,1)
	set YR=EDT.year()
	
	set utblirs=Db.getRecord("UTBLIRS","KEY='1099-C'",1)
	if 'utblirs.getMode() set CTOF="" quit
	set CTOF=utblirs.fctoff
	
	set TCNT=0
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(1).get() do VDTL1 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(3)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3
	set (vb1,vb2,vb3)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc5,vc4,vc6
	type Public String VSTATS

	set VT(3)=VT(3)+1
	set VT(3,1,1)=VT(3,1,1)+vc5			// @TOT([M1099]COINT)
	set VT(3,2,1)=VT(3,2,1)+vc4			// @TOT([M1099]COTOT)
	set VT(3,3,1)=VT(3,3,1)+vc6			// @TOT([M1099]FMKTV)
	quit

VDTL1	// Detail
	type public String %TIM,CTOF,EDT,IOSL,SDT,TCNT,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),YR,vc1,vc2,vc3,vc4,vc5,vc6,vc7,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	quit:VD(1)  set VD(1)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set V=vc3,VO=V do VP1 quit:VFMQ!verror.get()  set V=$J(V,3) set VL=V
	set VL=VL_$J("",7-VL.length())_$E(vc7,1,40)
	set VL=VL_$J("",48-VL.length())_$E(vc2,1,11)
	set VL=VL_$J("",60-VL.length())_$J(vc1,12)
	set VL=VL_$J("",73-VL.length())_$J(vc4,16,2)
	set VL=VL_$J("",90-VL.length())_$J(vc5,19,2)
	set VL=VL_$J("",110-VL.length())_$J(vc6,18,2)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,CTOF,EDT,RID,RN,SDT,TCNT,VL,YR,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="                                                      "_"Cancellation of Debt"
	do VOM
	set VL="                                                                               "_"Total Debt          Charge-off      Market Value"
	do VOM
	set VL="Seq    Name                                     Tax ID      Account Nbr     Cancellation      Amount Interest       of Property"
	do VOM
	set VL="================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,CTOF,EDT,SDT,TCNT,VPN,YR,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="       "_"Report Totals: "
	set VL=VL_$J("",22-VL.length())_$J(TCNT,8)
	set VL=VL_$J("",32-VL.length())_"Accounts"
	set V=0						// @TOT([M1099]COTOT,0)
	for I=0:1:3 set V=V+VT(I,2,1)
	set VL=VL_$J("",77-VL.length())_$J(V,12,2)
	set V=0						// @TOT([M1099]COINT,0)
	for I=0:1:3 set V=V+VT(I,1,1)
	set VL=VL_$J("",97-VL.length())_$J(V,12,2)
	set V=0						// @TOT([M1099]FMKTV,0)
	for I=0:1:3 set V=V+VT(I,3,1)
	set VL=VL_$J("",116-VL.length())_$J(V,12,2)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - [SYSDEV,M1099]SEQ

	type public Number TCNT

	set TCNT=TCNT+1
	quit
