vj1(RecordCMSRECCID cmsreccid)	// MPSRECCID  Table MPSREC  MPS Account record

	type RecordACN acn=Db.getRecord("ACN","CID=:cmsreccid.cid",1)

	if +acn.carduf=1
	else  quit
	if cmsreccid.mpsjrnl'=""
	else  quit

	type Public String TJD
	type String v1,v2,v3,v4,v5,vlastkey
	set v1=TJD
	set v2=$$GACTYP^MPSRECU(cmsreccid.cid)
	set v3=$$GETCO^MPSRECU(cmsreccid.cid)
	set v4=$$GACNUM^MPSRECU(cmsreccid.cid)
	set v5="CID"
	set vlastkey=1
	type RecordMPSREC mpsrec=Db.getRecord("MPSREC","PROCDATE=:v1,CRDTYP=:v2,CO=:v3,CRDNUM=:v4,FID=:v5,MODE=:vlastkey",1)
	set mpsrec.refnum=cmsreccid.cid

	do mpsrec.save("/NOVALFK/NOVALDD/NOVALRI")

	quit



vkchged // Access key changed

	type public RecordCMSRECCID cmsreccid

	type public Boolean ER = 0
	type public String RM,vpar,vx()

