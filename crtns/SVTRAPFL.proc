vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSVTRAP svtrap

	set X = svtrap.job if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("JOB", vRM) quit
	if svtrap.errormsg.length()>80 set vRM = $$^MSG(1076,80) do vdderr("ERRORMSG", vRM) quit
