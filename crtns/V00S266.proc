VLOD(RecordLNAPCHGE fEABE,RecordDEP DEP,RecordLN LN,RecordLNAPCHG fEABL) // User defined access section
 //
 type RecordLN ln
 type public Number CID,TYPE,BR
 type public Date APCND 
 if 'APCND.exists() set APCND=LN.APCND if APCND="" set APCND=0
 if 'BR.exists() set BR=LN.BOO if BR="" set BR=0
 if 'TYPE.exists() set TYPE=LN.TYPE if TYPE="" set TYPE=0
 
 type ResultSet rs=Db.select("APCND,CID","LNAPCHG","APCND=:APCND AND CID=:CID")
 while rs.next() do {
 set fLNAPCHG=rs.getRow()
 }
 type ResultSet rs1=Db.select("APCND,CID,ECID","LNAPCHGE","APCND=:APCND AND CID=:CID AND ECID=:ECID")
 while rs.next() do {
 set fLNAPCHGE=rs1.getRow()
 }
 quit
 #ACCEPT date=11/05/03;pgm=Screen compiler
 quit
