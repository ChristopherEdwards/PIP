VSCRPRE(RecordLNAPCHGE fEABE,RecordDEP DEP,RecordLN LN,RecordLNAPCHG fEABL)  // Screen Pre-Processor
 new %TAB,vtab // Disable .MACRO. references to %TAB()
 //
 type public Boolean ER
 type public Date JD,NJD,PCHND
 type public Number CID,ER,PINC,PMTPI
 set (ER,PINC)=0
                          
 if LN.PCHND set PINC=LN.PPINC 	
 set PMTPI=LN.PMTPI
                          
 // If interest is in advance then use PINC if the dates agree.  Otherwise, look
 // ahead one frequency.
 if PINC,LN.ICM,LN.APCND'<LN.PCHND set PMTPI=PINC
 else  if PINC,'LN.ICM,LN.APCND'<LN.PCHND do {  quit:ER
 		set FRE=LN.DIST1FRE,JD=LN.PCHND
		do ^UFRE if ER quit
		if LN.APCND'<NJD set PMTPI=PINC
		}
 set OVER=fEABE.ovg
 set CHKAMT=fEABE.AMTC
 set BEGBAL=DATA.get().piece("|",3)
	
 set BEGBAL=BEGBAL+fEABE.AMTD
 set BEGBAL=BEGBAL+fEABE.AMTB
 set BEGBAL=BEGBAL+fEABE.AMTL
 set NEWPMT=fEABL.NTRS+PMTPI    // New total loan payment
 #ACCEPT date=11/05/03;PGM=Screen Compiler
 quit
