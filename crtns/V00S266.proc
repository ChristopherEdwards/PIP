 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNANE1",VPGM=$T(+0),VSNAME="Display Escrow Analysis"
 set VFSN("LN")="zLN",VFSN("LNAPCHG")="zfEABL",VFSN("LNAPCHGE")="zfEABE"
 set vPSL=1
 set KEYS(1)=DEP.CID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fEABE,.DEP,.LN,.fEABL),VDA1(.fEABE,.DEP,.LN,.fEABL),^DBSPNT()

 set ER=0 do VSCRPRE(.fEABE,.DEP,.LN,.fEABL) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fEABE,.DEP,.LN,.fEABL),VPR(.fEABE,.DEP,.LN,.fEABL),VDA1(.fEABE,.DEP,.LN,.fEABL)
 if %ProcessMode do VLOD(.fEABE,.DEP,.LN,.fEABL) quit:$G(ER)  do VPR(.fEABE,.DEP,.LN,.fEABL),VDA1(.fEABE,.DEP,.LN,.fEABL)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fEABE,.DEP,.LN,.fEABL)
	quit


