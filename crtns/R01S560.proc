VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: SUB

 type public Number FD
 type public String SUB

 if FD=0 set SUB="Subtotals Foreign Prod Type"
 else  set SUB="Subtotals Domestic Prod Type"
	quit

VP2	// Column pre-processor - Variable: DESCR

	type public String vc4,vc5,vc6,vc12
  type public Number FD
  type public String CNTRY,MSTATE,DESCR
  
  set FD=vc4
  set CNTRY=vc5
  set MSTATE=vc6
 
  if FD=0 do {
	type ResultSet rs=Db.select("DESC","STBLCNTRY","CNTRY=:CNTRY")
 	if rs.next() set DESCR=rs.getCol("DESC")
	set vc12=DESCR.get()
	}
  else  do {
	type ResultSet rs1=Db.select("DESC","STBLCNTRY1","CNTRY=:CNTRY AND STATE=:MSTATE")
	if rs1.next() set DESCR=rs1.getCol("DESC")
	set vc12=DESCR.get()
	}
  quit
	quit

VP3	// Column pre-processor - Variable: from

 type public Number Day,from,Mon,Year
 type public String Date
 type public Date TJD,VAR

 set VAR=TJD
 set Mon=$$MON^SCADAT(VAR)
 set Year=$$YEAR^SCADAT(VAR)

 if Mon=1 set Year=Year-1 set Mon=12
 else  set Mon=Mon-1

 set Day=$$DAY^SCADAT(VAR)
 set Date=Mon_"/"_Day_"/"_Year
 set from=$$BOMJD^SCADAT(Date)
	quit

VP4	// Column pre-processor - Variable: to

 type public Number Day,Mon,to,Year
 type public String Date
 type public Date TJD,VAR
 
 set VAR=TJD
 set Mon=$$MON^SCADAT(VAR)
 set Year=$$YEAR^SCADAT(VAR)
 
 if Mon=1 set Year=Year-1 set Mon=12
 else  set Mon=Mon-1
 
 set Day=$$DAY^SCADAT(VAR)
 set Date=Mon_"/"_Day_"/"_Year
 set to=$$EOMJD^SCADAT(Date)
	quit
