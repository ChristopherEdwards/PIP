	type public String %TIM,DESCR,I,IOSL,SUB,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),from,to,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate

