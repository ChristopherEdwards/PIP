vEXEC(vCONTEXT,SRVCAT,SRVTYP,SRVLOC,SRVID) //
	type RecordSRVITM srvitm=Db.getRecord("SRVITM","SRVCAT,SRVTYP,SRVLOC,SRVID")

	do srvitm.setAuditFlag(1)
	
	if srvitm.acn.isNull() quit	

	set srvitm.srvamt=srvitm.srvamt+srvitm.fee  // Amount Due + Service Fee
 	
	if srvitm.balavl>srvitm.srvamt do {
		set srvitm.balavl=srvitm.balavl-srvitm.srvamt
		set srvitm.srvamt=0
		}
	else  do {
		set srvitm.srvamt=srvitm.srvamt-srvitm.balavl
		set srvitm.balavl=0
		}
 
 	set srvitm.lddt=srvitm.nddt
	set srvitm.nddt=%SystemDate.nextFreqDate(srvitm.srvfre)

 	do srvitm.save()

 	quit
	
 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
