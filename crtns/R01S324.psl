R01S324	// SCA334 - Loan Budget Performance Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:56 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN2="ALL"
	type String VIN3="ALL"
	type String VIN4="ALL"
	type String VIN5="ALL"

	set RID="SCA334"
	set RN="Loan Budget Performance Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN2")="|255||[LN]GRP|[STBLGRP]:QU ""[STBLGRP]CLS=""""L"""""":NOVAL||D EXT^DBSQRY||T|Product Group|||||"
	set %TAB("VIN3")="|255||[LN]TYPE|[PRODDFTL]:NOVAL||D EXT^DBSQRY||T|Product Type|||||"
	set %TAB("VIN4")="|255||[LN]BOO|[UTBLBRCD]:NOVAL||D EXT^DBSQRY||T|Branch of Ownership|||||"
	set %TAB("VIN5")="|255||[LNENT]CID|[ACN]:NOVAL||D EXT^DBSQRY||T|Account Number|||||"

	set %READ="IO/REQ,VIN2#0,VIN3#0,VIN4#0,VIN5#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String VIN2,VIN3,VIN4,VIN5
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,DESC,EUNTDSC,GRP,OCT,RID,RN,RPUNT,TYPE,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA334",RN="Loan Budget Performance Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA334","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[LNENT]USESRC#1#4,[LNENT]BUD#32#13,[LNENT]ACT#50#16,[LNENT]ADD#71#16,[LNENT]AVL#92#13,[LNENT]REP#110#15"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		set DQQRY(SEQ)="[SYSDEV,LN]CLS=""L""",SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[LN]GRP "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[LN]TYPE "_VIN3,SEQ=SEQ+1
		if VIN4.get()="" set VIN4="ALL"
		if VIN4'="ALL" set DQQRY(SEQ)="[LN]BOO "_VIN4,SEQ=SEQ+1
		if VIN5.get()="" set VIN5="ALL"
		if VIN5'="ALL" set DQQRY(SEQ)="[LNENT]CID "_VIN5,SEQ=SEQ+1
		set DQQRY(SEQ)="([LN]PCIDSTAT=0)!([LN]PCIDSTAT="""")",SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("LNENT,LN") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:5 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"LNENT.CID,LNENT.USESRC,LN.GRP,LN.TYPE,LN.NAM,LNENT"
	set VSELECT=VSELECT_".BUD,LNENT.ACT,LNENT.ADD,LNENT.AVL,LNENT.REP,LNENT"
	set VSELECT=VSELECT_".OCTANT,LNENT.REPUNT"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"LNENT,LN",VWHERE,"LNENT.CID,LNENT.USESRC,LN.GRP,LN.TYPE","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12
	set vc1=V.piece($C(9),1)			// LNENT.CID
	set vc2=V.piece($C(9),2)			// LNENT.USESRC
	set vc3=V.piece($C(9),3)			// LN.GRP
	set vc4=V.piece($C(9),4)			// LN.TYPE
	set vc5=V.piece($C(9),5)			// LN.NAM
	set vc6=V.piece($C(9),6)			// LNENT.BUD
	set vc7=V.piece($C(9),7)			// LNENT.ACT
	set vc8=V.piece($C(9),8)			// LNENT.ADD
	set vc9=V.piece($C(9),9)			// LNENT.AVL
	set vc10=V.piece($C(9),10)			// LNENT.REP
	set vc11=V.piece($C(9),11)			// LNENT.OCTANT
	set vc12=V.piece($C(9),12)			// LNENT.REPUNT
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(1).get() do VDTL1 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	set VH0=1					// Page break
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,VH0=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do VSUM2 quit:VFMQ  do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc7,vc8,vc9,vc6,vc10
	type Public String VSTATS

	set VT(4)=VT(4)+1
	set VT(4,1,1)=VT(4,1,1)+vc7			// @TOT([LNENT]ACT)
	set VT(4,2,1)=VT(4,2,1)+vc8			// @TOT([LNENT]ADD)
	set VT(4,3,1)=VT(4,3,1)+vc9			// @TOT([LNENT]AVL)
	set VT(4,4,1)=VT(4,4,1)+vc6			// @TOT([LNENT]BUD)
	set VT(4,5,1)=VT(4,5,1)+vc10			// @TOT([LNENT]REP)
	quit

VDTL1	// Detail
	type public String %TIM,DESC,EUNTDSC,GRP,IOSL,OCT,RPUNT,TYPE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(1)  set VD(1)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="Account Number: "
	set VL=VL_$J("",18-VL.length())_$J(vc1,12)
	set VL=VL_$J("",34-VL.length())_$E(vc5,1,27)
	do VOM
	set VL="" do VOM
	quit

VDTL2	// Detail
	type public String %TIM,DESC,EUNTDSC,GRP,IOSL,OCT,RPUNT,TYPE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(2)  set VD(2)=1				// Print flag
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set V=vc2,VO=V set V=$E(V,1,4) do VP1 quit:VFMQ!verror.get()  set VL=V
	set VL=VL_$J("",6-VL.length())_$E(DESC,1,20)
	set VL=VL_$J("",31-VL.length())_$S(vc6="":"",1:$J(vc6,0,2))
	set VL=VL_$J("",49-VL.length())_$S(vc7="":"",1:$J(vc7,0,2))
	set VL=VL_$J("",70-VL.length())_$S(vc8="":"",1:$J(vc8,0,2))
	set VL=VL_$J("",91-VL.length())_$S(vc9="":"",1:$J(vc9,0,2))
	set VL=VL_$J("",109-VL.length())_$S(vc10="":"",1:$J(vc10,0,2))
	do VOM
	do VP2 quit:VFMQ!verror.get()  set V=$E(EUNTDSC,1,20) set VL="      "_V
	set VL=VL_$J("",50-VL.length())_$E(OCT,1,15)
	set VL=VL_$J("",109-VL.length())_$E(RPUNT,1,15)
	do VOM
	set VL="" do VOM
	quit

VSUM2	// Summary
	type public String %TIM,DESC,EUNTDSC,GRP,I,IOSL,OCT,RPUNT,TYPE,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="      "_"Account Totals: "
	set V=VT(2,4,1)					// @TOT([LNENT]BUD,)
	set VL=VL_$J("",29-VL.length())_$S(V="":"",1:$J(V,0,2))
	set V=VT(2,1,1)					// @TOT([LNENT]ACT,)
	set VL=VL_$J("",47-VL.length())_$S(V="":"",1:$J(V,0,2))
	set V=VT(2,2,1)					// @TOT([LNENT]ADD,)
	set VL=VL_$J("",68-VL.length())_$S(V="":"",1:$J(V,0,2))
	set V=VT(2,3,1)					// @TOT([LNENT]AVL,)
	set VL=VL_$J("",89-VL.length())_$S(V="":"",1:$J(V,0,2))
	set V=VT(2,5,1)					// @TOT([LNENT]REP,)
	set VL=VL_$J("",106-VL.length())_$S(V="":"",1:$J(V,0,2))
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,DESC,EUNTDSC,GRP,OCT,RID,RN,RPUNT,TYPE,VL,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+11,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="" do VOM
	set VL="                                                   "_"Loan Budget Summary"
	do VOM
	set VL="" do VOM
	set VL="GROUP: "
	set VL=VL_$J("",8-VL.length())_$E(vc3,1,4)
	set VL=VL_$J("",16-VL.length())_"TYPE: "
	set VL=VL_$J("",23-VL.length())_$E(vc4,1,7)
	do VOM
	set VL="" do VOM
	set VL="Code  Description                  Projected               Actual             Add Back         Available          Repayments"
	do VOM
	set VL="      "_"Unit Description                               Actual Units                                            Repayment Units"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,LNENT]USESRC

	type public String vc2
	type public String DESC,EUNCD,USESRC	

	set USESRC=vc2
	if USESRC="" set DESC="Unassigned" quit

	type RecordUTBLECT utblect
	set utblect=Db.getRecord("UTBLECT","KEY=:USESRC")
	set DESC=utblect.desc 

	set EUNCD=utblect.euncd  // Needed in <<EUNTDSC>> Pre-Processor
	quit

VP2	// Column pre-processor - Variable: EUNTDSC

	type public String vc2,vc11,vc12
	type public String EUNCD,EUNTDSC,OCT,RPUNT,TMP,TMP1,USESRC

	set EUNTDSC="",OCT="",RPUNT=""

	if vc2="" quit
	set USESRC=vc2

	if EUNCD="" quit  // set in LNENT.USESRC post-processor

	type RecordUTBLEUNT utbleunt
	set utbleunt=Db.getRecord("UTBLEUNT","KEY=:EUNCD")
	if utbleunt.desc="" quit
	set EUNTDSC=EUNCD_" - "_utbleunt.desc
 
	set OCT=vc11
	set TMP=$$^SCARND(OCT,0,,,5)
	set OCT=$J("",(15 - '(TMP.isNull())))_TMP

	set RPUNT=vc12
	set TMP1=$$^SCARND(RPUNT,0,,,5)
	set RPUNT=$J("",(15 - '(TMP1.isNull())))_TMP1
	quit
