	type Public String vovc2,vc2,vovc4,vc4,vovc5,vc5,vovc6,vc6,vovc7,vc7
	set vovc2=vc2,vovc4=vc4,vovc5=vc5,vovc6=vc6,vovc7=vc7
	quit


