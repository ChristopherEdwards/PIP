VFPOST	// FETCH post-processor

	type public String vc1,vc2,vc3,vc4
 type public Number CID,LCID
 type public String ACR(),BAL(),DESC,NRM(),NUM(),PRM(),TY,TYP,TRTYPE,PAYID,PAYEE,PDES

 type Number ACOLL,AREF
 
 set LCID=vc1
 set TY=vc2
 set DESC=""
 if NUM(TY).get().isNull() set (NUM(TY),BAL(TY),ACR(TY),PRM(TY),NRM(TY))=0
 set NUM(TY)=NUM(TY)+1
 set PRM(TY)=PRM(TY)+vc3
 
 set AREF=vc1
 set ACOLL=+vc3
 
 type RecordLNCOL lncol=Db.getRecord("LNCOL","CID=:AREF,COLL=:ACOLL",1)
 if lncol.getMode() set DESC=lncol.desc
 
 set TRTYPE=vc2
 set PAYEE=vc4
 type RecordLNTRS1 lntrs1=Db.getRecord("LNTRS1","TRTYPE=:TRTYPE,PAYID=:PAYEE",1)
 set PDES=lntrs1.paydes
	quit

VRPOST	// Report post-processor

 // Go to SCA152B for report summary
 type public Number %PG,%O
 type public String IOSL,IOTYP,KVAR,%PGM,TY,VFMQ,VLC

 type Boolean BANNER

 set TY="" 
 do Db.fastDelete("LESTBREP")
 do ZESTB
 if IOTYP="TRM" set VLC=IOSL do VWAIT        
 if VFMQ="Q" do VEXIT()
 else  do CONT
 quit

CONT // Do report summary SCA152B
 type public String IOTYP,PGM,RID,RN
 type public Boolean %NOBANNER
 // I18N=OFF
 set RID="SCA152B"
 set RN="Single Payee Remittance (By Borrower) Summary"
 do ^URID
 // I18N=ON
 type RecordCUVAR cuvar=Db.getRecord("CUVAR")
 if '(%NOBANNER.exists())!(IOTYP="TRM")!('cuvar.banner) write #   // Form Feed
 do @("V0^"_PGM) 
 quit

ZESTB // Set count, balance and accrued interest into ^ZESTB
 type public String ACR(),BAL(),PRM(),NRM(),NUM()
 type public Number TY
 set TY=NUM(TY).order() if TY="" quit
 type RecordLESTBREP lestbrep=Db.getRecord("LESTBREP","TY=:TY",1)

 set lestbrep.num=NUM(TY)
 set lestbrep.bal=BAL(TY)
 set lestbrep.acr=ACR(TY)
 set lestbrep.prm=PRM(TY)
 set lestbrep.nrm=NRM(TY)
 do lestbrep.save() 
 do ZESTB
 quit

VWAIT
 type public String IOSL,VL,VLC
 type public Number VRG,X
 set VL="" 
 for X=VLC+1:1:IOSL do VOM // SCROLL SCREEN UP 
 set VLC=0,VRG=0 // SOM page break logic 
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0,vs(5)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG3 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(5)
	type Public String vc2,vovc2,vc4,vovc4,vc5,vovc5,vc6,vovc6,vc7,vovc7
	type Number vb1,vb2,vb3,vb4,vb5
	set (vb1,vb2,vb3,vb4,vb5)=0
	if vb1!(vovc2'=vc2) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,vb5=1,VH0=1
	if vb2!(vovc4'=vc4) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,vb5=1,VH0=1
	if vb3!(vovc5'=vc5) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,vb5=1
	if vb4!(+vovc6'=+vc6) set vs(5)=0,vh(5)=0,VD(4)=0,vb5=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(5) set vs(5)=1 do stat^DBSRWUTL(5)
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
