	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%NOBANNER,%O,%PG,%PGM,%TIM,ACR(),BAL(),CID,CONAM,DESC,IOTYP,KVAR,LCID,NRA,NRM(),NUM(),PAYEE,PAYID,PAYTOT,PDES,PGM,PRM(),RID,RN,TRTYPE,TY,TYP,VL,X,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc2,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",99-VL.length())_"Run Date:"
	set VL=VL_$J("",109-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",101-VL.length())_"System:"
	set VL=VL_$J("",109-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="" do VOM
	set VL="Loan Account  Borrower                                          Prt Rem    Last Rem   Assessment   Coll Num   RM SP  RI    SR"
	do VOM
	set VL="              "_"Collateral                       Next Rem Amt     Rem Ytd    Effective   PRA RIP RE  Next Rem   RB  % Inc    AN "
	do VOM
	set VL="=============================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,TRTYPE]PAYID

	type public String vc4
 type public String PAYE

 set PAYE = vc4
	quit

VP2	// Column pre-processor - Variable: PAYTOT

 type public String PAYE,PAYTOT

 set PAYTOT="Payee "_PAYE_" Totals:"
	quit

VP3	// Column pre-processor - Variable: NRA

	type public String vc6,vc7,vc20
 type public Date NRD,NRM()
 type public Number CID,NRA
 type public String TY,TYP
 
 set CID=vc6
 set TYP=vc7
 set NRD=vc20
 
 set NRA=0
 
 type RecordRAMT ramt=Db.getRecord("RAMT","CID=:CID,TYP=:TYP,REMDT=:NRD",1)
 if ramt.getMode() set NRA=ramt.remamt,NRM(TY)=NRM(TY)+NRA
	quit
