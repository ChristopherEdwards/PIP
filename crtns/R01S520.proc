R01S520	// SCA598 - Direct Debits by Submit Institution
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:51 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"
	type String VIN2="ALL"
	type String VIN3="ALL"
	type String VIN4="ALL"

	set RID="SCA598"
	set RN="Direct Debits by Submit Institution"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[EFTPAY]DATECRE|||D EXT^DBSQRY||T|Date of Creation|||||"
	set %TAB("VIN2")="|255||[EFTPAY]RECINST|[UTBLEFTINST]:NOVAL||D EXT^DBSQRY||T|Recipient Institution|||||"
	set %TAB("VIN3")="|255||[EFTPAY]CID|[EFTPAY]CID:DISTINCT:NOVAL||D EXT^DBSQRY||T|Account Number|||||"
	set %TAB("VIN4")="|255||[EFTPAY]SEQ|[EFTPAY]:NOVAL||D EXT^DBSQRY||T|Sequence Number|||||"

	set %READ="IO/REQ,VIN1#0,VIN2#0,VIN3#0,VIN4#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String VIN1,VIN2,VIN3,VIN4
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,INSTNAM,RID,RN,SOURCEDESC,STATUSCODE,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA598",RN="Direct Debits by Submit Institution"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA598","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[EFTPAY]EFTTYPE#2#6,[EFTPAY]CID#38#12,[EFTPAY]SEQ#53#12,[EFTPAY]AMOUNT#67#18,[EFTPAY]CRCD#117#3\[EFTPAY]RECINST#2#12,[EFTPAY]RECACCT#38#34,[EFTPAY]ACTAMT#81#18"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[EFTPAY]DATECRE "_VIN1,SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[EFTPAY]RECINST "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[EFTPAY]CID "_VIN3,SEQ=SEQ+1
		if VIN4.get()="" set VIN4="ALL"
		if VIN4'="ALL" set DQQRY(SEQ)="[EFTPAY]SEQ "_VIN4,SEQ=SEQ+1
		set DQQRY(SEQ)="[EFTPAY]EFTTYPE=""DD""",SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("EFTPAY") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"Date of Creation"_"|VIN1|"_VIN1.get()
		set VBNRINFO("PROMPTS",2)="WC2|"_"Recipient Institution"_"|VIN2|"_VIN2.get()
		set VBNRINFO("PROMPTS",3)="WC2|"_"Account Number"_"|VIN3|"_VIN3.get()
		set VBNRINFO("PROMPTS",4)="WC2|"_"Sequence Number"_"|VIN4|"_VIN4.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Direct Debits by Submit Institution"
		set VBNRINFO("PGM")="R01S520"
		set VBNRINFO("RID")="SCA598"
		set VBNRINFO("TABLES")="EFTPAY"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,EFTPAY]RECINST"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,EFTPAY]DATECRE"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,EFTPAY]CID"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,EFTPAY]SEQ"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"EFTPAY.RECINST,EFTPAY.DATECRE,EFTPAY.CID,EFTPAY.SE"
	set VSELECT=VSELECT_"Q,EFTPAY.EFTTYPE,EFTPAY.AMOUNT,EFTPAY.CRCD,EFTPAY."
	set VSELECT=VSELECT_"RECACCT,EFTPAY.ACTAMT,EFTPAY.STATUS,EFTPAY.EFTSRCD"
	set VSELECT=VSELECT_"D"

	#ACCEPT DATE=01/31/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"EFTPAY",VWHERE,"EFTPAY.RECINST,EFTPAY.DATECRE,EFTPAY.CID,EFTPAY.SEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11
	set vc1=V.piece($C(9),1)			// EFTPAY.RECINST
	set vc2=V.piece($C(9),2)			// EFTPAY.DATECRE
	set vc3=V.piece($C(9),3)			// EFTPAY.CID
	set vc4=V.piece($C(9),4)			// EFTPAY.SEQ
	set vc5=V.piece($C(9),5)			// EFTPAY.EFTTYPE
	set vc6=V.piece($C(9),6)			// EFTPAY.AMOUNT
	set vc7=V.piece($C(9),7)			// EFTPAY.CRCD
	set vc8=V.piece($C(9),8)			// EFTPAY.RECACCT
	set vc9=V.piece($C(9),9)			// EFTPAY.ACTAMT
	set vc10=V.piece($C(9),10)			// EFTPAY.STATUS
	set vc11=V.piece($C(9),11)			// EFTPAY.EFTSRCDD
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VDTL2	// Detail
	type public String %TIM,INSTNAM,IOSL,SOURCEDESC,STATUSCODE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(2)  set VD(2)=1				// Print flag
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="   "_"Date of Creation:"
	set VL=VL_$J("",32-VL.length())_$J($$DAT^%ZM(vc2),10)
	do VOM
	set VL="        "_"Institution:"
	set VL=VL_$J("",21-VL.length())_$E(vc1,1,12)
	do VP1 quit:VFMQ!verror.get()  set V=$E(INSTNAM,1,20)
	set VL=VL_$J("",38-VL.length())_V
	do VOM
	set VL="" do VOM
	quit

VDTL4	// Detail
	type public String %TIM,INSTNAM,IOSL,SOURCEDESC,STATUSCODE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL=" "_$E(vc5,1,6)
	set VL=VL_$J("",8-VL.length())_$J(vc3,41)
	set VL=VL_$J("",50-VL.length())_$J(vc4,14)
	set VL=VL_$J("",65-VL.length())_$J($FN(vc6,",",2),19)
	do VP2 quit:VFMQ!verror.get()  set V=$E(STATUSCODE,1,20)
	set VL=VL_$J("",86-VL.length())_V
	set VL=VL_$J("",116-VL.length())_$E(vc7,1,3)
	do VOM
	set VL=" "_$E(vc1,1,12)
	do VP3 quit:VFMQ!verror.get()  set V=$E(INSTNAM,1,20)
	set VL=VL_$J("",14-VL.length())_V
	set VL=VL_$J("",37-VL.length())_$E(vc8,1,34)
	set VL=VL_$J("",72-VL.length())_$J($FN(vc9,",",2),26)
	do VP4 quit:VFMQ!verror.get()  set V=$E(SOURCEDESC,1,20)
	set VL=VL_$J("",100-VL.length())_V
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,INSTNAM,RID,RN,SOURCEDESC,STATUSCODE,VL,vc1,vc10,vc11,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL="" do VOM
	set VL=$E($G(CONAM),1,40)
	set VL=VL_$J("",96-VL.length())_"Run Date:"
	set VL=VL_$J("",106-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",118-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",98-VL.length())_"System:"
	set VL=VL_$J("",106-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",117-VL.length())_"Page:"
	set VL=VL_$J("",123-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL=" "_"Payment Type                    Customer Account   Sequence                  Amount                Status     Currency"
	do VOM
	set VL=" "_"Recipient  Institution           Recipient  Account                                 Actual Amount                Source"
	do VOM
	set VL="================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: INSTNAM

	type public String vc1
	// Converted to PSL by CNV70RW
	type Public String INSTNAM

	//10/29/2001 - REENA - ARQ 45497
	
	do { 
	    type String RECINST

	    set RECINST=vc1
	    type ResultSet rs=Db.select("NAM","UTBLEFTINST","KEY=:RECINST") 
	    set INSTNAM="" if 'rs.next()  quit
	    set INSTNAM=rs.getCol("NAM")
	   } 
	
	
	quit

