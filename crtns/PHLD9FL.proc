	type public RecordPHLD9 phld9

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("PHLD9","CID = :phld9.cid and SEQ = :phld9.seq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))
