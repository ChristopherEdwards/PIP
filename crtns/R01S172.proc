	type public String %TIM,CNT,CTOF,EDT,IDCNT,IOSL,SDT,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VSKIPREC,VT(),YR,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc12,vovc13,vrundate,vsysdate

