	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,M1099]INTWH

	type public Number CNT

	set CNT=CNT+1
	quit

VP2	// Column post-processor - @TOT([M1099]GPPLS,,$,15)

	type public Number CNT
	type public String V,VL

	if CNT=1 set (V,VL)=""
	quit

VP3	// Column post-processor - Variable: " "

	type public Number CNT
	type public String V,VL

	if CNT=1 set (V,VL)=""
	if CNT-1 set V=0.char()
	quit

VP4	// Column pre-processor - Variable: TAXID

	type public Number IDCNT

	set IDCNT=IDCNT+1
	quit

VP5	// Column post-processor - @TOT([M1099]INTWH,,$,15)

	type public Number CNT
	type public String V,VL

	if CNT=1 set (V,VL)=""
	quit

VP6	// Column post-processor - @TOT([M1099]GOLDPARA,,$,15)

	type public Number CNT
	type public String V,VL

	if CNT=1 set (V,VL)=""
	quit

VP7	// Column post-processor - Variable: " "

	type public Number CNT
	type public String V,VL

	if CNT=1 set (V,VL)=""
	if CNT-1 set V=0.char()
	quit
