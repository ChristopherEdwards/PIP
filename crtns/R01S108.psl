VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CAMT,CONAM,DNCALTD,DNCRD,DNDEB,DNET,DNOAMT,DNRBAL,DYCALTD,DYCRD,DYDEB,DYOAMT,DYRBAL,GTCRD,GTDEB,PAYEE,PLAN,PNCALTD,PNCRD,PNDEB,PNET,PNOAMT,PNRBAL,PT,PYCALTD,PYCRD,PYDEB,PYOAMT,PYRBAL,RID,RN,TCRD,TDEB,TYP,VL,fAOAMO,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
