VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,LNCYCDLD]CID

	type public String vc13
 type public Boolean RCM

 set RCM=vc13
	quit

VP2	// Column pre-processor - [SYSDEV,LNCYCDLD]AMT

 type public Boolean RCM
 type public String DNOAMT,DYOAMT,PNOAMT,PYOAMT,V

 if 'RCM set PNOAMT=PNOAMT+V set DNOAMT=DNOAMT+V
 if RCM=1 set PYOAMT=PYOAMT+V set DYOAMT=DYOAMT+V
	quit

VP3	// Column pre-processor - [SYSDEV,LNCYCDLD]CALTD

 type public Boolean RCM
 type public String DNCALTD,DYCALTD,PNCALTD,PYCALTD,V

 if 'RCM set PNCALTD=PNCALTD+V set DNCALTD=DNCALTD+V
 if RCM=1 set PYCALTD=PYCALTD+V set DYCALTD=DYCALTD+1
	quit

VP4	// Column pre-processor - [SYSDEV,LNCYCDLD]RBAL

 type public Boolean RCM
 type public String DNRBAL,DYRBAL,PNRBAL,PYRBAL,V

 if 'RCM set PNRBAL=PNRBAL+V set DNRBAL=DNRBAL+V
 if RCM=1 set PYRBAL=PYRBAL+V set DYRBAL=DYRBAL+V
	quit

VP5	// Column pre-processor - [SYSDEV,LNCYCDLD]RAMT

 type public Boolean RCM
 type public String DNDEB,DNET,DYDEB,GTDEB,PNDEB,PNET,PYDEB,TDEB,V

 set PNET=PNET+V set DNET=DNET+V if V'<0 quit 
 set TDEB=TDEB+(-V)
 set GTDEB=GTDEB+(-V)
 if 'RCM set PNDEB=PNDEB+(-V) set DNDEB=DNDEB+(-V)
 if RCM=1 set PYDEB=PYDEB+(-V) set DYDEB=DYDEB+(-V)
 set V=-V
	quit

VP6	// Column post-processor - [SYSDEV,LNCYCDLD]RAMT

	type public String vc16
 type public String V

 type Number TMP

 set TMP = vc16

 if TMP'<0 set V="" quit
	quit

VP7	// Column pre-processor - Variable: CAMT

	type public String vc16
 type public Boolean RCM
 type public String CAMT,DNCRD,DYCRD,GTCRD,PNCRD,PYCRD,TCRD

 set CAMT=vc16
 if CAMT'>0 quit 
 set TCRD=TCRD+CAMT
 set GTCRD=GTCRD+CAMT
 if 'RCM set PNCRD=PNCRD+CAMT set DNCRD=DNCRD+CAMT
 if RCM=1 set PYCRD=PYCRD+CAMT set DYCRD=DYCRD+CAMT
	quit

VP8	// Column post-processor - Variable: CAMT

	type public String vc16
 type public String V

 type Number TMP

 set TMP = vc16

 if TMP'>0 set V = "" quit
	quit

VP9	// Column post-processor - Variable: PNET

 type Public String PNCALTD,PNCRD,PNDEB,PNET,PNOAMT,PNRBAL,PYCALTD,PYCRD,PYDEB,PYOAMT,PYRBAL,TCRD,TDEB

 set (PNOAMT,PNCALTD,PNRBAL,PNDEB,PNCRD,PYOAMT,PYCALTD,PYRBAL,PYDEB)=0
 set (PYCRD,TDEB,TCRD,PNET)=0
	quit

VP10	// Column post-processor - Variable: PAYEE

 type public String PT,V,VL

 if PT set (VL,V)=""
	quit

VP11	// Column post-processor - [SYSDEV,AOAMO]PDES

 type public String PT,V,VL

 if PT set (VL,V)=""
	quit

VP12	// Column pre-processor - Variable: ADDR(1)

	type public String vc17
 type public String ADDR
 type Number CIF,I

 for I=1:1:3 set ADDR(I)=""
 set CIF=vc17 if 'CIF.length() quit 
 do ^UTLADDR(,"ADDR",3,"","CIF",CIF)
	quit

VP13	// Column post-processor - Variable: ADDR(1)

 type public String PT,VL

 if PT set VL=""
	quit

VP14	// Column post-processor - Variable: ADDR(2)

 type public String PT,VL

 if PT set VL=""
	quit

VP15	// Column post-processor - Variable: ADDR(3)

 type public String PT,VL

 if PT set VL=""
	quit

VP16	// Column post-processor - TRANS DT     DEBIT    CREDIT

 type public String PT,VL

 if PT set VL=""
	quit

VP17	// Column post-processor - @CHR(=,132)

	type Public String PT,V,VL

	if PT set (VL,V)=""
	quit
