	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set V=vc2,VO=V set V=$E(V,1,20) do VP10 quit:VFMQ!verror.get()  set VL="    "_V
	do VOM
	set V=vc5,VO=V set V=$E(V,1,40) do VP11 quit:VFMQ!verror.get()  set VL="    "_V
	do VOM
	do VP12 quit:VFMQ!verror.get()  set V=$E(ADDR(1),1,40) do VP13 quit:VFMQ!verror.get()  set VL="    "_V
	do VOM
	set V=$E(ADDR(2),1,40) do VP14 quit:VFMQ!verror.get()  set VL="    "_V
	do VOM
	set V=$E(ADDR(3),1,50) do VP15 quit:VFMQ!verror.get()  set VL="    "_V
	do VOM
	set VL="" do VOM
	set VL="     "_"ACCOUNT  NAME                  LOAN RTE  DDEL  DLR % BAL  ORIG AMT  COL  #PD   PD LIFE   REM BAL"
	set V="TRANS DT     DEBIT    CREDIT" do VP16 quit:VFMQ!verror.get()
	set VL=VL_$J("",103-VL.length())_V
	do VOM
