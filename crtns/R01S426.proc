		set VBNRINFO("ORDERBY",2)="[SYSDEV,DMJ]DATETIME"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DMJ]CID"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,DMJ]TSEQ"


