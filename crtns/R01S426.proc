	// User-defined pre/post-processor code

VFPOST	// FETCH post-processor

	type public String vc1,vc2,vc3,vc4
	type Public String OPT,POU
	type String CID,CRCD,CRDR,ITC,PM,TAMT

	// Summarize activity by transaction code for report SCA464

	// Internal transaction code
	set ITC=vc1

	// Debit or credit indicator
	set CRDR=+$E(ITC)

	// PM indicator
	set PM=+$E(ITC,8)

	// PMed activity only.
	if OPT=2,'PM quit

	// Non PMed activity only.
	if OPT=3,PM quit

	set TAMT=vc2
	set CRCD=vc3
	if CRCD="" set CRCD=%SystemCurrency

	// SUM // Create summary record.
	set CID=vc4

	// select record from the database
	type RecordDMJTMP dmjtmp=Db.getRecord("DMJTMP","JOB=:%ProcessID,CRCD=:CRCD,ACN=:CID",1)

	if 'CRDR do {
		set dmjtmp.dramt=dmjtmp.dramt+vc2
		set dmjtmp.drcnt=dmjtmp.drcnt+1
		}
	else  do {
		set dmjtmp.cramt=dmjtmp.cramt+vc2
		set dmjtmp.crcnt=dmjtmp.crcnt+1
		}

	do dmjtmp.bypassSave()

	set POU=$S($E(ITC,8):"P",1:" ")
	set POU=POU_$S($E(ITC,10):"O",1:" ")
	set POU=POU_$S($E(ITC,11):"U",1:" ")	

	quit 

VPREAQ	// Pre-processor (after query)

	type Public Number DET

	// No Detail, just summaries.
	if DET=0 do rwopt^DBSRWUTL("NODTL","")

	quit

VPREBQ	// Pre-processor (before query)

	type Public String OPT

	// kill temp table
	do Db.fastDelete("DMJTMP","JOB=:%ProcessID")

	// Report options
	// All activity
	set OPT(1)=$$^MSG(4609)

	// PMed activity only
	set OPT(2)=$$^MSG(4611)

	// Non-PMed activity only
	set OPT(3)=$$^MSG(4610)

	quit

VRPOST	// Report post-processor

	type Public String %BLK,%NOBANNER
	type Public String IO,IOTYP
	set %BLK="/,"_IO

	if '($D(%NOBANNER)!(IOTYP="TRM")!('CUVAR.banner)) write #

	// Transaction Summary Report
	do rwopt^DBSRWUTL("LINKRPT","SCA464")

	// kill temp table
	do Db.fastDelete("DMJTMP","JOB=:%ProcessID")

	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc5,vovc5,vc6,vovc6,vc4,vovc4,vc7,vovc7
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc5'=vc5) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(+vovc6'=+vc6) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(+vovc4'=+vc4) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

