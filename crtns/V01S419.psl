VTAB(RecordLN LN,RecordCIF CIF)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=10,VPT=1,VPB=17,PGM=$T(+0),DLIB="SYSDEV",DFID="LN,CIF"
 set OLNTB=17025
 
 set VFSN("CIF")="zCIF",VFSN("LN")="zLN"
 //
 // 
	
	
 set %TAB(1)=$C(0,21,12)_"20N12401|1|[LN]CID|[ACN]"
 set %TAB(2)=$C(1,21,27)_"21T|*NAM|[LN]NAM"
 set %TAB(3)=$C(7,24,1)_"00L12413|1|[LN]RFMO"
 set %TAB(4)=$C(7,63,1)_"00T12415|1|[LN]RRMO|[UTBLRRMO]"
 set %TAB(5)=$C(8,63,1)_"00L12414|1|[LN]RPMO"
 set %TAB(6)=$C(10,24,12)_"00T12402|1|[LN]EPPENMET|[UTBLLNPPN]"
 set %TAB(7)=$C(10,63,12)_"00T12409|1|[LN]UPPEN|[UTBLLNPPN]"
