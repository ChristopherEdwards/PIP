VRV(V,L) quit V_$J("",L-$L(V))
VREPRNT
 type Public RecordLN LN
 type Public RecordCIF CIF
 do VPR(.LN,.CIF)
 do VDA1(.LN,.CIF)
 do ^DBSPNT()
 quit

VW(RecordLN LN,RecordCIF CIF)
 do VDA1(.LN,.CIF)
 do ^DBSPNT(10)
