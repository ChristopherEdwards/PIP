 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'LN.exists() set LN = Class.new("RecordLN")
  if 'CIF.exists() set CIF = Class.new("RecordCIF")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNROLLEFD2",VPGM=$T(+0),VSNAME="EFD Interest Rate Maintenance "
 set VFSN("CIF")="zCIF",VFSN("LN")="zLN"
 set vPSL=1
 set KEYS(1)=LN.CID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.LN,.CIF),VDA1(.LN,.CIF),^DBSPNT() quit

 if '%ProcessMode do VNEW(.LN,.CIF),VPR(.LN,.CIF),VDA1(.LN,.CIF)
 if %ProcessMode do VLOD(.LN,.CIF) quit:$G(ER)  do VPR(.LN,.CIF),VDA1(.LN,.CIF)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.LN,.CIF)
	quit


VNEW(RecordLN LN,RecordCIF CIF) // Initialize arrays if %O=0
 
 do VDEF(.LN,.CIF)
 do VLOD(.LN,.CIF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordLN LN,RecordCIF CIF)
 quit:LN.CID.isNull() 
 quit:%ProcessMode  set ER=0 if (LN.CID="") set ER=1,RM=$$^MSG(1767,"CID") Q
 if Db.isDefined("LN","LN.CID=:LN.CID") S ER=1,RM=$$^MSG(2327) Q
 do LN.setAuditFlag(1)
 if LN.AOMCODE="" set LN.AOMCODE=0
