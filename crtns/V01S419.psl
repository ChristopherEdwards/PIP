 set VO="25|14|13|"
 set VO(14)=$C(1,22,12,2,0,0,0,0,0,0)_"01N"_LN.CID
 set VO(15)=$C(1,51,0,0,0,0,0,0,0,0)_"01T"_"Effective: "_$S(%ProcessMode=5:"",1:$$DAT^%ZM(EFD))
 set VO(16)=$C(2,22,27,2,0,0,0,0,0,0)_"01T"_$E(LN.NAM,1,27)
 set VO(17)=$C(2,53,0,0,0,0,0,0,0,0)_"01T"_"Through: "_$S(%ProcessMode=5:"",1:$$DAT^%ZM(EJD))
