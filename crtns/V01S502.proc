 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="NOSTTRF2",VPGM=$T(+0),VSNAME="Nostro Transfer Screen 2"
 set VFSN("NOSTTRF")="zfNOSTTRF"
 set vPSL=1
 set KEYS(1)=fNOSTTRF.TRREFNO
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fNOSTTRF),VDA1(.fNOSTTRF),^DBSPNT()

 if '%ProcessMode do VNEW(.fNOSTTRF),VPR(.fNOSTTRF),VDA1(.fNOSTTRF)
 if %ProcessMode do VLOD(.fNOSTTRF) quit:$G(ER)  do VPR(.fNOSTTRF),VDA1(.fNOSTTRF)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fNOSTTRF)
	quit


VNEW(RecordNOSTTRF fNOSTTRF) // Initialize arrays if %O=0
 
 do VDEF(.fNOSTTRF)
 do VLOD(.fNOSTTRF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
