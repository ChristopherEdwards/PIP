	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String LNAM,TYPE,ZZRID
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA658A"
	set RN="Inv Sweep Src Acct Div Proof - Inv Prod"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String LNAM,TYPE,ZZRID
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,RID,RN,RPNAME,STYPE,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vovc1,vovc2,vovc3,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA658A",RN="Inv Sweep Src Acct Div Proof - Inv Prod"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA658A","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[DIV1099]ITYPE#66#4,[DIV1099]DES#74#14,[DIV1099]IPY#101#15,[DIV1099]BWP#119#12"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()



		set VBNRINFO("DESC")="Inv Sweep Src Acct Div Proof - Inv Prod"
		set VBNRINFO("PGM")="R01S585"
		set VBNRINFO("RID")="SCA658A"
		set VBNRINFO("TABLES")="DIV1099"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,DIV1099]PID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,DIV1099]PTYPE"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DIV1099]ITYPE"

		set VBNRINFO("DOC",1)=" Totals by Investment Product Type (SCA658A)."
		set VBNRINFO("DOC",2)=" "
		set VBNRINFO("DOC",3)=" This is Investment Product Type subtotals portion of SCA658, SCA569, SCA660"
		set VBNRINFO("DOC",4)=" and SCA661 reports."
		set VBNRINFO("DOC",5)=" The accounts displayed are grouped by their Investment Product Type."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:3 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:3 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("DIV1099.PID,DIV1099.PTYPE,DIV1099.ITYPE,DIV1099.DES,DIV1099.IPY,DIV1099.BWP,DIV1099.CNT","DIV1099","","DIV1099.PID,DIV1099.PTYPE,DIV1099.ITYPE","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7
	set vc1=V.piece($C(9),1)			// DIV1099.PID
	set vc2=V.piece($C(9),2)			// DIV1099.PTYPE
	set vc3=V.piece($C(9),3)			// DIV1099.ITYPE
	set vc4=V.piece($C(9),4)			// DIV1099.DES
	set vc5=V.piece($C(9),5)			// DIV1099.IPY
	set vc6=V.piece($C(9),6)			// DIV1099.BWP
	set vc7=V.piece($C(9),7)			// DIV1099.CNT
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

 type public String LNAM,RPNAME
	quit

VPREBQ	// Pre-processor (before query)

 //Incoming=LNAM,TYPE,ZZRID
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(2)=1,vskp(3)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG3 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(3)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3
	set (vb1,vb2,vb3)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,VH0=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc6,vc7,vc5
	type Public String VSTATS

	set VT(3)=VT(3)+1
	set VT(3,1,1)=VT(3,1,1)+vc6			// @TOT([DIV1099]BWP)
	set VT(3,2,1)=VT(3,2,1)+vc7			// @TOT([DIV1099]CNT)
	set VT(3,3,1)=VT(3,3,1)+vc5			// @TOT([DIV1099]IPY)
	quit

VHDG3	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,LNAM,RID,RN,RPNAME,STYPE,TYPE,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,ZZRID,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vcrt,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	quit:vh(3)  set vh(3)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="          "_"Subtotals By Investments:   "
	do VOM
	quit

VDTL3	// Detail
	type public String %TIM,IOSL,LNAM,RPNAME,STYPE,TYPE,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),ZZRID,vc1,vc2,vc3,vc4,vc5,vc6,vc7,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(3)=0 do VHDG3 quit:VFMQ

	set VL="                                       "_"Investment Product Type"
	set VL=VL_$J("",65-VL.length())_$J(vc3,4)
	set VL=VL_$J("",73-VL.length())_$E(vc4,1,14)
	set VL=VL_$J("",100-VL.length())_$J($FN(vc5,",",2),15)
	set V=vc6,VO=V set V=$J($FN(V,",",2),12) do VP1 quit:VFMQ!verror.get()
	set VL=VL_$J("",118-VL.length())_V		// [SYSDEV,DIV1099]BWP
	do VOM
	quit

VSUM3	// Summary
	type public String %TIM,I,IOSL,LNAM,RPNAME,STYPE,TYPE,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),ZZRID,vc1,vc2,vc3,vc4,vc5,vc6,vc7,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+0>IOSL do VHDG0 quit:VFMQ  set vh(3)=0 do VHDG3 quit:VFMQ

	set VL="          "_"Product Totals -             Product Type  "
	set VL=VL_$J("",65-VL.length())_$J(STYPE,4)
	set V=VT(3,2,1)					// @TOT([DIV1099]CNT,)
	set VL=VL_$J("",73-VL.length())_$J(V,9)
	set VL=VL_$J("",83-VL.length())_"Accounts  "
	set V=VT(3,3,1)					// @TOT([DIV1099]IPY,)
	set VL=VL_$J("",100-VL.length())_$J($FN(V,",",2),15)
	set V=VT(3,1,1)					// @TOT([DIV1099]BWP,)
	set V=$J($FN(V,",",2),12) do VP2 quit:VFMQ!verror.get()
	set VL=VL_$J("",118-VL.length())_V		// @TOT([DIV1099]BWP,)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,LNAM,RID,RN,RPNAME,STYPE,TYPE,VL,ZZRID,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+6,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	do VP3 quit:VFMQ!verror.get()  set V=$E(RPNAME,1,95) set VL=V
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	do VP4 quit:VFMQ!verror.get()  set V=$J(VPN,3)
	set VL=VL_$J("",128-VL.length())_V
	do VOM
	set VL="" do VOM
	set VL="                                                            "_"     Invest   Fund                             Interest    Backup"
	do VOM
	set VL="                                                                 "_"Type     Name                             Paid        Withholding"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,DIV1099]BWP

	type public String vc2
 type public Number STYPE

 set STYPE=vc2
	quit

VP2	// Column post-processor - @TOT([DIV1099]BWP,,E,12)

 type public Number STYPE

 type RecordCD1099 cd1099=Db.getRecord("CD1099","PID=:%ProcessID,TYPE=:STYPE",1)
 type ResultSet rs=Db.select("IPY,BWP,CNT","DIV1099","PID=:%ProcessID AND PTYPE=:STYPE")
 while rs.next() do {
        // calculate total Divident Paid
        set cd1099.ipy=cd1099.ipy+rs.getCol("IPY")
        
        //calculate total Backup Withholding
        set cd1099.bwp=cd1099.bwp+rs.getCol("BWP")
        
        //calculate total accounts
        set cd1099.cnt=cd1099.cnt+rs.getCol("CNT")
        do cd1099.save()
        }
	quit

VP3	// Column pre-processor - Variable: RPNAME

 type public String LNAM,RPNAME,ZZRID 

 set RPNAME=LNAM_"  ("_ZZRID_")"
	quit

VP4	// Column pre-processor - Variable: VPN

 type public String OLDPAGE,VPN
 set VPN=OLDPAGE+1
 set OLDPAGE=VPN
	quit
