	set VL=VL_$J("",11-VL.length())_"TOTALS:"
	set V=VT(3,4,1)					// @TOT([LN]IPTY,)
	set VL=VL_$J("",50-VL.length())_$J(V,12,2)
	set V=VT(3,1,1)					// @TOT(<<TAX>>,)
	set VL=VL_$J("",66-VL.length())_$J(V,12,2)
	set V=VT(3,2,1)					// @TOT([LN]ADBPTY,)
	set VL=VL_$J("",85-VL.length())_$J(V,12,2)
	set V=VT(3,3,1)					// @TOT([LN]BALPTYE,)
	set VL=VL_$J("",101-VL.length())_$J(V,12,2)
	set V=VT(3,5,1)					// @TOT([LN]PENPTY,)
	set VL=VL_$J("",117-VL.length())_$J(V,12,2)
	do VOM
	set VL="" do VOM
	quit

VDTL4	// Detail
	type public String %TIM,IOSL,STGRP,STTYPE,TAX,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc1,12)
