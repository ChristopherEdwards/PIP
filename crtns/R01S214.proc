VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,STGRP,STTYPE,TAX,VL,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+8,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  (SCA160)"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="GROUP "
	set V=vc3,VO=V set V=$E(V,1,4) do VP1 quit:VFMQ!verror.get()
	set VL=VL_$J("",6-VL.length())_V		// [SYSDEV,LN]GRP
	set VL=VL_$J("",12-VL.length())_"TYPE "
	set V=vc4,VO=V set V=$J(V,3) do VP2 quit:VFMQ!verror.get()
	set VL=VL_$J("",18-VL.length())_V		// [SYSDEV,LN]TYPE
	do VOM
	set VL="" do VOM
	set VL="     "_"ACCOUNT    BORROWER                              INTEREST    TAX REMITTED    AVERAGE BALANCE     Y/E BALANCE         PENALTY"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,STGRP,STTYPE,TAX,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                                  "_"------------    ------------       ------------    ------------    ------------"
	do VOM
	set VL="REPORT TOTALS:"
	set V=0						// @TOT([LN]IPTY,0)
	for I=0:1:4 set V=V+VT(I,4,1)
	set VL=VL_$J("",50-VL.length())_$J(V,12,2)
	set V=0						// @TOT(<<TAX>>,0)
	for I=0:1:4 set V=V+VT(I,1,1)
	set VL=VL_$J("",66-VL.length())_$J(V,12,2)
	set V=0						// @TOT([LN]ADBPTY,0)
	for I=0:1:4 set V=V+VT(I,2,1)
	set VL=VL_$J("",85-VL.length())_$J(V,12,2)
	set V=0						// @TOT([LN]BALPTYE,0)
	for I=0:1:4 set V=V+VT(I,3,1)
	set VL=VL_$J("",101-VL.length())_$J(V,12,2)
	set V=0						// @TOT([LN]PENPTY,0)
	for I=0:1:4 set V=V+VT(I,5,1)
	set VL=VL_$J("",117-VL.length())_$J(V,12,2)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,LN]GRP

	type public String vc3
 type public String STGRP

 set STGRP = vc3
	quit

VP2	// Column post-processor - [SYSDEV,LN]TYPE

	type public String vc4
 type public Number STTYPE

 set STTYPE = vc4
	quit
