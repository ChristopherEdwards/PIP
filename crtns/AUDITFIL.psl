	set X = sysaudit.sjd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("SJD", vRM) quit
	set X = sysaudit.brcd if 'X.isNull(),'Db.isDefined("UTBLBRCD","X") set vRM = $$^MSG(1485,X) do vdderr("BRCD", vRM) quit
