 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=14,VPT=1,VPB=17,PGM=$T(+0),DLIB="SYSDEV",DFID="CUVAR"
 set OLNTB=17028
 
 set VFSN("CUVAR")="zfCUVAR"
 //
 // 
	
	
 set %TAB(1)=$C(2,27,1)_"01N12405|1|[CUVAR]TAXREQ|[STBLTAXREQ]"
 set %TAB(2)=$C(2,64,1)_"00L12423|1|[CUVAR]DUPTIN"
 set %TAB(3)=$C(3,27,4)_"01N12404|1|[CUVAR]CIFPRGD"
 set %TAB(4)=$C(3,64,1)_"00L12424|1|[CUVAR]TINCO"
