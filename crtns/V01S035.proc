 quit

VP2(RecordCUVAR fCUVAR) //
 set GRP="CIF"
 quit

VP3(RecordCUVAR fCUVAR) //
 if 'X do { quit
 	do UNPROT^DBSMACRO("CUVAR.ORCIFN")
	do UNPROT^DBSMACRO("CUVAR.MINCIFL")
	do UNPROT^DBSMACRO("CUVAR.MAXCIFL")
	}
 
 do DEFAULT^DBSMACRO("CUVAR.ORCIFN",0,"1","0","0")
 do PROTECT^DBSMACRO("CUVAR.ORCIFN")
 do PROTECT^DBSMACRO("CUVAR.MINCIFL")
 do PROTECT^DBSMACRO("CUVAR.MAXCIFL")
 
