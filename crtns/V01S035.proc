 new V
 //
 set VO="31|18|13|0"
 set VO(18)=$C(3,28,1,2,0,0,0,0,0,0)_"00N"_fCUVAR.TAXREQ
 set VO(19)=$C(3,65,1,2,0,0,0,0,0,0)_"00L"_$S(fCUVAR.DUPTIN:"Y",1:"N")
