 set VO(0)="|0"
 set VO(1)=$C(1,32,17,1,0,0,0,0,0,0)_"01T CIF Information "
 set VO(2)=$C(3,11,16,1,0,0,0,0,0,0)_"01T TIN/SIN Option:"
