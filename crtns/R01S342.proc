R01S342	// SCA353 - ARS Check Register Report (Queued)
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:48 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA353"
	set RN="ARS Check Register Report (Queued)"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,DES,RID,RN,VAL,VAL1,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA353",RN="ARS Check Register Report (Queued)"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA353","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[ARS]CKNO#16#12,[ARS]IACTDT#31#10,[ARS]IAMT#43#12,[ARS]PACTDT#57#10,[ARS]PNAME#91#20"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE=""
		set VWHERE=VWHERE_"(ARSCID.ARSLDT+ARSCID.ARSOFST+1)=:%SystemDate AND "
		set VWHERE=VWHERE_"ARSH.ACTDT>ARSCID.ARSPDT AND ARSH.ACTDT NOT >ARSCI"
		set VWHERE=VWHERE_"D.ARSLDT"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="ARS Check Register Report (Queued)"
		set VBNRINFO("PGM")="R01S342"
		set VBNRINFO("RID")="SCA353"
		set VBNRINFO("TABLES")="ARSH,ARS,DEP,ARSCID"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,ARSH]CID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,ARSH]ACTDT"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,ARSH]CKNO"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,ARSH]CSEQ"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:1 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("ARSH.CID,ARSH.ACTDT,ARSH.CKNO,ARSH.CSEQ,ARS.CID,DEP.TITLE1,DEP.BAL,DEP.TYPE,ARS.CKNO,ARS.IACTDT,ARS.IAMT,ARS.PACTDT,ARS.STAT,ARS.PNAME,ARSCID.CID,ARSCID.ARSLDT,ARSCID.ARSNDT","ARSH,ARS,DEP,ARSCID","(ARSCID.ARSLDT+ARSCID.ARSOFST+1)=:%SystemDate AND ARSH.ACTDT>ARSCID.ARSPDT AND ARSH.ACTDT NOT >ARSCID.ARSLDT","ARSH.CID,ARSH.ACTDT,ARSH.CKNO,ARSH.CSEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17
	set vc1=V.piece($C(9),1)			// ARSH.CID
	set vc2=V.piece($C(9),2)			// ARSH.ACTDT
	set vc3=V.piece($C(9),3)			// ARSH.CKNO
	set vc4=V.piece($C(9),4)			// ARSH.CSEQ
	set vc5=V.piece($C(9),5)			// ARS.CID
	set vc6=V.piece($C(9),6)			// DEP.TITLE1
	set vc7=V.piece($C(9),7)			// DEP.BAL
	set vc8=V.piece($C(9),8)			// DEP.TYPE
	set vc9=V.piece($C(9),9)			// ARS.CKNO
	set vc10=V.piece($C(9),10)			// ARS.IACTDT
	set vc11=V.piece($C(9),11)			// ARS.IAMT
	set vc12=V.piece($C(9),12)			// ARS.PACTDT
	set vc13=V.piece($C(9),13)			// ARS.STAT
	set vc14=V.piece($C(9),14)			// ARS.PNAME
	set vc15=V.piece($C(9),15)			// ARSCID.CID
	set vc16=V.piece($C(9),16)			// ARSCID.ARSLDT
	set vc17=V.piece($C(9),17)			// ARSCID.ARSNDT
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(1).get() do VDTL1 quit:VFMQ
	do VHDG2 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VDTL1	// Detail
	type public String %TIM,DES,IOSL,V,VAL,VAL1,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(1)  set VD(1)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc5,12)
	set VL=VL_$J("",14-VL.length())_$E(vc6,1,40)
	set VL=VL_$J("",55-VL.length())_$J($FN(vc7,",",2),13)
	set VL=VL_$J("",69-VL.length())_$J(vc8,5)
	do VP1 quit:VFMQ!verror.get()  set V=$E(DES,1,20)
	set VL=VL_$J("",76-VL.length())_V
	do VOM
	quit

VHDG2	// Group Header
	type public String %MSKD,%TIM,CONAM,DES,ER,IOSL,RID,RN,V,VAL,VAL1,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:vh(2)  set vh(2)=1				// Print flag
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="                 "_"Check Nbr    Issue Date        Amount  Paid Date   Status                Payee"
	do VOM
	set VL="                "_"-----------------------------------------------------------------------------------------------"
	do VOM
	quit

VDTL3	// Detail
	type public String %TIM,DES,IOSL,V,VAL,VAL1,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(3)  set VD(3)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="               "_$E(vc9,1,12)
	set VL=VL_$J("",30-VL.length())_$J($$DAT^%ZM(vc10),10)
	set VL=VL_$J("",41-VL.length())_$J($FN(vc11,",",2),13)
	set VL=VL_$J("",56-VL.length())_$J($$DAT^%ZM(vc12),10)
	set V=$$VP3(vc13)				// @TBL([ARS]STAT)
	set VL=VL_$J("",68-VL.length())_$E(V,1,20)
	set VL=VL_$J("",90-VL.length())_$E(vc14,1,20)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,DES,RID,RN,VAL,VAL1,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	do VP2 quit:VFMQ!verror.get()  set V="FROM DATE: "_VAL set VL=V
	set VL=VL_$J("",29-VL.length())_"To Date: "_VAL1
	do VOM
	set VL="" do VOM
	set VL="Account Nbr   Title                                          Balance  Type  Description"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: DES

	type public String vc8
 type public String DES
 type String XTYPE

 set XTYPE=vc8

 type RecordPRODCTL prodctl=Db.getRecord("PRODCTL","TYPE=:XTYPE",1)
 if prodctl.getMode() do {
	set DES=prodctl.des
	set DES=DES.extract(1,20)
 }

	quit

VP2	// Column pre-processor - FROM DATE: <<VAL>>

	type public String vc15,vc16,vc17
 type public Date VAL,VAL1
 type public String fARSCID

 set (VAL,VAL1)="" quit:(vc15="")

 set VAL=$$DAT^%ZM(vc16+1)
 set VAL1=$$DAT^%ZM(vc17)
	quit

VP3(KEY1)	// @TBL([ARS]STAT)

	if KEY1="" quit ""
	type Public Cache %CACHE()
	type RecordSTBLARSSTAT tbl=%CACHE("STBLARSSTAT").getRecord("STBLARSSTAT","STAT=:KEY1")
	quit tbl.DESC
