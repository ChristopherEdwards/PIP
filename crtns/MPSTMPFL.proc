vreqn // Validate required data items

	type public RecordMPSTMP mpstmp

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordMPSTMP mpstmp

	if mpstmp.co.isNull() do vreqerr("CO") quit
	if mpstmp.cardtyp.isNull() do vreqerr("CARDTYP") quit
	if mpstmp.acn.isNull() do vreqerr("ACN") quit
	if mpstmp.cid.isNull() do vreqerr("CID") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("MPSTMP","MSG",1767,"MPSTMP."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordMPSTMP mpstmp

	set X = mpstmp.co if 'X.isNull(),'Db.isDefined("UTBLCO","X") set vRM = $$^MSG(1485,X) do vdderr("CO", vRM) quit
	if mpstmp.cardtyp.length()>12 set vRM = $$^MSG(1076,12) do vdderr("CARDTYP", vRM) quit
	set X = mpstmp.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
	set X = mpstmp.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	set X = mpstmp.orderby if 'X.isNull(),X'?1.2N,X'?1"-"1.1N set vRM=$$^MSG(742,"N") do vdderr("ORDERBY", vRM) quit
	set X = mpstmp.ordt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ORDT", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("MPSTMP","MSG",979,"MPSTMP."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordMPSTMP mpstmp

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("CO").exists() set vux("CO") = vx("CO")
	if vx("CARDTYP").exists() set vux("CARDTYP") = vx("CARDTYP")
	if vx("ACN").exists() set vux("ACN") = vx("ACN")
	if vx("CID").exists() set vux("CID") = vx("CID")
	do vkey(1) set voldkey = mpstmp.co_","_mpstmp.cardtyp_","_mpstmp.acn_","_mpstmp.cid // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = mpstmp.co_","_mpstmp.cardtyp_","_mpstmp.acn_","_mpstmp.cid // Copy new keys into object
	type RecordMPSTMP vnewrec = mpstmp.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("MPSTMP",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordMPSTMP mpstmp

	if vux("CO").exists() set mpstmp.co = vux("CO").piece("|",i)
	if vux("CARDTYP").exists() set mpstmp.cardtyp = vux("CARDTYP").piece("|",i)
	if vux("ACN").exists() set mpstmp.acn = vux("ACN").piece("|",i)
	if vux("CID").exists() set mpstmp.cid = vux("CID").piece("|",i)
	quit
