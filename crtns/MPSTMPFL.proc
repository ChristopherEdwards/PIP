vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordMPSTMP mpstmp

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("MPSTMP","CO = :mpstmp.co and CARDTYP = :mpstmp.cardtyp and ACN = :mpstmp.acn and CID = :mpstmp.cid") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(mpstmp))  S ^MPSTMP(vobj(mpstmp,-3),vobj(mpstmp,-4),vobj(mpstmp,-5),vobj(mpstmp,-6))=vobj(mpstmp)
		#ENDBYPASS
	}

	quit

vdelete(Boolean vkeychg) // Record Delete

	type public String vobj(),vpar
	type public RecordMPSTMP mpstmp


	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	ZWI ^MPSTMP(vobj(mpstmp,-3),vobj(mpstmp,-4),vobj(mpstmp,-5),vobj(mpstmp,-6)) // Delete Record
