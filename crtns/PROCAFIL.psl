vrequ // Valid required columns on update


	type public String vx()
	type public RecordPROCESSACT processact

	if processact.pid.isNull() do vreqerr("PID") quit
	if processact.seqnum.isNull() do vreqerr("SEQNUM") quit

		if vx("ACTION").exists(),processact.action.isNull() do vreqerr("ACTION") quit
		if vx("ISSDATE").exists(),processact.issdate.isNull() do vreqerr("ISSDATE") quit
		if vx("ISSPID").exists(),processact.isspid.isNull() do vreqerr("ISSPID") quit
		if vx("ISSTIME").exists(),processact.isstime.isNull() do vreqerr("ISSTIME") quit
		if vx("ISSTLO").exists(),processact.isstlo.isNull() do vreqerr("ISSTLO") quit
		if vx("ISSUSRNM").exists(),processact.issusrnm.isNull() do vreqerr("ISSUSRNM") quit
		if vx("STATUS").exists(),processact.status.isNull() do vreqerr("STATUS") quit
	quit

