	if processact.issdate.isNull() do vreqerr("ISSDATE") quit
	if processact.isspid.isNull() do vreqerr("ISSPID") quit
	if processact.isstime.isNull() do vreqerr("ISSTIME") quit
	if processact.isstlo.isNull() do vreqerr("ISSTLO") quit
	if processact.issusrnm.isNull() do vreqerr("ISSUSRNM") quit
	if processact.status.isNull() do vreqerr("STATUS") quit
