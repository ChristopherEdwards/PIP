R01S427	// SCA464 - Miscellaneous Transaction Summary
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:49 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA464"
	set RN="Miscellaneous Transaction Summary"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,CRCD,CURRENCY,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA464",RN="Miscellaneous Transaction Summary"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA464","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[DMJTMP]ACN#1#12,[DMJTMP]DRAMT#19#14,[DMJTMP]DRCNT#35#7,[DMJTMP]CRAMT#46#14,[DMJTMP]CRCNT#62#7,[GLAD]DESC1#73#40"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE="DMJTMP.JOB=:%ProcessID"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="Miscellaneous Transaction Summary"
		set VBNRINFO("PGM")="R01S427"
		set VBNRINFO("RID")="SCA464"
		set VBNRINFO("TABLES")="DMJTMP,GLAD"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,DMJTMP]JOB"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,DMJTMP]CRCD"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DMJTMP]ACN"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:3 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:4 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("DMJTMP.JOB,DMJTMP.CRCD,DMJTMP.ACN,DMJTMP.DRAMT,DMJTMP.DRCNT,DMJTMP.CRAMT,DMJTMP.CRCNT,GLAD.DESC1","DMJTMP,GLAD","DMJTMP.JOB=:%ProcessID","DMJTMP.JOB,DMJTMP.CRCD,DMJTMP.ACN","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8
	set vc1=V.piece($C(9),1)			// DMJTMP.JOB
	set vc2=V.piece($C(9),2)			// DMJTMP.CRCD
	set vc3=V.piece($C(9),3)			// DMJTMP.ACN
	set vc4=V.piece($C(9),4)			// DMJTMP.DRAMT
	set vc5=V.piece($C(9),5)			// DMJTMP.DRCNT
	set vc6=V.piece($C(9),6)			// DMJTMP.CRAMT
	set vc7=V.piece($C(9),7)			// DMJTMP.CRCNT
	set vc8=V.piece($C(9),8)			// GLAD.DESC1
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

	do rwopt^DBSRWUTL("NOOPEN","")
	quit

VRPOST	// Report post-processor

	// Converted to PSL by CNV70RW
	do rwopt^DBSRWUTL("NOCLOSE","")
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(3)=1		// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(3)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3
	set (vb1,vb2,vb3)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,VH0=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc6,vc7,vc4,vc5
	type Public String VSTATS

	set VT(3)=VT(3)+1
	set VT(3,1,1)=VT(3,1,1)+vc6			// @TOT([DMJTMP]CRAMT)
	set VT(3,2,1)=VT(3,2,1)+vc7			// @TOT([DMJTMP]CRCNT)
	set VT(3,3,1)=VT(3,3,1)+vc4			// @TOT([DMJTMP]DRAMT)
	set VT(3,4,1)=VT(3,4,1)+vc5			// @TOT([DMJTMP]DRCNT)
	quit

VDTL3	// Detail
	type public String %TIM,CRCD,CURRENCY,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc3,12)
	set VL=VL_$J("",13-VL.length())_$J($FN(vc4,",",2),19)
	set V=vc5,VO=V do VP1 quit:VFMQ!verror.get()  set V=$J(V,8)
	set VL=VL_$J("",33-VL.length())_V		// [SYSDEV,DMJTMP]DRCNT
	set VL=VL_$J("",42-VL.length())_$J($FN(vc6,",",2),17)
	set V=vc7,VO=V do VP2 quit:VFMQ!verror.get()  set V=$J(V,8)
	set VL=VL_$J("",60-VL.length())_V		// [SYSDEV,DMJTMP]CRCNT
	set VL=VL_$J("",72-VL.length())_$E(vc8,1,40)
	do VOM
	quit

VSUM3	// Summary
	type public String %TIM,CRCD,CURRENCY,I,IOSL,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,verror,vh(),vovc1,vovc2,vovc3,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                  "_"-"
	set VL=VL_$J("",19-VL.length())_"-------------"
	set VL=VL_$J("",34-VL.length())_"-------"
	set VL=VL_$J("",45-VL.length())_"-"
	set VL=VL_$J("",46-VL.length())_"-------------"
	set VL=VL_$J("",61-VL.length())_"-------"
	do VOM
	set VL="   "_$E(vc2,1,3)
	set VL=VL_$J("",7-VL.length())_"Total"
	set V=0						// @TOT([DMJTMP]DRAMT,0)
	for I=0:1:3 set V=V+VT(I,3,1)
	set VL=VL_$J("",18-VL.length())_$J($FN(V,",",2),14)
	set V=0						// @TOT([DMJTMP]DRCNT,0)
	for I=0:1:3 set V=V+VT(I,4,1)
	set VL=VL_$J("",34-VL.length())_$J(V,7)
	set V=0						// @TOT([DMJTMP]CRAMT,0)
	for I=0:1:3 set V=V+VT(I,1,1)
	set VL=VL_$J("",45-VL.length())_$J($FN(V,",",2),14)
	set V=0						// @TOT([DMJTMP]CRCNT,0)
	for I=0:1:3 set V=V+VT(I,2,1)
	set VL=VL_$J("",61-VL.length())_$J(V,7)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,CRCD,CURRENCY,RID,RN,VL,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+5,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL="Transaction Summary Report - "
	do VP3 quit:VFMQ!verror.get()  set V=$E(CURRENCY,1,20)
	set VL=VL_$J("",29-VL.length())_V
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="     "_"Account              Debits    Count           Credits    Count    Description"
	do VOM
	set VL="=================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - [SYSDEV,DMJTMP]DRCNT

	/* Revision History
 
	   03/14/05 - SmithCD - CR 13782
	              Converted to PSL.
 
	*/

	type Public String V

	set V = +V
	quit

VP2	// Column pre-processor - [SYSDEV,DMJTMP]CRCNT

	/* Revision History
 
	   03/09/05 - SmithCD - CR 13782
	              Converted to PSL.

	*/

	type Public String V

	set V = +V
	quit

VP3	// Column pre-processor - Variable: CURRENCY

	type public String vc2
	/* Revision History

	   03/14/05 - SmithCD - CR 13782
		      Converted to PSL.

	*/

	type Public String CURRENCY

	type String CO, CRCD

	type RecordCUVAR cuvar = Db.getRecord("CUVAR")

	set CO = cuvar.co 
	set CRCD = vc2
	if CRCD.isNull() set CRCD = %SystemCurrency

	type RecordCRCD crcd = Db.getRecord("CRCD", "CO=:CO,CRCD=:CRCD")

	set CURRENCY = crcd.desc
	quit
