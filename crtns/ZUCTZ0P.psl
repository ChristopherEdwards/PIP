	// #####################################################################
	// Test function signatures, method with 0 parameters
	// #####################################################################

	// =====================================================================
public mtd0p()	// Validate TestClass.method0p()
	/* ---------------------------------------------------------------------
	Procedure that validates code relating to TestClass.method0p().
	Validation values (see ZUCT for meaning of Cn and Rn):
	C0/R0: "", "P0value"
	calls:
	* public mtd0p()		// test TestClass.method0p()
	* public mtd0p0()		// test C0.method0p()
	* public mtd0p1(R0)		// test R0.method0p()
	*/
	type Primitive R0

	do mtd0p0()
	for R0 = "", "P0value" do {
		do mtd0p1( R0)
	}
	quit

	// =====================================================================
public mtd0p0()
	// validate C0.method0p()
	//
	type Primitive rRes
	/* ================ begin commented out ================
	set rRes = ({TestClass}"").method0p()		// C0=""
	write "mtd0p0().E:""P0value"".method0p()="_rRes,!

	set rRes = ({TestClass}"P0value").method0p()	// C0="P0value"
	write "mtd0p0().V:""P0value"".method0p()="_rRes,!
	================ end commented out ================ */
	quit

	// =====================================================================
public mtd0p1( Primitive R0)
	// validate R0.method0p() -- 1 "combination"
	//
	type Primitive rRes

	/* ================ begin commented out ================
	set rRes = R0.method0p()
	write "mtd0p1(R0):<"_R0_">.method0p()="_rRes,!
	================ end commented out ================ */
	quit
