	// Journal File Definition (1)                 12/17/2004
	/*
		vpar      Runtime qualifiers:      /NOREQ/MECH=REFARR:R

		/[NO]CASDEL   - Cascade delete
		/[NO]FRMFILER - Called from another filer
		/[NO]INDEX    - Update Indexes
		/[NO]JOURNAL  - Journal update
		/[NO]TRIGAFT  - After update triggers
		/[NO]TRIGBEF  - Before update triggers
		/[NO]UPDATE   - Update primary table
		/[NO]VALDD    - Validate column values
		/[NO]VALFK    - Validate foreign keys
		/[NO]VALREQ   - Validate not null values
		/[NO]VALRI    - Validate transaction integrity
		/[NO]VALST    - Validate database state
	*/

	type public String verrors()

	type String vx(), vxins()                              // audit column array
	type Number %O = tiklacn.getMode()                     // Processing mode
	set vpar = vpar.get()                                  // Initialize vpar

	if %O = 0 do AUDIT^UCUTILN(tiklacn,.vxins(),1,"|")
	if %O = 1 do AUDIT^UCUTILN(tiklacn,.vx(),1,"|")

	if 'vparNorm.get() set vpar = $$initPar^UCUTILN(vpar)  // Run-time qualifiers

	// Define local variables for access keys for legacy triggers
	type String CID = tiklacn.cid
	type String TKS = tiklacn.tks

	if %O = 0 do { quit                                    // Create record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		if vpar["/VALDD/" do vddver                           // Check values
		do vexec
		if vpar["/TRIGAFT/" do VAI                            // After insert triggers
	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("CID").exists()!vx("TKS").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("TIKLACN",.vx)
		set %O = 1 do vexec
		if vpar["/TRIGAFT/" do VAU                            // After update triggers
	}

	if %O = 2 do { quit                                    // Verify record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		set vpar = $$setPar^UCUTILN(vpar,"NOJOURNAL/NOUPDATE")
		do vexec
		if vpar["/TRIGAFT/" do VAI                            // After insert triggers
	}

	if %O = 3 do { quit                                    // Delete record control block
		quit:'Db.isDefined("TIKLACN","CID = :tiklacn.cid and TKS = :tiklacn.tks") // No record exists
		do vdelete(0)
	}

	quit

vlegacy(Number %ProcessMode,String vpar) // Legacy interface (^DBSDF9 for %O=0, EXT^DBSFILER for %O = 2)

	type public String CID
	type public String TKS

	type RecordTIKLACN tiklacn = Db.getRecord("TIKLACN","CID=:CID,TKS=:TKS")
	if (%ProcessMode = 2) do {
		do tiklacn.setMode(2)

		do TIKLACNF(tiklacn,vpar)
	}
	else  do VINDEX(tiklacn)

	quit

vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordTIKLACN tiklacn

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("TIKLACN","CID = :tiklacn.cid and TKS = :tiklacn.tks") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))
	if vpar["/VALFK/" do CHKFKS                            // Check foreign keys
	if vpar["/VALRI/" do VFKEYS                            // Foreign key definition

	if vpar'["/NOUPDATE/" do {

		set tiklacn.LDU = %SystemDate
		set tiklacn.UID = %UserID
		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(tiklacn)) S ^TIKL("ACN",vobj(tiklacn,-3),vobj(tiklacn,-4))=vobj(tiklacn)
		#ENDBYPASS

		if vpar["/JOURNAL/" do VJOURNAL(.tiklacn)             // Create journal files
	}

	if vpar["/INDEX/",'(%O = 1)!'vx("").order().isNull() do VINDEX(.tiklacn) // Update Index files

	quit

vdelete(Boolean vkeychg) // Record Delete

	type public String vobj(),vpar
	type public RecordTIKLACN tiklacn

	if vpar["/INDEX/" do VINDEX(.tiklacn)                  // Delete index entries
	if vpar["/JOURNAL/" do VJOURNAL(.tiklacn)              // Create journal entries

	// Allow global reference - Delete record
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	ZWI ^TIKL("ACN",vobj(tiklacn,-3),vobj(tiklacn,-4))
	#ENDBYPASS
	quit


vreqn // Validate required data items

	type public RecordTIKLACN tiklacn

	if tiklacn.arnd.isNull() do vreqerr("ARND") quit
	if tiklacn.rrc.isNull() do vreqerr("RRC") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordTIKLACN tiklacn

	if tiklacn.cid.isNull() do vreqerr("CID") quit
	if tiklacn.tks.isNull() do vreqerr("TKS") quit

		if vx("ARND").exists(),tiklacn.arnd.isNull() do vreqerr("ARND") quit
		if vx("RRC").exists(),tiklacn.rrc.isNull() do vreqerr("RRC") quit
	quit

