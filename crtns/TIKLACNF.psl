vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordTIKLACN tiklacn

	set X = tiklacn.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	set X = tiklacn.tks if 'X.isNull(),X'?1.6N,X'?1"-"1.5N set vRM=$$^MSG(742,"N") do vdderr("TKS", vRM) quit
	set X = tiklacn.arf if 'X.isNull() set vRM = "" do DBSEDT^UFRE("[TIKLACN]ARF",0) if 'vRM.get().isNull() do vdderr("ARF", vRM) quit
	set X = tiklacn.arld if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ARLD", vRM) quit
	set X = tiklacn.arnd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ARND", vRM) quit
	set X = tiklacn.expd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("EXPD", vRM) quit
	if tiklacn.ft1.length()>30 set vRM = $$^MSG(1076,30) do vdderr("FT1", vRM) quit
	if tiklacn.ft2.length()>30 set vRM = $$^MSG(1076,30) do vdderr("FT2", vRM) quit
	if tiklacn.ft3.length()>30 set vRM = $$^MSG(1076,30) do vdderr("FT3", vRM) quit
	set X = tiklacn.ldu if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("LDU", vRM) quit
	if tiklacn.rrc.length()>3 set vRM = $$^MSG(1076,3) do vdderr("RRC", vRM) quit
	if tiklacn.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

