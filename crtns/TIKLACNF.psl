vai1 // Trigger AFTER_INSERT - After Insert

	type public RecordTIKLACN tiklacn
	/*
	Revision History
	
	04/20/05 - Balaji Gopalakrishnan - CR 15268
		   Modified the code TIKL to TIKLACN in setErrXBAD.   	 

	01/12/04 - RussellDS - CR 7514
		   Moved from table TIKL (obsoleted) to here

	09/17/99 - CHOK - 31126
		   Converted to PSL
	*/
	
	// Next tickler date in the past
	if (tiklacn.expd>%SystemDate!(tiklacn.expd=""))&(tiklacn.arnd<%SystemDate) do Runtime.setErrXBAD("TIKLACN",83) quit:ER

	quit

