VJOURNAL(RecordTIKLACN tiklacn)	//TIKLACN Journal file entries

	type Public Date %EffectiveDate
	type Public String %TSRC,vpar,vx()
	type String TSRC,vdi,vdx()

	if %TSRC.get().isNull() set TSRC="O"
	else  set TSRC=%TSRC

	if TSRC="B" do {
		do vj1(.tiklacn)	// Mode=I,U,D Tran=B EFD=N,E Seq=1 JRNID=ACN_HIST
		}
	else  if TSRC="O" do {
		do vj1(.tiklacn)	// Mode=I,U,D Tran=O EFD=N,E Seq=1 JRNID=ACN_HIST
		}

	quit


vj1(RecordTIKLACN tiklacn)	// ACN_HIST  Table HIST  Account History

	type Public String EFD,%IDENT,%O,%UID,TJD,TLO
	type String v1,vlastkey
	set v1=tiklacn.cid
	set vlastkey=Db.nextVal("HIST","CID=:v1")
	type RecordHIST hist=Db.getRecord("HIST","CID=:v1,TSEQ=:vlastkey",1)
	set hist.cdt=+$H
	set hist.efd=$G(EFD)
	set hist.ident=%IDENT
	set hist.tcmt=$$^MSG($S(%O=0:5764,%O=1:5765,%O=3:5766),tiklacn.tks)
	set hist.time=$P($H,",",2)
	set hist.tjd=TJD
	set hist.tlo=TLO
	set hist.uid=%UID

	do hist.save("/NOVALFK/NOVALDD/NOVALRI")

	quit



public VINDEX(RecordTIKLACN tiklacn) // Update index entries

		type Public String vx()

	if %ProcessMode=1 do { quit
		if vx("ARND").exists() do vi1(.tiklacn)
	}
	do vi1(.tiklacn)

	quit
	

vi1(RecordTIKLACN tiklacn) // Maintain ARND index entries (Next Occurrence (Account))

	type Public String vx()
	type Boolean vdelete = 0
	type String v1 = tiklacn.arnd
	if v1.isNull() set v1=(PSL.maxCharValue-1).char()
	type String v3 = tiklacn.cid
	type String v4 = tiklacn.tks

	if %ProcessMode=2 do { quit

		// Allow global reference
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
		#BYPASS
		if $D(^TIKL("ACN",vobj(tiklacn,-3),vobj(tiklacn,-4)))#2,'$D(^DAYEND(v1,"ARFACN",v3,v4)) do vidxerr("ARND")
		#ENDBYPASS
		}

	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	if %O<2 set ^DAYEND(v1,"ARFACN",v3,v4)="" 
	#ENDBYPASS
	quit:%ProcessMode=0

	if %ProcessMode=3 set vdelete=1
	if vx("ARND").exists() set v1=vx("ARND").piece("|",1) set:v1.isNull() v1=(PSL.maxCharValue-1).char()

	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	kill ^DAYEND(v1,"ARFACN",v3,v4)
	#ENDBYPASS
	quit

public VIDXBLD(List vlist) // Rebuild index files (External call)


	type Number %ProcessMode=0                             // Create mode
	type Number i

	if vlist.get().isNull() set vlist="VINDEX"             // Build all

	type DbSet ds=Db.selectDbSet("TIKLACN")

	while ds.next() do {
		type RecordTIKLACN tiklacn=ds.getRecord("TIKLACN")
		if vlist.contains("VINDEX") do VINDEX(.tiklacn) quit
		if vlist.contains("ARND") do vi1(.tiklacn)
	}

	quit


public VIDXBLD1(RecordTIKLACN tiklacn, List vlist) // Rebuild index files for one record (External call)


	type Number i

	if vlist.contains("VINDEX") do VINDEX(.tiklacn) quit
	if vlist.contains("ARND") do vi1(.tiklacn)

	quit


vidxerr(di) // Error message

	D SETERR^DBSEXECU("TIKLACN","MSG",1225,"TIKLACN."_di)

	quit


vkchged // Access key changed

	type public RecordTIKLACN tiklacn

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux
	type String voldpar = vpar.get()                       // Save filer switches

	if vx("CID").exists() set vux("CID") = vx("CID")
	if vx("TKS").exists() set vux("TKS") = vx("TKS")
	do vkey(1) set voldkey = tiklacn.cid_","_tiklacn.tks   // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = tiklacn.cid_","_tiklacn.tks   // Copy new keys into object
	type RecordTIKLACN vnewrec = tiklacn.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("TIKLACN",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update
	set vpar = voldpar
	if vpar["/TRIGAFT/" do VAU

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordTIKLACN tiklacn

	if vux("CID").exists() set tiklacn.cid = vux("CID").piece("|",i)
	if vux("TKS").exists() set tiklacn.tks = vux("TKS").piece("|",i)
	quit

VIDXPGM()	quit "TIKLACNF"	// Location of index program

