vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLAUDIT utblaudit

	if utblaudit.key.length()>15 set vRM = $$^MSG(1076,15) do vdderr("KEY", vRM) quit
	if utblaudit.addr.length()>40 set vRM = $$^MSG(1076,40) do vdderr("ADDR", vRM) quit
	if utblaudit.addr2.length()>40 set vRM = $$^MSG(1076,40) do vdderr("ADDR2", vRM) quit
	if utblaudit.audcon.length()>40 set vRM = $$^MSG(1076,40) do vdderr("AUDCON", vRM) quit
	if utblaudit.audphon.length()>12 set vRM = $$^MSG(1076,12) do vdderr("AUDPHON", vRM) quit
	if utblaudit.city.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CITY", vRM) quit
	set X = utblaudit.cntry if 'X.isNull(),'Db.isDefined("STBLCNTRY","X") set vRM = $$^MSG(1485,X) do vdderr("CNTRY", vRM) quit
	if utblaudit.mzip.length()>10 set vRM = $$^MSG(1076,10) do vdderr("MZIP", vRM) quit
	if utblaudit.name.length()>40 set vRM = $$^MSG(1076,40) do vdderr("NAME", vRM) quit
