vkchged // Access key changed

	type public RecordACHINCDA achincda

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("FILEID").exists() set vux("FILEID") = vx("FILEID")
	if vx("BCHSEQ").exists() set vux("BCHSEQ") = vx("BCHSEQ")
	if vx("DTLSEQ").exists() set vux("DTLSEQ") = vx("DTLSEQ")
	if vx("ASEQ").exists() set vux("ASEQ") = vx("ASEQ")
	do vkey(1) set voldkey = achincda.fileid_","_achincda.bchseq_","_achincda.dtlseq_","_achincda.aseq // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = achincda.fileid_","_achincda.bchseq_","_achincda.dtlseq_","_achincda.aseq // Copy new keys into object
	type RecordACHINCDA vnewrec = achincda.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("ACHINCDA",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordACHINCDA achincda

	if vux("FILEID").exists() set achincda.fileid = vux("FILEID").piece("|",i)
	if vux("BCHSEQ").exists() set achincda.bchseq = vux("BCHSEQ").piece("|",i)
	if vux("DTLSEQ").exists() set achincda.dtlseq = vux("DTLSEQ").piece("|",i)
	if vux("ASEQ").exists() set achincda.aseq = vux("ASEQ").piece("|",i)
	quit
