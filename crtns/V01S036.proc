VPO1(RecordCUVAR fCUVAR)  // user library CUVAR_GLACN
 /* 04/18/05 - SWARNALATHA PONMAR RUPAKUMAR - 13665
    Modified the code to pass the third parameter in to the DB.getRecord()
    method, so that the error will not be thrown if the record does not
    exists.
 */
 type Public String X
 if X.isNull() quit
 if X?.E1L.E do Runtime.setErrMSG("CUVAR", 1663) quit
 //Debit/credit indicator
 type RecordTRN trn = Db.getRecord("TRN", "ETC=:X", 1)
 if trn.itc.isNull() do Runtime.setErrMSG("CUVAR", 1498) quit
 //Trancode is not credit
 if 'trn.itc.extract(1) do Runtime.setErrMSG("CUVAR", 2695)		
 quit
 #ACCEPT DATE=11/05/03;pgm=screen compiler
 quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
VP2(RecordCUVAR fCUVAR) //
 do VPO1(.fCUVAR)
 quit
VP3(RecordCUVAR fCUVAR) //
 do VPO1(.fCUVAR)
 quit
