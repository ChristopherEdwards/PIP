 set %MAX=15,VPT=1,VPB=21,PGM=$T(+0),DLIB="SYSDEV",DFID="QUEUEJ"
 set OLNTB=21028
 
 set VFSN("QUEUEJ")="zfQUEUEJ"
 //
 // 
	
	
 set %TAB(1)=$C(2,17,0)_"21T|*BCHNUM|[*]@HBCHNUM1"
 set %TAB(2)=$C(2,48,0)_"21T|*JOBNUM|[*]@HJOBNUM1"
 set %TAB(3)=$C(3,17,20)_"00T12402|1|[QUEUEJ]FUN|[SCATBL]DESC,QUEUE||do VP1^V00S438(.fQUEUEJ)|do VP2^V00S438(.fQUEUEJ)"
 set %TAB(4)=$C(6,17,50)_"00T12401|1|[QUEUEJ]DESC||||do VP3^V00S438(.fQUEUEJ)"
 set %TAB(5)=$C(8,48,8)_"00N12410|1|[QUEUEJ]RETI||||do VP4^V00S438(.fQUEUEJ)"
 set %TAB(6)=$C(9,48,2)_"00N12411|1|[QUEUEJ]MAXR|||do VP5^V00S438(.fQUEUEJ)|do VP6^V00S438(.fQUEUEJ)"
