 set %TAB(12)=$C(18,24,11)_"21C12404|1|[QUEUEJ]STIM||||||43200"
 set %TAB(13)=$C(19,24,11)_"21C12405|1|[QUEUEJ]CTIM||||||86400"
 set %TAB(14)=$C(20,24,1)_"21N12407|1|[QUEUEJ]LSTAT"
 set %TAB(15)=$C(20,27,0)_"21T|*COMPMSG|[*]@COMPMSG"
 do VTBL(.fQUEUEJ),VDEPRE(.fQUEUEJ) if $G(ER) quit
