 quit
VP7(RecordQUEUEJ fQUEUEJ) //
 if '$G(BATCH) do CHANGE^DBSMACRO("REQ","")

 // CONTINUE WITH NEXT JOB
 set AOF(0)=$$^MSG(6177) 
 set AOF(1)=$$^MSG(6178)
 set I(3)="AOF("     

 quit
VP8(RecordQUEUEJ fQUEUEJ) //
 if (X?.N)!(X="") quit

 // Enter an integer, or 'UNL' for unlimited bypasses 
 if X'="UNL" set ER=1,RM=$$^MSG(931) 

 quit 
