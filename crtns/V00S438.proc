 //
 set VO="32|18|13|"
 set VO(18)=$C(3,18,0,2,0,0,0,0,0,0)_"01T"_$G(BCHNUM)
 set VO(19)=$C(3,49,0,2,0,0,0,0,0,0)_"01T"_$G(JOBNUM)
 set VO(20)=$C(4,18,20,2,0,0,0,0,0,0)_"00T"_$E(fQUEUEJ.FUN,1,20)
 set VO(21)=$C(7,18,50,2,0,0,0,0,0,0)_"00T"_$E(fQUEUEJ.DESC,1,50)
