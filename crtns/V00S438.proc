VDEPRE(RecordQUEUEJ fQUEUEJ)  // Data Entry Pre-processor
 
 if '$G(BATCH) quit 

 do PROTECT^DBSMACRO("QUEUEJ.FUN")

 do PROTECT^DBSMACRO("QUEUEJ.DESC")

 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


  //user-defined post procs
 //
VP1(RecordQUEUEJ fQUEUEJ) //
 if X="" quit 

 do DESC^QUEMANT

 if $G(ER) quit 

 type RecordSCATBL scatbl=Db.getRecord("SCATBL","FN=:X")
 if fQUEUEJ.desc="" D DEFAULT^DBSMACRO("QUEUEJ.DESC",scatbl.desc,"1","0","0")

 quit
VP2(RecordQUEUEJ fQUEUEJ) //
 if '$G(BATCH) do CHANGE^DBSMACRO("REQ","")
 quit
VP3(RecordQUEUEJ fQUEUEJ) //
 if '$G(BATCH) do CHANGE^DBSMACRO("REQ","")
 quit
VP4(RecordQUEUEJ fQUEUEJ) //
 if '$G(BATCH) do CHANGE^DBSMACRO("REQ","")
 quit
VP5(RecordQUEUEJ fQUEUEJ) //
