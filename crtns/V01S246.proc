	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


VDA1(RecordDEALENT fDEALENT)  // Display screen data
 new V
 //
 set VO="29|16|13|"
 set VO(16)=$C(2,31,12,2,0,0,0,0,0,0)_"01T"_$E(fDEALENT.DEALTYPE,1,12)
 set VO(17)=$C(3,31,1,2,0,0,0,0,0,0)_"01T"_$E(fDEALENT.BUYSELL,1,1)
 set VO(18)=$C(4,31,12,2,0,0,0,0,0,0)_"01T"_$E(fDEALENT.OFFTYPE,1,12)
 set VO(19)=$C(5,31,3,2,0,0,0,0,0,0)_"01N"_fDEALENT.SEQ
 set VO(20)=$C(7,31,12,2,0,0,0,0,0,0)_"01T"_$E(fDEALENT.OFFEVENT,1,12)
 set VO(21)=$C(9,31,26,2,0,0,0,0,0,0)_"00T"_$E(fDEALENT.OFFSET,1,26)
 set VO(22)=$C(10,31,8,2,0,0,0,0,0,0)_"00U"_$E(fDEALENT.DRCR,1,8)
 set VO(23)=$C(11,31,1,2,0,0,0,0,0,0)_"00T"_$E(fDEALENT.TYPE,1,1)
 set VO(24)=$C(12,31,26,2,0,0,0,0,0,0)_"00T"_$E(fDEALENT.CID,1,26)
 set VO(25)=$C(13,31,26,2,0,0,0,0,0,0)_"00T"_$E(fDEALENT.CIDNRES,1,26)
