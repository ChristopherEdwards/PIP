	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
vREAD(fid,di)
 type Public RecordDEALENT fDEALENT
 if fid="DEALENT" quit $$vREAD1(.fDEALENT,di)
 quit ""
vREAD1(RecordDEALENT fDEALENT,di)
 quit fDEALENT.@di
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
 //
VSCRPRE(RecordDEALENT fDEALENT)  // Screen Pre-Processor
 new %TAB,vtab // Disable .MACRO. references to %TAB()
 //
	set fDEALENT.offevent=fDEALENT.offtype
	do DEFAULT^DBSMACRO("DEALENT.OFFEVENT",fDEALENT.offtype)
	
	do GOTO^DBSMACRO("fDEALENT.OFFSET")
	
	quit
 #ACCEPT date=11/05/03;PGM=Screen Compiler
