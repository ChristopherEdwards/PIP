 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="DEALENT",VPGM=$T(+0),VSNAME="Deal Accounting Entry Screen"
 set VFSN("DEALENT")="zfDEALENT"
 set vPSL=1
 set KEYS(1)=fDEALENT.DEALTYPE
 set KEYS(2)=fDEALENT.BUYSELL
 set KEYS(3)=fDEALENT.OFFTYPE
 set KEYS(4)=fDEALENT.SEQ
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fDEALENT),VDA1(.fDEALENT),^DBSPNT()

 set ER=0 do VSCRPRE(.fDEALENT) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fDEALENT),VPR(.fDEALENT),VDA1(.fDEALENT)
 if %ProcessMode do VLOD(.fDEALENT) quit:$G(ER)  do VPR(.fDEALENT),VDA1(.fDEALENT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fDEALENT)
	quit


VNEW(RecordDEALENT fDEALENT) // Initialize arrays if %O=0
 
 do VDEF(.fDEALENT)
 do VLOD(.fDEALENT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordDEALENT fDEALENT)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordDEALENT fDEALENT) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

