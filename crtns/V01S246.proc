 set VO(9)=$C(11,34,13,0,0,0,0,0,0,0)_"01T(G=GL, I=IBS)"
 set VO(10)=$C(12,2,27,1,0,0,0,0,0,0)_"01T Account Number (Resident):"
 set VO(11)=$C(13,3,26,1,0,0,0,0,0,0)_"01T Account Number (Non-Res):"
