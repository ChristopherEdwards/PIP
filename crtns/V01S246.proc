 set VO(2)=$C(3,8,21,0,0,0,0,0,0,0)_"01T Bank Buying/Selling:"
 set VO(3)=$C(4,16,13,0,0,0,0,0,0,0)_"01T Offset Type:"
 set VO(4)=$C(5,12,17,0,0,0,0,0,0,0)_"01T Sequence Number:"
 set VO(5)=$C(7,10,19,0,0,0,0,0,0,0)_"01T Offset Event Name:"
 set VO(6)=$C(9,16,13,1,0,0,0,0,0,0)_"01T Offset Days:"
 set VO(7)=$C(10,5,24,1,0,0,0,0,0,0)_"01T Debit (DR)/Credit (CR):"
