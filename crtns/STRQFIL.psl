vbi1 // Trigger BEFORE_INSERT - Before Insert Strike Rate Query

	type public RecordUTBLSTRQ utblstrq
	/*
								----Revision History---------------------------------------------------
	12/14/99  DOUGANM - 31126
		  Removed unecessary variable STRQ, and set utblstrq.desc

								09/22/99 - CARROLLJ - 31126
																			Convert to PSL.
								*/
	if utblstrq.desc'="" quit
	new DTMP,rs,QRY

	set QRY=utblstrq.strqry
	type ResultSet rs=Db.select("DESC","DBTBL4","QID=:QRY") 
	if rs.isEmpty() quit
	set DTMP=rs.next(),DTMP=rs.getCol(1)
	set utblstrq.desc=DTMP

	quit
	


