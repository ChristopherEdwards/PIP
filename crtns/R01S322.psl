R01S322	// SCA332 - System User Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:56 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"
	type String VIN10="ALL"
	type String VIN2="ALL"
	type String VIN3="ALL"
	type String VIN4="ALL"
	type String VIN5="ALL"
	type String VIN6="ALL"
	type String VIN7="ALL"
	type String VIN8="ALL"
	type String VIN9="ALL"

	set RID="SCA332"
	set RN="System User Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[SCAU]%UCLS|[SCAU0]:NOVAL||D EXT^DBSQRY||T|Userclass|||||"
	set %TAB("VIN10")="|255||[SCAU]ROCR|[GLAD]:NOVAL||D EXT^DBSQRY||T|Reconciling Offset (CR)|||||"
	set %TAB("VIN2")="|255||[SCAU]UID|[SCAU]UID:DISTINCT:NOVAL||D EXT^DBSQRY||T|User ID|||||"
	set %TAB("VIN3")="|255||[SCAU]PEXPR|||D EXT^DBSQRY||T|Password Expiration Date|||||"
	set %TAB("VIN4")="|255||[SCAU]LSGN|||D EXT^DBSQRY||T|Date Last Signed On|||||"
	set %TAB("VIN5")="|255||[SCAU]TSDR|[GLAD]:NOVAL||D EXT^DBSQRY||T|Transaction Suspense (DR)|||||"
	set %TAB("VIN6")="|255||[SCAU]TSCR|[GLAD]:NOVAL||D EXT^DBSQRY||T|Transaction Suspense (CR)|||||"
	set %TAB("VIN7")="|255||[SCAU]CSOVR|[GLAD]:NOVAL||D EXT^DBSQRY||T|Cash Over G/L Acct|||||"
	set %TAB("VIN8")="|255||[SCAU]CSSHRT|[GLAD]:NOVAL||D EXT^DBSQRY||T|Cash Short G/L Acct|||||"
	set %TAB("VIN9")="|255||[SCAU]RODR|[GLAD]:NOVAL||D EXT^DBSQRY||T|Reconciling Offset (DR)|||||"

	set %READ="IO/REQ,VIN1#0,VIN2#0,VIN3#0,VIN4#0,VIN5#0,VIN6#0,VIN7#0,VIN8#0,VIN9#0,VIN10#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String VIN1,VIN2,VIN3,VIN4,VIN5,VIN6,VIN7,VIN8,VIN9,VIN10
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA332",RN="System User Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA332","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[SCAU]UID#1#6,[SCAU]%UFN#10#40,[SCAU]TSDR#52#12,[SCAU]CSOVR#66#12,[SCAU]RODR#80#12,[SCAU]PEXPR#94#10,[SCAU]LSGN#105#10,[SCAU]BRCD#116#6,[SCAU]MARTY#128#2\[SCAU]%UCLS#10#12,[SCAU]ACN#24#12,[SCAU]TSCR#52#12,[SCAU]CSSHRT#66#12,[SCAU]ROCR#80#12,[SCAU]RTUID#94#6,[SCAU]BATREJ#108#1,[SCAU]TPM#119#1,[SCAU]EDITOR#127#3"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[SCAU]%UCLS "_VIN1,SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[SCAU]UID "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[SCAU]PEXPR "_VIN3,SEQ=SEQ+1
		if VIN4.get()="" set VIN4="ALL"
		if VIN4'="ALL" set DQQRY(SEQ)="[SCAU]LSGN "_VIN4,SEQ=SEQ+1
		if VIN5.get()="" set VIN5="ALL"
		if VIN5'="ALL" set DQQRY(SEQ)="[SCAU]TSDR "_VIN5,SEQ=SEQ+1
		if VIN6.get()="" set VIN6="ALL"
		if VIN6'="ALL" set DQQRY(SEQ)="[SCAU]TSCR "_VIN6,SEQ=SEQ+1
		if VIN7.get()="" set VIN7="ALL"
		if VIN7'="ALL" set DQQRY(SEQ)="[SCAU]CSOVR "_VIN7,SEQ=SEQ+1
		if VIN8.get()="" set VIN8="ALL"
		if VIN8'="ALL" set DQQRY(SEQ)="[SCAU]CSSHRT "_VIN8,SEQ=SEQ+1
		if VIN9.get()="" set VIN9="ALL"
		if VIN9'="ALL" set DQQRY(SEQ)="[SCAU]RODR "_VIN9,SEQ=SEQ+1
		if VIN10.get()="" set VIN10="ALL"
		if VIN10'="ALL" set DQQRY(SEQ)="[SCAU]ROCR "_VIN10,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("SCAU") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"Userclass"_"|VIN1|"_VIN1.get()
		set VBNRINFO("PROMPTS",2)="WC2|"_"User ID"_"|VIN2|"_VIN2.get()
		set VBNRINFO("PROMPTS",3)="WC2|"_"Password Expiration Date"_"|VIN3|"_VIN3.get()
		set VBNRINFO("PROMPTS",4)="WC2|"_"Date Last Signed On"_"|VIN4|"_VIN4.get()
		set VBNRINFO("PROMPTS",5)="WC2|"_"Transaction Suspense (DR)"_"|VIN5|"_VIN5.get()
		set VBNRINFO("PROMPTS",6)="WC2|"_"Transaction Suspense (CR)"_"|VIN6|"_VIN6.get()
		set VBNRINFO("PROMPTS",7)="WC2|"_"Cash Over G/L Acct"_"|VIN7|"_VIN7.get()
		set VBNRINFO("PROMPTS",8)="WC2|"_"Cash Short G/L Acct"_"|VIN8|"_VIN8.get()
		set VBNRINFO("PROMPTS",9)="WC2|"_"Reconciling Offset (DR)"_"|VIN9|"_VIN9.get()
		set VBNRINFO("PROMPTS",10)="WC2|"_"Reconciling Offset (CR)"_"|VIN10|"_VIN10.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="System User Report"
		set VBNRINFO("PGM")="R01S322"
		set VBNRINFO("RID")="SCA332"
		set VBNRINFO("TABLES")="SCAU"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,SCAU]1"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,SCAU]UID"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:2 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"SCAU.UID,SCAU.%UFN,SCAU.TSDR,SCAU.CSOVR,SCAU.RODR,"
	set VSELECT=VSELECT_"SCAU.PEXPR,SCAU.LSGN,SCAU.BRCD,SCAU.MARTY,SCAU.%UC"
	set VSELECT=VSELECT_"LS,SCAU.ACN,SCAU.TSCR,SCAU.CSSHRT,SCAU.ROCR,SCAU.R"
	set VSELECT=VSELECT_"TUID,SCAU.BATREJ,SCAU.TPM,SCAU.EDITOR"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"SCAU",VWHERE,"SCAU.UID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1
	set vovc1=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1
	set vovc1=vc1
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18
	set vc1=V.piece($C(9),1)			// SCAU.UID
	set vc2=V.piece($C(9),2)			// SCAU.%UFN
	set vc3=V.piece($C(9),3)			// SCAU.TSDR
	set vc4=V.piece($C(9),4)			// SCAU.CSOVR
	set vc5=V.piece($C(9),5)			// SCAU.RODR
	set vc6=V.piece($C(9),6)			// SCAU.PEXPR
	set vc7=V.piece($C(9),7)			// SCAU.LSGN
	set vc8=V.piece($C(9),8)			// SCAU.BRCD
	set vc9=V.piece($C(9),9)			// SCAU.MARTY
	set vc10=V.piece($C(9),10)			// SCAU.%UCLS
	set vc11=V.piece($C(9),11)			// SCAU.ACN
	set vc12=V.piece($C(9),12)			// SCAU.TSCR
	set vc13=V.piece($C(9),13)			// SCAU.CSSHRT
	set vc14=V.piece($C(9),14)			// SCAU.ROCR
	set vc15=V.piece($C(9),15)			// SCAU.RTUID
	set vc16=V.piece($C(9),16)			// SCAU.BATREJ
	set vc17=V.piece($C(9),17)			// SCAU.TPM
	set vc18=V.piece($C(9),18)			// SCAU.EDITOR
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(2)
	type Public String vc1,vovc1
	type Number vb1,vb2
	set (vb1,vb2)=0
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(2)=VT(2)+1
	quit

VDTL2	// Detail
	type public String %TIM,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vrundate,vsysdate

	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set V=vc1,VO=V do VP1 quit:VFMQ!verror.get()  set V=$E(V,1,6) set VL=V
	set VL=VL_$J("",9-VL.length())_$E(vc2,1,40)
	set VL=VL_$J("",51-VL.length())_$J(vc3,12)
	set VL=VL_$J("",65-VL.length())_$J(vc4,12)
	set VL=VL_$J("",79-VL.length())_$J(vc5,12)
	set VL=VL_$J("",93-VL.length())_$J($$DAT^%ZM(vc6),10)
	set VL=VL_$J("",104-VL.length())_$J($$DAT^%ZM(vc7),10)
	set VL=VL_$J("",115-VL.length())_$E(vc8,1,6)
	set VL=VL_$J("",127-VL.length())_$J(vc9,2)
	do VOM
	set VL="         "_$E(vc10,1,12)
	set VL=VL_$J("",23-VL.length())_$J(vc11,12)
	set VL=VL_$J("",51-VL.length())_$J(vc12,12)
	set VL=VL_$J("",65-VL.length())_$J(vc13,12)
	set VL=VL_$J("",79-VL.length())_$J(vc14,12)
	set VL=VL_$J("",93-VL.length())_$E(vc15,1,6)
	set VL=VL_$J("",107-VL.length())_$J(vc16,1)
	set VL=VL_$J("",118-VL.length())_$S(vc17:"Y",1:"N")
	set VL=VL_$J("",126-VL.length())_$E(vc18,1,3)
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="USER ID  USER FULL NAME                            TRN SPS (DR)     CASH OVER    RECON (DR)   PWD EXP  LAST SIGNON BRCD MAX RETRY"
	do VOM
	set VL="         "_"USERCLASS        ACCOUNT #                TRN SPS (CR)    CASH SHORT    RECON (CR)  RETRY ID  BATCH REJ   PM'ING  EDITOR"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - [SYSDEV,SCAU]UID

	// Converted to PSL by CNV70RW
	// TEST THIS
	// TEST THIS TOO
	quit
