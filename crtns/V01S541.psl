 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fSINSTR.exists() set fSINSTR = Class.new("RecordSINSTR")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="SINSTR",VPGM=$T(+0),VSNAME="Customer Settlement Instructions"
 set VFSN("SINSTR")="zfSINSTR"
 set vPSL=1
 set KEYS(1)=fSINSTR.ACN
 set KEYS(2)=fSINSTR.CRCD
 set KEYS(3)=fSINSTR.FXPROD
 set KEYS(4)=fSINSTR.PAYRECV
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fSINSTR),VDA1(.fSINSTR),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fSINSTR),VPR(.fSINSTR),VDA1(.fSINSTR)
 if %ProcessMode do VLOD(.fSINSTR) quit:$G(ER)  do VPR(.fSINSTR),VDA1(.fSINSTR)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fSINSTR)
	quit


VNEW(RecordSINSTR fSINSTR) // Initialize arrays if %O=0
 
 do VDEF(.fSINSTR)
 do VLOD(.fSINSTR)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
