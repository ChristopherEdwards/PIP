R01S036	// STPXCHG - On-Us Stop Record Exchange
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 15:03 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String TJD
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Date FD
	type Number SREMODE

	set RID="STPXCHG"
	set RN="On-Us Stop Record Exchange"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("FD")="|10|||||||D|Starting Date|||||"
	set %TAB("IO")=$$IO^SCATAB
	set %TAB("SREMODE")="|1|||||||L|SRE Mode|||||"

	set %READ="IO/REQ,FD#0,SREMODE#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String TJD
	type public Date FD
	type public Number SREMODE
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,COUNT,PIPE,RID,RN,TER,VL,VLOF,VRF(),VSTATS(),io,vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate

	set CONAM=CUVAR.conam
	set ER=0,RID="STPXCHG",RN="On-Us Stop Record Exchange"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","STPXCHG","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=81")
		write $$CLEARXY^%TRMVT
		}

	do INIT^%ZM()


	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("STPXCHG.PID,STPXCHG.TER,STPXCHG.SEQ,STPXCHG.ACTFLG,STPXCHG.TYP,STPXCHG.INST,STPXCHG.OUTACC,STPXCHG.OUTCHK,STPXCHG.DOCTYP,STPXCHG.DOCNUM,STPXCHG.ACT,STPXCHG.EXP,STPXCHG.OWN,STPXCHG.OUTCHKE","STPXCHG","","STPXCHG.PID,STPXCHG.TER,STPXCHG.SEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3
	set vovc1="",vovc2="",vovc3=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3
	set vovc1=vc1,vovc2=vc2,vovc3=vc3
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14
	set vc1=V.piece($C(9),1)			// STPXCHG.PID
	set vc2=V.piece($C(9),2)			// STPXCHG.TER
	set vc3=V.piece($C(9),3)			// STPXCHG.SEQ
	set vc4=V.piece($C(9),4)			// STPXCHG.ACTFLG
	set vc5=V.piece($C(9),5)			// STPXCHG.TYP
	set vc6=V.piece($C(9),6)			// STPXCHG.INST
	set vc7=V.piece($C(9),7)			// STPXCHG.OUTACC
	set vc8=V.piece($C(9),8)			// STPXCHG.OUTCHK
	set vc9=V.piece($C(9),9)			// STPXCHG.DOCTYP
	set vc10=V.piece($C(9),10)			// STPXCHG.DOCNUM
	set vc11=V.piece($C(9),11)			// STPXCHG.ACT
	set vc12=V.piece($C(9),12)			// STPXCHG.EXP
	set vc13=V.piece($C(9),13)			// STPXCHG.OWN
	set vc14=V.piece($C(9),14)			// STPXCHG.OUTCHKE
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

 type public Boolean ER
 type public Number COUNT
 type public IO io
 type public String PIPE, TER, TJD

 set PIPE = "|"

 if 'TER.exists() set TER=""
 if 'TJD.exists() set TJD=CUVAR.tjd

 set io = Class.new("IO")

 set io.directory = CUVAR.spldir
 set io.fileName = "STPTER"_TER_"."_TJD
 set io.openParams = "NEWV/WRITE"
 set io.timeout = 5

 do io.open()

 if 'COUNT.exists() set COUNT=0

 set COUNT=COUNT+1
	quit

VPREBQ	// Pre-processor (before query)

 //Incoming=TJD 
	quit

VRPOST	// Report post-processor

 type public IO io

 do io.close()
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG4 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb2!(vovc1'=vc1) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(vovc2'=vc2) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do VSUM4 quit:VFMQ  do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VHDG4	// Group Header
	type public String %MSKD,%TIM,CONAM,COUNT,ER,IOSL,PIPE,RID,RN,SREMODE,TER,TJD,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,io,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vrundate

	quit:vh(4)  set vh(4)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="*** SRE"
	set VL=VL_$J("",7-VL.length())_$E(PIPE,1,1)
	set V=$J(SREMODE,1) do VP1 quit:VFMQ!verror.get()
	set VL=VL_$J("",8-VL.length())_V
	do VOM
	quit

VDTL4	// Detail
	type public String COUNT,ER,IOSL,PIPE,SREMODE,TER,TJD,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VRG,VT(),io,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc4,0)
	set VL=VL_$J("",1-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",2-VL.length())_$J(vc5,0)
	set VL=VL_$J("",3-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",4-VL.length())_$E(vc6,1,9999)
	set VL=VL_$J("",5-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",6-VL.length())_$E(vc7,1,9999)
	set VL=VL_$J("",7-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",8-VL.length())_$E(vc8,1,9999)
	set VL=VL_$J("",9-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",10-VL.length())_$E(vc9,1,9999)
	set VL=VL_$J("",11-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",12-VL.length())_$E(vc10,1,9999)
	set VL=VL_$J("",13-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",14-VL.length())_$J(vc11,1)
	set VL=VL_$J("",15-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",16-VL.length())_$J($$DAT^%ZM(vc12),0)
	set VL=VL_$J("",17-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",18-VL.length())_$E(vc13,1,9999)
	set VL=VL_$J("",19-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",20-VL.length())_$E(vc14,1,9999)
	do VOM
	quit

VSUM4	// Summary
	type public String COUNT,ER,I,IOSL,PIPE,SREMODE,TER,TJD,V,VFMQ,VL,VLC,VO,VOFFLG,VT(),VX(),io,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3

	if 'VT(4) quit
	if VLC+0>IOSL do VHDG0 quit:VFMQ

	set VL="*** SRE"
	set VL=VL_$J("",7-VL.length())_$E(PIPE,1,1)
	set VL=VL_$J("",8-VL.length())_$E(COUNT,1,5)
	do VOM
	quit

VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,COUNT,PIPE,RID,RN,SREMODE,TER,TJD,VL,io,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+0,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - Variable: SREMODE

 type Number VLC

 set VLC=1
	quit
