V01S296( Number %ProcessMode,RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)   // -  - SID= <INTPAYHQ00A> International Payments, HQ (MT103)
 ;;Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 11:54 - shetyes
 #WARN ACCESS,MISMATCH OFF
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
 type Public Number ER
 type Public String %PAG,%PAGE,%PG,%PGSV,MULSCR,RM,VFMQ
 type String KVAR,VPG(),vPSL,VSID
 set VSID="INTPAYHQ00A"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+3 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+4
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Outgoing Foreign Pmt-HQ Page 1 (MT103)|INTPAYHQ10A"
 set VPG(%PG+1)="Outgoing Foreign Pmt-HQ Page 2 (MT103)|INTPAYHQ20A"
 set VPG(%PG+2)="Outgoing Foreign Pmt-HQ Page 3 (MT103)|INTPAYHQ30A"
 set VPG(%PG+3)="Outgoing Foreign Pmt-HQ Page 4 (MT103)|INTPAYHQ40A"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fEFTHLD,.fEFTPAY,.DEP)
 if %ProcessMode do VLOD(.fEFTHLD,.fEFTPAY,.DEP) if $G(ER) set VFMQ="Q" quit
 do VPG(.fEFTHLD,.fEFTPAY,.DEP)
 quit

VNEW(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 
 quit
 //
 ;
VLOD(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // User defined access section
	quit

VPG(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fEFTHLD,.fEFTPAY,.DEP),VPG0(.fEFTHLD,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fEFTHLD,.fEFTPAY,.DEP),VPG0(.fEFTHLD,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.fEFTHLD,.fEFTPAY,.DEP),VPG0(.fEFTHLD,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.fEFTHLD,.fEFTPAY,.DEP),VPG0(.fEFTHLD,.fEFTPAY,.DEP) quit:vDONE
  }
 quit

VPG0(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fEFTHLD,.fEFTPAY,.DEP) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Outgoing Foreign Pmt-HQ Page 1 (MT103)
 type Public String %MODS,%REPEAT,ET,VPGM,VPTBL
 type String DFID,PGM,SID
 set SID="INTPAYHQ10A",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S301
 if %ProcessMode>1 quit
 do VTAB^V01S301(.fEFTPAY)
 quit
 
