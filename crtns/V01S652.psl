 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLCTF.exists() set fUTBLCTF = Class.new("RecordUTBLCTFPLN")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCTFPLN",VPGM=$T(+0),VSNAME="Commissions, Taxes, Fees - Data Entry"
 set VFSN("UTBLCTFPLN")="zfUTBLCTF"
 set vPSL=1
 set KEYS(1)=fUTBLCTF.CTFPLN
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLCTF),VDA1(.fUTBLCTF),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fUTBLCTF),VPR(.fUTBLCTF),VDA1(.fUTBLCTF)
 if %ProcessMode do VLOD(.fUTBLCTF) quit:$G(ER)  do VPR(.fUTBLCTF),VDA1(.fUTBLCTF)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLCTF)
	quit


VNEW(RecordUTBLCTFPLN fUTBLCTF) // Initialize arrays if %O=0
 
 do VDEF(.fUTBLCTF)
 do VLOD(.fUTBLCTF)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordUTBLCTFPLN fUTBLCTF)
 quit:fUTBLCTF.CTFPLN.isNull() 
 quit:%ProcessMode  set ER=0 if (fUTBLCTF.CTFPLN="") set ER=1,RM=$$^MSG(1767,"CTFPLN") Q
 if Db.isDefined("UTBLCTFPLN","UTBLCTFPLN.CTFPLN=:fUTBLCTF.CTFPLN") S ER=1,RM=$$^MSG(2327) Q
 do fUTBLCTF.setAuditFlag(1)
 if fUTBLCTF.STRTDATE="" set fUTBLCTF.STRTDATE=$$^CUVAR("TJD")
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
