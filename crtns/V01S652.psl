  //user-defined post procs
 //
VP1(RecordUTBLCTFPLN fUTBLCTF) //
 type public Date V,X

 // Date must be today or future.
 if X'=V,X<%SystemDate do Runtime.setErrMSG("UTBLCTFPLN",6047)
VP2(RecordUTBLCTFPLN fUTBLCTF) //
 type public Number X

 // If Collection Method=0, G/L account number required
 if fUTBLCTF.cmth="0",X.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4855)
VP3(RecordUTBLCTFPLN fUTBLCTF) //
 type public Number X

 // If Plan's assesment method is not dayend, turn off and protect MTSPROCF

 if X'=2 do {
 	set fUTBLCTF.mtsprocf=0  
	do PROTECT^DBSMACRO("UTBLCTFPLN.MTSPROCF")
 	do DEFAULT^DBSMACRO("UTBLCTFPLN.MTSPROCF","0")
	}

 if X=2  do UNPROT^DBSMACRO("UTBLCTFPLN.MTSPROCF")
VP4(RecordUTBLCTFPLN fUTBLCTF) //
 type public Boolean X
 
 // If plan's assessment method is NOT dayend, keep flag turned off.
 if fUTBLCTF.amth'=2 set X=0
VP5(RecordUTBLCTFPLN fUTBLCTF) //
 type public Number X

 // If Misc Tax Status Flag is on, suspense G/L account is required
 if fUTBLCTF.mtsprocf=1,X.isNull() do Runtime.setErrMSG("UTBLCTFPLN",4855)
VP6(RecordUTBLCTFPLN fUTBLCTF) //
 type public String X

 // Day-end Assessment Method Frequency required if Assessment Method = 2 
 if X.isNull(),fUTBLCTF.amth=2 do Runtime.setErrMSG("UTBLCTFPLN",5002) 
VP7(RecordUTBLCTFPLN fUTBLCTF) //
 type public Boolean ER
 type public Date X
 type String FRE

 // If Frequency is defined and Next Date is not, defauld Next Date.
 set FRE=fUTBLCTF.damfreq
 if X.isNull(),'fUTBLCTF.damfreq.isNull() set X=$$NJD^UFRE(%SystemDate,FRE) quit:ER
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit								// User defined post processor's


