R01S450	// SCA495 - EFT Purged Records Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:59 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type String EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA495"
	set RN="EFT Purged Records Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA495",RN="EFT Purged Records Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA495","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[EFTPAY]RECACCT#27#34\[EFTPAY]EFD#1#10,[EFTPAY]CID#13#12,[EFTPAY]EFTTYPE#45#6,[EFTPAY]AMOUNT#53#18,[EFTPAY]CRCD#72#3,[EFTPAY]MET#77#10,[EFTPAY]CONSTANT#89#10,[EFTPAY]SPECIFIC#101#10,[EFTPAY]VARIABLE#113#10,[EFTPAY]STATUS#125#3\[EFTPAY]SEQ#13#12"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE=""
		set VWHERE=VWHERE_"(EFTPAY.STATUS='C' AND EFTPAY.EFD NOT >(:EFTCOM)) "
		set VWHERE=VWHERE_"OR (EFTPAY.STATUS='D' AND EFTPAY.EFD NOT >(:EFTDEL"
		set VWHERE=VWHERE_")) OR (EFTPAY.STATUS='CA' AND EFTPAY.EFD NOT >(:EF"
		set VWHERE=VWHERE_"TCAN)) OR (EFTPAY.STATUS='R' AND EFTPAY.EFD NOT >("
		set VWHERE=VWHERE_":EFTREJ)) OR (EFTPAY.STATUS='SUP' AND EFTPAY.EFD N"
		set VWHERE=VWHERE_"OT >(:EFTSUP))"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="EFT Purged Records Report"
		set VBNRINFO("PGM")="R01S450"
		set VBNRINFO("RID")="SCA495"
		set VBNRINFO("TABLES")="EFTPAY"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,EFTPAY]EFTTYPE"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,EFTPAY]EFD"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,EFTPAY]CID"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,EFTPAY]SEQ"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("EFTPAY.EFTTYPE,EFTPAY.EFD,EFTPAY.CID,EFTPAY.SEQ,EFTPAY.RECACCT,EFTPAY.AMOUNT,EFTPAY.CRCD,EFTPAY.MET,EFTPAY.CONSTANT,EFTPAY.SPECIFIC,EFTPAY.VARIABLE,EFTPAY.STATUS,EFTPAY.RECINST,EFTPAY.FREQUENCY","EFTPAY","(EFTPAY.STATUS='C' AND EFTPAY.EFD NOT >(:EFTCOM)) OR (EFTPAY.STATUS='D' AND EFTPAY.EFD NOT >(:EFTDEL)) OR (EFTPAY.STATUS='CA' AND EFTPAY.EFD NOT >(:EFTCAN)) OR (EFTPAY.STATUS='R' AND EFTPAY.EFD NOT >(:EFTREJ)) OR (EFTPAY.STATUS='SUP' AND EFTPAY.EFD NOT >(:EFTSUP))","EFTPAY.EFTTYPE,EFTPAY.EFD,EFTPAY.CID,EFTPAY.SEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14
	set vc1=V.piece($C(9),1)			// EFTPAY.EFTTYPE
	set vc2=V.piece($C(9),2)			// EFTPAY.EFD
	set vc3=V.piece($C(9),3)			// EFTPAY.CID
	set vc4=V.piece($C(9),4)			// EFTPAY.SEQ
	set vc5=V.piece($C(9),5)			// EFTPAY.RECACCT
	set vc6=V.piece($C(9),6)			// EFTPAY.AMOUNT
	set vc7=V.piece($C(9),7)			// EFTPAY.CRCD
	set vc8=V.piece($C(9),8)			// EFTPAY.MET
	set vc9=V.piece($C(9),9)			// EFTPAY.CONSTANT
	set vc10=V.piece($C(9),10)			// EFTPAY.SPECIFIC
	set vc11=V.piece($C(9),11)			// EFTPAY.VARIABLE
	set vc12=V.piece($C(9),12)			// EFTPAY.STATUS
	set vc13=V.piece($C(9),13)			// EFTPAY.RECINST
	set vc14=V.piece($C(9),14)			// EFTPAY.FREQUENCY
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 type public Date EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP,EFDMAX

 // Populate Maximum Days variables
 do CUVAR^EFTPURG(.EFTCAN,.EFTCOM,.EFTDEL,.EFTREJ,.EFTSUP,.EFDMAX)
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(1).get() do VDTL1 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do VSUM2 quit:VFMQ  do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VDTL1	// Detail
	type public String %TIM,EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(1)  set VD(1)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set V=vc1,VO=V do VP1 quit:VFMQ!verror.get()  set V=$E(V,1,40) set VL=V
	do VOM
	quit

VSUM2	// Summary
	type public String %TIM,EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP,I,IOSL,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                            "_"========"
	do VOM
	set VL="                     "_"Total Count for Type:"
	set V=VT(2)					// @CNT(,N,8)
	set VL=VL_$J("",44-VL.length())_$J(V,8)
	do VOM
	set VL="" do VOM
	quit

VDTL4	// Detail
	type public String %TIM,EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="                          "_$E(vc5,1,34)
	do VOM
	set VL=$J($$DAT^%ZM(vc2),10)
	set VL=VL_$J("",12-VL.length())_$J(vc3,12)
	set VL=VL_$J("",44-VL.length())_$E(vc1,1,6)
	set VL=VL_$J("",52-VL.length())_$J($FN(vc6,",",2),18)
	set VL=VL_$J("",71-VL.length())_$E(vc7,1,3)
	set VL=VL_$J("",76-VL.length())_$E(vc8,1,10)
	set VL=VL_$J("",88-VL.length())_$E(vc9,1,10)
	set VL=VL_$J("",100-VL.length())_$E(vc10,1,10)
	set VL=VL_$J("",112-VL.length())_$E(vc11,1,10)
	set VL=VL_$J("",124-VL.length())_$E(vc12,1,3)
	do VOM
	set VL="            "_$J(vc4,12)
	set VL=VL_$J("",26-VL.length())_$E(vc13,1,12)
	set VL=VL_$J("",44-VL.length())_$E(vc14,1,12)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,EFDMAX,EFTCAN,EFTCOM,EFTDEL,EFTREJ,EFTSUP,RID,RN,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="                          "_"Recip Account"
	do VOM
	set VL="Effective    Account No.                    Type              Amount  Curr  Payment     Constant      Specific  Variable    Status"
	do VOM
	set VL="Date        Sequence No.  Recip. Inst.      Frequency                       Method      Field            Field  Field"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,VPN,vc1,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="                                            "_"========"
	do VOM
	set VL="                              "_"Total Count:"
	set V=(VT(0)+VT(1)+VT(2)+VT(3)+VT(4))		// @CNT(0,N,8)
	set VL=VL_$J("",44-VL.length())_$J(V,8)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: EFTTYPE

	type public String vc1
 type Public String EFTTYPE

 type String ZTYP
 
 set ZTYP=vc1
 set EFTTYPE=""
 type ResultSet rs=Db.select("DESC","CTBLEFTTYPE","KEY=:ZTYP") 
 if 'rs.next() quit
 set EFTTYPE=rs.getCol("DESC")

	quit
