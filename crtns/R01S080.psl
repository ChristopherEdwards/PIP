	set VL="                                    "_"----- ALLOCATION OF CURRENT INTEREST -----"
	do VOM
	set VL="                             "_"PD THRU       FROM TOT  ALLOCATE CALCULATE               DUE     NOT DUE    CURRENT     REQ   RE-CALC"
	do VOM
	set VL="   "_"ACCOUNT #      BALANCE   INT RATE        TO  CNT  DATE DUE COLLECTED  SUBTOTAL  + UNPAID   - PAID   - ACCRUAL  =  ADJ   ACCRUAL"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM
	set VL="GROUP"
	set VL=VL_$J("",6-VL.length())_$E(vc2,1,4)
	set VL=VL_$J("",13-VL.length())_"TYPE"
	set VL=VL_$J("",18-VL.length())_$J(vc3,4)
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - [SYSDEV,LNACR1]%ACR

	/*
	Revision History

	12/29/04 - SmithCD - CR 13661
		   Converted to PSL.
	*/

	type public Number GTIC,GTPPI,TIC,TPPI,V

	if V<0 set TPPI=TPPI+V,GTPPI=GTPPI+V
	else  set TIC=TIC+V,GTIC=GTIC+V

	quit

VP2	// Column post-processor - [SYSDEV,LNACR1]%ACR

	type public String vc25,vc21,vc26,vc20
	/*
	Revision History
 
	12/29/04 - SmithCD - CR 13661
		   Converted to PSL.
	*/

	// If applicable display Promotional rate instead of nominal 
	// interest rate
	if vc25,vc21<vc26 set vc20=vc25

	quit
