R01S032	// IRSO99OIDC - IRS 1099-OID Report by Tax ID
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 09/10/2007 15:37 - root

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="IRSO99OIDC"
	set RN="IRS 1099-OID Report by Tax ID"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CC,CONAM,EWP,FIW,NAM,OID,OPI,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="IRSO99OIDC",RN="IRS 1099-OID Report by Tax ID"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","IRSO99OIDC","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[TMPRPT3]KEY3#60#12"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE="TMPRPT3.PID=:%ProcessID"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="IRS 1099-OID Report by Tax ID"
		set VBNRINFO("PGM")="R01S032"
		set VBNRINFO("RID")="IRSO99OIDC"
		set VBNRINFO("TABLES")="TMPRPT3"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,TMPRPT3]PID"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,TMPRPT3]KEY1"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,TMPRPT3]KEY2"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,TMPRPT3]KEY3"

		set VBNRINFO("DOC",1)="06/27/07	NATRAJAH - CR 22466"
		set VBNRINFO("DOC",2)="		This report allows an institution to print Form 1099-OID "
		set VBNRINFO("DOC",3)="		Records."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:4 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("TMPRPT3.PID,TMPRPT3.KEY1,TMPRPT3.KEY2,TMPRPT3.KEY3,TMPRPT3.DATA","TMPRPT3","TMPRPT3.PID=:%ProcessID","TMPRPT3.PID,TMPRPT3.KEY1,TMPRPT3.KEY2,TMPRPT3.KEY3","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5
	set vc1=V.piece($C(9),1)			// TMPRPT3.PID
	set vc2=V.piece($C(9),2)			// TMPRPT3.KEY1
	set vc3=V.piece($C(9),3)			// TMPRPT3.KEY2
	set vc4=V.piece($C(9),4)			// TMPRPT3.KEY3
	set vc5=V.piece($C(9),5)			// TMPRPT3.DATA
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

	type Number ACN,AMT,CID,CUTOFF,I,OID,YEAR
	type String FORM,TIN
	
	type DbSet ds
	type RecordCIF cif
	type RecordDEP dep
	type RecordOID lyoid,oid
	
	set YEAR=%SystemDate.year() - 1	
	for I=1:1:9 set AMT(I)=0
        set FORM="1099-OID"	
	type RecordUTBLIRS utblirs = Db.getRecord("UTBLIRS", "KEY = :FORM", 1)
  
	// IRS Year-End Reporting Tape Cutoff amount
	set CUTOFF = utblirs.tctoff
	
	set ds=Db.selectDbSet("DEP","OID>0")
	while ds.next() do {
		set dep=ds.getRecord("DEP")
		set CID=dep.cid
		
		if dep.irsexm quit	// exempt 1099 reporting
		
		set oid=Db.getRecord("OID","CID=:CID,TAXYR=:YEAR",1)
		if 'oid.getMode() quit
		
		set lyoid=Db.getRecord("OID","CID=:CID,TAXYR=:(YEAR-1)",1)
		if 'lyoid.getMode() set OID=oid.ial
		else  set OID=oid.ial-lyoid.ial
		
		if dep.ipty<CUTOFF,('dep.bwpty!'dep.penpty!'OID) quit

		set cif=Db.getRecord("CIF","ACN=:dep.acn")
		set ACN=dep.acn
		set TIN=cif.taxid

		set AMT(1)=OID			// Total OID 
		set AMT(2)=dep.ipty		// Interest earnings
		set AMT(3)=dep.penpty		// Early withdrawal penalty
		set AMT(4)=dep.bwpty		// Federal income tax withheld
		type RecordTMPRPT3 tmprpt3=Db.getRecord("TMPRPT3","PID=:%ProcessID, KEY1=:TIN, KEY2=:ACN, KEY3=:CID",1)
		set tmprpt3.data=AMT(1)_"|"_AMT(2)_"|"_AMT(3)_"|"_AMT(4)
		do tmprpt3.save()
		
		}
	quit

VRPOST	// Report post-processor

	do Db.delete("TMPRPT3","PID=:%ProcessID")
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG3 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),EWP,FIW,OID,OPI
	type Public String VSTATS

	set VT(4)=VT(4)+1
	set VT(4,1,1)=VT(4,1,1)+EWP			// @TOT(<<EWP>>)
	set VT(4,2,1)=VT(4,2,1)+FIW			// @TOT(<<FIW>>)
	set VT(4,3,1)=VT(4,3,1)+OID			// @TOT(<<OID>>)
	set VT(4,4,1)=VT(4,4,1)+OPI			// @TOT(<<OPI>>)
	quit

VHDG3	// Group Header
	type public String %MSKD,%TIM,CC,CONAM,ER,EWP,FIW,IOSL,NAM,OID,OPI,RID,RN,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,vc1,vc2,vc3,vc4,vc5,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:vh(3)  set vh(3)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set V=vc2,VO=V set V=$E(V,1,12) do VP1 quit:VFMQ!verror.get()  set VL=V
	set VL=VL_$J("",13-VL.length())_$E(vc3,1,12)
	set VL=VL_$J("",26-VL.length())_$E(NAM,1,30)
	set VL=VL_$J("",57-VL.length())_$J(CC,6,0)
	do VOM
	quit

VSUM3	// Summary
	type public String %TIM,CC,EWP,FIW,I,IOSL,NAM,OID,OPI,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc2,vc3,vc4,vc5,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+0>IOSL do VHDG0 quit:VFMQ  set vh(3)=0 do VHDG3 quit:VFMQ

	set VL="                                                                "_"Total:"
	set V=VT(3,3,1)					// @TOT(<<OID>>,)
	set VL=VL_$J("",72-VL.length())_$J($FN(V,",",2),14)
	set V=VT(3,4,1)					// @TOT(<<OPI>>,)
	set VL=VL_$J("",87-VL.length())_$J($FN(V,",",2),14)
	set V=VT(3,1,1)					// @TOT(<<EWP>>,)
	set VL=VL_$J("",102-VL.length())_$J($FN(V,",",2),14)
	set V=VT(3,2,1)					// @TOT(<<FIW>>,)
	set VL=VL_$J("",117-VL.length())_$J($FN(V,",",2),14)
	do VOM
	quit

VDTL4	// Detail
	type public String %TIM,CC,EWP,FIW,IOSL,NAM,OID,OPI,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc2,vc3,vc4,vc5,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set V=vc4,VO=V set V=$J(V,12) do VP2 quit:VFMQ!verror.get()  set VL="                                                           "_V
	set VL=VL_$J("",72-VL.length())_$J($FN(OID,",",2),14)
	set VL=VL_$J("",87-VL.length())_$J($FN(OPI,",",2),14)
	set VL=VL_$J("",102-VL.length())_$J($FN(EWP,",",2),14)
	set VL=VL_$J("",117-VL.length())_$J($FN(FIW,",",2),14)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CC,CONAM,EWP,FIW,NAM,OID,OPI,RID,RN,VL,vc1,vc2,vc3,vc4,vc5,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="                                                                              "_"Original          Other          Early        Federal"
	do VOM
	set VL="                                                                "_"Account          Issue       Periodic     Withdrawal     Income Tax"
	do VOM
	set VL="Tax ID       CIF Number   Name                      Cost Center  Number      Discounts       Interest        Penalty       Withheld"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,CC,EWP,FIW,NAM,OID,OPI,VPN,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="Report Totals:"
	set V=(VT(0)+VT(1)+VT(2)+VT(3)+VT(4))		// @CNT(0,N,8)
	set VL=VL_$J("",15-VL.length())_$J(V,8)
	set VL=VL_$J("",24-VL.length())_"Accounts"
	set V=0						// @TOT(<<OID>>,0)
	for I=0:1:4 set V=V+VT(I,3,1)
	set VL=VL_$J("",72-VL.length())_$J($FN(V,",",2),14)
	set V=0						// @TOT(<<OPI>>,0)
	for I=0:1:4 set V=V+VT(I,4,1)
	set VL=VL_$J("",87-VL.length())_$J($FN(V,",",2),14)
	set V=0						// @TOT(<<EWP>>,0)
	for I=0:1:4 set V=V+VT(I,1,1)
	set VL=VL_$J("",102-VL.length())_$J($FN(V,",",2),14)
	set V=0						// @TOT(<<FIW>>,0)
	for I=0:1:4 set V=V+VT(I,2,1)
	set VL=VL_$J("",117-VL.length())_$J($FN(V,",",2),14)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - [SYSDEV,TMPRPT3]KEY1

	type public String vc3
 type Number ACN 
 type public String NAM
 type public Number CC
 set ACN=vc3
 type RecordCIF cif=Db.getRecord("CIF","ACN=:ACN")
 set NAM=cif.nam
 set CC=cif.cc
	quit

VP2	// Column post-processor - [SYSDEV,TMPRPT3]KEY3

	type public String vc5
 type public String DATA
 type public Number EWP,FIW,OID,OPI
 set DATA=vc5
 set OID=DATA.piece("|",1)
 set OPI=DATA.piece("|",2)
 set EWP=DATA.piece("|",3)
 set FIW=DATA.piece("|",4)

	quit
