VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,CONT,CONV,EDCONT,FMV,IRATYP,MCZCCONT,NAM,RECHAR,ROLL,ROTHCONT,SEPCONT,SIMPCONT,TAXID,VPN,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="Report Totals:"
	set V=(VT(0)+VT(1)+VT(2)+VT(3)+VT(4)+VT(5))	// @CNT(0,N,4)
	set VL=VL_$J("",16-VL.length())_$J(V,4)
	set VL=VL_$J("",21-VL.length())_"Customers"
	set V=0						// @TOT(<<CONT>>,0)
	for I=0:1:5 set V=V+VT(I,1,1)
	set VL=VL_$J("",66-VL.length())_$J(V,12,2)
	set V=0						// @TOT(<<CONV>>,0)
	for I=0:1:5 set V=V+VT(I,2,1)
