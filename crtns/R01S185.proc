	set VL=VL_$J("",92-VL.length())_$J(V,12,2)
	set V=0						// @TOT(<<EDCONT>>,0)
	for I=0:1:5 set V=V+VT(I,3,1)
	set VL=VL_$J("",105-VL.length())_$J(V,12,2)
	set V=0						// @TOT(<<FMV>>,0)
	for I=0:1:5 set V=V+VT(I,4,1)
