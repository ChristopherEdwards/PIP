	type public String %MSKD,%TIM,CONAM,CONT,CONV,EDCONT,FMV,IRATYP,MCZCCONT,NAM,RECHAR,RID,RN,ROLL,ROTHCONT,SEPCONT,SIMPCONT,TAXID,VL,vc1,vc2,vc3,vc4,vc5,vc6,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate
