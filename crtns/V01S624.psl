 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLAWT.exists() set fUTBLAWT = Class.new("RecordUTBLAWTI")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLAWTI",VPGM=$T(+0),VSNAME="Accrued withholding tax mass index chang"
 set VFSN("UTBLAWTI")="zfUTBLAWT"
 set vPSL=1
 set KEYS(1)=fUTBLAWT.AWTI
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLAWT),VDA1(.fUTBLAWT),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fUTBLAWT),VPR(.fUTBLAWT),VDA1(.fUTBLAWT)
 if %ProcessMode do VLOD(.fUTBLAWT) quit:$G(ER)  do VPR(.fUTBLAWT),VDA1(.fUTBLAWT)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLAWT)
	quit


