vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordDBTBL6SQ dbtbl6sq

	if dbtbl6sq.libs.length()>12 set vRM = $$^MSG(1076,12) do vdderr("LIBS", vRM) quit
	if dbtbl6sq.qid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("QID", vRM) quit
	set X = dbtbl6sq.seq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("SEQ", vRM) quit
	if dbtbl6sq.qbase.length()>40 set vRM = $$^MSG(1076,40) do vdderr("QBASE", vRM) quit
	if dbtbl6sq.qdi.length()>40 set vRM = $$^MSG(1076,40) do vdderr("QDI", vRM) quit
