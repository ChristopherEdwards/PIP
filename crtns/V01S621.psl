 set VO="14||13|"
 set VO(0)="|"
 set VO(1)=$C(1,26,29,1,0,0,0,0,0,0)_"01T Maintain Accrual User Exits "
 set VO(2)=$C(3,13,13,1,0,0,0,0,0,0)_"01T Description:"
 set VO(3)=$C(5,4,22,0,0,0,0,0,0,0)_"01TConditional Execution:"
 set VO(4)=$C(7,2,24,1,0,0,0,0,0,0)_"01T File Name (ACN,DEP,LN):"
 set VO(5)=$C(9,30,17,2,0,0,0,0,0,0)_"01TSelect Data Items"
 set VO(6)=$C(13,26,27,2,0,0,0,0,0,0)_"01TRecord Selection Conditions"
 set VO(7)=$C(17,5,27,0,0,0,0,0,0,0)_"01TUser Pre-Processor Defined:"
 set VO(8)=$C(17,37,7,0,0,0,0,0,0,0)_"01TModify?"
 set VO(9)=$C(18,1,31,0,0,0,0,0,0,0)_"01TRecord Level User Code Defined:"
 set VO(10)=$C(18,37,7,0,0,0,0,0,0,0)_"01TModify?"
 set VO(11)=$C(19,4,28,0,0,0,0,0,0,0)_"01TUser Post-Processor Defined:"
 set VO(12)=$C(19,37,7,0,0,0,0,0,0,0)_"01TModify?"
 set VO(13)=$C(21,12,14,0,0,0,0,0,0,0)_"01TRecord Format:"
 set VO(14)=$C(22,19,7,0,0,0,0,0,0,0)_"01TDevice:"
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


VDA1(RecordUTBLACCRXPO fUTBLAC)  // Display screen data
 new V
 if %ProcessMode=5 new MODPOST,MODPREP,MODUCODE
 if  set (MODPOST,MODPREP,MODUCODE)=""
 else  set MODPOST=$G(MODPOST),MODPREP=$G(MODPREP),MODUCODE=$G(MODUCODE)
 
 s MODPOST=$G(MODPOST)
 s MODPREP=$G(MODPREP)
 s MODUCODE=$G(MODUCODE)
 //
 set VO="29|15|13|"
 set VO(15)=$C(3,27,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.DESC,1,40)
 set VO(16)=$C(5,27,54,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.CONDX,1,255)
 set VO(17)=$C(7,27,12,2,0,0,0,0,0,0)_"00U"_$E(fUTBLAC.FROM,1,12)
 set VO(18)=$C(10,1,80,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.SELECT1,1,255)
 set VO(19)=$C(11,1,80,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.SELECT2,1,255)
 set VO(20)=$C(14,1,80,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.WHERE1,1,255)
 set VO(21)=$C(15,1,80,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.WHERE2,1,255)
 set VO(22)=$C(17,33,1,2,0,0,0,0,0,0)_"01L"_$S(fUTBLAC.PREP:"Y",1:"N")
 set VO(23)=$C(17,46,1,2,0,0,0,0,0,0)_"00L"_$S(MODPREP.get():"Y",1:"N")
 set VO(24)=$C(18,33,1,2,0,0,0,0,0,0)_"01L"_$S(fUTBLAC.URLCODE:"Y",1:"N")
 set VO(25)=$C(18,46,1,2,0,0,0,0,0,0)_"00L"_$S(MODUCODE.get():"Y",1:"N")
 set VO(26)=$C(19,33,1,2,0,0,0,0,0,0)_"01L"_$S(fUTBLAC.POSTP:"Y",1:"N")
 set VO(27)=$C(19,46,1,2,0,0,0,0,0,0)_"00L"_$S(MODPOST.get():"Y",1:"N")
 set VO(28)=$C(21,27,12,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.RECFMT,1,12)
 set VO(29)=$C(22,27,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBLAC.DEVICE,1,40)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
	
VTAB(RecordUTBLACCRXPO fUTBLAC)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=15,VPT=1,VPB=22,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLACCRXPO",VSCRPP=1,VSCRPP=1
 set OLNTB=22027
 
 set VFSN("UTBLACCRXPO")="zfUTBLAC"
 //
 // 
	
	
 set %TAB(1)=$C(2,26,40)_"01T12401|1|[UTBLACCRXPO]DESC"
 set %TAB(2)=$C(4,26,54)_"00T12402|1|[UTBLACCRXPO]CONDX|||||||||255"
 set %TAB(3)=$C(6,26,12)_"01U12403|1|[UTBLACCRXPO]FROM|||do VP1^V01S621(.fUTBLAC)"
 set %TAB(4)=$C(9,0,80)_"01T12404|1|[UTBLACCRXPO]SELECT1|||do VP2^V01S621(.fUTBLAC)|do VP3^V01S621(.fUTBLAC)|||||255"
 set %TAB(5)=$C(10,0,80)_"00T12405|1|[UTBLACCRXPO]SELECT2|||do VP4^V01S621(.fUTBLAC)|do VP5^V01S621(.fUTBLAC)|||||255"
 set %TAB(6)=$C(13,0,80)_"00T12406|1|[UTBLACCRXPO]WHERE1|||do VP6^V01S621(.fUTBLAC)||||||255"
 set %TAB(7)=$C(14,0,80)_"00T12407|1|[UTBLACCRXPO]WHERE2|||do VP7^V01S621(.fUTBLAC)||||||255"
 set %TAB(8)=$C(16,32,1)_"20L|*PREP|[UTBLACCRXPO]PREP"
 set %TAB(9)=$C(16,45,1)_"00L|*MODPREP|[*]@MODPREP"
 set %TAB(10)=$C(17,32,1)_"20L|*URLCODE|[UTBLACCRXPO]URLCODE"
 set %TAB(11)=$C(17,45,1)_"00L|*MODUCODE|[*]@MODUCODE"
 set %TAB(12)=$C(18,32,1)_"20L|*POSTP|[UTBLACCRXPO]POSTP"
 set %TAB(13)=$C(18,45,1)_"00L|*MODPOST|[*]@MODPOST|||do VP8^V01S621(.fUTBLAC)"
 set %TAB(14)=$C(20,26,12)_"00T12408|1|[UTBLACCRXPO]RECFMT|[STBLTFMT]"
 set %TAB(15)=$C(21,26,40)_"00T12409|1|[UTBLACCRXPO]DEVICE||||do VP9^V01S621(.fUTBLAC)"
 do VTBL(.fUTBLAC)
	do ^DBSCRT8 	// data entry
	quit


VREQ   // Create REQ() array
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
