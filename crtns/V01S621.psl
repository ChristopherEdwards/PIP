 type Public RecordUTBLACCRXPO fUTBLAC
 do VSPP1(.fUTBLAC)
 #ACCEPT Date=11/05/03; pgm=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
VSPP1(RecordUTBLACCRXPO fUTBLAC)
 set fUTBLAC.updtdate=%CurrentDate
