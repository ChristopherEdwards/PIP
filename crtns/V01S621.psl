 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fUTBLAC.exists() set fUTBLAC = Class.new("RecordUTBLACCRXPO")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab,MODPREP,MODUCODE,MODPOST",VSID="UTBLACCRXPO",VPGM=$T(+0),VSNAME="Accrual User Exits Maintenance"
 set VFSN("UTBLACCRXPO")="zfUTBLAC"
 set vPSL=1
 set KEYS(1)=fUTBLAC.ACCRXNAM
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fUTBLAC),VDA1(.fUTBLAC),^DBSPNT() quit

 if '%ProcessMode do VNEW(.fUTBLAC),VPR(.fUTBLAC),VDA1(.fUTBLAC)
 if %ProcessMode do VLOD(.fUTBLAC) quit:$G(ER)  do VPR(.fUTBLAC),VDA1(.fUTBLAC)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fUTBLAC)
	quit


