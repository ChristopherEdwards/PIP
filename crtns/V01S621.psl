  //user-defined post procs
 //
VP1(RecordUTBLACCRXPO fUTBLAC) //
 if '((X="LN")!(X="ACN")!(X="DEP")) do Runtime.setErrMSG("", 1337,X) quit
 set (FILES,FID)=X
VP2(RecordUTBLACCRXPO fUTBLAC) //
 do PPDI^DBSEXEQ
VP3(RecordUTBLACCRXPO fUTBLAC) //
 set I(3)="@SELDI^DBSFUN(FILES,""""):LIST"
VP4(RecordUTBLACCRXPO fUTBLAC) //
 do PPDI^DBSEXEQ
VP5(RecordUTBLACCRXPO fUTBLAC) //
 set I(3)="@SELDI^DBSFUN(FILES,""""):LIST"
VP6(RecordUTBLACCRXPO fUTBLAC) //
 do ^DBSQRY
VP7(RecordUTBLACCRXPO fUTBLAC) //
 do ^DBSQRY
VP8(RecordUTBLACCRXPO fUTBLAC) //
 // If have user code of any kind, don't allow format and device
 if (fUTBLAC.urlcode)!(fUTBLAC.prep)!(fUTBLAC.postp)!(MODUCODE)!(MODPREP)!(X) do {
	 do DEFAULT^DBSMACRO("UTBLACCRXPO.RECFMT","","0","0","0")
	 do DEFAULT^DBSMACRO("UTBLACCRXPO.DEVICE","","0","0","0")
	 do PROTECT^DBSMACRO("UTBLACCRXPO.RECFMT")
	 do PROTECT^DBSMACRO("UTBLACCRXPO.DEVICE")
 	}
 else  do {
	 do UNPROT^DBSMACRO("UTBLACCRXPO.RECFMT")
	 do UNPROT^DBSMACRO("UTBLACCRXPO.RECFMT")
	}
 quit
VP9(RecordUTBLACCRXPO fUTBLAC) //
 // Require device field if no user code
 if '(fUTBLAC.URLCODE)!(fUTBLAC.PREP)!(fUTBLAC.POSTP)!(MODUCODE)!(MODPOST)!(MODPREP) do CHANGE^DBSMACRO("REQ","") 
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit								// User defined post processor's


