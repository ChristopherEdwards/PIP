	type public RecordLNLS6C lnls6c

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("LNLS6C","CID = :lnls6c.cid and PSEQ = :lnls6c.pseq and TSEQ = :lnls6c.tseq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))
	if vpar["/VALFK/" do CHKFKS                            // Check foreign keys
	if vpar["/VALRI/" do VFKEYS                            // Foreign key definition

	if vpar'["/NOUPDATE/" do {

