vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLCOLCD utblcolcd

	set X = utblcolcd.grp if 'X.isNull(),'Db.isDefined("STBLGRP","CLS=""L"",GRP=:X") set vRM = $$^MSG(1485,X) do vdderr("GRP", vRM) quit
	if utblcolcd.key.length()>6 set vRM = $$^MSG(1076,6) do vdderr("KEY", vRM) quit
	set X = utblcolcd.abmc if 'X.isNull(),'Db.isDefined("STBLABMC","X") set vRM = $$^MSG(1485,X) do vdderr("ABMC", vRM) quit
	if '("01"[utblcolcd.ADR) set vRM=$$^MSG(742,"L") do vdderr("ADR", vRM) quit
	if utblcolcd.cbc.length()>2 set vRM = $$^MSG(1076,2) do vdderr("CBC", vRM) quit
	set X = utblcolcd.colpct if 'X.isNull() set vRM = $$VAL^DBSVER("N",9,0,,,,,5) if 'vRM.isNull() set vRM = $$^MSG(979,"UTBLCOLCD.COLPCT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if utblcolcd.des.length()>20 set vRM = $$^MSG(1076,20) do vdderr("DES", vRM) quit
	if utblcolcd.pcs.length()>60 set vRM = $$^MSG(1076,60) do vdderr("PCS", vRM) quit
	if utblcolcd.pfwdw.length()>40 set vRM = $$^MSG(1076,40) do vdderr("PFWDW", vRM) quit
	if utblcolcd.pfwdwa.length()>40 set vRM = $$^MSG(1076,40) do vdderr("PFWDWA", vRM) quit
	set X = utblcolcd.phc if 'X.isNull(),'Db.isDefined("UTBLPHC","X") set vRM = $$^MSG(1485,X) do vdderr("PHC", vRM) quit
	if '("01"[utblcolcd.SEC) set vRM=$$^MSG(742,"L") do vdderr("SEC", vRM) quit
	set X = utblcolcd.type if 'X.isNull(),'Db.isDefined("STBLCOLTYP","X") set vRM = $$^MSG(1485,X) do vdderr("TYPE", vRM) quit
	quit

