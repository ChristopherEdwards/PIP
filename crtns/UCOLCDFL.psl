	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordUTBLCOLCD utblcolcd

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("UTBLCOLCD","GRP = :utblcolcd.grp and KEY = :utblcolcd.key") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		if %O = 0, vpar'["/NOLOG/" do ^DBSLOGIT(utblcolcd,%O,.vxins())
		if %O = 1, vpar'["/NOLOG/" do ^DBSLOGIT(utblcolcd,%O,.vx())

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(utblcolcd)) S ^UTBL("COLCD",vobj(utblcolcd,-3),vobj(utblcolcd,-4))=vobj(utblcolcd)
		#ENDBYPASS
	}

	quit

