	quit

public	DBRCD(%UserStation)	// Get default branch code

	new BRCD,FEPNAM

	if $L($G(%BRCDDFT)) quit %BRCDDFT
	
	set FEPNAM="HOST"
	if %UserStation["." set %UserStation=$P(%UserStation,".",2)

	type RecordUTBLDEVICE udevice=Db.getRecord("UTBLDEVICE","FEPNAM,%UserStation",1)
	set BRCD=udevice.brcd

	quit BRCD
 #OPTION ResultClass ON
Public String vSIG()	quit "59886^43622^Sanchez SCM Administrator^621"	// Signature - LTD^TIME^USER^SIZE
