R01S568	// SCA646A - CD Year-End Regular 1099 by Product
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:52 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String LNAM,OLDPAGE,ZZRID
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ

	set RID="SCA646A"
	set RN="CD Year-End Regular 1099 by Product"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String LNAM,OLDPAGE,ZZRID
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,RID,RN,RPNAME,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc2,vc3,vc4,vc5,vovc1,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA646A",RN="CD Year-End Regular 1099 by Product"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA646A","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[CD1099]TYPE#31#4,[CD1099]CNT#39#10,[CD1099]IPY#77#15,[CD1099]PPY#93#12,[CD1099]BWP#107#12"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()



		set VBNRINFO("DESC")="CD Year-End Regular 1099 by Product"
		set VBNRINFO("PGM")="R01S568"
		set VBNRINFO("RID")="SCA646A"
		set VBNRINFO("TABLES")="CD1099"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,CD1099]$J"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,CD1099]TYPE"

		set VBNRINFO("DOC",1)=" CD Year-End Ragular 1099 by Product (SCA646A)."
		set VBNRINFO("DOC",2)=" This is Product Type subtotals portion of SCA646 report."
		set VBNRINFO("DOC",3)=" The accounts displayed are grouped by their Product Type."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:2 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:4 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("CD1099.TYPE,CD1099.CNT,CD1099.IPY,CD1099.PPY,CD1099.BWP","CD1099","","CD1099.TYPE","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1
	set vovc1=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1
	set vovc1=vc1
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5
	set vc1=V.piece($C(9),1)			// CD1099.TYPE
	set vc2=V.piece($C(9),2)			// CD1099.CNT
	set vc3=V.piece($C(9),3)			// CD1099.IPY
	set vc4=V.piece($C(9),4)			// CD1099.PPY
	set vc5=V.piece($C(9),5)			// CD1099.BWP
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

 //Incoming=LNAM,OLDPAGE,ZZRID
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG2 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(2)
	type Public String vc1,vovc1
	type Number vb1,vb2
	set (vb1,vb2)=0
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc5,vc2,vc3,vc4
	type Public String VSTATS

	set VT(2)=VT(2)+1
	set VT(2,1,1)=VT(2,1,1)+vc5			// @TOT([CD1099]BWP)
	set VT(2,2,1)=VT(2,2,1)+vc2			// @TOT([CD1099]CNT)
	set VT(2,3,1)=VT(2,3,1)+vc3			// @TOT([CD1099]IPY)
	set VT(2,4,1)=VT(2,4,1)+vc4			// @TOT([CD1099]PPY)
	quit

VHDG2	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,LNAM,OLDPAGE,RID,RN,RPNAME,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,ZZRID,vc1,vc2,vc3,vc4,vc5,vcrt,verror,vh(),vovc1,vrundate,vsysdate

	quit:vh(2)  set vh(2)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="       "_"Totals -"
	do VOM
	quit

VDTL2	// Detail
	type public String %TIM,IOSL,LNAM,OLDPAGE,RPNAME,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),ZZRID,vc1,vc2,vc3,vc4,vc5,verror,vh(),vovc1,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(2)=0 do VHDG2 quit:VFMQ

	set VL="                "_"Product Type"
	set VL=VL_$J("",29-VL.length())_$J(vc1,5)
	set VL=VL_$J("",35-VL.length())_$J(vc2,13)
	set VL=VL_$J("",50-VL.length())_"Accounts"
	set VL=VL_$J("",59-VL.length())_$J($FN(vc3,",",2),32)
	set VL=VL_$J("",92-VL.length())_$J($FN(vc4,",",2),12)
	set VL=VL_$J("",105-VL.length())_$J($FN(vc5,",",2),13)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,LNAM,OLDPAGE,RID,RN,RPNAME,VL,ZZRID,vc1,vc2,vc3,vc4,vc5,vovc1,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+6,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	do VP1 quit:VFMQ!verror.get()  set V=$E(RPNAME,1,95) set VL=V
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	do VP2 quit:VFMQ!verror.get()  set V=$J(VPN,3)
	set VL=VL_$J("",128-VL.length())_V
	do VOM
	set VL="" do VOM
	set VL="                                                                              "_"Interest Paid     Interest   Backup       "
	do VOM
	set VL="                                                                                                 "_"Penalty   Withholding"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,RPNAME,VPN,vovc1,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="------------------------------------------------------------------------------------------------------------------------------------"
	do VOM
	set VL="                "_"Report Totals"
	set V=0						// @TOT([CD1099]CNT,0)
	for I=0:1:2 set V=V+VT(I,2,1)
	set VL=VL_$J("",38-VL.length())_$J(V,10)
	set V=0						// @TOT([CD1099]IPY,0)
	for I=0:1:2 set V=V+VT(I,3,1)
	set VL=VL_$J("",76-VL.length())_$J($FN(V,",",2),15)
	set V=0						// @TOT([CD1099]PPY,0)
	for I=0:1:2 set V=V+VT(I,4,1)
	set VL=VL_$J("",92-VL.length())_$J($FN(V,",",2),12)
	set V=0						// @TOT([CD1099]BWP,0)
	for I=0:1:2 set V=V+VT(I,1,1)
	set VL=VL_$J("",106-VL.length())_$J($FN(V,",",2),12)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: RPNAME

	type Public String LNAM,RPNAME,ZZRID

	set RPNAME=LNAM_"  ("_ZZRID_")"
	quit

VP2	// Column pre-processor - Variable: VPN

	type Public String OLDPAGE,VPN

	set VPN=OLDPAGE+1
	quit
