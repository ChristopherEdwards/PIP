vkchged // Access key changed

	type public RecordACHORIG achorig

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("SJD")
	set voldkey = vux.piece("|",1),vobj(achorig,-3) = voldkey // Copy old keys into object

