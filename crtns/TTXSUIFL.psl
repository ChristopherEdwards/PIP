	if ttxsrtuid.pm.isNull() do vreqerr("PM") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordTTXSRTUID ttxsrtuid

	if ttxsrtuid.sjd.isNull() do vreqerr("SJD") quit
	if ttxsrtuid.brcd.isNull() do vreqerr("BRCD") quit
	if ttxsrtuid.uid.isNull() do vreqerr("UID") quit

		if vx("PM").exists(),ttxsrtuid.pm.isNull() do vreqerr("PM") quit
	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("TTXSRTUID","MSG",1767,"TTXSRTUID."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
