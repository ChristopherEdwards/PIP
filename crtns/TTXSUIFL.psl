vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordTTXSRTUID ttxsrtuid

	set X = ttxsrtuid.sjd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("SJD", vRM) quit
	set X = ttxsrtuid.brcd if 'X.isNull(),X'?1.6N,X'?1"-"1.5N set vRM=$$^MSG(742,"N") do vdderr("BRCD", vRM) quit
	set X = ttxsrtuid.uid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("UID", vRM) quit
	set X = ttxsrtuid.badttx if 'X.isNull(),X'?1N set vRM=$$^MSG(742,"N") do vdderr("BADTTX", vRM) quit
	set X = ttxsrtuid.lpseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("LPSEQ", vRM) quit
	if ttxsrtuid.pid.length()>8 set vRM = $$^MSG(1076,8) do vdderr("PID", vRM) quit
	if '("01"[ttxsrtuid.PM) set vRM=$$^MSG(742,"L") do vdderr("PM", vRM) quit
	if ttxsrtuid.src.length()>5 set vRM = $$^MSG(1076,5) do vdderr("SRC", vRM) quit
	set X = ttxsrtuid.trnsrt if 'X.isNull(),'Db.isDefined("STBLTRNSRT","X") set vRM = $$^MSG(1485,X) do vdderr("TRNSRT", vRM) quit
	if ttxsrtuid.ttime.length()>16 set vRM = $$^MSG(1076,16) do vdderr("TTIME", vRM) quit
	quit

