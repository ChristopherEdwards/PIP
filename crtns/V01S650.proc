 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCRDTYP",VPGM=$T(+0),VSNAME="Card Type Screen"
 set VFSN("CRDTYP")="zfCRDTYP"
 set vPSL=1
 set KEYS(1)=fCRDTYP.CARDTYPE
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fCRDTYP),VDA1(.fCRDTYP),^DBSPNT()

 if '%ProcessMode do VNEW(.fCRDTYP),VPR(.fCRDTYP),VDA1(.fCRDTYP)
 if %ProcessMode do VLOD(.fCRDTYP) quit:$G(ER)  do VPR(.fCRDTYP),VDA1(.fCRDTYP)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fCRDTYP)
	quit


VNEW(RecordCRDTYP fCRDTYP) // Initialize arrays if %O=0
 
 do VDEF(.fCRDTYP)
 do VLOD(.fCRDTYP)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
