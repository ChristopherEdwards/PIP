ERPRIVNP		/*
	 ORIG: SANTHUMS- 08/04/2006
	 DESC: End Revised Privacy Notice Period
	
	---- Comments --------------------------------------------------------
	
	This New Procedure will set the values of the columns CIF.PRIV and 
	CIF.SOL from Option 3 to 1.
	
	Function ERPRIVNP executes this procedure. 
	
	This New function has to be executed after the execution of the function 
	Start Revised Privacy Notice Period(SRPRIVNP). 	    		   
			
	---- Revision History ------------------------------------------------
	
	08/04/06 - SANTHUMS - CR 22468
	
	  	  Created new Procedure	
	*/
	
	type Number ACN
	
	type public ResultSet rs = Db.select("ACN,PRIV,SOL","CIF","REVNOTDT=:%SystemDate-30")

	while rs.next() do {
		
		set ACN=rs.getCol("ACN")

		type RecordCIF cif=Db.getRecord("CIF","ACN=:ACN")

		do cif.setAuditFlag(1)
			
		if rs.getCol("PRIV")=3 set cif.priv=1
		
		if rs.getCol("SOL")=3  set cif.sol=1
		
		do cif.save()	
		 	
		}			
	quit			
			
 #OPTION ResultClass ON
Public String vSIG()	quit "60648^51765^Diane Spatola^903"	// Signature - LTD^TIME^USER^SIZE
