V00S554(%ProcessMode,RecordUTBLBRCD fUTBRCD)   // -  - SID= <UTBLBRCD> Branch Code Screen Linkage
 ;;Copyright(c)2006 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/24/2006 12:20 - kini
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="UTBLBRCD"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+1 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+2
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Branch Codes|UTBLBRCD1"
 set VPG(%PG+1)="Branch Code Screen - Shared Branch Ntwrk|UTBLBRCD2"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fUTBRCD)
 if %ProcessMode do VLOD(.fUTBRCD) if $G(ER) set VFMQ="Q" quit
 do VPG(.fUTBRCD)
 quit

VNEW(RecordUTBLBRCD fUTBRCD)
 
 quit
 //
 ;
VLOD(RecordUTBLBRCD fUTBRCD)  // User defined access section
	quit

VPG(RecordUTBLBRCD fUTBRCD)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fUTBRCD),VPG0(.fUTBRCD) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fUTBRCD),VPG0(.fUTBRCD) quit:vDONE
  }
 quit

VPG0(RecordUTBLBRCD fUTBRCD)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fUTBRCD) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordUTBLBRCD fUTBRCD)  // Branch Codes
 
 new x
 set SID="UTBLBRCD1",DFID="UTBLBRCD"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S557
 if %ProcessMode>1 quit
 do VTAB^V00S557(.fUTBRCD)
 quit
 
VPG2(RecordUTBLBRCD fUTBRCD)  // Branch Code Screen - Shared Branch Ntwrk
 
 new x
 set SID="UTBLBRCD2",DFID="UTBLBRCD"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S558
 if %ProcessMode>1 quit
 do VTAB^V00S558(.fUTBRCD)
 quit
 

VBTM(RecordUTBLBRCD fUTBRCD)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
