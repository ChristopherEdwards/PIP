vreqn // Validate required data items

	type public RecordDBTBL33 dbtbl33

	if dbtbl33.des.isNull() do vreqerr("DES") quit
	if dbtbl33.mglobal.isNull() do vreqerr("MGLOBAL") quit
	if dbtbl33.mtz.isNull() do vreqerr("MTZ") quit
	if dbtbl33.nonrand.isNull() do vreqerr("NONRAND") quit
	if dbtbl33.restind.isNull() do vreqerr("RESTIND") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordDBTBL33 dbtbl33

	if dbtbl33.%libs.isNull() do vreqerr("%LIBS") quit
	if dbtbl33.bchid.isNull() do vreqerr("BCHID") quit

		if vx("DES").exists(),dbtbl33.des.isNull() do vreqerr("DES") quit
		if vx("MGLOBAL").exists(),dbtbl33.mglobal.isNull() do vreqerr("MGLOBAL") quit
		if vx("MTZ").exists(),dbtbl33.mtz.isNull() do vreqerr("MTZ") quit
		if vx("NONRAND").exists(),dbtbl33.nonrand.isNull() do vreqerr("NONRAND") quit
		if vx("RESTIND").exists(),dbtbl33.restind.isNull() do vreqerr("RESTIND") quit
	quit

