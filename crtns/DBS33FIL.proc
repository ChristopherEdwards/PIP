	type RecordDBTBL33 dbtbl33 = Db.getRecord("DBTBL33","%LIBS=:%LIBS,BCHID=:BCHID")
	if (%ProcessMode = 2) do {
		do dbtbl33.setMode(2)

		do DBS33FIL(dbtbl33,vpar)
	}

	quit

vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL33 dbtbl33

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL33","%LIBS = :dbtbl33.%libs and BCHID = :dbtbl33.bchid") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))

	if vpar'["/NOUPDATE/" do {

		set dbtbl33.LTD = %CurrentDate
		set dbtbl33.TIME = %CurrentTime
		set dbtbl33.USER = %UserName
		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(dbtbl33))  S ^DBTBL(vobj(dbtbl33,-3),33,vobj(dbtbl33,-4))=vobj(dbtbl33)
		#ENDBYPASS
	}

	quit

vdelete(Boolean vkeychg) // Record Delete

