 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'%ProcessMode.exists() %ProcessMode=5
 if (%ProcessMode = 5) do {
  if 'fMPSPR.exists() set fMPSPR = Class.new("RecordUTBLMPSPROD")
 }
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLMPSPROD",VPGM=$T(+0),VSNAME="MPS Card Extract Options-Product Type"
 set VFSN("UTBLMPSPROD")="zfMPSPR"
 set vPSL=1
 set KEYS(1)=fMPSPR.CO
 set KEYS(2)=fMPSPR.CARDTYP
 set KEYS(3)=fMPSPR.TYP
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fMPSPR),VDA1(.fMPSPR),^DBSPNT() quit

 set ER=0 do VSCRPRE(.fMPSPR) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fMPSPR),VPR(.fMPSPR),VDA1(.fMPSPR)
 if %ProcessMode do VLOD(.fMPSPR) quit:$G(ER)  do VPR(.fMPSPR),VDA1(.fMPSPR)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=XECUTE
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fMPSPR)
	quit


VNEW(RecordUTBLMPSPROD fMPSPR) // Initialize arrays if %O=0
 
 do VDEF(.fMPSPR)
 do VLOD(.fMPSPR)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit
	
VDEF(RecordUTBLMPSPROD fMPSPR)
 quit:fMPSPR.CO.isNull() ! fMPSPR.CARDTYP.isNull() ! fMPSPR.TYP.isNull() 
 quit:%ProcessMode  set ER=0 if (fMPSPR.CO="")!(fMPSPR.CARDTYP="")!(fMPSPR.TYP="") set ER=1,RM=$$^MSG(1767,"CO,CARDTYP,TYP") Q
 if Db.isDefined("UTBLMPSPROD","UTBLMPSPROD.CO=:fMPSPR.CO AND UTBLMPSPROD.CARDTYP=:fMPSPR.CARDTYP AND UTBLMPSPROD.TYP=:fMPSPR.TYP") S ER=1,RM=$$^MSG(2327) Q
 do fMPSPR.setAuditFlag(1)
 if fMPSPR.MPSADDCRD="" set fMPSPR.MPSADDCRD=1
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 ;
VLOD(RecordUTBLMPSPROD fMPSPR) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


	type Public String %MODS,%REPEAT
	quit
	

