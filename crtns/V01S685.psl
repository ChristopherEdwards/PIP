 set VO="14|8|13|0"
 set VO(8)=$C(9,44,1,0,0,0,0,0,0,0)_"00L"_$S(fMPSPR.FNDOPT:"Y",1:"N")
 set VO(9)=$C(10,43,2,0,0,0,0,0,0,0)_"00N"_fMPSPR.OFST
 set VO(10)=$C(12,44,1,0,0,0,0,0,0,0)_"00L"_$S(fMPSPR.ACNADDR:"Y",1:"N")
