VP2(RecordEFTPAY fEFTPAY) //
 // The following lines prevent ACN & BENACN from being defaulted in the
 // the case where an NDPO is being created from an NDPOIN.  In this case,
 // ACN should be null and BENACN should contain the original BENACN (BENACNSW)
 // from the NDPOIN.    - Pat Kelly 36033
 
 type public Boolean INTTRN
 
 if INTTRN.get() do {
 	set fEFTPAY.ACN="" do DISPLAY^DBSMACRO("EFTPAY.ACN",fEFTPAY.ACN)
 	set fEFTPAY.BENACN=fEFTPAY.BENACNSW do DISPLAY^DBSMACRO("EFTPAY.BENACN",fEFTPAY.BENACN)
 	}
 
 quit
