VPR(RecordUTBLRFLG fUTRFLG) // Display screen prompts
 set VO="8||13|"
 set VO(0)="|"
 set VO(1)=$C(1,22,38,1,0,0,0,0,0,0)_"01T User-Defined Restriction Maintenance "
 set VO(2)=$C(4,9,15,0,0,0,0,0,0,0)_"01T Product Group:"
 set VO(3)=$C(5,11,13,0,0,0,0,0,0,0)_"01T Restriction:"
 set VO(4)=$C(7,11,13,1,0,0,0,0,0,0)_"01T Description:"
 set VO(5)=$C(11,5,71,0,0,0,0,0,0,0)_"01TEnter below those transaction codes for which this restriction does not"
 set VO(6)=$C(12,5,70,0,0,0,0,0,0,0)_"01Tapply.  Separate all entries by commas; e.g., LP,LPC,LPCO, etc.  Enter"
 set VO(7)=$C(13,5,72,0,0,0,0,0,0,0)_"01TDR* to indicate all debit transactions. Enter CR* to indicate all credit"
 set VO(8)=$C(14,5,13,0,0,0,0,0,0,0)_"01Ttransactions."
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


