vreqn // Validate required data items

	type public RecordXCHECK xcheck

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordXCHECK xcheck

	if xcheck.cid.isNull() do vreqerr("CID") quit
	if xcheck.chks.isNull() do vreqerr("CHKS") quit
	if xcheck.high.isNull() do vreqerr("HIGH") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("XCHECK","MSG",1767,"XCHECK."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordXCHECK xcheck

	set X = xcheck.cid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CID", vRM) quit
	set X = xcheck.chks if 'X.isNull(),'Db.isDefined("UTBLCHKS","X") set vRM = $$^MSG(1485,X) do vdderr("CHKS", vRM) quit
	if xcheck.high.length()>12 set vRM = $$^MSG(1076,12) do vdderr("HIGH", vRM) quit
	set X = xcheck.acn if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("ACN", vRM) quit
	if xcheck.low.length()>12 set vRM = $$^MSG(1076,12) do vdderr("LOW", vRM) quit
	if xcheck.nam.length()>40 set vRM = $$^MSG(1076,40) do vdderr("NAM", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("XCHECK","MSG",979,"XCHECK."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordXCHECK xcheck

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("CID").exists() set vux("CID") = vx("CID")
	if vx("CHKS").exists() set vux("CHKS") = vx("CHKS")
	if vx("HIGH").exists() set vux("HIGH") = vx("HIGH")
	do vkey(1) set voldkey = xcheck.cid_","_xcheck.chks_","_xcheck.high // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = xcheck.cid_","_xcheck.chks_","_xcheck.high // Copy new keys into object
	type RecordXCHECK vnewrec = xcheck.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("XCHECK",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordXCHECK xcheck

	if vux("CID").exists() set xcheck.cid = vux("CID").piece("|",i)
	if vux("CHKS").exists() set xcheck.chks = vux("CHKS").piece("|",i)
	if vux("HIGH").exists() set xcheck.high = vux("HIGH").piece("|",i)
	quit
