	type public RecordCMTINC cmtinc


	// Allow global reference - Delete record
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	ZWI ^CMTINC(vobj(cmtinc,-3),vobj(cmtinc,-4),vobj(cmtinc,-5))
	#ENDBYPASS
	quit


vinit // Initialize default values

	type public RecordCMTINC cmtinc

	// Type local variables for access keys for defaults
	type public String CDT
	type public String TYPE
	type public String ELEMENT

	if cmtinc.newflg.isNull() set cmtinc.newflg = 0        // newflg
	quit


vreqn // Validate required data items

	type public RecordCMTINC cmtinc

	if cmtinc.newflg.isNull() do vreqerr("NEWFLG") quit
	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordCMTINC cmtinc

	if cmtinc.cdt.isNull() do vreqerr("CDT") quit
	if cmtinc.type.isNull() do vreqerr("TYPE") quit
	if cmtinc.element.isNull() do vreqerr("ELEMENT") quit

		if vx("NEWFLG").exists(),cmtinc.newflg.isNull() do vreqerr("NEWFLG") quit
	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("CMTINC","MSG",1767,"CMTINC."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordCMTINC cmtinc

	set X = cmtinc.cdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("CDT", vRM) quit
	if cmtinc.type.length()>4 set vRM = $$^MSG(1076,4) do vdderr("TYPE", vRM) quit
	if cmtinc.element.length()>50 set vRM = $$^MSG(1076,50) do vdderr("ELEMENT", vRM) quit
	set X = cmtinc.coredate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("COREDATE", vRM) quit
	if cmtinc.coreuser.length()>40 set vRM = $$^MSG(1076,40) do vdderr("COREUSER", vRM) quit
	set X = cmtinc.custdate if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("CUSTDATE", vRM) quit
	if cmtinc.custuser.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CUSTUSER", vRM) quit
	if '("01"[cmtinc.NEWFLG) set vRM=$$^MSG(742,"L") do vdderr("NEWFLG", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("CMTINC","MSG",979,"CMTINC."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordCMTINC cmtinc

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("CDT").exists() set vux("CDT") = vx("CDT")
	if vx("TYPE").exists() set vux("TYPE") = vx("TYPE")
	if vx("ELEMENT").exists() set vux("ELEMENT") = vx("ELEMENT")
	do vkey(1) set voldkey = cmtinc.cdt_","_cmtinc.type_","_cmtinc.element // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = cmtinc.cdt_","_cmtinc.type_","_cmtinc.element // Copy new keys into object
	type RecordCMTINC vnewrec = cmtinc.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("CMTINC",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordCMTINC cmtinc

	if vux("CDT").exists() set cmtinc.cdt = vux("CDT").piece("|",i)
	if vux("TYPE").exists() set cmtinc.type = vux("TYPE").piece("|",i)
	if vux("ELEMENT").exists() set cmtinc.element = vux("ELEMENT").piece("|",i)
	quit
