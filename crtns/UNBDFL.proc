		do vexec
	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("NBDC").exists() do vkchged quit                // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("UTBLNBD",.vx)
