public VINDEX(RecordUTBLNBD utblnbd) // Update index entries

		type Public String vx()

	if %ProcessMode=1 do { quit
		if vx("CNTRY").exists() do vi1(.utblnbd)
	}
	do vi1(.utblnbd)

	quit
	

vi1(RecordUTBLNBD utblnbd) // Maintain CNTRY index entries (Country Code)

	type Public String vx()
	type Boolean vdelete = 0
	type String v1 = utblnbd.cntry
	if v1.isNull() set v1=(PSL.maxCharValue-1).char()
	type String v2 = utblnbd.nbdc

	if %ProcessMode=2 do { quit

		// Allow global reference
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
		if $D(^UTBL("NBD",vobj(utblnbd,-3)))#2,'$D(^XCAL(v1,v2)) do vidxerr("CNTRY")
		}


	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	if %ProcessMode<2 set ^XCAL(v1,v2)="" 
	quit:%ProcessMode=0

	if %ProcessMode=3 set vdelete=1
	if vx("CNTRY").exists() set v1=vx("CNTRY").piece("|",1) set:v1.isNull() v1=(PSL.maxCharValue-1).char()

	// Allow global reference
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602
	kill ^XCAL(v1,v2)
	quit

public VIDXBLD(List vlist) // Rebuild index files (External call)


	type Number %ProcessMode=0                             // Create mode
	type Number i

	if vlist.get().isNull() set vlist="VINDEX"             // Build all

	type DbSet ds=Db.selectDbSet("UTBLNBD")

	while ds.next() do {
		type RecordUTBLNBD utblnbd=ds.getRecord("UTBLNBD")
		if vlist.contains("VINDEX") do VINDEX(.utblnbd) quit
		if vlist.contains("CNTRY") do vi1(.utblnbd)
	}

	quit


public VIDXBLD1(RecordUTBLNBD utblnbd, List vlist) // Rebuild index files for one record (External call)


	type Number i

	if vlist.contains("VINDEX") do VINDEX(.utblnbd) quit
	if vlist.contains("CNTRY") do vi1(.utblnbd)

	quit


vidxerr(di) // Error message

	D SETERR^DBSEXECU("UTBLNBD","MSG",1225,"UTBLNBD."_di)

	quit


vkchged // Access key changed

	type public RecordUTBLNBD utblnbd

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	set vux = vx("NBDC")
	set voldkey = vux.piece("|",1),vobj(utblnbd,-3) = voldkey // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	set vnewkey = vux.piece("|",2),vobj(utblnbd,-3) = vnewkey // Copy new keys into object
	type RecordUTBLNBD vnewrec = utblnbd.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("UTBLNBD",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	set vobj(utblnbd,-3) = vux.piece("|",1)                // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

