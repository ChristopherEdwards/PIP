	if %O = 0 do AUDIT^UCUTILN(utblnbd,.vxins(),1,"|")
	if %O = 1 do AUDIT^UCUTILN(utblnbd,.vx(),1,"|")

	if 'vparNorm.get() set vpar = $$initPar^UCUTILN(vpar)  // Run-time qualifiers

