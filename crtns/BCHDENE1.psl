		set CSEQ = rs.getCol("CSEQ")
		
 		type RecordDENACTU denactu = Db.getRecord("DENACTU","BRCD = :BRCD, UID = :UID, CRCD = :CRCD, CSEQ = :CSEQ", 1)
 		
 		if denactu.getMode() quit

		set denactu.cnt = 0
		
		do denactu.bypassSave()
