 set %TAB(5)=$C(10,21,12)_"00N12441|1|[UTBLGLSC]LGLIO|[GLAD]"
 set %TAB(6)=$C(11,21,12)_"00N12443|1|[UTBLGLSC]LGLLC|[GLAD]"
