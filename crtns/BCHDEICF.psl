vOPEN(String vINPUT, Boolean %BatchExit) //
	// Don't run if all of DEP, LN, and CIF done today, i.e., if
	// any one was not run, then don't quit

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	new EXIT
	set EXIT=1

	if '(cuvar.icdnd=%SystemDate!(cuvar.icdld=%SystemDate)) set EXIT=0
	else  if '(cuvar.iclnd=%SystemDate!(cuvar.iclld=%SystemDate)) set EXIT=0
	else  if '(cuvar.iccnd=%SystemDate!(cuvar.iccld=%SystemDate)) set EXIT=0

	if EXIT set %BatchExit=1 quit

