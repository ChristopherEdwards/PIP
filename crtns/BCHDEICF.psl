private vTHRINIT(vINPUT,vRETURN) //
	// If all have been done for any of CIF, DEP, or LN, don't redo

	set (DOCIF,DODEP,DOLN)=0

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	if cuvar.iccff,'(cuvar.iccnd=%SystemDate!(cuvar.iccld=%SystemDate)) set DOCIF=1
	if cuvar.icdff,'(cuvar.icdnd=%SystemDate!(cuvar.icdld=%SystemDate)) set DODEP=1
	if cuvar.iclff,'(cuvar.iclnd=%SystemDate!(cuvar.iclld=%SystemDate)) set DOLN=1

 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
