R01S512	// SCA590 - Country Sovereign Limits Detail Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 01/31/2007 12:51 - shetyes

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String CNTRY

	set RID="SCA590"
	set RN="Country Sovereign Limits Detail Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("CNTRY")="|2|||[STBLCNTRY]||||T|Country Code|||||"
	set %TAB("IO")=$$IO^SCATAB

	set %READ="IO/REQ,CNTRY#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String CNTRY
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,RID,RN,VL,VLOF,VRF(),VSTATS(),vCOL,vHDG,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA590",RN="Country Sovereign Limits Detail Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA590","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[SCA590]CID#31#12,[SCA590]TYPE#46#4,[SCA590]COMEXP#60#15,[SCA590]OUTEXP#79#15,[SCA590]MDT#99#10,[SCA590]TOTCOL#112#15,[SCA590]MCOL#129#1"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE="CIF.GOVT=1"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="Country Sovereign Limits Detail Report"
		set VBNRINFO("PGM")="R01S512"
		set VBNRINFO("RID")="SCA590"
		set VBNRINFO("TABLES")="SCA590,CIF"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,SCA590]JOB"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,SCA590]ACN"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,SCA590]TYPE"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,SCA590]CID"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:2 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("SCA590.JOB,SCA590.ACN,SCA590.TYPE,SCA590.CID,SCA590.COMEXP,SCA590.OUTEXP,CIF.NAM,SCA590.MDT,SCA590.TOTCOL,SCA590.MCOL","SCA590,CIF","CIF.GOVT=1","SCA590.JOB,SCA590.ACN,SCA590.TYPE,SCA590.CID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10
	set vc1=V.piece($C(9),1)			// SCA590.JOB
	set vc2=V.piece($C(9),2)			// SCA590.ACN
	set vc3=V.piece($C(9),3)			// SCA590.TYPE
	set vc4=V.piece($C(9),4)			// SCA590.CID
	set vc5=V.piece($C(9),5)			// SCA590.COMEXP
	set vc6=V.piece($C(9),6)			// SCA590.OUTEXP
	set vc7=V.piece($C(9),7)			// CIF.NAM
	set vc8=V.piece($C(9),8)			// SCA590.MDT
	set vc9=V.piece($C(9),9)			// SCA590.TOTCOL
	set vc10=V.piece($C(9),10)			// SCA590.MCOL
	quit

	// User-defined pre/post-processor code

VPREAQ	// Pre-processor (after query)

	type Public String CNTRY
	type Number ACN

	do Db.delete("SCA590","JOB=:%ProcessID")
	
	set ACN=0

	type ResultSet rscif=Db.select("ACN","CIF","GOVT=1")
	while rscif.next() do {

		set ACN=rscif.getCol("ACN")

		type RecordCIF cif=Db.getRecord("CIF","ACN=:ACN",1)
		if cif.pcntry'=CNTRY quit

		do IBS(ACN)
		do NONIBS(ACN)
		}
	
	quit


IBS(Number ACN)

	type Number CID
	type String TOT

	type ResultSet rsrelcif=Db.select("CID","RELCIF","ACN=:ACN")
	while rsrelcif.next() do {

		set CID=rsrelcif.getCol("CID")

		type RecordACN acn=Db.getRecord("ACN","CID=:CID",1)

		if acn.cls="D" set TOT=$$DEPOSIT^PROCLIM(CID,0)
		else  set TOT=$$LOAN^PROCLIM(CID,0)

		type RecordSCA590 sca590=Class.new("RecordSCA590")

		set sca590.job=%ProcessID
		set sca590.acn=ACN
		set sca590.type=acn.type
		set sca590.cid=CID
		set sca590.comexp=TOT.piece("|",1)
		set sca590.outexp=TOT.piece("|",2)

		if acn.cls="L" do {

			set sca590.mdt=acn.mdt
			set sca590.totcol=$$XLN16^LNCDI(CID).piece("|",3)
			set sca590.mcol=$$MCOL^LNCDI(CID)
			}

		do sca590.bypassSave()
		}

	quit


NONIBS(Number ACN)

	type String INTPROD,RECID,SYS,TOT

	set (INTPROD,RECID,SYS)=""

	type ResultSet rsexpos=Db.select("SYS,INTPROD,RECID","EXPOS","ACN=:ACN")
	while rsexpos.next() do {

		set SYS=rsexpos.getCol("SYS")
		set INTPROD=rsexpos.getCol("INTPROD")
		set RECID=rsexpos.getCol("RECID")

		type RecordSCA590 sca590=Class.new("RecordSCA590")
			
		set sca590.job=%ProcessID
		set sca590.acn=ACN
		set sca590.type=INTPROD
		set sca590.cid=RECID

		set TOT=$$EXPREC^PROCLIM(ACN,SYS,INTPROD,RECID)

		set sca590.comexp=TOT.piece("|",1)
		set sca590.outexp=TOT.piece("|",2)

		type RecordEXPOS expos=Db.getRecord("EXPOS","ACN=:ACN,SYS=:SYS,INTPROD=:INTPROD,RECID=:RECID",1)
		set sca590.mdt=expos.mdt

		do sca590.bypassSave()
		}
	
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'VFMQ do VRSUM
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(2).get() do VDTL2 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(+vovc1'=+vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(+vovc2'=+vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1
	if vb3!(vovc3'=vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),vc5,vc6
	type Public String VSTATS

	set VT(4)=VT(4)+1
	set VT(4,1,1)=VT(4,1,1)+vc5			// @TOT([SCA590]COMEXP)
	set VT(4,2,1)=VT(4,2,1)+vc6			// @TOT([SCA590]OUTEXP)
	quit

VDTL2	// Detail
	type public String %TIM,CNTRY,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(2)  set VD(2)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc2,12)
	set VL=VL_$J("",13-VL.length())_$E(vc7,1,40)
	do VOM
	quit

VDTL4	// Detail
	type public String %TIM,CNTRY,IOSL,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="                              "_$J(vc4,12)
	set VL=VL_$J("",43-VL.length())_$J(vc3,6)
	set VL=VL_$J("",50-VL.length())_$J($FN(vc5,",",2),24)
	set VL=VL_$J("",75-VL.length())_$J($FN(vc6,",",2),18)
	set VL=VL_$J("",98-VL.length())_$J($$DAT^%ZM(vc8),10)
	set VL=VL_$J("",109-VL.length())_$J($FN(vc9,",",2),17)
	set VL=VL_$J("",128-VL.length())_$S(vc10:"Y",1:"N")
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CNTRY,CONAM,RID,RN,VL,vc1,vc10,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+6,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,40)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",122-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",121-VL.length())_"Page:"
	set VL=VL_$J("",127-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="    "_"Customer  Borrower        Account        Prod          ------- Exposure Amounts -------       Maturity     Total Col    Multiple"
	do VOM
	set VL="     "_"Number                   Number         Type            Committed        Outstanding         Date         Value        Loan"
	do VOM
	set VL="==================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VRSUM	// Report Summary
	type Public Number IOSL,verror,VFMQ,vh,VLC,VT(),VX()
	type Public String %TIM,CNTRY,VPN,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number I
	type String V,VL

	set VL=""
	if 'vh(0) do VHDG0 quit:VFMQ
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="" do VOM
	set VL="REPORT TOTALS: "
	set V=0						// @TOT([SCA590]COMEXP,0)
	for I=0:1:4 set V=V+VT(I,1,1)
	set VL=VL_$J("",59-VL.length())_$J($FN(V,",",2),15)
	set V=0						// @TOT([SCA590]OUTEXP,0)
	for I=0:1:4 set V=V+VT(I,2,1)
	set VL=VL_$J("",78-VL.length())_$J($FN(V,",",2),15)
	do VOM
	quit

VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors
