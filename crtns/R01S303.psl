VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,NET,NEXTD,PAYDES,PAYDET,PAYEE,PAYID,PAYTOT,REC,REMNDTOT,RID,RN,SEQ,TRTYPE,TRTYPE1,TYP,VL,vc1,vc10,vc11,vc12,vc13,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+12,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="" do VOM
	set VL="Transfer Type: "_vc1_" "
	do VOM
	set VL="" do VOM
	set VL="         "_"Next                                                     R  A  S  ------------- Next Remittance -------------"
	do VOM
	set VL="Payee Remittance Type   Loan Account Seq  Borrower                E  E  P         Amount    -   Partial      =     Net       Balance"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM
	do VP7 quit:VFMQ!verror.get()  set V=$E(PAYDET,1,60) do VP8 quit:VFMQ!verror.get()  set VL=V
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: PAYTOT

 type public String PAYE,PAYTOT

 set PAYTOT="Payee "_PAYE_" Totals:"
	quit

VP2	// Column pre-processor - Variable: REMNDTOT

 type public String REMND,REMNDTOT

 set REMNDTOT=REMND_" TOTAL:"

 
	quit

VP3	// Column post-processor - Variable: REMNDTOT

 type public Boolean PT

 set PT=1
	quit

VP4	// Column post-processor - [SYSDEV,TRTYPE]REMND

	type public String vc3
 type public Boolean PT
 type public String REMND,V
 
 type Date REMDT

 set REMDT=vc3
 set REMND=REMDT.toString("MM/DD/YEAR")
 if 'PT set V="      "
 set PT=0
	quit

VP5	// Column pre-processor - Variable: SEQ

 type public Number SEQ
 set SEQ=1
	quit

VP6	// Column pre-processor - Variable: NET

	type public String vc11,vc12
 type public Number REMACT,NET

 set REMACT=vc11
 set NET=REMACT-vc12
	quit

VP7	// Column pre-processor - Variable: PAYDET

	type public String vc1
 type public Boolean REC
 type public Date NEXTD
 type public String PAYDES,PAYDET,PAYID,TRTYPE1

 if REC=0 set PAYDET="" quit
 set PAYDET=PAYID_" - "_PAYDES_", Next Remittance: "_NEXTD.toString("MM/DD/YEAR")
 set TRTYPE1=vc1
 
	quit

VP8	// Column post-processor - Variable: PAYDET

 type public Boolean PT,REC
 type public String PAYE,PAYID

 if REC=0 quit
 set PT=1
 set PAYE = PAYID
	quit
