 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="UTBLACC"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+2 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+3
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Provision Classification|UTBLPROV"
 set VPG(%PG+1)="Delinquency Asset Reclassification|UTBLDAR1"
 set VPG(%PG+2)="Reclassification G/L Set Code User Table|UTBLDAR2"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fUTBLACC)
 if %ProcessMode do VLOD(.fUTBLACC) if $G(ER) set VFMQ="Q" quit
 do VPG(.fUTBLACC)
 quit

VNEW(RecordUTBLACC fUTBLACC)
 
 quit
 //
 ;
VLOD(RecordUTBLACC fUTBLACC)  // User defined access section
	quit

VPG(RecordUTBLACC fUTBLACC)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fUTBLACC),VPG0(.fUTBLACC) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fUTBLACC),VPG0(.fUTBLACC) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.fUTBLACC),VPG0(.fUTBLACC) quit:vDONE
  }
 quit

VPG0(RecordUTBLACC fUTBLACC)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fUTBLACC) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordUTBLACC fUTBLACC)  // Provision Classification
 
 new x
 set SID="UTBLPROV",DFID="UTBLACC"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S620
 if %ProcessMode>1 quit
 do VTAB^V00S620(.fUTBLACC)
 quit
 
VPG2(RecordUTBLACC fUTBLACC)  // Delinquency Asset Reclassification
 
 new x
 set SID="UTBLDAR1",DFID="UTBLACC"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S581
 if %ProcessMode>1 quit
 do VTAB^V00S581(.fUTBLACC)
 quit
 
VPG3(RecordUTBLACC fUTBLACC)  // Reclassification G/L Set Code User Table
 
 new x
 set SID="UTBLDAR2",DFID="UTBLACC"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S582
 if %ProcessMode>1 quit
 do VTAB^V00S582(.fUTBLACC)
 quit
 

VBTM(RecordUTBLACC fUTBLACC)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
