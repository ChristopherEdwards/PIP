V00S520(%ProcessMode,RecordTRN %A)   // -  - SID= <TRNM> Tran Code Maintenance - Misc Linkage 
 ;;Copyright(c)2006 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/24/2006 12:15 - kini
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="TRNM"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+3 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+4
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Tran Code Maintenance - General |TRN"
 set VPG(%PG+1)="Tran Code Maintenance - Misc Fields |TRNMF1"
 set VPG(%PG+2)="Tran Code Maintenance - Misc Fields |TRNMF2"
 set VPG(%PG+3)="Linked Transaction Codes |TRNLNK"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.%A)
 if %ProcessMode do VLOD(.%A) if $G(ER) set VFMQ="Q" quit
 do VPG(.%A)
 quit

VNEW(RecordTRN %A)
 
 do VLOD(.%A)
 quit
VNEWDQ(RecordTRN %A) // Original VNEW section
 
 quit
 //
 ;
VLOD(RecordTRN %A)  // User defined access section
 ;
 quit
VLODDQ(RecordTRN %A) // Original VLOD section
 ;
	quit

VPG(RecordTRN %A)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.%A),VPG0(.%A) quit:vDONE
  }
 quit

VPG0(RecordTRN %A)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.%A) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordTRN %A)  // Tran Code Maintenance - General 
 
 new x
 set SID="TRN",DFID="TRN"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S501
 if %ProcessMode>1 quit
 do VTAB^V00S501(.%A)
 quit
 
VPG2(RecordTRN %A)  // Tran Code Maintenance - Misc Fields 
 
 new x
 set SID="TRNMF1",DFID="TRN"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S524
 if %ProcessMode>1 quit
 do VTAB^V00S524(.%A)
 quit
 
VPG3(RecordTRN %A)  // Tran Code Maintenance - Misc Fields 
 
 new x
 set SID="TRNMF2",DFID="TRN"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S525
 if %ProcessMode>1 quit
 do VTAB^V00S525(.%A)
 quit
 
VPG4(RecordTRN %A)  // Linked Transaction Codes 
 
 new x
 set SID="TRNLNK",DFID="TRN"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S519
 if %ProcessMode>1 quit
 do VTAB^V00S519(.%A)
 quit
 

VBTM(RecordTRN %A)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
