	            Also modified the code to remove deprecated feature warnings.
	 */             
	quit

GRP(XCID) //
	/*
        The function is used to compute [LNDELNDA]GRP field.
        
        ARGUMENTS:
         . XCID         Account Number         /TYP=N/REQ/MECH=VAL
        
         RETURNS: Product Group
        
         EXAMPLE:  $$GRP^LNDELCDI(CID)
	*/
	
	new XGRP
	if $G(XCID)="" quit ""
	type ResultSet rs=Db.select("GRP","LN","CID=:XCID")
	if rs.next() set XGRP=rs.getCol("GRP")
	quit XGRP
	
TYPE(XCID) //
	/*
        The function is used to compute [LNDELNDA]TYPE field.
        
        ARGUMENTS:
         . XCID         Account Number         /TYP=N/REQ/MECH=VAL
        
         RETURNS: Product Type
        
         EXAMPLE:  $$GRP^LNDELCDI(CID)
	*/

	new XTYPE
	if $G(XCID)="" quit ""
	type ResultSet rs1=Db.select("TYPE","LN","CID=:XCID")
	if rs1.next() set XTYPE=rs1.getCol("TYPE")
	quit XTYPE
	
public	R138	// Called by function LNR010 (delinquent notices)

	type public String RID

	set RID = CUVAR.DLQNT
	if RID.isNull() set RID = "SCA138"

	quit
