VPO1(RecordEFTPAY fEFTPAY)  // user library EFT_BENACN
 type Number SAVX,TRUNC,XACN
 
 type public Boolean ER
 type public Number X
 type public String RM
 if X.isNull() quit
 
 set SAVX=X
 set XACN=X
 
 type RecordCIF cif=Db.getRecord("CIF","ACN=:XACN",1)
 set fEFTPAY.BENAD1=(cif.nam).extract(1,35)
 set fEFTPAY.BENAD2=(cif.mad1).extract(1,35)
 set fEFTPAY.BENAD3=(cif.mad2).extract(1,35)
 if '(cif.mstate).isNull() do {
 	if '(cif.mcity).isNull() set fEFTPAY.BENAD4=cif.mcity
 	set fEFTPAY.BENAD4=fEFTPAY.BENAD4_", "_cif.mstate
 	if '(cif.mzip).isNull(),cif.mzip'=" " set fEFTPAY.BENAD4=fEFTPAY.BENAD4_", "_cif.mzip
 	if '(cif.mcntry).isNull() set fEFTPAY.BENAD4=fEFTPAY.BENAD4_", "_cif.mcntry
 	if (fEFTPAY.BENAD4).length()>35 set fEFTPAY.BENAD4=(fEFTPAY.BENAD4).extract(1,35),TRUNC=1
 	}
 else  do {
 	if '(cif.mcity).isNull() set fEFTPAY.BENAD4=cif.mcity
 	if '(cif.mzip).isNull(),cif.mzip'=" " set fEFTPAY.BENAD4=fEFTPAY.BENAD4_", "_cif.mzip
 	if '(cif.mcntry).isNull() set fEFTPAY.BENAD4=fEFTPAY.BENAD4_", "_cif.mcntry
 	if (fEFTPAY.BENAD4).length()>35 set fEFTPAY.BENAD4=(fEFTPAY.BENAD4).extract(1,35),TRUNC=1
 	}
 if (fEFTPAY.BENAD3).isNull() do {
 	set fEFTPAY.BENAD3=fEFTPAY.BENAD4
 	set fEFTPAY.BENAD4=""
 	}
 do DISPLAY^DBSMACRO("EFTPAY.BENAD1",fEFTPAY.BENAD1,"0")
 do DISPLAY^DBSMACRO("EFTPAY.BENAD2",fEFTPAY.BENAD2,"0")
 do DISPLAY^DBSMACRO("EFTPAY.BENAD3",fEFTPAY.BENAD3,"0")
 do DISPLAY^DBSMACRO("EFTPAY.BENAD4",fEFTPAY.BENAD4,"0")
 set X=fEFTPAY.BENAD1 do CHECKUP^SWFTEDIT if ER  do GOTO^DBSMACRO("EFTPAY.BENAD1") quit
 set X=fEFTPAY.BENAD2 do CHECKUP^SWFTEDIT if ER  do GOTO^DBSMACRO("EFTPAY.BENAD2") quit
 set X=fEFTPAY.BENAD3 do CHECKUP^SWFTEDIT if ER  do GOTO^DBSMACRO("EFTPAY.BENAD3") quit
 set X=fEFTPAY.BENAD4 do CHECKUP^SWFTEDIT if ER  do GOTO^DBSMACRO("EFTPAY.BENAD4") quit
 set X=SAVX
 if (((cif.nam).length()>35)!((cif.mad1).length()>35)!((cif.mad2).length()>35)) set TRUNC=1
 // Address truncated
 if TRUNC.get() set ER="W",RM=$$^MSG(2425)
 quit
 #ACCEPT DATE=11/05/03;pgm=screen compiler
 quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
