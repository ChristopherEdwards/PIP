 set VO(43)=$C(1,1,80,1,0,0,0,0,0,0)_"01T"_$S(%ProcessMode=5:"",1:$$BANNER^UTLREAD($G(%FN)))
 set VO(44)=$C(3,18,8,2,0,0,0,0,0,0)_"01T"_$E(fSCASYS.SYSSN,1,8)
 set VO(45)=$C(5,18,40,2,0,0,0,0,0,0)_"00T"_$E(fSCASYS.DES,1,40)
