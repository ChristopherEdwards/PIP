VTAB(RecordSCASYS fSCASYS)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=7,VPT=1,VPB=19,PGM=$T(+0),DLIB="SYSDEV",DFID="SCASYS"
 set OLNTB=19001
 
 set VFSN("SCASYS")="zfSCASYS"
 //
 // 
	
	
 set %TAB(1)=$C(2,17,8)_"21U12401|1|[SCASYS]SYSSN"
 set %TAB(2)=$C(4,17,40)_"00T12401|1|[SCASYS]DES"
 set %TAB(3)=$C(8,3,77)_"00T12402|1|[SCASYS]XECUT|||||||||255"
 set %TAB(4)=$C(13,3,75)_"00T12404|1|[SCASYS]XKILL|||do VP1^V00S454(.fSCASYS)"
 set %TAB(5)=$C(15,44,1)_"00L12405|1|[SCASYS]DBSDOMREQ"
 set %TAB(6)=$C(16,44,1)_"00L12406|1|[SCASYS]DBSMDDREQ"
 set %TAB(7)=$C(17,44,12)_"00U12407|1|[SCASYS]DBSMDD|[DBTBL1]"
