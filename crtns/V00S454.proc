 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="SCASYS",VPGM=$T(+0),VSNAME="Application Definition Table"
 set VFSN("SCASYS")="zfSCASYS"
 set vPSL=1
 set KEYS(1)=fSCASYS.SYSSN
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fSCASYS),VDA1(.fSCASYS),^DBSPNT()

 if '%ProcessMode do VNEW(.fSCASYS),VPR(.fSCASYS),VDA1(.fSCASYS)
 if %ProcessMode do VLOD(.fSCASYS) quit:$G(ER)  do VPR(.fSCASYS),VDA1(.fSCASYS)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fSCASYS)
	quit


VNEW(RecordSCASYS fSCASYS) // Initialize arrays if %O=0
 
 do VDEF(.fSCASYS)
 do VLOD(.fSCASYS)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordSCASYS fSCASYS)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordSCASYS fSCASYS) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

