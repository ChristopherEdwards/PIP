 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="LNAMOMSC",VPGM=$T(+0),VSNAME="Maintenance - Miscellaneous "
 set VFSN("AOAMO")="zfAOAMO"
 set vPSL=1
 set KEYS(1)=fAOAMO.TYP
 set KEYS(2)=fAOAMO.PAYEE
 set KEYS(3)=fAOAMO.PLAN
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fAOAMO),VDA1(.fAOAMO),^DBSPNT()

 // Display Pre-Processor
 //
 if '%ProcessMode do VNEW(.fAOAMO),VDSPPRE(.fAOAMO) quit:$G(ER)  do VPR(.fAOAMO),VDA1(.fAOAMO)
 if %ProcessMode do VLOD(.fAOAMO) quit:$G(ER)  do VDSPPRE(.fAOAMO) quit:$G(ER)  do VPR(.fAOAMO),VDA1(.fAOAMO)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fAOAMO)
	quit


