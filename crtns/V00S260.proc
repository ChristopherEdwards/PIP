VP1(RecordAOAMO fAOAMO) //
 type public String X

 // Virginia fixed rate must be defined for amortization method
 if fAOAMO.met=9,X.isNull() do Runtime.setErrMSG("",4939)

 quit

VP2(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public Number %EXT
 type public String ET,X,ZCLS

 if X.isNull() quit 

 if 'fAOAMO.gld set %EXT=1,ZCLS="D" do CID^UACN1 quit 

 if 'Db.isDefined("GLAD","ACN=:X") set ER=1,ET="INVLDACN" do ^UTLERR

 quit

VP3(RecordAOAMO fAOAMO) //
 if 'fAOAMO.gld do CHANGE^DBSMACRO("TBL","")

 if fAOAMO.gld do CHANGE^DBSMACRO("TBL","[GLAD]")

 quit

VP4(RecordAOAMO fAOAMO) //
 type String  CLS,GRP,QRY,quot

 set quot=$CHAR(34)

 if fAOAMO.gld do { quit
         set QRY="%A.cls="_"""M"""
 
         // I18N=OFF
         if  set QRY=QRY_" & %A.dc=0"
 
         // I18N=ON
         if  do CHANGE^DBSMACRO("TBL","[TRN]:QU QRY")
         }

 type RecordLN ln=Db.getRecord("LN","CID=:fAOAMO.dgl")
 set CLS=ln.cls
 set GRP=ln.grp
 set QRY="if %A.cls="_quot_CLS_quot_"&(%A.grp="_quot_GRP_quot_")"
 set QRY=QRY_"&(%A.dc=0)"

 do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY")

 quit

VP5(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public String ET,X

 if 'X.length() quit

 if 'fAOAMO.gld,'X set ER=1,ET="NO2DEP" do ^UTLERR

 quit

VP6(RecordAOAMO fAOAMO) //
 type public Boolean ER
 type public Number %EXT=1
 type public String ET,X,ZCLS

 if X.isNull() quit

 if 'fAOAMO.glc set %EXT=1,ZCLS="D" do CID^UACN1 quit

 if 'Db.isDefined("GLAD","ACN=:X") set ER=1,ET="INVLDACN" do ^UTLERR

 quit

VP7(RecordAOAMO fAOAMO) //
 if 'fAOAMO.glc do CHANGE^DBSMACRO("TBL","")

 if fAOAMO.glc do CHANGE^DBSMACRO("TBL","[GLAD]")
 
 quit
 
VP8(RecordAOAMO fAOAMO) //
 type public String CLS,GRP,quot
 type String QRY

 set quot=$CHAR(34)

 if fAOAMO.glc do { quit
 	set QRY="if %A.cls="_"""M""""&(%A.dc=1)"
 	if  do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY")
        }
 
 type RecordLN ln=Db.getRecord("LN","CID=:fAOAMO.cgl")
 set CLS=ln.cls
 set GRP=ln.grp
 set QRY="if %A.cls="_quot_CLS_quot_"&(%A.grp="_quot_GRP_quot_")"_"&(%A.dc=1)"

 do CHANGE^DBSMACRO("TBL","[TRN]:QUERY QRY")

 quit

