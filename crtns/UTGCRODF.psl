	if vux("TBLNM").exists() set utblgcrod.tblnm = vux("TBLNM").piece("|",i)
	if vux("MINAGE").exists() set utblgcrod.minage = vux("MINAGE").piece("|",i)
	if vux("GCRCD").exists() set utblgcrod.gcrcd = vux("GCRCD").piece("|",i)
	quit
