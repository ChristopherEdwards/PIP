 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCHKDEF2",VPGM=$T(+0),VSNAME="Check Line Definition"
 set VFSN("UTBLCHKDEF2")="zfCHKDEF2"
 set vPSL=1
 set KEYS(1)=fCHKDEF2.DEFCHK
 set KEYS(2)=fCHKDEF2.CHKLIN
 set KEYS(3)=fCHKDEF2.CHKCOL
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fCHKDEF2),VDA1(.fCHKDEF2),^DBSPNT()

 if '%ProcessMode do VNEW(.fCHKDEF2),VPR(.fCHKDEF2),VDA1(.fCHKDEF2)
 if %ProcessMode do VLOD(.fCHKDEF2) quit:$G(ER)  do VPR(.fCHKDEF2),VDA1(.fCHKDEF2)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fCHKDEF2)
	quit


VNEW(RecordUTBLCHKDEF2 fCHKDEF2) // Initialize arrays if %O=0
 
 do VDEF(.fCHKDEF2)
 do VLOD(.fCHKDEF2)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordUTBLCHKDEF2 fCHKDEF2)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordUTBLCHKDEF2 fCHKDEF2) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

VPR(RecordUTBLCHKDEF2 fCHKDEF2) // Display screen prompts
 set VO="9||13|0"
 set VO(0)="|0"
 set VO(1)=$C(1,29,23,1,0,0,0,0,0,0)_"01T CHECK LINE DEFINITION "
 set VO(2)=$C(4,6,18,1,0,0,0,0,0,0)_"01T Check Definition:"
 set VO(3)=$C(6,18,6,1,0,0,0,0,0,0)_"01T Line:"
