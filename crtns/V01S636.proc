 new V
 //
 set VO="17|10|13|0"
 set VO(10)=$C(4,25,6,2,0,0,0,0,0,0)_"01T"_$E(fCHKDEF2.DEFCHK,1,6)
 set VO(11)=$C(6,25,12,2,0,0,0,0,0,0)_"01N"_fCHKDEF2.CHKLIN
 set VO(12)=$C(8,25,12,2,0,0,0,0,0,0)_"01N"_fCHKDEF2.CHKCOL
 set VO(13)=$C(11,25,12,2,0,0,0,0,0,0)_"00T"_$E(fCHKDEF2.DISP,1,12)
 set VO(14)=$C(13,25,3,2,0,0,0,0,0,0)_"00N"_fCHKDEF2.LGTH
 set VO(15)=$C(15,25,2,2,0,0,0,0,0,0)_"00N"_fCHKDEF2.DEC
 set VO(16)=$C(17,25,1,2,0,0,0,0,0,0)_"00N"_fCHKDEF2.VARTYP
 set VO(17)=$C(19,25,40,2,0,0,0,0,0,0)_"00T"_$E(fCHKDEF2.VARNAM,1,40)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
	
VTAB(RecordUTBLCHKDEF2 fCHKDEF2)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=8,VPT=1,VPB=19,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLCHKDEF2"
 set OLNTB=19025
 
 set VFSN("UTBLCHKDEF2")="zfCHKDEF2"
 //
 // 
	
	
 set %TAB(1)=$C(3,24,6)_"21T12401|1|[UTBLCHKDEF2]DEFCHK|[UTBLCHKDEF1]"
 set %TAB(2)=$C(5,24,12)_"21N12402|1|[UTBLCHKDEF2]CHKLIN"
 set %TAB(3)=$C(7,24,12)_"21N12403|1|[UTBLCHKDEF2]CHKCOL|[UTBLCHKDEF2]"
 set %TAB(4)=$C(10,24,12)_"01T12401|1|[UTBLCHKDEF2]DISP|[DBCTLDVFM]"
 set %TAB(5)=$C(12,24,3)_"01N12402|1|[UTBLCHKDEF2]LGTH"
 set %TAB(6)=$C(14,24,2)_"00N12403|1|[UTBLCHKDEF2]DEC|||||0|18|0"
 set %TAB(7)=$C(16,24,1)_"01N12404|1|[UTBLCHKDEF2]VARTYP|,1#Data Item,2#Text,3#Variable"
 set %TAB(8)=$C(18,24,40)_"01T12405|1|[UTBLCHKDEF2]VARNAM|||do VP1^V01S636(.fCHKDEF2)"
 do VTBL(.fCHKDEF2)
	do ^DBSCRT8 	// data entry
	quit


