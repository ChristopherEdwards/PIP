 set VO(5)=$C(11,8,16,1,0,0,0,0,0,0)_"01T Format Display:"
 set VO(6)=$C(13,16,8,1,0,0,0,0,0,0)_"01T Length:"
 set VO(7)=$C(15,5,19,0,0,0,0,0,0,0)_"01T Decimal Precision:"
 set VO(8)=$C(17,9,15,1,0,0,0,0,0,0)_"01T Variable Type:"
 set VO(9)=$C(19,9,15,1,0,0,0,0,0,0)_"01T Variable Name:"
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


