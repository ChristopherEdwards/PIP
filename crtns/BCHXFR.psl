vEXEC(vCONTEXT,TJD,SEQ) //
	type public Date XTJD

	type Number %CC,CC,CID,JOB,PRN,TYPE
	type String CLS,CRCD,GLSC,GRP
	
	type RecordDAYENDXFR dexfr=Db.getRecord("DAYENDXFR","TJD=:XTJD,SEQ=:SEQ")
	
	set CID=dexfr.cid,PRN=dexfr.prn,TYPE=dexfr.type
	set CC=dexfr.cc,GLSC=dexfr.glsc,CRCD=dexfr.crcd	

	type RecordACN acn=Db.getRecord("ACN","CID")
	if CRCD.get().isNull() set CRCD=acn.crcd

	set CLS=acn.cls
	set GRP=acn.grp

	set JOB=%ProcessID

	set %CC=CC

	type RecordTTL ttl=Db.getRecord("TTL","TJD=:XTJD,CRCD=:CRCD,CLS=:CLS,GRP=:GRP,TYPE=:TYPE,GLSC=:GLSC,CC=:CC",1)

	if dexfr.xflg set ttl.trin=ttl.trin+PRN,ttl.otrin=ttl.otrin+1
	else  set ttl.trout=ttl.trout+PRN,ttl.ctrout=ttl.ctrout+1

	do ttl.bypassSave()
	quit
	
