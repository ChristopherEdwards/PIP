				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Fee Trial Balance by Fee Type"
		set VBNRINFO("PGM")="R01S417"
		set VBNRINFO("RID")="SCA450"
		set VBNRINFO("TABLES")="LNFEE,LNFEEP,LN,PRODDFTL"

