		set VBNRINFO("ORDERBY",2)="[SYSDEV,LN]TYPE"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,LNFEE]75"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,LNFEE]FEETYP"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,LNFEE]CID"

		set VBNRINFO("DOC",1)="The SCA450 report provides trial balance data for loan fee receivables,"
		set VBNRINFO("DOC",2)="assessments, and collections (listed by fee type).  Typically, loan and"
		set VBNRINFO("DOC",3)="accounting personnel generate this report to obtain detail support data"
		set VBNRINFO("DOC",4)="pertaining to loan fee receivable amounts.  The report identifies the"
		set VBNRINFO("DOC",5)="individual and/or total fee balances in the general ledger.  Run the"
		set VBNRINFO("DOC",6)="Loan Fee Trial Balance by Fee Type function (REP450) to generate the"
		set VBNRINFO("DOC",7)="SCA450 report."
		set VBNRINFO("DOC",8)=""
