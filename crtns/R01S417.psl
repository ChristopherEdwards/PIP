	set VSELECT=VSELECT_"LN.GRP,LN.TYPE,LNFEE.FEETYP,LNFEE.CID,PRODDFTL.DES"
	set VSELECT=VSELECT_",LNFEE.FEEREM,LNFEE.CMPAMT,LNFEE.ASYTD,LNFEE.FEEYT"
	set VSELECT=VSELECT_"D,LNFEE.CNAMT,LNFEE.ASND,LNFEEP.GLFR,LNFEEP.GLFI,L"
	set VSELECT=VSELECT_"NFEEP.GLFDI"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"LNFEE,LNFEEP,LN,PRODDFTL",VWHERE,"LN.GRP,LN.TYPE,LNFEE.FEETYP,LNFEE.CID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
