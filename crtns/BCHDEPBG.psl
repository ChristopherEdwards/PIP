vEXEC(vCONTEXT,CID) //
	type RecordDEP dep = Db.getRecord("DEP","CID",1)
			
	do EXEC(dep.bgtyrsd,dep.bgtyrnd,dep.cid)

	quit

EXEC(STARTDT,NEXTDT,XCID)	
		
	type DbSet ds=Db.selectDbSet("BUDGET","CID=:XCID AND BGTYRSD=:STARTDT")

	while ds.next() do {
 		
		type RecordBUDGET budget=ds.getRecord()
	
		type RecordBUDGET budget1=Class.new("RecordBUDGET")
	
		//Set up new budget record from old values

		set budget1.bgtseq=budget.bgtseq
		set budget1.BGTYRSD=NEXTDT 
		set budget1.cid=XCID
		set budget1.bgtcd=budget.bgtcd
		set budget1.cocd=budget.cocd
		set budget1.bgtamtcy=budget.bgtamtcy
		set budget1.billnbr=budget.billnbr
		set budget1.instnbr=budget.instnbr
		do budget1.bypassSave()     //Save new budget record
	
	
	}
	
	quit

