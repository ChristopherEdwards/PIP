HLDDFL(RecordHLDD hldd, String vpar, Boolean vparNorm) // HLDD - Check/Float Hold Detail  Filer
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:16 - joynerd
	// Generated from DATA-QWIK schema in: /profile/v72qa_gtmlx  by: /v72qa_gtmlx/crtns/DBSFILB.obj

	// Data Dictionary Data Items (11)             05/17/2006
	/*
		vpar      Runtime qualifiers:      /NOREQ/MECH=REFARR:R

		/[NO]CASDEL   - Cascade delete
		/[NO]FRMFILER - Called from another filer
		/[NO]INDEX    - Update Indexes
		/[NO]JOURNAL  - Journal update
		/[NO]TRIGAFT  - After update triggers
		/[NO]TRIGBEF  - Before update triggers
		/[NO]UPDATE   - Update primary table
		/[NO]VALDD    - Validate column values
		/[NO]VALFK    - Validate foreign keys
		/[NO]VALREQ   - Validate not null values
		/[NO]VALRI    - Validate transaction integrity
		/[NO]VALST    - Validate database state
	*/

	type public String verrors()

	type String vx(), vxins()                              // audit column array
	type Number %O = hldd.getMode()                        // Processing mode
	set vpar = vpar.get()                                  // Initialize vpar

	if %O = 0 do AUDIT^UCUTILN(hldd,.vxins(),1,"|")
	if %O = 1 do AUDIT^UCUTILN(hldd,.vx(),1,"|")

	if 'vparNorm.get() set vpar = $$initPar^UCUTILN(vpar)  // Run-time qualifiers

	if %O = 0 do { quit                                    // Create record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		if vpar["/VALDD/" do vddver                           // Check values
		do vexec
	}

	if %O = 1 do { quit                                    // Update record control block
		if vx("TJD").exists()!vx("CID").exists()!vx("HLDSEQ").exists()!vx("CHKSEQ").exists() do vkchged quit // Primary key changed
		if vpar["/VALREQ/" do vrequ                           // Check required
		if vpar["/VALDD/" do VDDUX^DBSFILER("HLDD",.vx)
		set %O = 1 do vexec
	}

	if %O = 2 do { quit                                    // Verify record control block
		if vpar["/VALREQ/" do vreqn                           // Check required
		set vpar = $$setPar^UCUTILN(vpar,"NOJOURNAL/NOUPDATE")
		do vexec
	}

	if %O = 3 do { quit                                    // Delete record control block
		quit:'Db.isDefined("HLDD","TJD = :hldd.tjd and CID = :hldd.cid and HLDSEQ = :hldd.hldseq and CHKSEQ = :hldd.chkseq") // No record exists
		do vdelete(0)
	}

	quit

vlegacy(Number %ProcessMode,String vpar) // Legacy interface (^DBSDF9 for %O=0, EXT^DBSFILER for %O = 2)

	type public String TJD
	type public String CID
	type public String HLDSEQ
	type public String CHKSEQ

	type RecordHLDD hldd = Db.getRecord("HLDD","TJD=:TJD,CID=:CID,HLDSEQ=:HLDSEQ,CHKSEQ=:CHKSEQ")
	if (%ProcessMode = 2) do {
		do hldd.setMode(2)

		do HLDDFL(hldd,vpar)
	}

	quit

vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordHLDD hldd

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("HLDD","TJD = :hldd.tjd and CID = :hldd.cid and HLDSEQ = :hldd.hldseq and CHKSEQ = :hldd.chkseq") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))
	if vpar["/VALFK/" do CHKFKS                            // Check foreign keys
	if vpar["/VALRI/" do VFKEYS                            // Foreign key definition

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(hldd)) S ^HLDD(vobj(hldd,-3),vobj(hldd,-4),vobj(hldd,-5),vobj(hldd,-6))=vobj(hldd)
		#ENDBYPASS
	}

	quit

vdelete(Boolean vkeychg) // Record Delete

	type public String vobj(),vpar
	type public RecordHLDD hldd


	// Allow global reference - Delete record
	#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; Group=BYPASS
	#BYPASS
	ZWI ^HLDD(vobj(hldd,-3),vobj(hldd,-4),vobj(hldd,-5),vobj(hldd,-6))
	#ENDBYPASS
	quit


vreqn // Validate required data items

	type public RecordHLDD hldd

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordHLDD hldd

	if hldd.tjd.isNull() do vreqerr("TJD") quit
	if hldd.cid.isNull() do vreqerr("CID") quit
	if hldd.hldseq.isNull() do vreqerr("HLDSEQ") quit
	if hldd.chkseq.isNull() do vreqerr("CHKSEQ") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("HLDD","MSG",1767,"HLDD."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


VFKEYS // Foreign keys

	type public String vfkey(),vpar

	type public RecordHLDD hldd
	if 'hldd.cid.isNull() set vfkey("^ACN("_""""_hldd.cid_""""_","_50_")")="HLDD(CID) -> ACN"
	quit

CHKFKS   // Check foreign keys when not under buffer


	type public RecordHLDD hldd
	type public Number %O
	type String vERRMSG

	if 'Db.isDefined("ACN",":hldd.cid") set vERRMSG = $$^MSG(8563,"HLDD(CID) -> ACN") throw Class.new("Error","%PSL-E-DBFILER,"_vERRMSG.replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordHLDD hldd

	set X = hldd.tjd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("TJD", vRM) quit
	set X = hldd.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	set X = hldd.hldseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("HLDSEQ", vRM) quit
	set X = hldd.chkseq if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CHKSEQ", vRM) quit
	set X = hldd.chkamt if 'X.isNull() set vRM = $$VAL^DBSVER("$",12,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"HLDD.CHKAMT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if hldd.chkflt.length()>200 set vRM = $$^MSG(1076,200) do vdderr("CHKFLT", vRM) quit
	set X = hldd.chktyp if 'X.isNull(),'Db.isDefined("STBLCHKTYP","X") set vRM = $$^MSG(1485,X) do vdderr("CHKTYP", vRM) quit
	set X = hldd.hldamt if 'X.isNull() set vRM = $$VAL^DBSVER("$",12,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"HLDD.HLDAMT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	set X = hldd.rt if 'X.isNull(),'Db.isDefined("UTBLRT","X") set vRM = $$^MSG(1485,X) do vdderr("RT", vRM) quit
	set X = hldd.ted if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("TED", vRM) quit
	set X = hldd.trc if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("TRC", vRM) quit
	quit

