vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordDBTBL7 dbtbl7

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("DBTBL7","%LIBS = :dbtbl7.%libs and TABLE = :dbtbl7.table and TRGID = :dbtbl7.trgid") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))
	if vpar["/VALFK/" do CHKFKS                            // Check foreign keys
	if vpar["/VALRI/" do VFKEYS                            // Foreign key definition

	if vpar'["/NOUPDATE/" do {

		set dbtbl7.TLD = %CurrentDate
		set dbtbl7.TIME = %CurrentTime
		set dbtbl7.USER = %UserName
		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(dbtbl7))  S ^DBTBL(vobj(dbtbl7,-3),7,vobj(dbtbl7,-4),vobj(dbtbl7,-5))=vobj(dbtbl7)
		#ENDBYPASS
	}

	quit

