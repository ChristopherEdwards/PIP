 #WARN ACCESS,MISMATCH OFF
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	02/18/07 - Pete Chenard - CR 23555
		   Corrected compiler warnings.
		   
	09/24/03 - Pete Chenard - 45497
		   Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
 type Public Number ER
 type Public String %PAG,%PAGE,%PG,%PGSV,MULSCR,RM,VFMQ
 type String KVAR,VPG(),vPSL,VSID
 set VSID="TRND"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+6 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+7
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Tran Code Maintenance - General |TRN"
 set VPG(%PG+1)="Tran Code Processing Control Flags |TRN2D"
 set VPG(%PG+2)="Tran Code Processing Control Flags |TRN3D"
 set VPG(%PG+3)="Tran Code Maintenance - Misc Fields |TRNMF1"
 set VPG(%PG+4)="Tran Code Maintenance - Misc Fields |TRNMF2"
 set VPG(%PG+5)="Linked Transaction Codes |TRNLNK"
 set VPG(%PG+6)="Transaction Fee Information|TRNFEE"
	set:'%PG.exists() %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.%A)
 if %ProcessMode do VLOD(.%A) if $G(ER) set VFMQ="Q" quit
 do VPG(.%A)
 quit

VNEW(RecordTRN %A)
 
 do VLOD(.%A)
 quit
VNEWDQ(RecordTRN %A) // Original VNEW section
 
 quit
 //
 ;
VLOD(RecordTRN %A)  // User defined access section
 quit
VLODDQ(RecordTRN %A) // Original VLOD section
 ;
	quit

VPG(RecordTRN %A)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+4) do VPG5(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+5) do VPG6(.%A),VPG0(.%A) quit:vDONE
  if %PG=(%PGSV+6) do VPG7(.%A),VPG0(.%A) quit:vDONE
  }
 quit

VPG0(RecordTRN %A)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.%A) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordTRN %A)  // Tran Code Maintenance - General 
 type Public String %MODS,%REPEAT,ET,SID,VPGM,VPTBL
 type String DFID,PGM
 set SID="TRN",DFID="TRN"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S578
 if %ProcessMode>1 quit
 do VTAB^V01S578(.%A)
 quit
 
