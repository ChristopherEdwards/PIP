		if $D(vobj(stfmsg)) K:$D(vobj(stfmsg,1,1)) ^STFHOST(vobj(stfmsg,-3),"-1") S ^STFHOST(vobj(stfmsg,-3),"-1")=vobj(stfmsg)
		#ENDBYPASS
			// Allow global reference and M source code
			#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
			#BYPASS
