	type public String %MSKD,%TIM,CONAM,GRP,RID,RN,TYP,VL,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc27,vc28,vc29,vc3,vc30,vc31,vc32,vc33,vc34,vc35,vc36,vc37,vc38,vc39,vc4,vc40,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+11,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL="Closed Loan Report - Closed From"
	set VL=VL_$J("",33-VL.length())_$E($$DAT^%ZM(VIN1,$G(%MSKD)),1,10)
	set VL=VL_$J("",44-VL.length())_"To"
	set VL=VL_$J("",47-VL.length())_$E($$DAT^%ZM(VIN2,$G(%MSKD)),1,10)
	set VL=VL_$J("",99-VL.length())_"Effective:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"PAGE:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="      "_"Acct Nbr  Title                                    P&I Amt     Requestd    Acct Trm   Int Rate  Of1  Pmt Distr Priorities"
	do VOM
	set VL="                                                         "_"Tfr Amt     Lif Prem    Pmt Freq   Prepd To  Of2  Collateral Description"
	do VOM
	set VL="                                                         "_"Tot Pmt     Dis Prem    Pmt Due    Unearned  Of3  Interest Index Plan"
	do VOM
	set VL="                                                         "_"Pro Met     A&H Prem    Maturity   #Pmt Amo  Of4  Int Chg Freq"
	do VOM
	set VL="                                   "_"Crd Limit   Last Tran:   Date       Amount    Trn Code   User ID        Int Chg Next"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="GROUP "_vc3_"  TYPE "_vc4
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors
