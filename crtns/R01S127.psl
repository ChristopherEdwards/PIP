	set VSELECT=VSELECT_",LN.PRMET,LN.IAAMT,LN.MDT,LN.ONP,LN.AMBAS,LN.COLOF"
	set VSELECT=VSELECT_"F,LN.INTFRE,LN.CRLMT,LN.LPTSD,LN.LPTDA,LN.LPTTC,LN"
	set VSELECT=VSELECT_".LPTTN,LN.ICHND"

	#ACCEPT DATE=09/10/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"DTJCL,LN",VWHERE,"DTJCL.TJD,DTJCL.CLS,DTJCL.GRP,DTJCL.TYP,DTJCL.CID","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


