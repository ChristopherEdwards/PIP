	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Date VIN1
	type Date VIN2
	type String VIN4="ALL"
	type String VIN5="ALL"

	set RID="SCA058"
	set RN="Closed Loan Account Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|10||[DTJCL]TJD|[DTJCL]TJD:DISTINCT:NOVAL||||D|From Date|||||"
	set %TAB("VIN2")="|10||[DTJCL]TJD|[DTJCL]TJD:DISTINCT:NOVAL||||D|To Date|||||"
	set %TAB("VIN4")="|255||[DTJCL]GRP|[DTJCL]GRP:DISTINCT:NOVAL||D EXT^DBSQRY||T|Product Group|||||"
	set %TAB("VIN5")="|255||[DTJCL]TYP|[DTJCL]TYP:DISTINCT:NOVAL||D EXT^DBSQRY||T|Product Type|||||"

	set %READ="IO/REQ,VIN1#1,VIN2#1,VIN4#0,VIN5#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public Date VIN1,VIN2
	type public String VIN4,VIN5
