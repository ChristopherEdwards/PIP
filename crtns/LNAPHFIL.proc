vrequ // Valid required columns on update


	type public String vx()
	type public RecordLNAP1HIST lnap1hist

	if lnap1hist.acn.isNull() do vreqerr("ACN") quit
	if lnap1hist.apps.isNull() do vreqerr("APPS") quit
	if lnap1hist.appnum.isNull() do vreqerr("APPNUM") quit
	if lnap1hist.tseq.isNull() do vreqerr("TSEQ") quit

		if vx("CDT").exists(),lnap1hist.cdt.isNull() do vreqerr("CDT") quit
		if vx("SPD").exists(),lnap1hist.spd.isNull() do vreqerr("SPD") quit
	quit

