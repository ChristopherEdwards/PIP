vkchged // Access key changed

	type public RecordLNAP1HIST lnap1hist

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("ACN").exists() set vux("ACN") = vx("ACN")
	if vx("APPS").exists() set vux("APPS") = vx("APPS")
	if vx("APPNUM").exists() set vux("APPNUM") = vx("APPNUM")
	if vx("TSEQ").exists() set vux("TSEQ") = vx("TSEQ")
	do vkey(1) set voldkey = lnap1hist.acn_","_lnap1hist.apps_","_lnap1hist.appnum_","_lnap1hist.tseq // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = lnap1hist.acn_","_lnap1hist.apps_","_lnap1hist.appnum_","_lnap1hist.tseq // Copy new keys into object
	type RecordLNAP1HIST vnewrec = lnap1hist.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("LNAP1HIST",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordLNAP1HIST lnap1hist

	if vux("ACN").exists() set lnap1hist.acn = vux("ACN").piece("|",i)
	if vux("APPS").exists() set lnap1hist.apps = vux("APPS").piece("|",i)
	if vux("APPNUM").exists() set lnap1hist.appnum = vux("APPNUM").piece("|",i)
	if vux("TSEQ").exists() set lnap1hist.tseq = vux("TSEQ").piece("|",i)
	quit
