MRPC059(String return,Number versn,String INCD,String PL)	//Public; Transfer sale pools
	/*
	   ORIG: Mike Harrison - 05/23/97

	   Utility is used to for loans sold processing

	   [SCATBL5]RPCID=59

	   KEYWORDS: Client/Server, Loan Utilities

	   ARGUMENTS:
	   	. return     				/TYP=T/REQ
	         					/MECH=REFNAM:W

	   	. versn  	^MRPC059 version number	/TYP=N/REQ
	     			Current version = 1  	/MECH=VAL

	   	. INCD    	Investor code     	/TYP=T/REQ
	     			Table:            	/MECH=VAL

	   	. PL  		Pool                  	/TYP=N/REQ
	     			Table:      		/MECH=VAL

	   RETURNS:
	   	. $$  		Error message   	/TYP=T
	     			Null = No error

	   RELATED:
	   	. $$^PBSMRPC - MRPC Service Class Driver

	  ---- Revision History ------------------------------------------------
      
        07/26/05 - SkariahV- CR16679
	           Removed #WARN directive.
	
	01/08/03 - GRAY - 51351
		   Converted to PSL.


	*/
	
	type Public Number ER
	type Public String ET,RM

	set ER=0
	set (ET,RM)=""
	
	// Version number of client message is not compatible with server
	if versn.get()'=1 set RM=$$^MSG(2951) quit $$ERRMSG^PBSUTL(RM.get(),ET.get())

	do PLPP^LNLS5(PL,INCD) quit:ER $$ERRMSG^PBSUTL(RM.get(),ET.get())

	do FILE^LNLS5(PL,INCD) quit:ER $$ERRMSG^PBSUTL(RM.get(),ET.get())

	quit ""

vSIG()	quit "60107^17766^Viji Skariah^1160"	// Signature - LTD^TIME^USER^SIZE
