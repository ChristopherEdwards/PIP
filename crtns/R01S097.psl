	set VL=VL_$J("",69-VL.length())_"Action on Error:"
	set VL=VL_$J("",87-VL.length())_$J(vc7,1)
	do VOM
	set VL="                  "_"Frequency:"
	set VL=VL_$J("",30-VL.length())_$E(vc8,1,12)
	set VL=VL_$J("",68-VL.length())_"Restrict Restore:"
	set VL=VL_$J("",87-VL.length())_$S(vc9:"Y",1:"N")
	do VOM
	set VL="                     "_"Offset:"
	set VL=VL_$J("",30-VL.length())_$J(vc10,3)
	do VOM
	set VL="" do VOM
	set VL="              "_"Mail on Conditions:"
	set VL=VL_$J("",35-VL.length())_$E(vc11,1,10)
	set VL=VL_$J("",54-VL.length())_"Mail to Users:"
	set VL=VL_$J("",70-VL.length())_$E(vc12,1,50)
	do VOM
	set VL="         "_"Broadcast on Conditions:"
