	set VL=VL_$J("",69-VL.length())_"Delete if Zero:"
	set VL=VL_$J("",86-VL.length())_$S(vc20:"Y",1:"N")
	do VOM
	set VL="                         "_"Action On Failure:"
	set VL=VL_$J("",45-VL.length())_$J(vc21,1)
