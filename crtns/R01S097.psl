	set VL=VL_$J("",49-VL.length())_"Broadcast to Users:"
	set VL=VL_$J("",70-VL.length())_$E(vc14,1,50)
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM
	quit

