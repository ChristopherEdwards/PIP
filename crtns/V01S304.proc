VDEPRE(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Data Entry Pre-processor
 
 type public Boolean APPFLG
 
 if APPFLG.get() do { quit
 	do PROTECT^DBSMACRO("ALL")
 }
 
 if fEFTPAY.STATUS="V" do DEFAULT^DBSMACRO("EFTPAY.MT202FLG",$S(fEFTPAY.SENDCORR=fEFTPAY.RECV:0,1:1))
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


