VP1(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Number X
 
 if X=0 do DEFAULT^DBSMACRO("EFTPAY.SENDCORR","")
 
 quit
VP2(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public String X
 
 do DEFAULT^DBSMACRO("EFTPAY.MT202FLG",$S(X=fEFTPAY.RECV:0,1:1))
VP3(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type Date PROCDT,VALDT
 type String RECACCT,SENDCORR
 
 type public Boolean ER
 type public Number X
 
 if X=fEFTPAY.CID do Runtime.setErrMSG("EFTPAY",7657) quit
 
 // Default SENDCORR
 set SENDCORR=fEFTPAY.SENDCORR
 do NOSIP^PROCEFT(.fEFTPAY)
 if ER quit
 
 if fEFTPAY.SENDCORR'=SENDCORR do DEFAULT^DBSMACRO("EFTPAY.SENDCORR",SENDCORR)
 
 // Validate SENDNOS and SENDCORR
 do NOSCHECK^PROCEFT(.fEFTPAY)
 if ER quit
 
 // Default PROCDT & EFD
 set PROCDT=fEFTPAY.PROCDT
 set VALDT=fEFTPAY.EFD
 do DATIP^PROCEFT(fEFTPAY.CRCD,fEFTPAY.EFTTYPE,fEFTPAY.SENDCORR,.PROCDT,.VALDT)
 if ER quit
 
 if fEFTPAY.PROCDT'=PROCDT do DEFAULT^DBSMACRO("EFTPAY.PROCDT",PROCDT)
 if fEFTPAY.EFD'=VALDT do DEFAULT^DBSMACRO("EFTPAY.EFD",VALDT)
 
 set RECACCT=$S(fEFTPAY.INTEXT=0:"/C"_X,1:"")
 if fEFTPAY.RECACCT'=RECACCT do DEFAULT^DBSMACRO("EFTPAY.RECACCT",RECACCT)
 
 quit
VP4(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type String CRCD,SETCRCD
 
 // Reset currency to EMU currency if SETCRCD=1
 set CRCD=fEFTPAY.CRCD
 set SETCRCD=fEFTPAY.SETCRCD
 if SETCRCD=1 set CRCD=CUVAR.emucrcd
 
 if fEFTPAY.INTEXT=0 do CHANGE^DBSMACRO("TBL","[DEP]")
 if fEFTPAY.INTEXT'=0 do CHANGE^DBSMACRO("TBL","[DEP]CORRE,[DEP]CRCD:QU ""[DEP]CRCD="_CRCD_"""")
 
 quit
VP5(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public String X
 
 if X=fEFTPAY.SENDCORR do DEFAULT^DBSMACRO("EFTPAY.SENDCORR","")
 
 quit
VP6(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Number X
 
 if X do {
 	do PID(.fEFTPAY)
 	set X=0
 	do VDA
 	do ^DBSPNT()
 	}
 
 quit
 
PID(RecordEFTPAY fEFTPAY) //
 type String SID
 
 do INIT^%ZM()
 set SID="INTPAYHQPI"
 do DRV^USID(%ProcessMode,SID,.fEFTPAY)

 quit
VP7(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Boolean ER
 type public String X
 
 // Settlement date cannot be less than date of creation
 if X<fEFTPAY.DATECRE do Runtime.setErrMSG("EFTPAY",8368) quit
 
 // Validate EFD
 do DATCHK^PROCEFT(1,X,fEFTPAY.SENDCORR)
 if ER quit
 
 quit
VP8(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Boolean ER
 type public String X
 
 // Process date cannot be greater than payment date
 if X>fEFTPAY.EFD do Runtime.setErrMSG("EFTPAY",7760) quit
 
 // Validate PROCDT
 do DATCHK^PROCEFT(0,X,fEFTPAY.SENDCORR)
 if ER quit
 
 quit
VP9(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Boolean ER
 type public String X
 
 if X'="" do CHECKUP^SWFTEDIT quit:ER
 if X="" do GOTO^DBSMACRO("EFTPAY.ORDINST1") quit
 else  do GOTO^DBSMACRO("EFTPAY.RECV") quit
VP10(RecordEFTHLD fEFTHLD,RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Boolean ER
 type public String X
 
 if X'="" do { quit
 	// Intermediary may not be present if BANKOP = SPRI
 	if (fEFTPAY.BANKOP="SPRI") do { quit
 		set X="" do DISPLAY^DBSMACRO("EFTPAY.INTER",fEFTPAY.INTER)
 		do Runtime.setErrMSG("EFTPAY",4616,fEFTPAY.BANKOP_"~56")
 		}
 	do CHECKUP^SWFTEDIT quit:ER
 	do GOTO^DBSMACRO("EFTPAY.MSGFP1") quit
 	}
 else  do GOTO^DBSMACRO("EFTPAY.INTER1") quit
 
 quit
