VDEF(RecordEFTPAY fEFTPAY)
 quit:fEFTPAY.CID.isNull() ! fEFTPAY.SEQ.isNull() 
 quit:%ProcessMode  set ER=0 if (fEFTPAY.CID="")!(fEFTPAY.SEQ="") set ER=1,RM=$$^MSG(1767,"CID,SEQ") Q
 if Db.isDefined("EFTPAY","EFTPAY.CID=:fEFTPAY.CID AND EFTPAY.SEQ=:fEFTPAY.SEQ") S ER=1,RM=$$^MSG(2327) Q
 do fEFTPAY.setAuditFlag(1)
 if fEFTPAY.DATECRE="" set fEFTPAY.DATECRE=TJD
 if fEFTPAY.TIMECRE="" set fEFTPAY.TIMECRE=$P($H,",",2)
 if fEFTPAY.UID="" set fEFTPAY.UID=%UID
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
