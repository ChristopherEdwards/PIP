VPO1(RecordEFTPAY fEFTPAY)  // user library EFT_ACN_IP
 /*
  ---- Revision History ------------------------------------------------
  07/30/04 - KELLYP - CR 11405
	     Converted to PSL.
 
  11/20/98 - WHITMANC - ARQ 30799
             Modified to truncate the orderer to 35 characters
 
  ----------------------------------------------------------------------
 */
	type Public String X
	if X="" quit
 
	type Boolean TRUNC
	type String ORD4,XACN
	set SAVX=X 
	set TRUNC=0
	set XACN=X
	set ORD4=""
	type ResultSet rs=Db.select("NAM,MAD1,MAD2,MSTATE,MCITY,MZIP,MCNTRY","CIF","ACN=:XACN")
	if rs.isEmpty() quit
	set X=rs.next()
	set fEFTPAY.ord1=rs.getCol(1).extract(1,35)
	set fEFTPAY.ord2=rs.getCol(2).extract(1,35)
	set fEFTPAY.ord3=rs.getCol(3).extract(1,35)
	if rs.getCol(5)'="" set ORD4=rs.getCol(5)
	if rs.getCol(4)'="" set ORD4=ORD4_", "_rs.getCol(4)
	if rs.getCol(6)'="",rs.getCol(6)'=" " set ORD4=ORD4_", "_rs.getCol(6)
	if rs.getCol(7)'="" set ORD4=ORD4_", "_rs.getCol(7)
	if ORD4.length()>35 set ORD4=ORD4.extract(1,35),TRUNC=1
	set fEFTPAY.ord4=ORD4
	if fEFTPAY.ord3="" do {
		set fEFTPAY.ord3=fEFTPAY.ord4
		set fEFTPAY.ord4=""
		}
	if fEFTPAY.ord1'="" do DISPLAY^DBSMACRO("[EFTPAY]ORD1",fEFTPAY.ord1,"0")
	if fEFTPAY.ord2'="" do DISPLAY^DBSMACRO("[EFTPAY]ORD2",fEFTPAY.ord2,"0")
	if fEFTPAY.ord3'="" do DISPLAY^DBSMACRO("[EFTPAY]ORD3",fEFTPAY.ord3,"0")
	if fEFTPAY.ord4'="" do DISPLAY^DBSMACRO("[EFTPAY]ORD4",fEFTPAY.ord4,"0")
	set X=fEFTPAY.ord1 
	do CHECKUP^SWFTEDIT
	if ER do GOTO^DBSMACRO("[EFTPAY]ORD1") quit
	set X=fEFTPAY.ord2 
	do CHECKUP^SWFTEDIT
	if ER do GOTO^DBSMACRO("[EFTPAY]ORD2") quit
	set X=fEFTPAY.ord3 
	do CHECKUP^SWFTEDIT
	if ER do GOTO^DBSMACRO("[EFTPAY]ORD3") quit
	set X=fEFTPAY.ord4 
	do CHECKUP^SWFTEDIT 
	if ER do GOTO^DBSMACRO("[EFTPAY]ORD4") quit
	set X=SAVX
	if (rs.getCol(1).length()>35)!(rs.getCol(2).length()>35)!(rs.getCol(3).length()>35) set TRUNC=1
 	if TRUNC S ER="W",RM=$$^MSG(2425)
	quit
 #ACCEPT DATE=11/05/03;pgm=screen compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
 #ACCEPT Date=11/5/03;PGM=Screen Compiler;CR=UNKNOWN;GROUP=SYNTAX
 quit
