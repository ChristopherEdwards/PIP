ICPF	/*
	Verification logic added to ensure that only certain frequencies
	can be used for field ICPF. The frequency must be such that the
	IBS frequency utility will return the same next and last dates for
	each date in the period defined by the frequency.
	*/

	new BD,CD,DD,FRE,JD

	if X="" quit

	set JD=%SystemDate set FRE=X do ^UFRE quit:ER

	set BD=NJD
	set JD=NJD set FRE="-"_X do ^UFRE quit:ER

	set CD=NJD
	set JD=BD-1 set FRE=X do ^UFRE quit:ER

	set DD=NJD
	set JD=CD+1 set FRE="-"_X do ^UFRE quit:ER

	// Input is invalid.  Read documentation for detail.
	if (BD'=DD)!(CD'=NJD) set ER=1 set RM=$$^MSG(1234)

	quit

TERM(D1,D2)	//Return the term based upon the period elapsed between two specified dates.

	new DATE,TERM,MONTH,DAY,YEAR

	if D1=""!(D2="") quit ""

	set DATE=$$DD^SCADAT(D1,D2,1)
	set MONTH=$P(DATE,"/",1)
	set DAY=$P(DATE,"/",2)
	set YEAR=$P(DATE,"/",3)
	set TERM=""
	if YEAR set TERM=YEAR_"Y"
	if MONTH set TERM=TERM_MONTH_"M"
	if DAY set TERM=TERM_DAY_"D"

	quit TERM
