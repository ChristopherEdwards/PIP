 set VO="16||13|"
 set VO(0)="8|0"
 set VO(1)=$C(8,11,13,1,0,0,0,0,0,0)_"01T Description:"
 set VO(2)=$C(10,11,13,1,0,0,0,0,0,0)_"01T Branch Code:"
 set VO(3)=$C(10,43,15,1,0,0,0,0,0,0)_"01T Teller Number:"
 set VO(4)=$C(11,33,25,0,0,0,0,0,0,0)_"01TFormat Duplicate Batches:"
 set VO(5)=$C(12,4,20,1,0,0,0,0,0,0)_"01T Return File Option:"
 set VO(6)=$C(12,37,21,0,0,0,0,0,0,0)_"01TInput Format Program:"
 set VO(7)=$C(13,4,20,1,0,0,0,0,0,0)_"01T Return After Retry:"
 set VO(8)=$C(13,31,27,0,0,0,0,0,0,0)_"01TReturn File Format Program:"
