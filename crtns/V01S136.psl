 set VO(10)=$C(17,18,16,1,0,0,0,0,0,0)_"01T Default Action:"
 set VO(11)=$C(18,21,13,1,0,0,0,0,0,0)_"01T Default Fee:"
 set VO(12)=$C(19,12,22,0,0,0,0,0,0,0)_"01TDefault Reject Reason:"
 set VO(13)=$C(20,11,23,0,0,0,0,0,0,0)_"01TDefault Pay All Reason:"
 set VO(14)=$C(21,8,26,0,0,0,0,0,0,0)_"01TDefault Return All Reason:"
