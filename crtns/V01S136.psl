 set VO(20)=$C(11,59,1,2,0,0,0,0,0,0)_"00L"_$S(fUTBL.DUPFLG:"Y",1:"N")
 set VO(21)=$C(12,25,1,2,0,0,0,0,0,0)_"00L"_$S(fUTBL.RET:"Y",1:"N")
 set VO(22)=$C(12,59,10,2,0,0,0,0,0,0)_"00T"_$E(fUTBL.PGM,1,10)
 set VO(23)=$C(13,25,1,2,0,0,0,0,0,0)_"00L"_$S(fUTBL.RETSDR:"Y",1:"N")
 set VO(24)=$C(13,59,10,2,0,0,0,0,0,0)_"00T"_$E(fUTBL.RTRN,1,10)
