	#ACCEPT DATE=11/05/03; PGM=Screen Compiler;CR=UNKNOWN;GROUP=DEAD
	quit


VDA1(RecordCTBLINC fUTBL)  // Display screen data
 new V
 //
 set VO="31|17|13|"
 set VO(17)=$C(8,25,40,2,0,0,0,0,0,0)_"00T"_$E(fUTBL.DESC,1,40)
 set VO(18)=$C(10,25,6,2,0,0,0,0,0,0)_"00N"_fUTBL.BRCD
