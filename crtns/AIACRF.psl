		if dep.aiipld'="" do Runtime.setErrXBAD("DEP",184,"dep.AIIPLD") if ER quit      	//Avail Int Last Posting Date
		if dep.aiichnd'="" do Runtime.setErrXBAD("DEP",184,"dep.AIICHND") if ER quit     	//Avail Int Next Chg Date
		if dep.aiinp'="" do Runtime.setErrXBAD("DEP",184,"dep.AIINP") if ER quit         	//Avail Int Next Posting Date
		if dep.aiipf'="" do Runtime.setErrXBAD("DEP",184,"dep.AIIPF") if ER quit         	//Avail Int Posting Freq
		if dep.aiiopt'="" do Runtime.setErrXBAD("DEP",184,"dep.AIIOPT") if ER quit       	//Avail Int Disburesment Opt
		
