		if dep.aiindex'="" do Runtime.setErrXBAD("DEP",184,"dep.AIINDEX") if ER quit   		//Avail Int Index
		if dep.aiinttrf'="" do Runtime.setErrXBAD("DEP",184,"dep.AIINTTRF") if ER quit   	//Avail Int Intrnl Transf Acct
