VTAB(RecordSTBLMBAR fSTMBAR)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=4,VPT=1,VPB=18,PGM=$T(+0),DLIB="SYSDEV",DFID="STBLMBAR"
 set OLNTB=18001
 
 set VFSN("STBLMBAR")="zfSTMBAR"
 //
