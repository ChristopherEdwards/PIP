VP1(RecordSTBLMBAR fSTMBAR) //
 type Public String X,V,MENUID
 
 if X="" Q
 
 If X=V Q
 
 set MENUID=X
 
 do CHANGE^DBSMACRO("TBL","")
 do LOAD^DBSMACRO("ALL") 
 do DISPLAY^DBSMACRO("ALL")
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


