VDA1(RecordSTBLMBAR fSTMBAR)  // Display screen data
 new V
 s MENUID=$G(MENUID)
 //
 set VO="41|37|13|"
 set VO(37)=$C(1,1,80,1,0,0,0,0,0,0)_"01T"_$S(%ProcessMode=5:"",1:$$BANNER^UTLREAD($G(%FN)))
