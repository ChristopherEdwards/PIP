 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab,MENUID",VSID="STBLMBAR",VPGM=$T(+0),VSNAME="Menu options update screen"
 set VFSN("STBLMBAR")="zfSTMBAR"
 set vPSL=1
 set KEYS(1)=fSTMBAR.MENUID
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.fSTMBAR),VDA1(.fSTMBAR),^DBSPNT()

 set ER=0 do VSCRPRE(.fSTMBAR) if ER quit  // Screen Pre-Processor
 
 if '%ProcessMode do VNEW(.fSTMBAR),VPR(.fSTMBAR),VDA1(.fSTMBAR)
 if %ProcessMode do VLOD(.fSTMBAR) quit:$G(ER)  do VPR(.fSTMBAR),VDA1(.fSTMBAR)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fSTMBAR)
	quit


VNEW(RecordSTBLMBAR fSTMBAR) // Initialize arrays if %O=0
 
 do VDEF(.fSTMBAR)
 do VLOD(.fSTMBAR)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
