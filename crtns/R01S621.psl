	type public String %TIM,I,IOSL,TOTACCT,TOTACCTC,TOTACCTIRA,TOTACCTMT,TOTACCTTR,TOTAMT,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VT(),VX(),vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if 'VT(2) quit
	if VLC+7>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL="Total Accts Reported:"
	set VL=VL_$J("",33-VL.length())_$J(vc19,9)
	do VOM
