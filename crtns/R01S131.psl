	type public String %NOCLOSE,%NOOPEN,%O,%PG,%PGM,%TIM,ACR(),BAL(),BL,CID,DESC,ER,IOSL,IOTYP,LCID,NRA,NRM(),NUM(),PAYEE,PAYID,PAYTOT,PDES,PG,PGM,PRM(),PT2,RID,RM,RN,SRT,TJD,TRTYPE,TY,TYP,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),X,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	quit:VD(4)  set VD(4)=1				// Print flag
	if VLC+4>IOSL do VHDG0 quit:VFMQ

	set VL="" do VOM
	set VL=$J(vc3,12)
