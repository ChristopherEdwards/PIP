	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String TJD
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Number SRT
	type String VIN2="ALL"
	type String VIN3="ALL"

	set RID="SCA062"
	set RN="Single Payee Remittance Detail"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("SRT")="|1|||||||L|Sort by Borrower|||||"
	set %TAB("VIN2")="|255||[TRTYPE]TRTYPE|[LNTRS]||S TRTYPE=X D EXT^DBSQRY||T|Transfer Type|||||"
	set %TAB("VIN3")="|255||[TRTYPE]PAYID|[LNTRS1]:NOVAL||D EXT^DBSQRY||T|Transfer Payee Id|||||"

	set %READ="IO/REQ,SRT#0,VIN2#0,VIN3#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type Boolean VHIT
	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String TJD
	type public Number SRT
	type public String VIN2,VIN3
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %NOCLOSE,%NOOPEN,%O,%PG,%PGM,%TIM,ACR(),BAL(),BL,CID,CONAM,DESC,LCID,NRA,NRM(),NUM(),PAYEE,PAYID,PAYTOT,PDES,PG,PGM,PRM(),PT2,RID,RN,TRTYPE,TY,TYP,VL,VLC,VLOF,VRF(),VRG,VSTATS(),X,vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA062",RN="Single Payee Remittance Detail"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA062","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()


	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[TRTYPE]TRTYPE "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[TRTYPE]PAYID "_VIN3,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("TRTYPE,DEP") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",2)="WC2|"_"Transfer Type"_"|VIN2|"_VIN2.get()
		set VBNRINFO("PROMPTS",3)="WC2|"_"Transfer Payee Id"_"|VIN3|"_VIN3.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Single Payee Remittance Detail"
		set VBNRINFO("PGM")="R01S131"
		set VBNRINFO("RID")="SCA062"
		set VBNRINFO("TABLES")="TRTYPE,DEP"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,TRTYPE]TRTYPE"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,TRTYPE]PAYID"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,DEP]AREF"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,TRTYPE]CID"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,TRTYPE]TYP"

		set VBNRINFO("DOC",1)=" The Single Payee Remittance Detail reports, by loan, each escrow account controlled"
		set VBNRINFO("DOC",2)="by that loan.  In addition to account balance and accrual data, information"
		set VBNRINFO("DOC",3)="is supplied for remittance and analysis."
		set VBNRINFO("DOC",4)=" Report totals provide count, balance and accrual data for each escrow type"
		set VBNRINFO("DOC",5)="in use."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc20,vc21,vc22,vc23)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VHIT = 0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:5 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:2 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	if VDISTKEY.get()="" do { quit:VFMQ		// Report Pre-processor (after query)
		do VPREAQ
		if VFMQ set vh(0)=1 do VEXIT(0)
		}

	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"TRTYPE.TRTYPE,TRTYPE.PAYID,DEP.AREF,TRTYPE.CID,TRT"
	set VSELECT=VSELECT_"YPE.TYP,TRTYPE.REMPTP,DEP.LNM,TRTYPE.LDREM,TRTYPE."
	set VSELECT=VSELECT_"ASMT,TRTYPE.ACOLL,TRTYPE.RBMET,TRTYPE.SPF,TRTYPE.R"
	set VSELECT=VSELECT_"AMTD,TRTYPE.SRF,TRTYPE.REMYTD,TRTYPE.REMLD,TRTYPE."
	set VSELECT=VSELECT_"PREMA,TRTYPE.REMIPF,TRTYPE.ROEA,TRTYPE.REMND,TRTYP"
	set VSELECT=VSELECT_"E.RAMTP,DEP.ANFLG,DEP.BAL"

	#ACCEPT DATE=09/10/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"TRTYPE,DEP",VWHERE,"TRTYPE.TRTYPE,TRTYPE.PAYID,DEP.AREF,TRTYPE.CID,TRTYPE.TYP","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
	type Boolean VSKIPREC = 0
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VFPOST quit:(VFMQ ! VSKIPREC)  set VHIT = 1
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT('VHIT)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4,vovc5
	set vovc1="",vovc2="",vovc3="",vovc4="",vovc5=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4,vovc5,vc5
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4,vovc5=vc5
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc20,vc21,vc22,vc23
	set vc1=V.piece($C(9),1)			// TRTYPE.TRTYPE
	set vc2=V.piece($C(9),2)			// TRTYPE.PAYID
	set vc3=V.piece($C(9),3)			// DEP.AREF
	set vc4=V.piece($C(9),4)			// TRTYPE.CID
	set vc5=V.piece($C(9),5)			// TRTYPE.TYP
	set vc6=V.piece($C(9),6)			// TRTYPE.REMPTP
	set vc7=V.piece($C(9),7)			// DEP.LNM
	set vc8=V.piece($C(9),8)			// TRTYPE.LDREM
	set vc9=V.piece($C(9),9)			// TRTYPE.ASMT
	set vc10=V.piece($C(9),10)			// TRTYPE.ACOLL
	set vc11=V.piece($C(9),11)			// TRTYPE.RBMET
	set vc12=V.piece($C(9),12)			// TRTYPE.SPF
	set vc13=V.piece($C(9),13)			// TRTYPE.RAMTD
	set vc14=V.piece($C(9),14)			// TRTYPE.SRF
	set vc15=V.piece($C(9),15)			// TRTYPE.REMYTD
	set vc16=V.piece($C(9),16)			// TRTYPE.REMLD
	set vc17=V.piece($C(9),17)			// TRTYPE.PREMA
	set vc18=V.piece($C(9),18)			// TRTYPE.REMIPF
	set vc19=V.piece($C(9),19)			// TRTYPE.ROEA
	set vc20=V.piece($C(9),20)			// TRTYPE.REMND
	set vc21=V.piece($C(9),21)			// TRTYPE.RAMTP
	set vc22=V.piece($C(9),22)			// DEP.ANFLG
	set vc23=V.piece($C(9),23)			// DEP.BAL
	quit

	// User-defined pre/post-processor code

VFPOST	// FETCH post-processor

	type public String vc1,vc2,vc3
 type public Number LCID
 type public String PAYEE,PAYID,PDES,TRTYPE

 set (TRTYPE,PAYID,PDES)=""
 set TRTYPE=vc1  
 set PAYID=vc2
 set PAYEE=PAYID
 set LCID=vc3
 type RecordLNTRS1 lntrs1=Db.getRecord("LNTRS1","TRTYPE=:TRTYPE,PAYID=:PAYID",1) 
 if lntrs1.getMode() set PDES=lntrs1.paydes

	quit

VPREAQ	// Pre-processor (after query)

 type public Number PT2,SRT,VFMQ
 type public String BL,ER,PG,PGM,RID,RM,RN,%NOCLOSE,%NOOPEN
 type public Date TJD

 set BL="",PT2=0 
 if 'SRT.exists() set SRT=0
 if 'TJD.exists() set TJD=CUVAR.TJD
 if SRT do {
 	set RID="SCA152"
	// I18N=OFF
 	set RN="Single Payee Remittance (By Borrower)"
 	// I18N=ON
 	do ^URID quit:'PGM.length()  set PG="V0^"_PGM,%NOOPEN="" do @PG
 	//set ER=1,RM="",%NOCLOSE=""
	set VFMQ=1
 }
	quit

VPREBQ	// Pre-processor (before query)

 //Incoming=TJD
	quit

VRPOST	// Report post-processor

 type public String PGM,%PGM,%PG,RID,RN,IOTYP,%O,VFMQ,TY
 type public Number SRT

 // Go to SCA062B for report summary
 set TY=""
 do Db.fastDelete("LESTBREP") 
 do ZESTB
 
 if IOTYP="TRM" do VWAIT 
 if VFMQ="Q" do VEXIT()
 else  do {
	// Do report summary SCA062B
	// I18N=OFF
 	set RID="SCA062B"
	set RN="Single Payee Remittance Summary"
	do ^URID  
	// I18N=ON
	if PGM.length() do @("V0^"_PGM)
	}
 quit

ZESTB 
 type public String ACR(),BAL(),NRM(),NUM(),PRM(),TYP
 type public Number CID,TY

 set TY=NUM(TY).order() if TY="" quit 
 type RecordLESTBREP lestbrep=Db.getRecord("LESTBREP","TY=:TY",1) 
 set lestbrep.num=NUM(TY) 
 set lestbrep.bal=BAL(TY) 
 set lestbrep.acr=ACR(TY) 
 set lestbrep.prm=PRM(TY)
 set lestbrep.nrm=NRM(TY) 
 do lestbrep.save() 
 do ZESTB
 quit

VWAIT // 
 type public Number IOSL, VLC, VRG, X 
 type public String VL 

 set VL = "" 
 for X = VLC + 1:1:IOSL do VOM
 do ^DBSRWBR(2)    
 set VLC = 0 
 set VRG = 0 
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0,vs(5)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			do VRPOST			// Report Post-Processor
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1,vskp(5)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG3 quit:VFMQ
	if 'vskp(4).get() do VDTL4 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(5)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4,vc5,vovc5
	type Number vb1,vb2,vb3,vb4,vb5
	set (vb1,vb2,vb3,vb4,vb5)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,vb5=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,vb5=1,VH0=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,vb5=1
	if vb4!(+vovc4'=+vc4) set vs(5)=0,vh(5)=0,VD(4)=0,vb5=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(5) set vs(5)=1 do stat^DBSRWUTL(5)
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do VSUM3 quit:VFMQ  do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT(),NRA,vc6
	type Public String VSTATS

	set VT(5)=VT(5)+1
	set VT(5,1,1)=VT(5,1,1)+NRA			// @TOT(<<NRA>>)
	set VT(5,2,1)=VT(5,2,1)+vc6			// @TOT([TRTYPE]REMPTP)
	quit

VHDG3	// Group Header
	type public String %MSKD,%NOCLOSE,%NOOPEN,%O,%PG,%PGM,%TIM,ACR(),BAL(),BL,CID,CONAM,DESC,ER,IOSL,IOTYP,LCID,NRA,NRM(),NUM(),PAYEE,PAYID,PAYTOT,PDES,PG,PGM,PRM(),PT2,RID,RM,RN,SRT,TJD,TRTYPE,TY,TYP,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,X,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	quit:vh(3)  set vh(3)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL="Type:"
	set VL=VL_$J("",6-VL.length())_$E(vc1,1,10)
	set VL=VL_$J("",17-VL.length())_"Payee:"
	set V=vc2,VO=V set V=$E(V,1,6) do VP1 quit:VFMQ!verror.get()
	set VL=VL_$J("",24-VL.length())_V		// [SYSDEV,TRTYPE]PAYID
	set VL=VL_$J("",31-VL.length())_"[ "_PDES_" ]"
	do VOM
	quit

VSUM3	// Summary
	type public String %NOCLOSE,%NOOPEN,%O,%PG,%PGM,%TIM,ACR(),BAL(),BL,CID,DESC,ER,I,IOSL,IOTYP,LCID,NRA,NRM(),NUM(),PAYEE,PAYID,PAYTOT,PDES,PG,PGM,PRM(),PT2,RID,RM,RN,SRT,TJD,TRTYPE,TY,TYP,V,VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),VX(),X,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vrundate,vsysdate

	if 'VT(3) quit
	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(3)=0 do VHDG3 quit:VFMQ

	do VP2 quit:VFMQ!verror.get()  set V=$E(PAYTOT,1,25) set VL=V
	set V=VT(3,2,1)					// @TOT([TRTYPE]REMPTP,)
	set VL=VL_$J("",61-VL.length())_$S(V="":"",1:$J(V,0,2))
	do VOM
	set V=VT(3,1,1)					// @TOT(<<NRA>>,)
	set VL="                                              "_$S(V="":"",1:$J(V,0,2))
	do VOM
	quit

