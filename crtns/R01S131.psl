	set VL=VL_$J("",118-VL.length())_$S(vc13="":"",1:$J(vc13,0,2))
	set VL=VL_$J("",125-VL.length())_$S(vc14:"Y",1:"N")
	do VOM
	set VL="" do VOM
	do VP3 quit:VFMQ!verror.get()  set V=$E(DESC,1,30) set VL=V
	do VP4 quit:VFMQ!verror.get()  set V=$S(NRA="":"",1:$J(NRA,0,2))
