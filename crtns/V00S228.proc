VDSPPRE(RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Display Pre-Processor
 new %TAB,vtab // Disable .MACRO. references to %TAB()
 //
 type public String CID,D2CRCD,DCRCD,SEQ
 
 if 'fEFTPAY.getMode() do {
 	type RecordEFTPAY fEFTPAY=Db.getRecord("EFTPAY","CID=:CID,SEQ=:SEQ",1)
 	if 'fEFTPAY.getMode() quit
 	}
 if fEFTPAY.TTYPE=0 do {
 	type RecordDEP dep=Db.getRecord("DEP","CID=:fEFTPAY.CID",1)
 	set (DCRCD,D2CRCD)=dep.crcd
 	}
 else  set (DCRCD,D2CRCD)=fEFTPAY.CRCD
 #ACCEPT date=11/05/03;PGM=Screen Compiler
 quit
