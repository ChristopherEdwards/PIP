VP1(RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type public Date X
 
 if X<%SystemDate do Runtime.setErrMSG("EFTPAY",7647) quit

 quit
VP2(RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type Number FAMT
 type String CIDCRCD,PMTCRCD
 
 type public Number X
 type public String CO
 
 if 'X do { quit
 	set fEFTPAY.FEEAMTPMT=0
 	do DISPLAY^DBSMACRO("EFTPAY.FEEAMTPMT",fEFTPAY.FEEAMTPMT)
 	}
 
 set CO=CUVAR.co
 set PMTCRCD=fEFTPAY.CRCD
 type RecordDEP dep=Db.getRecord("DEP","CID=:fEFTPAY.CID",1)
 set CIDCRCD=dep.crcd
 if CIDCRCD="" set CIDCRCD=PMTCRCD
 
 set FAMT=fEFTPAY.ACTFEE
 if PMTCRCD'=CIDCRCD do CAMT^CRCDUTL(CIDCRCD,PMTCRCD,.FAMT) set fEFTPAY.FEEAMTPMT=FAMT
 if PMTCRCD=CIDCRCD set fEFTPAY.FEEAMTPMT=fEFTPAY.ACTFEE
 do DISPLAY^DBSMACRO("EFTPAY.FEEAMTPMT",fEFTPAY.FEEAMTPMT)
 
 quit
VP3(RecordEFTPAY fEFTPAY,RecordDEP DEP) //
 type Number ORIGFEE,RTNCCY,RTNFEE,RFEEAMT
 type String CIDCRCD,PMTCRCD
 
 type public String CO,D2CRCD,X
 
 type RecordEFTFEE eftfee=Db.getRecord("EFTFEE","CODE=:X",1)
 if (X'="")&('eftfee.getMode()) do Runtime.setErrMSG("EFTFEE",7194) quit
 if X="" set RTNFEE=0
 
 set (RTNFEE,RTNCCY)=""
 
 do RTNFEE^EFTPAY2(.RTNFEE,.RTNCCY)
 if RTNFEE="" set RTNFEE=0
 
 set fEFTPAY.RTNFEEAMT=RTNFEE
 
 set CO=CUVAR.co
 type RecordDEP dep=Db.getRecord("DEP","CID=:fEFTPAY.CID",1)
 set CIDCRCD=dep.crcd
 if CIDCRCD="" set CIDCRCD=fEFTPAY.CRCD
 
 set PMTCRCD=fEFTPAY.CRCD
 set RFEEAMT=fEFTPAY.RTNFEEAMT
 if PMTCRCD'=D2CRCD do CAMT^CRCDUTL(D2CRCD,PMTCRCD,.RFEEAMT) set fEFTPAY.RTNFEEAMT2=RFEEAMT
 if PMTCRCD=D2CRCD set fEFTPAY.RTNFEEAMT2=fEFTPAY.RTNFEEAMT
 
 set fEFTPAY.RTNTOTPMT=fEFTPAY.AMOUNT-(fEFTPAY.FEEAMTPMT+fEFTPAY.RTNFEEAMT2)
 if fEFTPAY.ORIGFEE set ORIGFEE=fEFTPAY.FEEAMT
 else  set ORIGFEE=0
 
 if fEFTPAY.RTNFEETYPE'="" set RTNFEE=fEFTPAY.RTNFEEAMT
 else  set RTNFEE=0
 
 set fEFTPAY.RTNTOTCID=fEFTPAY.ACTAMT-(ORIGFEE+RTNFEE)
 
 if CIDCRCD=fEFTPAY.CRCD set fEFTPAY.RTNTOTCID=fEFTPAY.RTNTOTPMT
 
 do DISPLAY^DBSMACRO("EFTPAY.RTNFEEAMT",fEFTPAY.RTNFEEAMT)
 do DISPLAY^DBSMACRO("EFTPAY.RTNFEEAMT2",fEFTPAY.RTNFEEAMT2)
 do DISPLAY^DBSMACRO("EFTPAY.RTNTOTCID",fEFTPAY.RTNTOTCID)
 do DISPLAY^DBSMACRO("EFTPAY.RTNTOTPMT",fEFTPAY.RTNTOTPMT)
 
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


