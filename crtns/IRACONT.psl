vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLIRACON utbliracon

	set X = utbliracon.key if 'X.isNull(),X'?1.10N,X'?1"-"1.9N set vRM=$$^MSG(742,"N") do vdderr("KEY", vRM) quit
	if '("01"[utbliracon.AGE) set vRM=$$^MSG(742,"L") do vdderr("AGE", vRM) quit
	if '("01"[utbliracon.DEPNOT) set vRM=$$^MSG(742,"L") do vdderr("DEPNOT", vRM) quit
	if utbliracon.desc.length()>40 set vRM = $$^MSG(1076,40) do vdderr("DESC", vRM) quit
	if '("01"[utbliracon.LIF) set vRM=$$^MSG(742,"L") do vdderr("LIF", vRM) quit
	if '("01"[utbliracon.LOCK) set vRM=$$^MSG(742,"L") do vdderr("LOCK", vRM) quit
	set X = utbliracon.rsn if 'X.isNull(),'Db.isDefined("STBLIRACON","X") set vRM = $$^MSG(1485,X) do vdderr("RSN", vRM) quit
	quit

