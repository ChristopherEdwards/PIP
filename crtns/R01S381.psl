	do VINILAST
	type ResultSet rwrs=Db.select("LN.GRP,LN.CBST,LN.CBDOO,LN.STAT,LN.LPDT,LN.FCLD,LN.DLCO,LN.SCHSEQ,LN.DSEQ,LN.DIST1AF,LN.DTC,LN.CNTDR,CIF.TAXID,LN.ONP,LN.CBC,LN.ODT,LN.PMT,LN.CLS,LN.TYPE,LN.CID,LN.DIST1LD,LN.HABL1,LN.BAL,CIF.MAR,CIF.PZIP,CIF.NAM,CIF.PAD1,CIF.PCITY,CIF.PSTATE,CIF.PAD2,CIF.OWN,LN.CRLMT,LN.DY3,LN.DY4,LN.DY5,LN.ACN","LN,CIF,PRODDFTL","LN.CLS='L' AND ((LN.PCIDSTAT=0) OR (LN.PCIDSTAT='')) AND PRODDFTL.CBC NOT ='99' AND PRODDFTL.CPF NOT =1","LN.CLS,LN.GRP,LN.TYPE,LN.CID","","DQMODE=1/PROTECTION=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
