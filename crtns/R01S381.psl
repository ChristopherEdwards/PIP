VFPOST	// FETCH post-processor

	type public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17
 type public Date CRLRD,DOO
 type public Number D5,D7,D8,D14,TREC,VSKIPREC,X
 type public String GRP
 
 type Date CBDOO,DLCO,FCLD,LPDT
 type Number A,CBST,DIST1AF,DSEQ,SCHSEQ,STAT
 type String B
 
 set TREC=TREC+1
 set GRP=vc1
 set CBST=vc2
 set CBDOO=vc3
 set STAT=vc4
 set LPDT=vc5
 set FCLD=vc6
 set DLCO=vc7
 set SCHSEQ=vc8
 set DSEQ=vc9
 set DIST1AF=vc10
 
 if (STAT=4),(vc11<(%SystemDate-360)) set VSKIPREC=1 quit
 if (GRP'="RC"),('vc12) set VSKIPREC=1 quit
 if 'DIST1AF set VSKIPREC=1 quit
 set (D5,DOO)=""
 set vc13=vc13.translate("-","")
 kill D7
 
 do D5
 
 set X=vc14
 if (X.isNull())!(DIST1AF.isNull()) set D7="010"
 else  set X=(X*12)/DIST1AF,A=0,B=vc15 if (B="08")!(B=19)!(B=25)!(B=26) set A=1
 if 'D7.exists() set D7=$S(A:(X/12),1:X)
 
 set D8=4
 if vc16>CRLRD set D8=1
 if (D5=11)!(D5=13) set D14=0
 else  set D14=vc17 if D14<0 set D14=0
 
 quit
 
D5 //
 
 type public Date CBDOO,DOO,DLCO,FCLD,LPDT
 type public Number CBST,D5,DIST1AF,DSEQ,SCHSEQ,STAT
 
 set D5=CBST
 if 'D5.isNull() set DOO=CBDOO quit
 if STAT=4 set D5=13,DOO=LPDT quit
 set DOO=FCLD
 if 'DOO.isNull() set D5=94 quit
 //  check last action file - problem loan sub-system
 do D5A
 if 'DLCO.isNull() set DOO=DLCO,D5=97 quit
 if 'DOO.isNull() quit
 set DOO=%SystemDate
 set D5=SCHSEQ-DSEQ
 set D5=(12*D5)/DIST1AF
 set D5=$S(D5<1:11,D5<2:71,D5<3:78,D5<4:80,D5<5:82,D5<6:83,1:84)
 
 quit
 
D5A //
 
 type public Date DLCO
 type public Number CID
 
 type String X
 
 type ResultSet rs=Db.select("CAD","LNPL2","CID=:CID","CAD DESC")
 //  not in restructuring file
 if rs.isEmpty() quit
 
 if rs.next() set X=rs.getCol("CAD")
 if ('DLCO.isNull()),(X.isNull()) quit
 if X>DLCO set DLCO=X
 
 quit
	quit

