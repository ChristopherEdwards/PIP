vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordUTBLWPFTQ utblwpftq

	set X = utblwpftq.wpft if 'X.isNull(),'Db.isDefined("UTBLWPFT","X") set vRM = $$^MSG(1485,X) do vdderr("WPFT", vRM) quit
	set X = utblwpftq.wpqrynum if 'X.isNull(),X'?1.3N,X'?1"-"1.2N set vRM=$$^MSG(742,"N") do vdderr("WPQRYNUM", vRM) quit
	set X = utblwpftq.wfct if 'X.isNull() set vRM = $$VAL^DBSVER("N",8,0,,,,,5) if 'vRM.isNull() set vRM = $$^MSG(979,"UTBLWPFTQ.WFCT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if utblwpftq.wpqrynam.length()>12 set vRM = $$^MSG(1076,12) do vdderr("WPQRYNAM", vRM) quit
	quit

