vkchged // Access key changed

	type public RecordUTBLWPFTQ utblwpftq

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux
	type String voldpar = vpar.get()                       // Save filer switches

	if vx("WPFT").exists() set vux("WPFT") = vx("WPFT")
	if vx("WPQRYNUM").exists() set vux("WPQRYNUM") = vx("WPQRYNUM")
	do vkey(1) set voldkey = utblwpftq.wpft_","_utblwpftq.wpqrynum // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = utblwpftq.wpft_","_utblwpftq.wpqrynum // Copy new keys into object
	type RecordUTBLWPFTQ vnewrec = utblwpftq.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("UTBLWPFTQ",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update
	set vpar = voldpar
	if vpar["/TRIGAFT/" do VAU

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordUTBLWPFTQ utblwpftq

	if vux("WPFT").exists() set utblwpftq.wpft = vux("WPFT").piece("|",i)
	if vux("WPQRYNUM").exists() set utblwpftq.wpqrynum = vux("WPQRYNUM").piece("|",i)
	quit
