V00S326(%ProcessMode,RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)   // -  - SID= <LNROLL> Loan Rollover Linked Screens
 ;;Copyright(c)2006 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/24/2006 11:39 - kini
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	09/24/03 - Pete Chenard - 45497
		       Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
V0 
 set VSID="LNROLL"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'$D(%PG) %PG=1 set %PAG=%PG+1 set %PAGE=$S($D(%PAGE):%PAGE-1,1:0)+2
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Loan Rollover Parameters|LNROLL1"
 set VPG(%PG+1)="Loan Rollover Parameters|LNROLL2"
	set:'$D(%PG) %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.fLNRNW,.LN,.CIF)
 if %ProcessMode do VLOD(.fLNRNW,.LN,.CIF) if $G(ER) set VFMQ="Q" quit
 do VPG(.fLNRNW,.LN,.CIF)
 quit

VNEW(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)
 
 quit
 //
 ;
VLOD(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)  // User defined access section
	quit

VPG(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)
 new vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.fLNRNW,.LN,.CIF),VPG0(.fLNRNW,.LN,.CIF) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.fLNRNW,.LN,.CIF),VPG0(.fLNRNW,.LN,.CIF) quit:vDONE
  }
 quit

VPG0(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.fLNRNW,.LN,.CIF) if $G(VFMQ)="D" set vDONE=1 quit
 if '$D(%PAGE) set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)  // Loan Rollover Parameters
 
 new x
 set SID="LNROLL1",DFID="LNRENEW"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S329
 if %ProcessMode>1 quit
 do VTAB^V00S329(.fLNRNW,.LN,.CIF)
 quit
 
VPG2(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)  // Loan Rollover Parameters
 
 new x
 set SID="LNROLL2",DFID="LNRENEW"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V00S330
 if %ProcessMode>1 quit
 do VTAB^V00S330(.fLNRNW,.LN,.CIF)
 quit
 

VBTM(RecordLNRENEW fLNRNW,RecordLN LN,RecordCIF CIF)

	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
