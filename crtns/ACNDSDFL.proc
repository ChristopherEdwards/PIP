	type public RecordACNDSD acndsd

	if vux("CID").exists() set acndsd.cid = vux("CID").piece("|",i)
	if vux("FRECOL").exists() set acndsd.frecol = vux("FRECOL").piece("|",i)
	if vux("SCHDT").exists() set acndsd.schdt = vux("SCHDT").piece("|",i)
	quit
