	set X = acndsd.cid if 'X.isNull(),'Db.isDefined("ACN","X") set vRM = $$^MSG(1485,X) do vdderr("CID", vRM) quit
	if acndsd.frecol.length()>40 set vRM = $$^MSG(1076,40) do vdderr("FRECOL", vRM) quit
