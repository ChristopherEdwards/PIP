vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSTFHSEQ stfhseq

	set X = stfhseq.stfseq if 'X.isNull(),X'?1.5N,X'?1"-"1.4N set vRM=$$^MSG(742,"N") do vdderr("STFSEQ", vRM) quit
	if stfhseq.%token.length()>10 set vRM = $$^MSG(1076,10) do vdderr("%TOKEN", vRM) quit
	if stfhseq.accts.length()>60 set vRM = $$^MSG(1076,60) do vdderr("ACCTS", vRM) quit
	set X = stfhseq.brcd if 'X.isNull(),X'?1.6N,X'?1"-"1.5N set vRM=$$^MSG(742,"N") do vdderr("BRCD", vRM) quit
	set X = stfhseq.cid if 'X.isNull(),X'?1.12N,X'?1"-"1.11N set vRM=$$^MSG(742,"N") do vdderr("CID", vRM) quit
	if stfhseq.context.length()>40 set vRM = $$^MSG(1076,40) do vdderr("CONTEXT", vRM) quit
	if stfhseq.fap.length()>40 set vRM = $$^MSG(1076,40) do vdderr("FAP", vRM) quit
	if stfhseq.mssgid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("MSSGID", vRM) quit
	set X = stfhseq.srvcls if 'X.isNull(),X'?1N set vRM=$$^MSG(742,"N") do vdderr("SRVCLS", vRM) quit
	set X = stfhseq.stftjd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("STFTJD", vRM) quit
	if stfhseq.tlo.length()>40 set vRM = $$^MSG(1076,40) do vdderr("TLO", vRM) quit
	if stfhseq.ucls.length()>12 set vRM = $$^MSG(1076,12) do vdderr("UCLS", vRM) quit
	if stfhseq.uid.length()>12 set vRM = $$^MSG(1076,12) do vdderr("UID", vRM) quit
	quit

