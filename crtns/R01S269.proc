	set VL=VL_$J("",100-VL.length())_$S(ROEA:"Y",1:"N")
	set VL=VL_$J("",103-VL.length())_$J($$DAT^%ZM(REMND),10)
	set VL=VL_$J("",114-VL.length())_$J(RAMTP,8,5)
	set VL=VL_$J("",125-VL.length())_$S(ANFLG:"Y",1:"N")
	do VOM
	set VL="" do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,ACOLL,ACR,ANFLG,ASMT,BAL,CID,CONAM,CRCD,CRCDDES,CURRAMT,GRP,IOTYP,IRN,LDREM,LNM,PAYID,PREMA,RAMTD,RAMTP,RBMET,REMIPF,REMND,REMPTP,REMYTD,RID,RN,ROEA,SPF,SRF,TOTESACR,TOTESBAL,TOTESNUM,TYPE,VL,X,vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vovc7,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+3,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",99-VL.length())_"Run Date:"
	set VL=VL_$J("",109-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",101-VL.length())_"System:"
	set VL=VL_$J("",109-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: CRCDDES

	type public String vc3
 type public String CRCDDES
 
 type String CO,CRCD
 
 type RecordCUVAR cuvar=Db.getRecord("CUVAR","CO")
 set CO=cuvar.co
 
 set CRCD=vc3
 
 type RecordCRCD crcd=Db.getRecord("CRCD","CO=:CO,CRCD=:CRCD")
 set CRCDDES="Currency Code "_CRCD_"- "_crcd.desc
	quit

VP2	// Column pre-processor - Variable: LNM

	type public String vc3,vc5
 type public String LNM
 type public Number ACR,BAL,CURRAMT,IRN

 type Number DCID,POSACR
 type String CRCD
 
 set CRCD=vc3
 set DCID=vc5

 type RecordDEP dep = Db.getRecord("DEP","CID=:DCID")

 set LNM=dep.lnm
 set BAL=dep.bal
 set ACR=dep.acr
 set IRN=dep.irn
 set CURRAMT=dep.curramt
 set POSACR=dep.posacr

 type RecordTOTESC totesc=Db.getRecord("TOTESC","CRCD=:CRCD",1)
 if 'totesc.getMode() do {
 	set totesc.totesacr=0
	set totesc.totesbal=0
	set totesc.totesnum=0
 	}

 set totesc.totesacr=totesc.totesacr+POSACR
 set totesc.totesbal=totesc.totesbal+BAL
 set totesc.totesnum=totesc.totesnum+1
 do totesc.save()
	quit

VP3	// Column pre-processor - [SYSDEV,TMPRPT6]KEY6

	type public String vc8,vc3,vc6
 type public Boolean ANFLG,PREMA,REMIPF,ROEA,SPF,SRF
 type public Date LDREM,REMND
 type public Number ACOLL,ASMT,RAMTD,RAMTP,RBMET,REMPTP,REMYTD
 type public String NUM(),PAYID,TY
 
 type String CRCD,DATA,TYP
 
 set DATA=vc8
 set REMPTP=DATA.piece("|",1)
 set LDREM=DATA.piece("|",2)
 set ASMT=DATA.piece("|",3)
 set ACOLL=DATA.piece("|",4)
 set RBMET=DATA.piece("|",5)
 set SPF=DATA.piece("|",6)
 set RAMTD=DATA.piece("|",7)
 set SRF=DATA.piece("|",8)
 set PAYID=DATA.piece("|",9)
 set REMYTD=DATA.piece("|",10)
 set PREMA=DATA.piece("|",11)
 set REMIPF=DATA.piece("|",12)
 set ROEA=DATA.piece("|",13)
 set REMND=DATA.piece("|",14)
 set RAMTP=DATA.piece("|",15)
 set ANFLG=DATA.piece("|",16)
 set CRCD=vc3
 set TYP=vc6
 set TY=$$TT^LNU(TYP)

 type RecordREP260 rep260=Db.getRecord("REP260","CRCD=:CRCD,TY=:TY",1)
 if 'rep260.getMode() set rep260.cnt=0
 set rep260.cnt=rep260.cnt+1
 do rep260.save()
	quit
