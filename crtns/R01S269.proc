VRPOST	// Report post-processor

 // Go to SCA260B for report summary

 type public Number VLC,VRG,IOSL,X
 type public String IOTYP,VFMQ,VL

 if IOTYP="TRM" do {
	set VL="" for X=VLC+1:1:IOSL do VOM
	do ^DBSRWBR(2)
	set VLC=0,VRG=0
 	}
 if VFMQ="Q" do VEXIT()
 else  do {
 	// Do report summary SCA260B
	 type String RID,PGM,VRWOPT()
