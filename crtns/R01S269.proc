VPREAQ	// Pre-processor (after query)

 type public Number CID,TOTESBAL,TOTESACR,TOTESNUM
 type public String CRCD,GRP,TYPE
 
 type Boolean ANFLG,FINISH=0
 type Number DCID,LCID,LTYPE
 type String LCRCD,TRTYPE,TYP

 type ResultSet rs1

 if GRP="ALL",TYPE="ALL",CRCD="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L'")
 else  if GRP="ALL",TYPE'="ALL",CRCD="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND TYPE=:TYPE")
 else  if GRP'="ALL",TYPE="ALL",CRCD="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND GRP=:GRP")
 else  if GRP'="ALL",TYPE'="ALL",CRCD="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND GRP=:GRP AND TYPE=:TYPE")
 else  if GRP="ALL",TYPE'="ALL",CRCD'="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND TYPE=:TYPE AND CRCD=:CRCD")
 else  if GRP'="ALL",TYPE="ALL",CRCD'="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND GRP=:GRP AND CRCD=:CRCD")
 else  if GRP'="ALL",TYPE'="ALL",CRCD'="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND GRP=:GRP AND TYPE=:TYPE AND CRCD=:CRCD")
 else  if GRP="ALL",TYPE="ALL",CRCD'="ALL" set rs1 = Db.select("CID,CRCD,TYPE","LN","CLS='L' AND CRCD=:CRCD")
 while rs1.next() do { quit:FINISH

        if CRCD'="ALL" set LCRCD=CRCD
	else  set LCRCD=rs1.getCol("CRCD")

 	set LCID=rs1.getCol("CID")

	if TYPE'="ALL" set LTYPE=TYPE
	else  set LTYPE=rs1.getCol("TYPE")

	type ResultSet rs2

	if CID="ALL" set rs2 = Db.select("CID,ANFLG","DEP","AREF=:LCID")
	else  set rs2 = Db.select("CID,ANFLG","DEP","AREF=:LCID AND CID=:CID")
	if rs2.next() do {

		if CID'="ALL" set FINISH=1

		set DCID=rs2.getCol("CID")
		set ANFLG=rs2.getCol("ANFLG")
		type DbSet dstrty=Db.selectDbSet("TRTYPE","CID=:DCID")
		while dstrty.next() do {
			type RecordTRTYPE trtype=dstrty.getRecord("TRTYPE")
			set TYP=trtype.typ
			set TRTYPE=trtype.trtype
			type RecordTMPRPT6 TMPRPT6 = Db.getRecord("TMPRPT6","PID=:%ProcessID,KEY1=:LTYPE,KEY2=:LCRCD,KEY3=:LCID,KEY4=:DCID,KEY5=TYP,KEY6=TRTYPE",1)
			set TMPRPT6.data=trtype.remptp_"|"_trtype.ldrem_"|"_trtype.asmt_"|"_trtype.acoll_"|"_trtype.rbmet_"|"_trtype.spf
			set TMPRPT6.data=TMPRPT6.data_"|"_trtype.ramtd_"|"_trtype.srf_"|"_trtype.payid_"|"_trtype.remytd_"|"_trtype.prema
			set TMPRPT6.data=TMPRPT6.data_"|"_trtype.remipf_"|"_trtype.roea_"|"_trtype.remnd_"|"_trtype.ramtp_"|"_ANFLG
			do TMPRPT6.save()
			}
		}
	}
 set TOTESBAL=0
 set TOTESACR=0
 set TOTESNUM=0	
	quit

