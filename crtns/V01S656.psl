VDEPRE(RecordUTBLDELUXEPR fUTBLDEL)  // Data Entry Pre-processor
 
 type public String CLS,GRP,TYP
 
 if GRP="SAV" do {
	do PROTECT^DBSMACRO("UTBLDELUXEPR.ADDSLP")
	do PROTECT^DBSMACRO("UTBLDELUXEPR.DELCUT")
	do GOTO^DBSMACRO("UTBLDELUXEPR.SLPDES")
	}
 
 type RecordPRODCTL prodctl=Db.getRecord("PRODCTL","TYPE=:TYP",1)
 set CLS=prodctl.cls
 
 /* Unprotected Offset Days for Loan Accounts
                          
  Loan accounts are never 'funded' so the funding
  date option will still be protected.  However,
  if a number of offset days is entered, this will be
  used in determining which new account file, [DTJNA],
  to use.						
 */
 
 if CLS="L" do { quit
	do PROTECT^DBSMACRO("UTBLDELUXEPR.FD")
	do DEFAULT^DBSMACRO("UTBLDELUXEPR.FD",0)
	if fUTBLDEL.OD.isNull() do DEFAULT^DBSMACRO("UTBLDELUXEPR.OD",0)
	}
 
 if fUTBLDEL.FD.isNull() do DEFAULT^DBSMACRO("UTBLDELUXEPR.FD",1)
 if fUTBLDEL.OD.isNull() do DEFAULT^DBSMACRO("UTBLDELUXEPR.OD",0)
