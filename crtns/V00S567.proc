VP1(RecordUTBLCO fUTBLCO) //
 /*

   ---- Revision History------------------------------------------------
 
	02/10/05 - KELLYP - CR 13662
		Converted to PSL.
 
   ---------------------------------------------------------------------
 
 */

 type Public String CNTRY,X

 if X.isNull() set CNTRY="0"	// Prevents err on state lookup when CNTRY=""
 else  set CNTRY=X

 if fUTBLCO.city.isNull() quit

 // Required
 if X.isNull() do Runtime.setErrMSG("UTBLCO","2385") quit:ER

 quit
VP2(RecordUTBLCO fUTBLCO) //
 /*
 
   ---- Revision History------------------------------------------------
 
	02/10/05 - KELLYP - CR 13662
		Converted to PSL.
 
   ---------------------------------------------------------------------
 
 */

 type Public String X

 if fUTBLCO.cntry.isNull() quit

 if X.isNull() do {

	type ResultSet rs=Db.select("STATE","STBLCNTRY1","CNTRY=:fUTBLCO.cntry")

	// Allow city with no state if country has no states	
	if rs.isEmpty() quit

	// Country has states, city is defined, state is therefore required

	// Required
	if 'fUTBLCO.city.isNull() do Runtime.setErrMSG("UTBLCO","2385") quit:ER
	}

 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit								// User defined post processor's


