R01S309	// SCA311 - Table Update Log
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:56 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type String RNAM
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type String VIN1="ALL"
	type String VIN2="ALL"
	type String VIN3="ALL"
	type String VIN4="ALL"
	type String VIN5="ALL"
	type String VIN6="ALL"
	type String VIN7="ALL"
	type String VIN8="ALL"

	set RID="SCA311"
	set RN="Table Update Log"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	do VPREBQ quit:VFMQ.get()			// Pre-processor (before query)

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[LOG1]TJD|[LOG1]TJD:DISTINCT:NOVAL||D EXT^DBSQRY||T|Calender Date|||||"
	set %TAB("VIN2")="|255||[LOG]FID|||D EXT^DBSQRY||T|File Name|||||"
	set %TAB("VIN3")="|255||[LOG1]SEQ|[LOG1]SEQ:DISTINCT:NOVAL||D EXT^DBSQRY||T|Sequence Number|||||"
	set %TAB("VIN4")="|255||[LOG]UID|||D EXT^DBSQRY||T|User ID|||||"
	set %TAB("VIN5")="|255||[LOG]TLO|||D EXT^DBSQRY||T|Transaction Location|||||"
	set %TAB("VIN6")="|255||[LOG]TIME|||D EXT^DBSQRY||T|Time|||||"
	set %TAB("VIN7")="|255||[LOG1]DILIST|||D EXT^DBSQRY||T|Data Item List|||||"
	set %TAB("VIN8")="|255||[LOG1]DVLIST|||D EXT^DBSQRY||T|New Value List|||||"

	set %READ="IO/REQ,VIN1#0,VIN2#0,VIN3#0,VIN4#0,VIN5#0,VIN6#0,VIN7#0,VIN8#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String RNAM
	type public String VIN1,VIN2,VIN3,VIN4,VIN5,VIN6,VIN7,VIN8
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String VWHERE
	type Literal String VSELECT
	type String %TIM,CONAM,RN,VL,VLOF,VRF(),VSTATS(),di,dv,option,ov,vCOL,vHDG,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA311",RN="Table Update Log"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA311","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[LOG]SEQ#1#4,[LOG]FID#7#30,[LOG]UID#54#20,[LOG]TLO#76#40,[LOG]TJD#121#10\[LOG]ACKEYS#7#45,[LOG]KEYVALUE#54#65,[LOG]TIME#121#11"

	// Build WHERE clause to use for dynamic query
	do {
		type Number SEQ=1
		type String DQQRY(),FROM
		if VIN1.get()="" set VIN1="ALL"
		if VIN1'="ALL" set DQQRY(SEQ)="[LOG1]TJD "_VIN1,SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[LOG]FID "_VIN2,SEQ=SEQ+1
		if VIN3.get()="" set VIN3="ALL"
		if VIN3'="ALL" set DQQRY(SEQ)="[LOG1]SEQ "_VIN3,SEQ=SEQ+1
		if VIN4.get()="" set VIN4="ALL"
		if VIN4'="ALL" set DQQRY(SEQ)="[LOG]UID "_VIN4,SEQ=SEQ+1
		if VIN5.get()="" set VIN5="ALL"
		if VIN5'="ALL" set DQQRY(SEQ)="[LOG]TLO "_VIN5,SEQ=SEQ+1
		if VIN6.get()="" set VIN6="ALL"
		if VIN6'="ALL" set DQQRY(SEQ)="[LOG]TIME "_VIN6,SEQ=SEQ+1
		if VIN7.get()="" set VIN7="ALL"
		if VIN7'="ALL" set DQQRY(SEQ)="[LOG1]DILIST "_VIN7,SEQ=SEQ+1
		if VIN8.get()="" set VIN8="ALL"
		if VIN8'="ALL" set DQQRY(SEQ)="[LOG1]DVLIST "_VIN8,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("LOG1,LOG") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO()

		set VBNRINFO("PROMPTS",1)="WC2|"_"Calender Date"_"|VIN1|"_VIN1.get()
		set VBNRINFO("PROMPTS",2)="WC2|"_"File Name"_"|VIN2|"_VIN2.get()
		set VBNRINFO("PROMPTS",3)="WC2|"_"Sequence Number"_"|VIN3|"_VIN3.get()
		set VBNRINFO("PROMPTS",4)="WC2|"_"User ID"_"|VIN4|"_VIN4.get()
		set VBNRINFO("PROMPTS",5)="WC2|"_"Transaction Location"_"|VIN5|"_VIN5.get()
		set VBNRINFO("PROMPTS",6)="WC2|"_"Time"_"|VIN6|"_VIN6.get()
		set VBNRINFO("PROMPTS",7)="WC2|"_"Data Item List"_"|VIN7|"_VIN7.get()
		set VBNRINFO("PROMPTS",8)="WC2|"_"New Value List"_"|VIN8|"_VIN8.get()


		do {
			type Number SEQ
			type String VALUE,VAR,X
			set X=VWHERE
			set SEQ=""
			for  set SEQ=VBNRINFO("PROMPTS",SEQ).order() quit:SEQ=""  do {
				set VAR=VBNRINFO("PROMPTS",SEQ).piece("|",3))
				set VALUE=VBNRINFO("PROMPTS",SEQ).piece("|",4,99)
				set X=$$replace^DBSRWUTL(X,":"_VAR,"'"_VALUE_"'")
				}
			set VBNRINFO("WHERE")=X
			}

		set VBNRINFO("DESC")="Table Update Log"
		set VBNRINFO("PGM")="R01S309"
		set VBNRINFO("RID")="SCA311"
		set VBNRINFO("TABLES")="LOG1,LOG"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,LOG1]TJD"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,LOG]FID"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,LOG1]SEQ"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,LOG1]SUBSEQ"

		set VBNRINFO("DOC",1)="The Table Update Log (SCA311) contains detailed information"
		set VBNRINFO("DOC",2)="about updates made to all system records."
		set VBNRINFO("DOC",3)=""
		set VBNRINFO("DOC",4)="For table maintenance purposes, this report includes entries"
		set VBNRINFO("DOC",5)="and updates made to the user, common, and system tables."
		set VBNRINFO("DOC",6)=""
		set VBNRINFO("DOC",7)="For further information, refer to the Table Maintenance"
		set VBNRINFO("DOC",8)="Reference Manual."

		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:4 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST

	set VSELECT=""
	set VSELECT=VSELECT_"LOG1.TJD,LOG.FID,LOG1.SEQ,LOG1.SUBSEQ,LOG.SEQ,LOG."
	set VSELECT=VSELECT_"UID,LOG.TLO,LOG.TJD,LOG.ACKEYS,LOG.KEYVALUE,LOG.TI"
	set VSELECT=VSELECT_"ME,LOG.%O,LOG1.DILIST,LOG1.DVLIST,LOG1.OVLIST"

	#ACCEPT DATE=08/30/2007;PGM=Report Writer Generator;CR=20967
	type ResultSet rwrs=Db.select(VSELECT,"LOG1,LOG",VWHERE,"LOG1.TJD,LOG.FID,LOG1.SEQ,LOG1.SUBSEQ","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12,vc13,vc14,vc15
	set vc1=V.piece($C(9),1)			// LOG1.TJD
	set vc2=V.piece($C(9),2)			// LOG.FID
	set vc3=V.piece($C(9),3)			// LOG1.SEQ
	set vc4=V.piece($C(9),4)			// LOG1.SUBSEQ
	set vc5=V.piece($C(9),5)			// LOG.SEQ
	set vc6=V.piece($C(9),6)			// LOG.UID
	set vc7=V.piece($C(9),7)			// LOG.TLO
	set vc8=V.piece($C(9),8)			// LOG.TJD
	set vc9=V.piece($C(9),9)			// LOG.ACKEYS
	set vc10=V.piece($C(9),10)			// LOG.KEYVALUE
	set vc11=V.piece($C(9),11)			// LOG.TIME
	set vc12=V.piece($C(9),12)			// LOG.%O
	set vc13=V.piece($C(9),13)			// LOG1.DILIST
	set vc14=V.piece($C(9),14)			// LOG1.DVLIST
	set vc15=V.piece($C(9),15)			// LOG1.OVLIST
	quit

	// User-defined pre/post-processor code

VPREBQ	// Pre-processor (before query)

	// Converted to PSL by CNV70RW
	type Public String RID,RNAM

	set RNAM=$G(RID)
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			do VBREAK,stat^DBSRWUTL(2)
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	if 'vskp(3).get() do VDTL3 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(4)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4
	set (vb1,vb2,vb3,vb4)=0
	if vb1!(vovc1'=vc1) set vs(2)=0,vh(2)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,VH0=1
	if vb2!(vovc2'=vc2) set vs(3)=0,vh(3)=0,VD(2)=0,vb3=1,vb4=1,VH0=1
	if vb3!(+vovc3'=+vc3) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(4)=VT(4)+1
	quit

VDTL3	// Detail
	type public String %TIM,IOSL,RID,RNAM,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),di,dv,option,ov,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:VD(3)  set VD(3)=1				// Print flag
	if VLC+3>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc5,4)
	set VL=VL_$J("",6-VL.length())_$E(vc2,1,30)
	do VP1 quit:VFMQ!verror.get()  set V=$E(option,1,13)
	set VL=VL_$J("",38-VL.length())_V
	set VL=VL_$J("",53-VL.length())_$E(vc6,1,20)
	set VL=VL_$J("",75-VL.length())_$E(vc7,1,40)
	set VL=VL_$J("",120-VL.length())_$J($$DAT^%ZM(vc8),10)
	do VOM
	set VL="      "_$E(vc9,1,45)
	set VL=VL_$J("",53-VL.length())_$E(vc10,1,65)
	set VL=VL_$J("",120-VL.length())_$J($$TIM^%ZM(vc11),11)
	do VOM
	do VP2 quit:VFMQ!verror.get()  set V=$E(di,1,30) set VL="      "_V
	set VL=VL_$J("",38-VL.length())_$E(dv,1,45)
	set VL=VL_$J("",85-VL.length())_$E(ov,1,45)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,RNAM,VL,di,dv,option,ov,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+7,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="Seq#  Table Name                      Option         User ID               Location                                       Date"
	do VOM
	set VL="      "_"Access Key(s)                                  Access Key Value(s)                                                  Time"
	do VOM
	set VL="      "_"Data Item Name                  New Data Item Value                            Old Data Item Value"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: option

	type public String vc12
	type Public String LOG,option

	type Number MODE

	set MODE=vc12

	if MODE=0 set option=$$^MSG(7953) quit    //Create Record
	if MODE=1 set option=$$^MSG(7957) quit    //Modify Record
	if MODE=3 set option=$$^MSG(7955) quit    //Delete Record
	set option=""
	quit 

VP2	// Column pre-processor - Variable: di

	type public String vc13,vc14,vc15
	type Public String DILIST,DVLIST,I,IOSL,LOG1,OVLIST,VFMQ,VL,VLC,di,dv,ov,verror

	for I=1:1:$L(vc13,",") do { quit:verror 
		set di=$P(vc13,",",I)
		set dv=$P(vc14,$C(1),I)
		set ov=$P(vc15,$C(1),I)
		//
		set VL=$J("",6)_di_$J("",32-$L(di))_dv_$J("",47-$L(dv))_ov do VOM
		set (di,dv,ov)=""
		//
		set verror=0
		if VLC+2>IOSL do VHDG0
		if VFMQ set verror=1 quit 
		}
	quit 
