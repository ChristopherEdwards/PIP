	type public String %NOCLOSE,%NOOPEN,%TIM,CC,CUVARCRCD,FEEREM,GLSC,I,IOSL,LN,PGM,RID,V,VFMQ,VL,VLC,VO,VOFFLG,VT(),VX(),ZJ,fDAYENDX,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc27,vc28,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vrundate,vsysdate

