	type Public Number VRWOPT(),VT(),vc16,vc15,vc18,vc23,vc19,vc13,vc21,vc17,vc11,vc20,vc14,vc22
	type Public String VSTATS

	set VT(7)=VT(7)+1
	set VT(7,1,1)=VT(7,1,1)+vc16			// @TOT([DAYENDXFR]ACR)
	set VT(7,2,1)=VT(7,2,1)+vc15			// @TOT([DAYENDXFR]COA)
	set VT(7,3,1)=VT(7,3,1)+vc18			// @TOT([DAYENDXFR]DEFTOT)
	set VT(7,4,1)=VT(7,4,1)+vc23			// @TOT([DAYENDXFR]FEEREM)
	set VT(7,5,1)=VT(7,5,1)+vc19			// @TOT([DAYENDXFR]IUNC)
	set VT(7,6,1)=VT(7,6,1)+vc13			// @TOT([DAYENDXFR]LCHG)
	set VT(7,7,1)=VT(7,7,1)+vc21			// @TOT([DAYENDXFR]NDFBAL)
	set VT(7,8,1)=VT(7,8,1)+vc17			// @TOT([DAYENDXFR]NEGACR)
	set VT(7,9,1)=VT(7,9,1)+vc11			// @TOT([DAYENDXFR]PRN)
	set VT(7,10,1)=VT(7,10,1)+vc20			// @TOT([DAYENDXFR]REC)
	set VT(7,11,1)=VT(7,11,1)+vc14			// @TOT([DAYENDXFR]UDBAL)
	set VT(7,12,1)=VT(7,12,1)+vc22			// @TOT([DAYENDXFR]UNAPF)
	quit

