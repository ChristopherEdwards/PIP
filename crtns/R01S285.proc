	set VL=VL_$J("",96-VL.length())_$J($FN(V,",",2),17)
	set V=VT(7,4,1)					// @TOT([DAYENDXFR]FEEREM,)
	set VL=VL_$J("",113-VL.length())_$J($FN(V,",",2),18)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%NOCLOSE,%NOOPEN,%TIM,CC,CONAM,CUVARCRCD,FEEREM,GLSC,LN,PGM,RID,RN,VL,ZJ,fDAYENDX,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc27,vc28,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+12,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL=$E(CONAM,1,40)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",122-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  (SCA280)"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$E($J(VPN,3),1,3)
	do VOM
	set VL="" do VOM
	set VL="Effective Date: "
	set VL=VL_$J("",16-VL.length())_$J($$DAT^%ZM(vc1),10)
	set VL=VL_$J("",28-VL.length())_"G/L Set Code: "
	set VL=VL_$J("",42-VL.length())_$E(vc28,1,8)
	do VOM
	set VL="      "_"Currency: "
	do VP4 quit:VFMQ!verror.get()  set V=$E(CUVARCRCD,1,10)
	set VL=VL_$J("",16-VL.length())_V
	set VL=VL_$J("",29-VL.length())_"Cost Center:"
	set VL=VL_$J("",42-VL.length())_$E(vc4,1,8)
	do VOM
	set VL="" do VOM
	set VL="                                            "_"-- Deps/Loans --  --- Deposits ---  ---------------------- Loans ----------------------"
	do VOM
	set VL="     "_"Account  Customer           Transfer          Principal  Uncred Avail Int      Late Chg Due   Undisbursed Prn      Chg Off Amt"
	do VOM
	set VL="                               "_"Type     CC       Accrued Int   Neg Accrued Int      Misc Fee Due   Uncollected Int     Recovery Amt"
	do VOM
	set VL="                                                                                    "_"Net Def Fee    Unapplied Funds   Fee Receivable"
	do VOM
	set VL="===================================================================================================================================="
	do VOM
	set VL="" do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: $S(XFLG:"In"

	type public String vc5
   type Public String XFLG
   set XFLG=vc5

	quit

VP2	// Column pre-processor - [SYSDEV,DAYENDXFR]PRN

	type public String vc5,vc10,vc4,vc24,vc25,vc26,vc27,vc28
	type Public String V
	type Public Boolean ER

	type Number CC1,CC2,MYCC
	type String C,XFLG,ZCLS,ZGLSC,ZGRP,ZJ,ZTYPE

	// CCI=FROM cost center  CC2=TO cost center
	set XFLG=vc5
	set CC1=$S(XFLG:vc10,1:vc4)
	set CC2=$S(XFLG:vc4,1:vc4)
	set MYCC=$E("000000",1,6-$L(CC1))_CC1_"#"_$E("000000",1,6-$L(CC2))_CC2
	set ZJ=%ProcessID

	type RecordCUVAR cuvar=Db.getRecord("CUVAR") 
	set C=$S(vc24=" ":cuvar.%crcd,1:(vc24))
	if C="" set C="USD"

	set ZCLS=vc25
	set ZGRP=vc26
	set ZTYPE=vc27
	set ZGLSC=vc28

	if Db.isDefined("DAYENDXFR2","JOB=:ZJ,CRCD=:C,CLS=:ZCLS,GRP=:ZGRP,TYPE=:ZTYPE,GLSC=:ZGLSC,CC=:MYCC") do {
		type RecordDAYENDXFR2 dnd2=Db.getRecord("DAYENDXFR2","JOB=:ZJ,CRCD=:C,CLS=:ZCLS,GRP=:ZGRP,TYPE=:ZTYPE,GLSC=:ZGLSC,CC=:MYCC")
		if XFLG set dnd2.trin=dnd2.trin+V
		else  set dnd2.trout=dnd2.trout+V
		do dnd2.save()
		}

	else  do {
		type RecordDAYENDXFR2 dnd2=Class.new("RecordDAYENDXFR2")
		set dnd2.job=ZJ
		set dnd2.crcd=C
		set dnd2.cls=ZCLS
		set dnd2.grp=ZGRP
		set dnd2.type=ZTYPE
		set dnd2.glsc=ZGLSC
		set dnd2.CC=MYCC
		set dnd2.CC1=CC1
		set dnd2.CC2=CC2
		if XFLG set dnd2.trin=dnd2.trin+V
                else  set dnd2.trout=dnd2.trout+V
                do dnd2.save()
		}
	quit

VP3	// Column pre-processor - Variable: FEEREM

	type public String vc23
	type Public Number FEEREM
	set FEEREM=+vc23
	quit

VP4	// Column pre-processor - Variable: CUVARCRCD

	type Public String CUVARCRCD

	type RecordCUVAR cuvar=Db.getRecord("CUVAR") 
	set CUVARCRCD=cuvar.%crcd 
	quit 
