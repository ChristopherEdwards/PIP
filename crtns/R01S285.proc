	type public String %MSKD,%NOCLOSE,%NOOPEN,%TIM,CC,CONAM,CUVARCRCD,ER,FEEREM,GLSC,IOSL,LN,PGM,RID,RN,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,ZJ,fDAYENDX,vc1,vc10,vc11,vc12,vc13,vc14,vc15,vc16,vc17,vc18,vc19,vc2,vc20,vc21,vc22,vc23,vc24,vc25,vc26,vc27,vc28,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vovc5,vovc6,vrundate,vsysdate

	quit:vh(7)  set vh(7)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="Transfers"
	do VP1 quit:VFMQ!verror.get()  set V=$E($S(XFLG:"In",1:"Out"),1,3)
	set VL=VL_$J("",10-VL.length())_V
	do VOM
	set VL="" do VOM
	quit

