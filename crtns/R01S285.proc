	set VL=VL_$J("",114-VL.length())_$J($FN(vc15,",",2),17)
	do VOM
	set VL="                                           "_$J($FN(vc16,",",2),16)
	set VL=VL_$J("",60-VL.length())_$J($FN(vc17,",",2),17)
	set VL=VL_$J("",78-VL.length())_$J(vc18,17)
	set VL=VL_$J("",96-VL.length())_$J($FN(vc19,",",2),17)
	set VL=VL_$J("",114-VL.length())_$J($FN(vc20,",",2),17)
