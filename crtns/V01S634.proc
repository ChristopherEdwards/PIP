 #WARN SCOPE OFF
	type Public String %MODS,%PAGE,%PG,%REPEAT,ER,RM
	type String KEYS(),KVAR,VFSN(),VO,VODFT,VPGM,vPSL,VSID,VSNAME

	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="UTBLCHKACT3",VPGM=$T(+0),VSNAME="Check Action Table - Check Status Level"
 set VFSN("UTBLCHKACT3")="zUTBL"
 set vPSL=1
 set KEYS(1)=UTBL.CHKS
 set KEYS(2)=UTBL.CHKACT
 set KEYS(3)=UTBL.CHKSTS
 set KEYS(4)=UTBL.CHKSEQ
 //
	// ==================== Display blank screen         (%O=5)

 if %ProcessMode=5 do VPR(.UTBL),VDA1(.UTBL),^DBSPNT()

 if '%ProcessMode do VNEW(.UTBL),VPR(.UTBL),VDA1(.UTBL)
 if %ProcessMode do VLOD(.UTBL) quit:$G(ER)  do VPR(.UTBL),VDA1(.UTBL)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.UTBL)
	quit


VNEW(RecordUTBLCHKACT3 UTBL) // Initialize arrays if %O=0
 
 do VDEF(.UTBL)
 do VLOD(.UTBL)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
VDEF(RecordUTBLCHKACT3 UTBL)
 #ACCEPT Date=11/5/03;PGM=Screen Compiler
 quit
 ;
VLOD(RecordUTBLCHKACT3 UTBL) // Load data from disc - %O = (1-5)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


	type Public String %MODS,%REPEAT
	quit
	

VPR(RecordUTBLCHKACT3 UTBL) // Display screen prompts
 set VO="7||13|0"
 set VO(0)="|0"
 set VO(1)=$C(1,31,20,1,0,0,0,0,0,0)_"01T CHECK ACTION TABLE "
 set VO(2)=$C(5,14,12,1,0,0,0,0,0,0)_"01T Check Type:"
 set VO(3)=$C(7,12,14,1,0,0,0,0,0,0)_"01T Check Action:"
 set VO(4)=$C(9,12,14,1,0,0,0,0,0,0)_"01T Check Status:"
 set VO(5)=$C(11,2,24,1,0,0,0,0,0,0)_"01T Check Routine Sequence:"
 set VO(6)=$C(14,1,25,1,0,0,0,0,0,0)_"01T Status After the Action:"
 set VO(7)=$C(16,8,18,0,0,0,0,0,0,0)_"01TExtrinsic Routine:"
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


VDA1(RecordUTBLCHKACT3 UTBL)  // Display screen data
 new V
 //
 set VO="13|8|13|0"
 set VO(8)=$C(5,27,6,2,0,0,0,0,0,0)_"01T"_$E(UTBL.CHKS,1,6)
 set VO(9)=$C(7,27,12,2,0,0,0,0,0,0)_"01T"_$E(UTBL.CHKACT,1,12)
 set VO(10)=$C(9,27,2,2,0,0,0,0,0,0)_"01N"_UTBL.CHKSTS
 set VO(11)=$C(11,27,2,2,0,0,0,0,0,0)_"01N"_UTBL.CHKSEQ
 set VO(12)=$C(14,27,2,2,0,0,0,0,0,0)_"00N"_UTBL.STSNEW
 set VO(13)=$C(16,27,40,2,0,0,0,0,0,0)_"00T"_$E(UTBL.STSRTN,1,40)
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit
	
	
VTAB(RecordUTBLCHKACT3 UTBL)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=6,VPT=1,VPB=16,PGM=$T(+0),DLIB="SYSDEV",DFID="UTBLCHKACT3"
 set OLNTB=16027
 
 set VFSN("UTBLCHKACT3")="zUTBL"
 //
 // 
	
	
 set %TAB(1)=$C(4,26,6)_"21T12401|1|[UTBLCHKACT3]CHKS|[UTBLCHKACT1]"
 set %TAB(2)=$C(6,26,12)_"21T12402|1|[UTBLCHKACT3]CHKACT|[UTBLCHKACT2]"
 set %TAB(3)=$C(8,26,2)_"21N12403|1|[UTBLCHKACT3]CHKSTS|[STBLCHKSTS]"
 set %TAB(4)=$C(10,26,2)_"21N12404|1|[UTBLCHKACT3]CHKSEQ|[UTBLCHKACT3]"
 set %TAB(5)=$C(13,26,2)_"01N12401|1|[UTBLCHKACT3]STSNEW|[STBLCHKSTS]"
 set %TAB(6)=$C(15,26,40)_"00T12402|1|[UTBLCHKACT3]STSRTN"
 do VTBL(.UTBL)
	do ^DBSCRT8 	// data entry
	quit


VREQ   // Create REQ() array
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	quit


