		if VIN1'="ALL" set DQQRY(SEQ)="[ACH2]COID "_VIN1,SEQ=SEQ+1
		if VIN2.get()="" set VIN2="ALL"
		if VIN2'="ALL" set DQQRY(SEQ)="[ACH2]DATNP "_VIN2,SEQ=SEQ+1
		set FROM=$$DQJOIN^SQLCONV("ACH2,ACH1") quit:ER
		set VWHERE=$$WHERE^SQLCONV(.DQQRY,"")
		}

