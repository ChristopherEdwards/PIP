		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column post-processor - Variable: TOTCD

 type public Number TOTCC,TOTCD

 set (TOTCC,TOTCD)=0
	quit

VP2	// Column post-processor - [SYSDEV,ACH2]SEQ

	type public String vc10
 type public String AMTOPT

 set AMTOPT=vc10

 if AMTOPT.extract() '?.A set AMTOPT=""
	quit

VP3	// Column pre-processor - [SYSDEV,ACH2]TAMT

	type public String vc12,vc19,vc20,vc10,vc16
 type public String ONUS,TC,ZACHRT
 type String DB
 
 set ZACHRT=vc12

 type RecordUTBLACHRT utblachart=Db.getRecord("UTBLACHRT","KEY=:ZACHRT",1)

 if 'utblachart.getMode() quit
 else  set ONUS=utblachart.onus

 if +ONUS  do { quit
	set TC=vc19
	type RecordTRN trn=Db.getRecord("TRN","ETC=:TC",1)
	set DB=(trn.itc).extract()
	do TOT 
	}

 set TC=vc20

 type RecordSTBLACHTC stblachtc=Db.getRecord("STBLACHTC","ACHTC=:TC",1)

 if 'stblachtc.getMode() quit
 else  set DB=stblachtc.dc
 
 do TOT
 quit

TOT //-------------------

 type public Number TOTCC,TOTCD,TOTRC,TOTRD,TOTGC,TOTGD,V
 type public String DB
 type String A,CID,CRCD,ZTAMT

 set ZTAMT=vc10
 if ZTAMT.extract()?.A do {
 	set CID=vc16
 	set (ZTAMT,V)=$$TAMT^ACHCB(ZTAMT,vc16)
	}


 set V=V.roundDec(13,CID.get(),CRCD.get())

 set TOTRC=TOTRC.get()+$S(DB:+ZTAMT.get(),1:0)
 set TOTRD=TOTRD.get()+$S('DB:+ZTAMT.get(),1:0)
 set TOTGC=TOTGC.get()+$S(DB:+ZTAMT.get(),1:0)
 set TOTGD=TOTGD.get()+$S('DB:+ZTAMT.get(),1:0)
 set TOTCC=TOTCC.get()+$S(DB:+ZTAMT.get(),1:0)
 set TOTCD=TOTCD.get()+$S('DB:+ZTAMT.get(),1:0)
 quit
 
	quit

VP4	// Column post-processor - [SYSDEV,ACH2]OFFCID

	type public String vc16
 type String VL

 if 'vc16 set VL=""
	quit

VP5	// Column post-processor - Variable: TOTGD

 type public Number TOTGC,TOTGD

 set (TOTGC,TOTGD)=0
	quit
