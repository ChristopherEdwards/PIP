vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordEXPOS expos

	set X = expos.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
	set X = expos.sys if 'X.isNull(),'Db.isDefined("STBLFTPSYS","X") set vRM = $$^MSG(1485,X) do vdderr("SYS", vRM) quit
	if expos.intprod.length()>40 set vRM = $$^MSG(1076,40) do vdderr("INTPROD", vRM) quit
	if expos.recid.length()>15 set vRM = $$^MSG(1076,15) do vdderr("RECID", vRM) quit
	set X = expos.comexp if 'X.isNull() set vRM = $$VAL^DBSVER("$",15,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"EXPOS.COMEXP"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if expos.crcd.length()>3 set vRM = $$^MSG(1076,3) do vdderr("CRCD", vRM) quit
	set X = expos.mdt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("MDT", vRM) quit
	set X = expos.outexp if 'X.isNull() set vRM = $$VAL^DBSVER("$",15,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"EXPOS.OUTEXP"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if expos.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

