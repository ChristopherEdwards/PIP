vkchged // Access key changed

	type public RecordEXPOS expos

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("ACN").exists() set vux("ACN") = vx("ACN")
	if vx("SYS").exists() set vux("SYS") = vx("SYS")
	if vx("INTPROD").exists() set vux("INTPROD") = vx("INTPROD")
	if vx("RECID").exists() set vux("RECID") = vx("RECID")
	do vkey(1) set voldkey = expos.acn_","_expos.sys_","_expos.intprod_","_expos.recid // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = expos.acn_","_expos.sys_","_expos.intprod_","_expos.recid // Copy new keys into object
	type RecordEXPOS vnewrec = expos.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("EXPOS",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordEXPOS expos

	if vux("ACN").exists() set expos.acn = vux("ACN").piece("|",i)
	if vux("SYS").exists() set expos.sys = vux("SYS").piece("|",i)
	if vux("INTPROD").exists() set expos.intprod = vux("INTPROD").piece("|",i)
	if vux("RECID").exists() set expos.recid = vux("RECID").piece("|",i)
	quit
