vEXEC(vCONTEXT,EFD,LDC,CID) //
 	do exec
 	quit

exec //

 	type RecordLN ln
 	set ln=Db.getRecord("LN","CID")
 
 	set ln.old=%SystemDate		// Overdue - Last Date
	if LDC="LDC1" do {
 		set ln.dy1=ln.dy1+1		// Update Delinq - Category 1 YTD
 		set ln.dl1=ln.dl1+1		// Update Delinq - Category 1 LTD
 		}
	if LDC="LDC2" do {
 		set ln.dy2=ln.dy2+1		// Update Delinq - Category 2 YTD
 		set ln.dl2=ln.dl2+1		// Update Delinq - Category 2 LTD
 		}
	if LDC="LDC3" do {
 		set ln.dy3=ln.dy3+1		// Update Delinq - Category 3 YTD
 		set ln.dl3=ln.dl3+1		// Update Delinq - Category 3 LTD
 		}
	if LDC="LDC4" do {
 		set ln.dy4=ln.dy4+1		// Update Delinq -Category 4 YTD
 		set ln.dl4=ln.dl4+1		// Update Delinq - Category 4 LTD
 		}
	if LDC="LDC5" do {
 		set ln.dy5=ln.dy5+1		// Update Delinq -Category 5 YTD
 		set ln.dl5=ln.dl5+1		// Update Delinq -Category 5 LTD
 		}
	if LDC="LDC6" do {
 		set ln.dy6=ln.dy6+1		// Update Delinq - Category 6 YTD
 		set ln.dl6=ln.dl6+1		// Update Delinq - Category 6 LTD
 		}
	if LDC="LDC7" do {
 		set ln.dy7=ln.dy7+1		// Update Delinq - Category 7 YTD
 		set ln.dl7=ln.dl7+1		// Update Delinq -Category 7 LTD
 		}

 	do ln.bypassSave()		// Fast save
 	quit
 
 #ACCEPT Date=07/15/03;PGM=Allan Mattson;CR=20967
 quit
