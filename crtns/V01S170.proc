VTAB(RecordCUVAR fCUVAR)
 
 kill VSCRPP,REQ,%TAB,%MOD,%MODOFF,%MODGRP,%REPREQ,vtab
 set %MAX=12,VPT=1,VPB=15,PGM=$T(+0),DLIB="SYSDEV",DFID="CUVAR"
 set OLNTB=15065
 
 set VFSN("CUVAR")="zfCUVAR"
 //
 // 
	
	
 set %TAB(1)=$C(3,26,3)_"01N12401|1|[CUVAR]IRAHIST"
 set %TAB(2)=$C(3,64,4)_"00N12421|1|[CUVAR]IRAPRGD"
 set %TAB(3)=$C(4,64,10)_"00D12416|1|[CUVAR]PRYRBKD|||do VP1^V01S170(.fCUVAR)"
 set %TAB(4)=$C(8,24,3)_"01N12402|1|[CUVAR]IRACON|[UTBLIRACON]"
