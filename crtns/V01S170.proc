 set %TAB(10)=$C(13,64,3)_"01N12408|1|[CUVAR]IRAAUT|[UTBLIRADIS]"
 set %TAB(11)=$C(14,24,3)_"01N12406|1|[CUVAR]IRAIWH|[UTBLIRADIS]"
