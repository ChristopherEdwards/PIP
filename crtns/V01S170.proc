 new V
 //
 set VO="28|17|13|"
 set VO(17)=$C(4,27,3,2,0,0,0,0,0,0)_"00N"_fCUVAR.IRAHIST
 set VO(18)=$C(4,65,4,2,0,0,0,0,0,0)_"00N"_fCUVAR.IRAPRGD
 set VO(19)=$C(5,65,10,2,0,0,0,0,0,0)_"00D"_$$DAT^%ZM(fCUVAR.PRYRBKD)
 set VO(20)=$C(9,25,3,2,0,0,0,0,0,0)_"00N"_fCUVAR.IRACON
 set VO(21)=$C(9,65,3,2,0,0,0,0,0,0)_"00N"_fCUVAR.IRADIS
 set VO(22)=$C(10,25,3,2,0,0,0,0,0,0)_"00N"_fCUVAR.IRAINT
