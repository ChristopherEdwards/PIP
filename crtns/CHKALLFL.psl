vinit // Initialize default values

	type public RecordCHKALLOC chkalloc

	// Type local variables for access keys for defaults
	type public String BRCD
	type public String TYPE
	type public String HIGH

	type public String %UID
	if chkalloc.uid.isNull() set chkalloc.uid = %UID       // uid
	quit


vreqn // Validate required data items

	type public RecordCHKALLOC chkalloc

	quit

vrequ // Valid required columns on update


	type public String vx()
	type public RecordCHKALLOC chkalloc

	if chkalloc.brcd.isNull() do vreqerr("BRCD") quit
	if chkalloc.type.isNull() do vreqerr("TYPE") quit
	if chkalloc.high.isNull() do vreqerr("HIGH") quit

	quit

vreqerr(di) // Required error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("CHKALLOC","MSG",1767,"CHKALLOC."_di)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit




vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordCHKALLOC chkalloc

	set X = chkalloc.brcd if 'X.isNull(),'Db.isDefined("UTBLBRCD","X") set vRM = $$^MSG(1485,X) do vdderr("BRCD", vRM) quit
	set X = chkalloc.type if 'X.isNull(),'Db.isDefined("UTBLCHKS","X") set vRM = $$^MSG(1485,X) do vdderr("TYPE", vRM) quit
	if chkalloc.high.length()>12 set vRM = $$^MSG(1076,12) do vdderr("HIGH", vRM) quit
	set X = chkalloc.issd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ISSD", vRM) quit
	if chkalloc.low.length()>12 set vRM = $$^MSG(1076,12) do vdderr("LOW", vRM) quit
	if chkalloc.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	quit

vdderr(di, vRM) // Column attribute error

	type public Boolean ER = 0
	type public String RM
	do SETERR^DBSEXECU("CHKALLOC","MSG",979,"CHKALLOC."_di_" "_vRM)
	if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~"))
	quit


vkchged // Access key changed

	type public RecordCHKALLOC chkalloc

	type public Boolean ER = 0
	type public String RM,vpar,vx()

	type Number %O = 1
	type String vnewkey,voldkey,vux

	if vx("BRCD").exists() set vux("BRCD") = vx("BRCD")
	if vx("TYPE").exists() set vux("TYPE") = vx("TYPE")
	if vx("HIGH").exists() set vux("HIGH") = vx("HIGH")
	do vkey(1) set voldkey = chkalloc.brcd_","_chkalloc.type_","_chkalloc.high // Copy old keys into object

	set vpar = $$setPar^UCUTILN(vpar,"NOINDEX")            // Switch Index off
	if vpar["/VALREQ/" do vrequ
	if vpar["/VALDD/" do vddver
	do vexec

	do vkey(2) set vnewkey = chkalloc.brcd_","_chkalloc.type_","_chkalloc.high // Copy new keys into object
	type RecordCHKALLOC vnewrec = chkalloc.copy()
	do vnewrec.setMode(0)
	do vnewrec.save("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")

	set %O = 1 do CASUPD^DBSEXECU("CHKALLOC",voldkey,vnewkey) if ER throw Class.new("Error","%PSL-E-DBFILER,"_RM.get().replace(",","~")) // Cascade update

	do vkey(1)                                             // Reset key for delete
	set vpar = $$initPar^UCUTILN("/NOVAL/NOCASDEL/NOJOURNAL/NOTRIGBEF/NOTRIGAFT/")
	set %O = 3 do vdelete(1)                               // Delete old record

	quit

vkey(Number i) // Restore access keys

	type public String vux()
	type public RecordCHKALLOC chkalloc

	if vux("BRCD").exists() set chkalloc.brcd = vux("BRCD").piece("|",i)
	if vux("TYPE").exists() set chkalloc.type = vux("TYPE").piece("|",i)
	if vux("HIGH").exists() set chkalloc.high = vux("HIGH").piece("|",i)
	quit
