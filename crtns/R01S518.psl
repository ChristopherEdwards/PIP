R01S518	// SCA596 - Customer Limits Review Report
	// Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 15:00 - joynerd

	type public Number ER=0
	type public Number vbatchq
	type public String IO,RM,VRWOPT()
	type public String TJD
	type Number OLNTB
	type String %READ,RID,RN,%TAB,VFMQ
	type Date date=TJD.get()

	set RID="SCA596"
	set RN="Customer Limits Review Report"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("date")="|10||||||K vhdg|D|Review  Date|||||"

	set %READ="IO/REQ,date#0,"

	// Skip device prompt option
	if VRWOPT("NOOPEN").get() set %READ=%READ.piece(",",2,99)

	set VFMQ=""
	if %READ'="" do { quit:VFMQ.get()="Q"
		set OLNTB=30
		set %READ="@RN/CEN#1,,"_%READ
		do ^UTLREAD
		}

	if 'vbatchq.get() do V0
	quit

V0	// External report entry point

	type public Number AUXPTR,ER,VTBLNAM
	type public String IO,IOPAR,IOSL,IOTYP,%MSKD,RM,VDISTKEY,VRWOPT()
	type public String TJD
	type public Date date
	type Number vcrt,VD(),VFMQ,vh(),vI,vlc,VLC,VNEWHDR,VOFFLG,VPN,VR,VRG,vs(),VSEQ,VT()
	type String %TIM,CONAM,RID,RN,VL,VLOF,VRF(),VSTATS(),type,vCOL,vHDG,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	type RecordCUVAR cuvar=Db.getRecord("CUVAR")

	set CONAM=CUVAR.conam
	set ER=0,RID="SCA596",RN="Customer Limits Review Report"
	set VL=""

	use 0 if 'VRWOPT("NOOPEN").get() do { quit:ER
		if 'VRWOPT("IOPAR").get().isNull() set IOPAR = VRWOPT("IOPAR")
		else  if ((IOTYP.get()="RMS")!(IOTYP.get()="PNTQ")),('IOPAR.get().isLike("%/OCHSET=%")),$$VALID^%ZRTNS("UCIOENCD") do {
			// Accept warning if ^UCIOENCD does not exist
			#ACCEPT Date=07/26/06; Pgm=RussellDS; CR=22121; Group=MISMATCH
			type String CHRSET=$$^UCIOENCD("Report","SCA596","V0","*")
			if 'CHRSET.isNull() set IOPAR = IOPAR_"/OCHSET="_CHRSET
		}
		do OPEN^SCAIO
	}
	set vcrt=(IOTYP="TRM")
	if 'vcrt set IOSL=60				// Non-interactive
	else  do {					// Interactive
		do TERM^%ZUSE(IO,"WIDTH=133")
		write $$CLEARXY^%TRMVT
		write $$SCR132^%TRMVT			// Switch to 132 col mode
		}

	do INIT^%ZM()

	set vCOL="[CIFTYPELIM]TYPELIM#15#12,[CIFTYPELIM]TYPEAMT#75#15,[CIFTYPELIM]TYPEDATE#96#10"

	// Print Report Banner Page
	if cuvar.banner,'VRWOPT("NOBANNER").get(),IOTYP'="TRM",'AUXPTR.get() do {
		type String VBNRINFO(),VWHERE


		set VWHERE=""
		set VWHERE=VWHERE_"CIF.CUSDATE IS NOT NULL AND CIF.CUSDATE NOT >:date"
		set VWHERE=VWHERE_" AND (CIF.PLAN IS NOT NULL OR CIF.CUSAMT>0) AND CI"
		set VWHERE=VWHERE_"FGRPLIM.GRPDATE IS NOT NULL AND CIFGRPLIM.GRPDATE "
		set VWHERE=VWHERE_"NOT >:date AND CIFTYPELIM.TYPEDATE IS NOT NULL AND"
		set VWHERE=VWHERE_" CIFTYPELIM.TYPEDATE NOT >:date"
		set VBNRINFO("WHERE")=VWHERE

		set VBNRINFO("DESC")="Customer Limits Review Report"
		set VBNRINFO("PGM")="R01S518"
		set VBNRINFO("RID")="SCA596"
		set VBNRINFO("TABLES")="CIFTYPELIM,CIFGRPLIM,CIF"

		set VBNRINFO("ORDERBY",1)="[SYSDEV,CIFTYPELIM]ACN"
		set VBNRINFO("ORDERBY",2)="[SYSDEV,CIFTYPELIM]75"
		set VBNRINFO("ORDERBY",3)="[SYSDEV,CIFTYPELIM]GRPLIM"
		set VBNRINFO("ORDERBY",4)="[SYSDEV,CIFTYPELIM]SYS"
		set VBNRINFO("ORDERBY",5)="[SYSDEV,CIFTYPELIM]TYPELIM"


		do ^DBSRWBNR(IO,.VBNRINFO)		// Print banner
		}

	// Initialize variables
	set (vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12)=""
	set (VFMQ,vlc,VLC,VOFFLG,VPN,VRG)=0
	set VNEWHDR=1
	set VLOF=""
	set %TIM=$$TIM^%ZM
	set vrundate=%CurrentDate.toString(),vsysdate=%SystemDate.toString()

	do {
		type Number I,J,K
		for I=0:1:5 do {
			set (vh(I),VD(I))=0,vs(I)=1	// Group break flags
			set VT(I)=0			// Group count
			for J=1:1:0 do {
				for K=1:1:3 set VT(I,J,K)=""	// Initialize function stats
				}
			}
		}

	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data
	set vh(0)=0

	// Run report directly
	do VINILAST
	type ResultSet rwrs=Db.select("CIFTYPELIM.ACN,CIFTYPELIM.GRPLIM,CIFTYPELIM.SYS,CIFTYPELIM.TYPELIM,CIF.NAM,CIF.CUSAMT,CIF.CUSDATE,CIFGRPLIM.GRPLIM,CIFGRPLIM.GRPAMT,CIFGRPLIM.GRPDATE,CIFTYPELIM.TYPEAMT,CIFTYPELIM.TYPEDATE","CIFTYPELIM,CIFGRPLIM,CIF","CIF.CUSDATE IS NOT NULL AND CIF.CUSDATE NOT >:date AND (CIF.PLAN IS NOT NULL OR CIF.CUSAMT>0) AND CIFGRPLIM.GRPDATE IS NOT NULL AND CIFGRPLIM.GRPDATE NOT >:date AND CIFTYPELIM.TYPEDATE IS NOT NULL AND CIFTYPELIM.TYPEDATE NOT >:date","CIFTYPELIM.ACN,CIFTYPELIM.GRPLIM,CIFTYPELIM.SYS,CIFTYPELIM.TYPELIM","","DQMODE=1")
	if ER.get() use 0 write $$MSG^%TRMVT(RM.get(),"",1)	// Debug Mode
	if rwrs.isEmpty() do VEXIT(1) quit
	while rwrs.next() do { quit:VFMQ
		type String V,VI
		set V=rwrs.getRow().toString()
		set VI=""
		do VGETDATA(V,VI)
		do VPRINT quit:VFMQ
		do VSAVLAST
		}
	do VEXIT(0)

	quit


VINILAST	// Initialize last access key values
	type Public String vovc1,vovc2,vovc3,vovc4
	set vovc1="",vovc2="",vovc3="",vovc4=""
	quit

VSAVLAST	// Save last access keys values
	type Public String vovc1,vc1,vovc2,vc2,vovc3,vc3,vovc4,vc4
	set vovc1=vc1,vovc2=vc2,vovc3=vc3,vovc4=vc4
	quit


VGETDATA(String V,String VI)	//
	type Public String vc1,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vc10,vc11,vc12
	set vc1=V.piece($C(9),1)			// CIFTYPELIM.ACN
	set vc2=V.piece($C(9),2)			// CIFTYPELIM.GRPLIM
	set vc3=V.piece($C(9),3)			// CIFTYPELIM.SYS
	set vc4=V.piece($C(9),4)			// CIFTYPELIM.TYPELIM
	set vc5=V.piece($C(9),5)			// CIF.NAM
	set vc6=V.piece($C(9),6)			// CIF.CUSAMT
	set vc7=V.piece($C(9),7)			// CIF.CUSDATE
	set vc8=V.piece($C(9),8)			// CIFGRPLIM.GRPLIM
	set vc9=V.piece($C(9),9)			// CIFGRPLIM.GRPAMT
	set vc10=V.piece($C(9),10)			// CIFGRPLIM.GRPDATE
	set vc11=V.piece($C(9),11)			// CIFTYPELIM.TYPEAMT
	set vc12=V.piece($C(9),12)			// CIFTYPELIM.TYPEDATE
	quit

VBRSAVE(Number LINE,String DATA)	// Save for report browser
	type RecordTMPRPTBR tmprptbr=Class.new("RecordTMPRPTBR")
	set tmprptbr.jobno=%ProcessID
	set tmprptbr.lineno=LINE
	set tmprptbr.pageno=0
	set tmprptbr.seq=0
	set tmprptbr.data=DATA
	do tmprptbr.bypassSave()
	quit

VEXIT(NOINFO)	// Exit from report
	type Public Number IOSL,vcrt,VFMQ,vh(),VLC,VPN,VRWOPT,VSTATS()
	type Public String IO,VTBLNAM
	type Number I,PN,vs(),z
	type String VL=""
	set vs(1)=0,vs(2)=0,vs(3)=0,vs(4)=0,vs(5)=0
	if 'VFMQ do VSUM
	if 'vh(0) do VHDG0
	if 'VFMQ do {
		// No information available to display
		if NOINFO=1 set VL=$$^MSG(4655) do VOM
		if vcrt set VL="" for z=VLC+1:1:IOSL do VOM

		if 'VTBLNAM.exists() do {
			set vs(2)=0
			}
		}

	if 'VFMQ,vcrt set PN=-1 do ^DBSRWBR(2)
	if 'VRWOPT("NOCLOSE").get() do CLOSE^SCAIO
	do Db.delete("TMPRPTBR","JOBNO=:%ProcessID")	// Report browser data

	quit

VPRINT	// Print section
	type Public Number VD(),VFMQ,VH0,vh(),VNEWHDR,VR,VRG,VRWOPT,VSEQ
	type Number vskp()

	if VRWOPT("NODTL").get() set vskp(1)=1,vskp(2)=1,vskp(3)=1,vskp(4)=1,vskp(5)=1	// Skip detail
	do VBREAK
	do VSUM quit:VFMQ

	if VH0.get() set vh(0)=0,VNEWHDR=1 kill VH0	// Page Break
	if 'vh(0) do VHDG0 quit:VFMQ
	do VHDG2 quit:VFMQ
	do VHDG4 quit:VFMQ
	do VHDG5 quit:VFMQ
	if 'vskp(5).get() do VDTL5 quit:VFMQ
	do VSTAT
	quit

VBREAK	//
	type Public Number VD(),vh(),VH0,vs(),VT()
	quit:'VT(5)
	type Public String vc1,vovc1,vc2,vovc2,vc3,vovc3,vc4,vovc4
	type Number vb1,vb2,vb3,vb4,vb5
	set (vb1,vb2,vb3,vb4,vb5)=0
	if vb1!(+vovc1'=+vc1) set vs(3)=0,vh(3)=0,VD(1)=0,vb2=1,vb3=1,vb4=1,vb5=1,VH0=1
	if vb3!(vovc2'=vc2) set vs(4)=0,vh(4)=0,VD(3)=0,vb4=1,vb5=1
	if vb4!(vovc3'=vc3) set vs(5)=0,vh(5)=0,VD(4)=0,vb5=1
	quit

VSUM	// Report Group Summary
	type Public Number VFMQ,vs()
	if 'vs(5) set vs(5)=1 do stat^DBSRWUTL(5)
	if 'vs(4) set vs(4)=1 do stat^DBSRWUTL(4)
	if 'vs(3) set vs(3)=1 do stat^DBSRWUTL(3)
	if 'vs(2) set vs(2)=1 do stat^DBSRWUTL(2)
	quit

VSTAT	// Data field statistics
	type Public Number VRWOPT(),VT()
	type Public String VSTATS

	set VT(5)=VT(5)+1
	quit

VHDG2	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,RID,RN,TJD,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,date,type,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:vh(2)  set vh(2)=1				// Print flag
	if VLC+1>IOSL do VHDG0 quit:VFMQ

	set VL=$J(vc1,12)
	set VL=VL_$J("",14-VL.length())_$E(vc5,1,40)
	set VL=VL_$J("",74-VL.length())_$J($FN(vc6,",",2),15)
	set VL=VL_$J("",95-VL.length())_$J($$DAT^%ZM(vc7),10)
	do VOM
	quit

VHDG4	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,RID,RN,TJD,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,date,type,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:vh(4)  set vh(4)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="              "_"Product Group                                           Product Group Limit      Product Group Review Date"
	do VOM
	set VL="              "_$E(vc8,1,12)
	set VL=VL_$J("",74-VL.length())_$J($FN(vc9,",",2),15)
	set VL=VL_$J("",95-VL.length())_$J($$DAT^%ZM(vc10),10)
	do VOM
	quit

VHDG5	// Group Header
	type public String %MSKD,%TIM,CONAM,ER,IOSL,RID,RN,TJD,V,VFMQ,VL,VLC,VNEWHDR,VO,VOFFLG,VPN,VRG,date,type,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vcrt,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	quit:vh(5)  set vh(5)=1				// Print flag
	if VLC+2>IOSL do VHDG0 quit:VFMQ

	set VL="      "_"System:"
	set VL=VL_$J("",14-VL.length())_$E(vc3,1,5)
	do VOM
	set VL="              "_"Product Type   Description                               Product Type Limit      Product Type Review Date"
	do VOM
	quit

VDTL5	// Detail
	type public String %TIM,IOSL,TJD,V,VD(),VFMQ,VL,VLC,VO,VOFFLG,VPN,VRG,VT(),date,type,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,verror,vh(),vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate

	if VLC+1>IOSL do VHDG0 quit:VFMQ  set vh(5)=0 do VHDG5 quit:VFMQ

	set VL="              "_$E(vc4,1,12)
	do VP1 quit:VFMQ!verror.get()  set V=$E(type,1,40)
	set VL=VL_$J("",29-VL.length())_V
	set VL=VL_$J("",74-VL.length())_$J($FN(vc11,",",2),15)
	set VL=VL_$J("",95-VL.length())_$J($$DAT^%ZM(vc12),10)
	do VOM
	quit


VHDG0	// Page Header
	type Public Number ER,IOSL,vcrt,verror,VFMQ,vh(),VLC,VNEWHDR,VPN,VRG,VRWOPT()
	type public String %MSKD,%TIM,CONAM,RID,RN,TJD,VL,date,type,vc1,vc10,vc11,vc12,vc2,vc3,vc4,vc5,vc6,vc7,vc8,vc9,vovc1,vovc2,vovc3,vovc4,vrundate,vsysdate
	type Number PN,V,VO
	if VRWOPT("NOHDR").get() quit			// Skip page header
	set vh(0)=1,VRG=0
	if VL'="" do VOM
	if vcrt,VPN>0 do { quit:VFMQ!'VNEWHDR
		type Number PN,X
		set VL=""
		for X=VLC+1:1:IOSL do VOM
		set PN=VPN
		do ^DBSRWBR(2)
		set VLC=0
		quit:VFMQ
		if VNEWHDR write $$CLEARXY^%TRMVT
		else  set VLC=VLC+8,VPN=VPN+1
		}

	set ER=0,VPN=VPN+1,VLC=0

	set VL="" do VOM
	set VL=$E($G(CONAM),1,45)
	set VL=VL_$J("",100-VL.length())_"Run Date:"
	set VL=VL_$J("",110-VL.length())_$E(vrundate,1,10)
	set VL=VL_$J("",123-VL.length())_$E(%TIM,1,8)
	do VOM
	set VL=RN_"  ("_RID_")"
	set VL=VL_$J("",102-VL.length())_"System:"
	set VL=VL_$J("",110-VL.length())_$E(vsysdate,1,10)
	set VL=VL_$J("",122-VL.length())_"Page:"
	set VL=VL_$J("",128-VL.length())_$J(VPN,3)
	do VOM
	set VL="" do VOM
	set VL="Review Date for Report: "
	set VL=VL_$J("",24-VL.length())_$J($$DAT^%ZM(date),10)
	do VOM
	set VL="" do VOM
	set VL="Customer Nbr  Customer Name                                                Customer Limit      Review Date"
	do VOM
	set VL="===================================================================================================================================="
	do VOM

	set VNEWHDR=0
	if vcrt set PN=VPN do ^DBSRWBR(2,1)		// Lock report page heading

	quit


VOM	// Output print line
	type Public Number AUXPTR,vcrt,vlc,VLC,VRG
	type Public String IO,VL

	use IO

	// Advance to a new page
	if 'VLC,'vcrt do {				// Non-CRT device (form feed)
		if 'AUXPTR.get() write $C(12),!
		else  write $$PRNTFF^%TRMVT,!
		set $Y=1
		}

	if vcrt<2 write VL,!				// Output line buffer
	if vcrt set vlc=vlc+1 do VBRSAVE(vlc,VL)	// Save in BROWSER buffer
	set VLC=VLC+1,VL=""				// Reset line buffer
	quit

	// Pre/post-processors

VP1	// Column pre-processor - Variable: type

	type public String vc4,vc3
	type Public String type
	type String XTYPE,ZSYS

	set XTYPE=vc4
	set ZSYS=vc3

	if ZSYS'="IBS" do {

		type RecordUTBLINTPROD utblintprod=Db.getRecord("UTBLINTPROD","SYS=:ZSYS,TYPE=:XTYPE",1)
		set type=utblintprod.desc
		}
	else  do {

		type RecordPRODCTL prodctl=Db.getRecord("PRODCTL","TYPE=:XTYPE",1)
		set type=prodctl.des
		}
	
	quit
