	if (%ProcessMode = 2) do {
		do cifserv.setMode(2)

		do CIFSRVFL(cifserv,vpar)
	}

	quit

vLITCHK() quit 0 // Table does not have columns involved in literals


vexec // Execute transaction

	type public Number %O
	type public String vpar,vobj(),vx(),vxins()

	type public RecordCIFSERV cifserv

	type String vERRMSG

	if vpar["/VALST/" if '(''Db.isDefined("CIFSERV","ACN = :cifserv.acn and SRVCAT = :cifserv.srvcat and SERVICE = :cifserv.service") = ''%O) set vERRMSG = $$^MSG($select(%O:7932,1:2327)) throw Class.new("Error","%PSL-E-DBSFILER,"_vERRMSG.replace(",","~"))
	if vpar["/VALFK/" do CHKFKS                            // Check foreign keys
	if vpar["/VALRI/" do VFKEYS                            // Foreign key definition

	if vpar'["/NOUPDATE/" do {

		// Allow global reference and M source code
		#ACCEPT DATE=04/22/04; PGM=Dan Russell; CR=20602; GROUP=BYPASS
		#BYPASS
		if $D(vobj(cifserv))  S ^CIF(vobj(cifserv,-3),900,vobj(cifserv,-4),vobj(cifserv,-5))=vobj(cifserv)
		#ENDBYPASS
	}

	quit

