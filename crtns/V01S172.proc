  //user-defined post procs
 //
VP1(RecordCUVAR fCUVAR) //
 type public Boolean ER
 type public String X

 if 'X.isNull() do { quit
	type String CO
	set CO=CUVAR.co

 	type RecordCRCD crcd=Db.getRecord("CRCD","CO=:CO,CRCD=:X",1)

	// Euro Currency cannot be an EMU member currency
 	if crcd.emu do Runtime.setErrMSG("CRCD","3348") quit:ER
	}

 type ResultSet rs=Db.select("EMU","CRCD","EMU=1")

 // EMU member currencies exist. EMU currency must have a value.
 if 'rs.isEmpty() do Runtime.setErrMSG("CUVAR","4003") quit:ER
	
 quit

VP2(RecordCUVAR fCUVAR) //
