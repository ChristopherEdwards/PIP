vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordMEMOECOM memoecom

	if (%O = 2) do vload

	if vobj(memoecom,1).exists() do {

		if memoecom.acctname.length()>25 set vRM = $$^MSG(1076,25) do vdderr("ACCTNAME", vRM) quit
		set X = memoecom.acn if 'X.isNull(),'Db.isDefined("CIF","X") set vRM = $$^MSG(1485,X) do vdderr("ACN", vRM) quit
		if memoecom.identifier.length()>25 set vRM = $$^MSG(1076,25) do vdderr("IDENTIFIER", vRM) quit
		set X = memoecom.memorelc if 'X.isNull(),'Db.isDefined("UTBLRELSRV","X") set vRM = $$^MSG(1485,X) do vdderr("MEMORELC", vRM) quit
		set X = memoecom.odt if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("ODT", vRM) quit
		set X = memoecom.status if 'X.isNull(),'Db.isDefined("STBLESTATUS","X") set vRM = $$^MSG(1485,X) do vdderr("STATUS", vRM) quit
	}
	if memoecom.service.length()>20 set vRM = $$^MSG(1076,20) do vdderr("SERVICE", vRM) quit
