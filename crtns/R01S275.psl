	set RID="SCA265"
	set RN="Commercial Loan Trial Balance by Curr Cd"
	if IO.get()="" set IO=$I

	do INIT^%ZM()

	set %TAB("IO")=$$IO^SCATAB
	set %TAB("VIN1")="|255||[LN]TYPE|[PRODDFTL]:NOVAL||D EXT^DBSQRY||T|Product Type|||||"
	set %TAB("VIN2")="|255||[LN]CRCD|||D EXT^DBSQRY||T|Currency Code|||||"
