VP1(RecordFEEPLN fFEEPLN,RecordFEESRV2 FEESRV2) //
 quit:X="" 
 if 'Db.isDefined("STBLFEECO","X") quit
 if X'=2,(fFEEPLN.PLTP) set ER=1 set RM=$$^MSG(3135) quit       // Usage credit plans only assess on analysis (charge option 2)
 if X'=1 if $G(%IPMODE)'["NOINT" do DELETE^DBSMACRO("[FEESRV2]MAXDLY9","1","0")
 quit
VP2(RecordFEEPLN fFEEPLN,RecordFEESRV2 FEESRV2) //
 if X="" quit 
 if $G(%IPMODE)'["NOINT" do DELETE^DBSMACRO("[FEESRV2]FEESCH9","1","0")
 if $G(%IPMODE)'["NOINT" do DELETE^DBSMACRO("[FEESRV2]FEETBL9","1","0")
