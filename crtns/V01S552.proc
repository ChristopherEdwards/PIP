 if '%ProcessMode do VNEW(.fFEEPLN,.FEESRV2),VPR(.fFEEPLN,.FEESRV2),VDA1(.fFEEPLN,.FEESRV2)
 if %ProcessMode do VLOD(.fFEEPLN,.FEESRV2) quit:$G(ER)  do VPR(.fFEEPLN,.FEESRV2),VDA1(.fFEEPLN,.FEESRV2)

	// ====================  Display Form
	do ^DBSPNT()
	#ACCEPT DATE=11/05/03; PGM=Screen Compiler
	if %ProcessMode=2!(%ProcessMode=3) do ^DBSCRT8A X:'$D(%PAGE) KVAR quit  // Inquiry/Delete
	// ====================  Set up data entry control table


 if %ProcessMode<2 do VTAB(.fFEEPLN,.FEESRV2)
	quit


