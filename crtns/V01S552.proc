	// %O (0-Create  1-Modify  2-Inquiry  3-Delete  4-Print  5-Blank screen)

	set:'$D(%ProcessMode) %ProcessMode=5
 set KVAR="kill %TAB,VFSN,VO,VPTBL,vtab",VSID="SRVPLN2B",VPGM=$T(+0),VSNAME="Service Fee Plan Definition "
 set VFSN("FEEPLN")="zfFEEPLN",VFSN("FEESRV2")="zFEESRV2"
 set vPSL=1
 set KEYS(1)=fFEEPLN.PLAN
 set KEYS(2)=fFEEPLN.FEEDT
