 set %TAB(5)=$C(6,25,2)_"00N12403|1|[CUVAR]EXTVALMAXDAY"
 set %TAB(6)=$C(6,51,2)_"00N12406|1|[CUVAR]EXTVALTYPE|[UTBLEXTYP]"
