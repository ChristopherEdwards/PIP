V01S318( Number %ProcessMode,RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)   // -  - SID= <INTRVWHQ00> International Payments (Headquarters)
 ;;Copyright(c)2007 Sanchez Computer Associates, Inc.  All Rights Reserved - 08/30/2007 14:28 - joynerd
 #WARN ACCESS,MISMATCH OFF
 	/*
 	ORIG: CHENARDP - 02/27/2003
 	DESC: Linked Screen Template

 	---- Comments --------------------------------------------------------
	This procedure is used as the base template for the PSL screen compiler.
	It is referenced by the PSL screen compiler - procedure DBS2PSL4


	 ---- Revision History ------------------------------------------------
	02/18/07 - Pete Chenard - CR 23555
		   Corrected compiler warnings.
		   
	09/24/03 - Pete Chenard - 45497
		   Created linked screen template for screen compiler.
	-----------------------------------------------------------------------

	 */
 
 set ER=0 do VSCRPRE(.EFTREV2,.fEFTPAY,.DEP) if ER quit  // Screen Pre-Processor
 //
 type Public Number ER
 type Public String %PAG,%PAGE,%PG,%PGSV,MULSCR,RM,VFMQ
 type String KVAR,VPG(),vPSL,VSID
 set VSID="INTRVWHQ00"
 set KVAR="kill %A,%TAB,vtab,VFSN,%OLD,%NEW,%FILE,%INDEX,%PAGE,%PG,UX,MULSCR"
 set:'%PG.exists() %PG=1 set %PAG=%PG+3 set %PAGE=$S(%PAGE.exists():%PAGE-1,1:0)+4
 set vPSL=1  //compiled for PSL
 //
 set VPG(%PG+0)="Int'l Payment (In) HQ Screen 1|INTRVWHQ10"
 set VPG(%PG+1)="Int'l Payment (In) HQ Screen 2|INTRVWHQ15"
 set VPG(%PG+2)="Int'l Payment (In) HQ Screen 2|INTRVWHQ20"
 set VPG(%PG+3)="Int'l Payment (In) HQ Screen 4|INTRVWHQ40"
	set:'%PG.exists() %PG=1 set %PGSV=%PG,MULSCR=""
 if '%ProcessMode do VNEW(.EFTREV2,.fEFTPAY,.DEP)
 if %ProcessMode do VLOD(.EFTREV2,.fEFTPAY,.DEP) if $G(ER) set VFMQ="Q" quit
 do VPG(.EFTREV2,.fEFTPAY,.DEP)
 quit

VNEW(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 
 quit
 //
 ;
VLOD(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // User defined access section
	quit

VPG(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 type Public Number %PG,%PGSV
 type Number vDONE
 set vDONE=0
 for  do { quit:vDONE
  if %PG=(%PGSV+0) do VPG1(.EFTREV2,.fEFTPAY,.DEP),VPG0(.EFTREV2,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+1) do VPG2(.EFTREV2,.fEFTPAY,.DEP),VPG0(.EFTREV2,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+2) do VPG3(.EFTREV2,.fEFTPAY,.DEP),VPG0(.EFTREV2,.fEFTPAY,.DEP) quit:vDONE
  if %PG=(%PGSV+3) do VPG4(.EFTREV2,.fEFTPAY,.DEP),VPG0(.EFTREV2,.fEFTPAY,.DEP) quit:vDONE
  }
 quit

VPG0(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)
 type Public String vDONE,VFMQ,VPG()
 type Public Number %PAG,%PAGE,%PG,%PGSV
 type String %LINK
 set %LINK=""
 if %ProcessMode=2!(%ProcessMode=3)!(%ProcessMode=4) do VBTM(.EFTREV2,.fEFTPAY,.DEP) if VFMQ.get()="D" set vDONE=1 quit
 if '%PAGE.exists() set vDONE=1 quit
 if %PG'<%PAG kill %PAG,%PGSV,VPG set vDONE=1 quit
 set %PG=%PG+1
 quit

 
VPG1(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Int'l Payment (In) HQ Screen 1
 type Public String %MODS,%REPEAT,ET,SID,VPGM,VPTBL
 type String DFID,PGM
 set SID="INTRVWHQ10",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S322
 if %ProcessMode>1 quit
 do VTAB^V01S322(.fEFTPAY)
 quit
 
