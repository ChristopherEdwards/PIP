VBTM(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)
	type Public String IO
	if %ProcessMode=4,IO'=$P quit
	do ^DBSCRT8A
	quit
VSCRPRE(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP) // Screen Pre-Processor
 new %TAB,vtab  // Disable .MACRO. references to %TAB()
 //
 type String BENACN,BENGL
 type public Number CID,ogl
 type public String GL
 
 if 'ogl set BENACN=CID.get()
 else  set BENGL=GL.get()
 
 quit
 quit
