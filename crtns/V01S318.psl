VPG4(RecordEFTREV2 EFTREV2,RecordEFTPAY fEFTPAY,RecordDEP DEP)  // Int'l Payment (In) HQ Screen 4
 type Public String %MODS,%REPEAT,ET,SID,VPGM,VPTBL
 type String DFID,PGM
 set SID="INTRVWHQ40",DFID="EFTPAY"
 do ^USID if PGM="" set ET="INVLDSCR" do ^UTLERR Q
 set %MODS=1 I '%REPEAT.exists() set %REPEAT=8
 kill VPTBL
 set VPGM=PGM
 do VREPRNT^V01S324
