vddver // Validate data dictionary attributes

	type public Number %O
	type public String vpar,vx()

	type String vRM,X
	type public RecordSBREJTRN sbrejtrn

	set X = sbrejtrn.spd if 'X.isNull(),X'?1.5N set vRM=$$^MSG(742,"D") do vdderr("SPD", vRM) quit
	set X = sbrejtrn.brcd if 'X.isNull(),'Db.isDefined("UTBLBRCD","X") set vRM = $$^MSG(1485,X) do vdderr("BRCD", vRM) quit
	if sbrejtrn.uid.length()>20 set vRM = $$^MSG(1076,20) do vdderr("UID", vRM) quit
	set X = sbrejtrn.tseq if 'X.isNull(),X'?1.6N,X'?1"-"1.5N set vRM=$$^MSG(742,"N") do vdderr("TSEQ", vRM) quit
	if sbrejtrn.msgid.length()>40 set vRM = $$^MSG(1076,40) do vdderr("MSGID", vRM) quit
	if sbrejtrn.etc.length()>12 set vRM = $$^MSG(1076,12) do vdderr("ETC", vRM) quit
	set X = sbrejtrn.isobin if 'X.isNull(),'Db.isDefined("UTBLSBCU","X") set vRM = $$^MSG(1485,X) do vdderr("ISOBIN", vRM) quit
	if sbrejtrn.rspcd.length()>3 set vRM = $$^MSG(1076,3) do vdderr("RSPCD", vRM) quit
	if sbrejtrn.rspdes.length()>60 set vRM = $$^MSG(1076,60) do vdderr("RSPDES", vRM) quit
	if sbrejtrn.srccid.length()>28 set vRM = $$^MSG(1076,28) do vdderr("SRCCID", vRM) quit
	set X = sbrejtrn.tamt if 'X.isNull() set vRM = $$VAL^DBSVER("$",18,0,,,,,2) if 'vRM.isNull() set vRM = $$^MSG(979,"SBREJTRN.TAMT"_" "_vRM) throw Class.new("Error","%PSL-E-DBFILER,"_vRM.replace(",","~"))
	if sbrejtrn.tocid.length()>28 set vRM = $$^MSG(1076,28) do vdderr("TOCID", vRM) quit
	if sbrejtrn.trntm.length()>8 set vRM = $$^MSG(1076,8) do vdderr("TRNTM", vRM) quit
	if sbrejtrn.trntyp.length()>40 set vRM = $$^MSG(1076,40) do vdderr("TRNTYP", vRM) quit
