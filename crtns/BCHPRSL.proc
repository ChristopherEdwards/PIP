vEXEC(vCONTEXT,TJD,CLS,GRP,TYP,CID) //
	/*
	   Collate through DTJNA for the date specified(TJD-30) 
 	   and change any CIF having values for CIF.SOL and CIF.PRIV
           from 0 to 1. Skip records that are 1 or 2. 
        */   
        
	type Boolean FLAG
	
	set FLAG=0
	
	type RecordCIF cif=Db.getRecord("CIF","ACN=:CID")
	
	do cif.setAuditFlag(1)	
	
	// PRIV column will be updated
	if cif.priv=0 do {
		set cif.priv=1
		set FLAG=1
		}
		
	// SOL column will be updated
	if cif.sol=0 do {
		set cif.sol=1
		set FLAG=1
		}	
	
	if FLAG do cif.save()
	
	quit	
	 		
