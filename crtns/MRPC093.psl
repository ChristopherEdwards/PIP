MRPC093(String return,Number versn,Number cidpar,String qrypar,Date efdpar,Number cnvpar)	//Public; Euro Account Conversion MRPC
	/*
	   ORIG: JERUCHIMC - 08/12/00
	   DESC: Converts accounts from an IN currency to EUR.

	   INPUTS:
	   	 . cidpar - account number
	     	 . qrypar - query
	   	 . efdpar - effective date
	   	 . cnvpar - conversion type

	   RETURNS:
	   	 empty string


	----- Revision History -----------------------------------------------
      
        07/26/05 - SkariahV- CR16679
	           Removed #WARN directive.
	
	03/18/03 - GRAY - 51351
		   Converted to PSL.

	02/25/01 - SCHWARTZC - 42104
	   	   Added version check.  Added a quit to return the error
	  	   message if one occurs when creating the dayend entries.

	*/

	// Version number of client message is not compatible with server
	if versn.get()'=1 quit $$ERRMSG^PBSUTL($$^MSG(2951))
	
	type Boolean MEURCNV
	type Number CID
	type Date %EffectiveDate
	type Public Number ER
	type Public String RM

	set %EffectiveDate=$$^SCAJD(efdpar)
	set CID=cidpar
	set MEURCNV=1

	// Converts single account
	if cnvpar=1 do CNVSING2^EURPRMT

	// Reverses single account
	if cnvpar=2 do RVSINAC2^EURPRMT

	// Delete request for single account conversion
	if cnvpar=3 do DLREQAC2^EURPRMT

	// Delete request to reverse single account conversion
	if cnvpar=4 do DLRQRVA2^EURPRMT

	if ER.get() quit $$ERRMSG^PBSUTL(RM.get())

	quit ""
 #OPTION ResultClass ON
Public String vSIG()	quit "60107^17765^Viji Skariah^1226"	// Signature - LTD^TIME^USER^SIZE
